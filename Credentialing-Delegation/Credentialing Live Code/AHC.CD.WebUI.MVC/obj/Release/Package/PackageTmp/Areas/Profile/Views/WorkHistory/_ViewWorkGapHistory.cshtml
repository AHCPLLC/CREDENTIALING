@model AHC.CD.WebUI.MVC.Areas.Profile.Models.WorkHistory.WorkGapViewModel

<fieldset class="wrap-words">
    <legend>
        Work Gap Information
    </legend>
    <div class="col-lg-12" style="word-wrap: break-word;">
        <div class="col-md-12">
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.StartDate, new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{wgHistory.StartDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.EndDate, new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{wgHistory.EndDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.Description, new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{wgHistory.Description || 'Not Available'}}</i>
                </div>
            </div>
        </div>

        <div class="clearfix"></div>
        <br />

        <div class="col-md-12 form-group">
            <div class="col-md-12 form-group">
                <button id="WorkGap_History_CancelButton" type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
            </div>
        </div>
    </div>
</fieldset>
