<fieldset>
    <legend>
        Sub Plan
    </legend>
    <div ng-repeat="lobsub in plob.SubPlans track by $index" class="col-lg-12 zero-padding-right-right-div">
        <fieldset>
            <div class="col-lg-4 form-group">
                <label class="control-label small" for="SubPlanName">SubPlanName:</label>
                <div>
                    <i class="data-label  wrap-words">{{lobsub.SubPlanName || 'Not available' }}</i>
                </div>
            </div>
            <div class="col-md-4 form-group">
                <div class="col-lg-4 form-group">
                    <label class="control-label small" for="SubPlanCode">SubPlanCode:</label>
                    <div>
                        <i class="data-label  wrap-words">{{lobsub.SubPlanCode || 'Not available'}}</i>
                    </div>

                </div>
            </div>
            <div class="col-md-4 form-group">
                <div class="col-lg-4 form-group">
                    <label class="control-label small" for="SubPlanDescription">SubPlanDescription:</label>
                    <div>
                        <i class="data-label  wrap-words">{{lobsub.SubPlanDescription || 'Not available'}}</i>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
</fieldset>


