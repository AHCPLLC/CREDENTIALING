<script>
    var showAttachMore = function () {
        $("#ATTACHmoreReCred").show();
        $("#SubmitDoneReCred").hide();
        $("#AddDoneReCred").hide();

    };
    var showSubmit = function () {
        $("#ATTACHmoreReCred").hide();
        $("#SubmitDoneReCred").show();
        $("#AddDoneReCred").hide();

    };
    var showAdd = function () {
        $("#ATTACHmoreReCred").hide();
        $("#SubmitDoneReCred").hide();
        $("#AddDoneReCred").show();
    };
    var cancelAdd = function () {
        $("#AddDoneReCred").hide();
        $("#SubmitDoneReCred").hide();
        $("#ATTACHmoreReCred").hide();
    };
</script>