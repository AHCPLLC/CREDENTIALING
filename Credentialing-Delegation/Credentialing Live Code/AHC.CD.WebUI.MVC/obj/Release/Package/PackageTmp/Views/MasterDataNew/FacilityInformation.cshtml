@using Microsoft.AspNet.Identity;
@using Microsoft.AspNet.Identity.Owin;

@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.PracticeLocationViewModel
@{
    ViewBag.Title = "CredentialingContact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section AddToHead{
    @Styles.Render("~/Content/profileCss")

    @Scripts.Render("~/bundles/jquery")

    @Scripts.Render("~/bundles/angular")
    <link href="~/Content/SharedCss/app.css" rel="stylesheet" />
    <script src="~/Scripts/Lib/Jquery/jquery-1.11.0.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/Lib/Angular/angular.min.js"></script>
    <script src="~/Scripts/Shared/Data/countryDialCodes.js"></script>
    <script src="~/Scripts/Lib/AHC_Lib/UtilDirectivesLib.js"></script>
    @*<script src="~/Scripts/Lib/Angular-DatePicker/ui-bootstrap-tpls-0.12.0.min.js"></script>*@

    <script src="~/Scripts/Lib/Angular/ui-bootstrap-tpls-0.13.0.min.js"></script>
    <script src="~/Scripts/Lib/AHC_Lib/AutoCompleteLib.js"></script>
    <script src="~/Scripts/Custom/MasterDataNew/FacilityInformation.js"></script>
    <script src="~/Scripts/Shared/Data/Languages.js"></script>
    <script src="~/Scripts/Lib/Angular/timepickerpop.js"></script>

}
<div id="page-wrapper">
    <div class="container-fluid wrap-words">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li>
                        <a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/MasterDataNew"><i class="fa fa-database"></i> Master Data</a>
                    </li>
                    <li>
                        <i class="fa fa-list"></i> Facility Information
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
        <!-- Add Provider content form div -->
        <div class="row" ng-app="FacilityInformationApp" ng-controller="FacilityInformationController" ng-cloak>
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Facility Information Management
                        <span class="pull-right">
                            @if (HttpContext.Current.User.Identity.IsAuthenticated)
                            {
                                var roles = Context.GetOwinContext().GetUserManager<AHC.CD.WebUI.MVC.ApplicationUserManager>().GetRoles(User.Identity.GetUserId());
                                if (roles.Contains("CRA"))
                                {
                                    <button ng-click="operateAddControlPracLoc('addPracticeLocationNew');resetPracticeDaysList();operateMidlevelAddControl('add')" class=" btn btn-xs btn-primary" ng-disabled="AddMode || ViewMode || EditMode"><i class="fa fa-plus"></i> Add</button>
                                }
                            }
                        </span>
                    </div>
                    <div class="panel-body">
                        <div ng-show="activeMessageDiv=='facility'" class="alert alert-success" role="alert">
                            @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
                            {{messageDesc}}
                        </div>
                        <div ng-show="activeMessageDiv=='facilityDataErrorMsg'" class="alert alert-danger" role="alert">
                            @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
                            {{messageDesc}}
                        </div>

                        @* List View *@
                        <div ng-show="ListMode && !IsLoadingData">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th class="col-lg-2">Facility Name</th>
                                        <th class="col-lg-2">Corporate Or Practice Name</th>
                                        <th class="col-lg-2">Street / P.O Box No</th>
                                        <th class="col-lg-2">Suite/Building</th>
                                        <th class="col-lg-1">City</th>
                                        <th class="col-lg-2">State</th>
                                        <th class="col-lg-1">Action</th>
                                    </tr>
                                    <tr ng-show="FaciltyInformations.length>0">
                                        <td><input type="text" placeholder="Filter Facility Name" class="form-control input-sm" ng-model="search.FacilityName" ng-change="filterData()" /></td>
                                        <td><input type="text" placeholder="Filter Corporate Or Practice Name" class="form-control input-sm" ng-model="search.Name" ng-change="filterData()" /></td>
                                        <td><input type="text" placeholder="Filter Street / P.O Box No" class="form-control input-sm" ng-model="search.Street" ng-change="filterData()" /></td>
                                        <td><input type="text" placeholder="Filter Suite / Building" class="form-control input-sm" ng-model="search.Building" ng-change="filterData()" /></td>
                                        <td><input type="text" placeholder="Filter City" class="form-control input-sm" ng-model="search.City" ng-change="filterData()" /></td>
                                        <td><input type="text" placeholder="Filter State" class="form-control input-sm" ng-model="search.State" ng-change="filterData()" /></td>
                                        <td></td>
                                    </tr>
                                </thead>
                                <tbody ng-show="FaciltyInformations.length>0">
                                    <tr ng-repeat="Facility in FaciltyInformationsData=(FaciltyInformations | filter:{FacilityName:search.FacilityName,Name:search.Name,Street:search.Street,Building:search.Building,City:search.City,State:search.State}) track by $index" ng-show="IsValidIndex($index)">
                                        <td>{{Facility.FacilityName||'Not Available'}}</td>
                                        <td>{{Facility.Name}}</td>
                                        <td>{{Facility.Street}}</td>
                                        <td>{{Facility.Building||'Not Available'}}</td>
                                        <td>{{Facility.City}}</td>
                                        <td>{{Facility.State}}</td>
                                        <td>
                                            <a class="btn btn-default btn-xs" ng-click="viewFacilityInformation(Facility)" tooltip title="View"><i class="fa fa-eye"></i></a>
                                            @if (HttpContext.Current.User.Identity.IsAuthenticated)
                                            {
                                                var roles = Context.GetOwinContext().GetUserManager<AHC.CD.WebUI.MVC.ApplicationUserManager>().GetRoles(User.Identity.GetUserId());
                                                if (roles.Contains("CRA"))
                                                {
                                                    <a class="btn btn-primary btn-xs" ng-click="editFacilityInformation(Facility,$index)" tooltip title="Edit"><i class="fa fa-edit"></i></a>
                                                }
                                            }

                                        </td>
                                    </tr>
                                </tbody>
                                <tbody ng-show="FaciltyInformationsData.length==0 || FaciltyInformations.length==0 ">
                                    <tr class="text-center">
                                        <td colspan="7"><b>No Data Available</b></td>
                                    </tr>
                                </tbody>
                            </table>

                            <div class="clearfix"></div>
                            <div ng-show="activeMessageDiv=='facilityDataErrorMsg'" class="alert alert-danger" role="alert">
                                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                                <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
                            </div>
                            <div ng-show="activeMessageDiv=='addedNewFacility'" class="alert alert-success" role="alert">
                                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                                <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
                            </div>
                            <div class="clearfix"></div>
                            <div ng-if="CurrentPage.length > 0" ccolicensedetailstable></div>
                            <pagination ng-show="CurrentPage.length > 0" total-items="bigTotalItems" ng-model="bigCurrentPage" max-size="maxSize" class="pagination-sm" ng-change="pageChanged(bigCurrentPage)" boundary-links="true" rotate="false" num-pages="numPages" style="margin:0;"></pagination>
                        </div>
                        <div ng-show="IsLoadingData" class="col-lg-12">
                            <div class="col-lg-offset-4">
                                <div>Loading Data....!!!</div>
                                <div id="loadingid" style="padding-left:20%"><img style="padding-top:10px; display:block; margin-left:-80px;" src="~/Areas/Credentialing/img/loading.GIF" /></div>
                            </div>
                        </div>
                        @* Add/Edit view *@
                        <form id="newFacilityDataForm_New">
                            <div id="GeneralInformation" ng-show="AddMode || EditMode" class="form-style">
                                <fieldset>
                                    <legend>
                                        Facility Information
                                    </legend>
                                    <div class="legend2 pull-right">
                                        <button ng-click="Cancel()" class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="top" title="close"><i class="fa fa-close"></i></button>
                                    </div>
                                    <fieldset>
                                        <legend>General Information</legend>

                                        <div class="col-md-12">
                                            @Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
                                            <span ng-if="tempObject.FacilityID">
                                                @Html.HiddenFor(model => model.FacilityID, new { id = "FacilityID", ng_value = "tempObject.FacilityID", data_ng_model = "tempObject.FacilityID" })
                                            </span>
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.FacilityName, htmlAttributes: new { @class = "control-label small" })
                                            @Html.EditorFor(model => model.FacilityName, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.FacilityName", placeholder = "Facility Name" } })
                                            @Html.ValidationMessageFor(model => model.FacilityName, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.Name, htmlAttributes: new { @class = "control-label small" })
                                            @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.Name", placeholder = "Corporate Name" } })
                                            @Html.ValidationMessageFor(model => model.Name, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.Street, htmlAttributes: new { @class = "control-label small" })
                                            @Html.EditorFor(model => model.Street, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.Street", placeholder = "street address" } })
                                            @Html.ValidationMessageFor(model => model.Street, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.Building, htmlAttributes: new { @class = "control-label small" })
                                            @Html.EditorFor(model => model.Building, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.Building", placeholder = "apartment/unit #" } })
                                            @Html.ValidationMessageFor(model => model.Building, "", new { @class = "text-danger" })
                                        </div>

                                        <div class="col-md-12">
                                            <div class="form-group col-md-3">
                                                @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
                                                @Html.TextBoxFor(model => model.City, new { @class = "form-control input-sm", data_ng_model = "tempObject.City", placeholder = "City", data_ng_change = "addressAutocomplete(tempObject.City)", onfocus = "showLocationList(this)" })
                                                <div class="ProviderTypeSelectAutoList popover fade bottom in" role="tooltip">
                                                    <table class="table table-striped table-bordered table-hover table-condensed marginBottomAutosearch">
                                                        <tbody>
                                                            <tr ng-repeat="location in Locations track by $index" ng-click="selectedLocation(location)" class="pointer">
                                                                <td>{{location.City}} - {{location.State}} - {{location.CountryCode}}</td>
                                                            </tr>
                                                            <tr ng-if="Locations.length == 0">
                                                                <td>Not Available</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                                @Html.ValidationMessageFor(model => model.City, "", new { @class = "text-danger" })
                                            </div>
                                            <div class="col-lg-3 form-group">
                                                @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
                                                @Html.EditorFor(model => model.State, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.State", placeholder = "state name" } })
                                                @Html.ValidationMessageFor(model => model.State, "", new { @class = "text-danger" })
                                            </div>
                                            <div class="form-group col-md-3">
                                                @Html.LabelFor(model => model.ZipCode, htmlAttributes: new { @class = "control-label small" })
                                                @Html.EditorFor(model => model.ZipCode, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.ZipCode", placeholder = "zip code" } })
                                                @Html.ValidationMessageFor(model => model.ZipCode, "", new { @class = "text-danger" })
                                            </div>
                                            <div class="col-lg-3 form-group" ng-init="tempObject.Country = 'United States'">
                                                @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label small" })
                                                @Html.EditorFor(model => model.Country, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.Country", placeholder = "Country name" } })
                                                @Html.ValidationMessageFor(model => model.Country, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="col-lg-3 form-group">
                                                @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
                                                @Html.EditorFor(model => model.County, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.County", placeholder = "County name" } })
                                                @Html.ValidationMessageFor(model => model.County, "", new { @class = "text-danger" })
                                            </div>
                                            <div class="form-group col-md-3">
                                                @Html.LabelFor(model => model.Telephone, htmlAttributes: new { @class = "control-label small" })
                                                <div>
                                                    <span ng-if="!tempObject.CountryCodeTelephone" ng-init="tempObject.CountryCodeTelephone='+1'"></span>
                                                    <div class="input-group">
                                                        <div class="input-group-btn">
                                                            <button type="button" class="btn btn-default countryCodeClass" tooltip data-toggle="tooltip" data-placement="top" title="Select Country Code" ng-click="showContryCodeDiv('TelephoneCountryCode'+$index)"> {{tempObject.CountryCodeTelephone}} <span class="caret"></span></button>
                                                        </div>
                                                        <div class="countryDailCodeContainer popover fade bottom in" role="tooltip" id='TelephoneCountryCode{{$index}}'>
                                                            <div class="arrow"></div>
                                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                                            <div class="popover-content">
                                                                <select ng-model="tempObject.CountryCodeTelephone" name="CountryCodeTelephone">
                                                                    <option ng-repeat="code in CountryDialCodes" value="{{code.dial_code}}" ng-selected="code.dial_code == tempObject.CountryCodeTelephone">{{code.name}}({{code.dial_code}})</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        @Html.EditorFor(model => model.Telephone, new { htmlAttributes = new { @class = "form-control", data_val = "true", data_val_required = "Please enter Telephone Number", data_ng_model = "tempObject.Telephone" } })
                                                    </div>
                                                    @Html.ValidationMessageFor(model => model.CountryCodeTelephone, "", new { @class = "text-danger" })
                                                    @Html.ValidationMessageFor(model => model.Telephone, "", new { @class = "text-danger" })
                                                </div>
                                            </div>
                                            <div class="form-group col-md-3">
                                                @Html.LabelFor(model => model.Fax, htmlAttributes: new { @class = "control-label small" })
                                                <div>
                                                    <span ng-if="!tempObject.CountryCodeFax" ng-init="tempObject.CountryCodeFax='+1'"></span>
                                                    <div class="input-group">
                                                        <div class="input-group-btn">
                                                            <button type="button" class="btn btn-default countryCodeClass" tooltip data-toggle="tooltip" data-placement="top" title="Select Country Code" ng-click="showContryCodeDiv('CountryCodeFax'+$index)"> {{tempObject.CountryCodeFax}} <span class="caret"></span></button>
                                                        </div>
                                                        <div class="countryDailCodeContainer popover fade bottom in" role="tooltip" id='CountryCodeFax{{$index}}'>
                                                            <div class="arrow"></div>
                                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                                            <div class="popover-content">
                                                                <select ng-model="tempObject.CountryCodeFax" name="CountryCodeFax">
                                                                    <option ng-repeat="code in CountryDialCodes" value="{{code.dial_code}}" ng-selected="code.dial_code == tempObject.CountryCodeFax">{{code.name}}({{code.dial_code}})</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        @Html.EditorFor(model => model.Fax, new { htmlAttributes = new { @class = "form-control", data_val = "true", data_val_required = "Please enter Fax Number", data_ng_model = "tempObject.Fax" } })
                                                    </div>
                                                    @Html.ValidationMessageFor(model => model.CountryCodeFax, "", new { @class = "text-danger" })
                                                    @Html.ValidationMessageFor(model => model.Fax, "", new { @class = "text-danger" })
                                                </div>
                                            </div>
                                            <div class="form-group col-md-3">
                                                @Html.LabelFor(model => model.EmailAddress, htmlAttributes: new { @class = "control-label small" })
                                                @Html.EditorFor(model => model.EmailAddress, new { htmlAttributes = new { @class = "form-control input-sm", data_ng_model = "tempObject.EmailAddress", placeholder = "Office email address" } })
                                                @Html.ValidationMessageFor(model => model.EmailAddress, "", new { @class = "text-danger" })
                                            </div>
                                        </div>


                                    </fieldset>
                                    <fieldset>
                                        <legend>
                                            Accessibilities
                                        </legend>
                                        <div id="newPracLocAccessibilitiesFormDiv">
                                            <div class="col-lg-12 zero-padding-left-right-div">
                                                <div class="col-md-12">
                                                    @Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
                                                    <span ng-if="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityID">
                                                        @Html.HiddenFor(model => model.FacilityDetail.Accessibility.FacilityAccessibilityID, new { id = "FacilityAccessibilityID", ng_value = "tempObject.FacilityDetail.Accessibility.FacilityAccessibilityID", data_ng_model = "tempObject.FacilityDetail.Accessibility.FacilityAccessibilityID" })
                                                    </span>
                                                </div>
                                                <div class="clearfix"></div>
                                                <div class="form-group zero-padding-left-right-div">
                                                    <div class="col-md-12">
                                                        <div class="col-md-6 ">
                                                            <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionId" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionId" value="{{masterAccessibilityQuestions[0].FacilityAccessibilityQuestionId}}" />
                                                            <span ng-if="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionAnswerID">
                                                                <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionAnswerID" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionAnswerID" value="{{tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].FacilityAccessibilityQuestionAnswerID}}" />
                                                            </span>
                                                            <label class="control-label small">{{masterAccessibilityQuestions[0].ShortTitle}}</label>
                                                            <div>
                                                                <label class="radio-inline">
                                                                    <input type="radio" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption" data-val="true" data-val-required="Please select whether this meets ADA accessibility requirements?" value=1 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption"> Yes
                                                                </label>
                                                                <label class="radio-inline">
                                                                    <input type="radio" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption" data-val="true" data-val-required="Please select whether this meets ADA accessibility requirements?" value=2 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption"> No
                                                                </label>
                                                            </div>
                                                            <span class="text-danger field-validation-error" data-valmsg-for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption" data-valmsg-replace="true">
                                                                <span for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].AnswerYesNoOption" class=""></span>
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    <br />
                                                    <hr class="zero-margin-top-bottom-hr">
                                                    <div class="clearfix"></div>
                                                    <br />
                                                    <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <label class="control-label small">{{masterAccessibilityQuestions[1].Title}}</label>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionId" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionId" value="{{masterAccessibilityQuestions[1].FacilityAccessibilityQuestionId}}" />
                                                            <span ng-if="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionAnswerID">
                                                                <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionAnswerID" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionAnswerID" value="{{tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].FacilityAccessibilityQuestionAnswerID}}" />
                                                            </span>
                                                            <label class="control-label small">{{masterAccessibilityQuestions[1].ShortTitle}}</label>
                                                            <div>
                                                                <label class="radio-inline">
                                                                    <input type="radio" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption" data-val="true" data-val-required="Please Select Whether This Site Offers Handicapped Access For Building" value=1 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption"> Yes
                                                                </label>
                                                                <label class="radio-inline">
                                                                    <input type="radio" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption" data-val="true" data-val-required="Please Select Whether This Site Offers Handicapped Access For Building" value=2 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption"> No
                                                                </label>
                                                            </div>
                                                            <span class="text-danger field-validation-error" data-valmsg-for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption" data-valmsg-replace="true">
                                                                <span for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].AnswerYesNoOption" class=""></span>
                                                            </span>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionId" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionId" value="{{masterAccessibilityQuestions[2].FacilityAccessibilityQuestionId}}" />
                                                            <span ng-if="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionAnswerID">
                                                                <input type="hidden" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionAnswerID" ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionAnswerID" value="{{tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].FacilityAccessibilityQuestionAnswerID}}" />
                                                            </span>
                                                            <label class="control-label small">{{masterAccessibilityQuestions[2].ShortTitle}}</label>
                                                            <div>
                                                                <label class="radio-inline">
                                                                    <input type="radio" data-val="true" data-val-required="Please Select Whether This Site Offers Handicapped Access For Parking" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption" value=1 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption"> Yes
                                                                </label>
                                                                <label class="radio-inline">
                                                                    <input type="radio" data-val="true" data-val-required="Please Select Whether This Site Offers Handicapped Access For Parking" name="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption" value=2 data-ng-model="tempObject.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption"> No
                                                                </label>
                                                            </div>
                                                            <span class="text-danger field-validation-error" data-valmsg-for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption" data-valmsg-replace="true">
                                                                <span for="FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].AnswerYesNoOption" class=""></span>
                                                            </span>
                                                        </div>
                                                        <div class="col-md-3">
