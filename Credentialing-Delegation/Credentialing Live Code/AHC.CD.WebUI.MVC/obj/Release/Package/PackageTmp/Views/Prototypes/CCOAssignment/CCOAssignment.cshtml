<style>
    .select-page {
        width: 50px;
        text-align: center;
    }

    .theadColor {
        color: white;
        background-color: rgba(66, 123, 225, 0.78);
    }
</style>

<div id="page-wrapper" ng-app="ccoassingmentApp">
    <toaster-container toaster-options="{'time-out': 2000,'position-class': 'toast-top-right','close-button':true}"></toaster-container>
    <div class="container-fluid" ng-controller="ccoAssignmentCtrl as pa">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="fa fa-dashboard"></i>  <a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Dashboard/Index">Dashboard</a>
                    </li>
                    <li class="active">
                        <i class="fa fa-search"></i> CCO/TL Assignment
                    </li>
                </ol>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12" style="padding:0px">
                @Html.Partial("~/Views/Prototypes/CCOAssignment/_CCOAssignment.cshtml")
            </div>
        </div>


    </div>

</div>

