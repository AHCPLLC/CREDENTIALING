<div class="form-style" id="MidLevelPractitionerViewDiv">
    <fieldset>
        <legend>
            Supervising Practitioner
        </legend>
        <div class="form-group zero-padding-left-right-div">
            <div class="col-md-12 form-group zero-padding-left-right-div">
                <div class="col-md-3 wrap-words">
                    <div>
                        <label class="control-label small">First Name</label>
                    </div>
                    <i>{{practitioner.FirstName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3 wrap-words">
                    <div>
                        <label class="control-label small">Middle Name</label>
                    </div>
                    <i>{{practitioner.MiddleName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3 wrap-words">
                    <div>
                        <label class="control-label small">Last Name</label>
                    </div>
                    <i>{{practitioner.LastName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3 wrap-words">
                    <div>
                        <label class="control-label small">NPI Number</label>
                    </div>
                    <i>{{practitioner.NPINumber || 'Not Availabe'}}</i>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div ng-show="activeMessageDiv=='midlevelPractionersEditSuccessMsg'" class="alert alert-success" role="alert">
                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                {{messageDesc}}
            </div>
            <div class="col-md-12 form-group">
                <button class="btn btn-warning btn-sm" ng-click="operateThirdCancelControl('MidLevelPractitionerViewDiv')">Cancel</button>
                @*<button class="btn btn-sm btn-primary pull-right" ng-click="operateThirdEditControl('MidLevelPractitionerEdit'+$index, practitioner)">Edit</button>*@
            </div>
        </div>
    </fieldset>
</div>