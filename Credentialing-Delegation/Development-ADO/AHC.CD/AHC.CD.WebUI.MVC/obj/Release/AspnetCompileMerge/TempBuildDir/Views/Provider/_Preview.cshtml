<div class="form-style">
    <fieldset>
        <legend>
            Basic Information
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.NPINumber, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{Profile.OtherIdentificationNumber.NPINumber}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.NPIUserName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{Profile.OtherIdentificationNumber.NPIUserName}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.NPIPassword, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i ng-if="Profile.OtherIdentificationNumber.NPIPassword" class="data-label">**********</i>
                </div>
            </div>
        </div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.CAQHNumber, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{Profile.OtherIdentificationNumber.CAQHNumber}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.CAQHUserName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{Profile.OtherIdentificationNumber.CAQHUserName}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.OtherIdentificationNumber.CAQHPassword, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i ng-if="Profile.OtherIdentificationNumber.CAQHPassword" class="data-label">***********</i>
                </div>
            </div>
        </div>
    </fieldset>
    <fieldset>
        <legend>
            Personal Information
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-2 form-group">
                @Html.LabelFor(model => model.PersonalDetail.SalutationType, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{SelectedSalutation}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.PersonalDetail.FirstName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{PersonalDetail.FirstName}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.PersonalDetail.MiddleName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{PersonalDetail.MiddleName}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.PersonalDetail.LastName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{PersonalDetail.LastName}}</i>
                </div>
            </div>
            <div class="col-lg-1 form-group" style="padding-left:0px;">
                @Html.LabelFor(model => model.PersonalDetail.Suffix, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{PersonalDetail.Suffix}}</i>
                </div>
            </div>
        </div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-2 form-group">
                @Html.LabelFor(model => model.PersonalDetail.GenderType, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i ng-if="PersonalDetail.GenderType==1" class="data-label">Male</i>
                    <i ng-if="PersonalDetail.GenderType==2" class="data-label">Female</i>
                </div>
            </div>
            <div class="col-lg-10">
                <div class="form-group">
                    <label class="control-label small">Titles *</label>
                    <div>
                        <span ng-repeat="tpt in PersonalDetail.ProviderTitles track by $index">
                            <span class="label label-default">
                                {{tpt.ProviderType.Title}}
                            </span>
                            &nbsp;&nbsp;
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
    <fieldset>
        <legend>
            Home Contact Information
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.Street, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.Street}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.UnitNumber, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.UnitNumber}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.City, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.City}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.State, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.State}}</i>
                </div>
            </div>
        </div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.ZipCode, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.ZipCode}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.Country, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.Country}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.HomeAddress.County, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{HomeAddress.County}}</i>
                </div>
            </div>
        </div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.ContactDetail.EmailIDs[0].EmailAddress, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{ContactDetail.EmailAddress}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.ContactDetail.PhoneDetails[0].Number, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{ContactDetail.CountryCode}}-{{ContactDetail.MobileNumber}}</i>
                </div>
            </div>
        </div>
    </fieldset>
    <fieldset>
        <legend>
            Contract Information
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.ContractInfo.ProviderRelationshipOption, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{SelectedProviderRelation}}</i>
                </div>
            </div>
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.ContractInfo.JoiningDate, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{ContractInfo.JoiningDate|date:'MM/dd/yyyy'}}</i>
                </div>
            </div>
            <div class="col-lg-6 form-group">
                @Html.LabelFor(model => model.ContractInfo.CVDocumentPath, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{selectedCV.name}}</i>
                </div>
            </div>
        </div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-3 form-group">
                @Html.LabelFor(model => model.GroupInfoes, new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{IsPartOfGroup}}</i>
                </div>
            </div>
            <div ng-if="IsPartOfGroup=='Yes'" class="col-lg-9 form-group">
                <label class="control-label small">Group Names</label>
                <div>
                    <span ng-repeat="group in GroupInfoes track by $index">
                        <span class="label label-default">
                            {{group.OrganizationGroup.GroupName}}
                        </span>
                        &nbsp;&nbsp;
                    </span>
                </div>
            </div>
        </div>
        @*<div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-6 form-group">
                @Html.LabelFor(model => model.ContractInfo.ContractFilePath, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{selectedContract.name}}</i>
                </div>
            </div>
        </div>*@
        <div class="col-lg-12">
            <input id="next1" type="button" value="Confirm" ng-click="confirmationAddProvider()" class="btn btn-primary btn-sm pull-right" />
            <input id="next1" type="button" value="Previous" ng-click="nextForm(2)" class="btn btn-warning btn-sm pull-left" />
        </div>
    </fieldset>
</div>