<link href="~/Content/SharedCss/jasny-bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/Lib/Jquery/jquery-1.11.0.js"></script>
<script src="~/Scripts/Lib/Bootstrap/jasny-bootstrap.js"></script>

<div class="col-lg-12" ng-cloak ng-if="profileDetail.length!=0" style="overflow-x:auto;">
    <table ng-table="tableParams4" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected ng-scope">

        <tr ng-repeat="user in $data track by $index">

            <td class=" wrap-words" data-title="'Plan'" header-class="text-left" sortable="'PlanName'" filter="{ 'PlanName': 'text' }">

                {{user.PlanName}}
            </td>

            <td class="wrap-words" data-title="'Specialties'" header-class="text-left" sortable="'Speciality'" filter="{ 'Speciality': 'text' }">
                {{user.Speciality}}
            </td>

            <td class="wrap-words" data-title="'LOB'" header-class="text-left" sortable="'LOB'" filter="{ 'LOB': 'text' }">
                {{user.LOB}}
            </td>

            <td class="wrap-words" data-title="'Location'" header-class="text-left" sortable="'Location'" filter="{ 'Location': 'text' }">
                {{user.Location}}
            </td>

            <td class="wrap-words" data-title="'Business Entity'" header-class="text-left" sortable="'GroupName'" filter="{ 'GroupName': 'text' }">
                {{user.GroupName}}
            </td>
            <td class="wrap-words" data-title="'Credentialed Date'" header-class="text-left" sortable="'InitiationDate'" filter="{ 'InitiationDate': 'text' }">

                {{user.InitiationDate | date:'MM-dd-yyyy'}}
            </td>
            <td header="'ng-table/headers/checkboxDecred.html'">
                <input ng-model="user.Check" ng-click="checkMe(user.Check)" type="checkbox" class="pull-right" />
            </td>
        </tr>

    </table>    
    <script type="text/ng-template" id="ng-table/headers/checkboxDecred.html">
        Select All
        <input type="checkbox" ng-model="IsAllChecked" ng-change="SelectAll(IsAllChecked,4)">
    </script>

        <div class="col-lg-12 form-group">
            <label>Disciplinary Report</label>
            <div class="col-lg-12 fileinput fileinput-new input-group" data-provides="fileinput" ng-init="fileinput=''">
                <div class="form-control jancyFileWrapTexts" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename wraptext"></span></div>
                <span class="input-group-addon btn btn-default btn-sm btn-file">
                    <span class="fileinput-new">Select Document</span><span class="fileinput-exists">Change</span>
                    <input type="file" ng-model="tempobject.ffll" />
                </span>
                <a href="#" class="input-group-addon btn btn-default btn-sm fileinput-exists" data-dismiss="fileinput">Remove</a>

            </div>

        </div>

    @*<div class="col-lg-6 text-left"><button class="btn btn-warning btn-sm" ng-click="SearchProviderPanelToggleDown('SearchProviderResultPanel'); tempObject.PlanId = '';"> Back to Search Result</button></div>*@

    @*<div ng-hide="initiateSuccess" class="col-lg-6 text-right"><button class="btn btn-primary btn-sm" ng-click="InitiateCredentialing();"> Initiate De-Credentialing</button></div>*@
    @*<a class="btn btn-primary btn-xs pull-right" ng-click="SelectProvidersForReCredentialingInitiation(user)" data-toggle="tooltip" data-placement="right" title="Initiate">Initiate DeCredentialing</a>*@
    <div class="col-lg-12">
        <div class="col-md-12 form-group">
            <button type="reset" class="btn btn-sm btn-warning" ng-click="SearchProviderPanelToggleDown('SearchProviderResultPanel'); tempObject.PlanId = ''">Cancel</button>
            <button type="button" class="btn btn-sm btn-primary pull-right" ng-click="InitiateDeCredentialing()" ng-disabled="isSelected">Initiate DeCredentialing</button>
        </div>
    </div>

</div>

