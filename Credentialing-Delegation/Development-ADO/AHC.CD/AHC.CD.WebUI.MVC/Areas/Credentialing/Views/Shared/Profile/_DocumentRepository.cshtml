<br />
<div class="panel-group" id="docrepo" role="tablist" aria-multiselectable="true" ng-controller="docRepo">
    <div class="panel panel-default">
        <div class="panel-heading text-right">
            <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-minus-square-o"></i></button>
            <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Expand All" onclick="expandAll(this)"><i class="fa fa-plus-square-o"></i></button>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="documentRepositoryHeading">
            <a class="collapsed" data-toggle="collapse" data-parent="#docrepo" href="#documentRepository" aria-expanded="false" aria-controls="documentRepository">
                Document Repository
                <span class="glyphicon glyphicon-minus pull-right"></span>
            </a>
        </div>
        <div id="documentRepository" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="documentRepositoryHeading">
            <div class="panel-body">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <div>Document Upload</div>
                    </div>
                    <div class="panel-body">


                        <div class="container-fluid">
                            
                            <div>
                               
                                <form action="/file-upload" class="dropzone" id="file-dropzone">
                                    <div class="fallback">
                                        <input name="file" type="file" multiple />
                                    </div>
                                </form>
                            </div>

                        </div>

                    </div>
                </div>


                <br />
                <div class="col-lg-12 panel-footer">
                    <button class="btn btn-sm btn-success pull-right">Upload & Initiate Verification</button>
                </div>
            </div>
        </div>
    </div>

</div>


