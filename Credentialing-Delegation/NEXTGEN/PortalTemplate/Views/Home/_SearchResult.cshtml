                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>State</label><span></span><i class="fa fa-sort "></i>
                            </span>
                        </th>
                    </tr>
                </thead>

            </table>
            <div class="table-responsive response-table" id="searchBodyDiv">
                <table id="newSearch" class="table table-striped table-hover custom-thead-back custom-thead-font custom-tbody custom-table-striped" cellspacing="0">
                    <tbody id="searhcResultBody"></tbody>
                </table>

            </div>*@

            </div>
        </div>
      
        <div id="ProviderProfile">

        </div>
    </div>

    
</div>


<style>
    .ProviderDetailsFrame {
        width: 325px;
        height: 110px;
        background-color: #dbe2ea;
        border-radius: 10px;
        float: left;
        margin-right: 5px;
        margin-top: 5px;
        font-size: 12px;
        font-weight: bold;
    }
    .profileImg {
        width: 90px;
        height: 90px;
        margin-top: .5em;
    }
    .providerInfo {
        float: left;
        margin-top: .5em;
    }
</style>
