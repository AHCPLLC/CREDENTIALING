                            @*<a class="inner_menu_item" href="" onclick="getTabContent(this);" action="{{childMenuItem.action}}">{{childMenuItem.title}}</a>*@
                        </li>
                    </ul>
                    <span class="label_on_collapsed">{{menu.collapsedLabel}}</span>
                </li>
            </ul>
