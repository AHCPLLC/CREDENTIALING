                    <a class="expand_click menu_click_expanded">
                        <i ng-class="menu.IconClass"></i><span class="text_beside_icon">{{menu.Heading}}</span>
                        <span ng-if="menu.childMenu.length>0" class="fa fa-chevron-down"></span>
                    </a>
                    <ul class="nav child_menu" style="display: none;" ng-if="menu.childMenu.length>0">
                        <li ng-repeat="childMenuItem in menu.childMenu">
