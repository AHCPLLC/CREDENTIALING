<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap core CSS -->
    @*<link href="~/Content/bootstrap.min.css" rel="stylesheet" />*@
    <link href="~/Content/LibCss/bootstrap.min.css" rel="stylesheet" />
    <link href="~/fonts/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Resources/Fonts/Roboto/roboto.css" rel="stylesheet" />
    <link href="~/Content/LibCss/jquery-ui.min.css" rel="stylesheet" />
    <!-- Custom styling plus plugins -->
    <link href="~/Content/AppCss/animate.min.css" rel="stylesheet" />
    <link href="~/Content/AppCss/custom.min.css" rel="stylesheet">
    <link href="~/Content/AppCss/AppStyle.css" rel="stylesheet" />
    <link href="~/Content/CustomTheme/colorpatch.css" rel="stylesheet" />
    <link href="~/Content/CustomTheme/BlueBack.css" rel="stylesheet" id="SidebarBackground" />
    <link href="~/Content/LibCss/icheck/square/green.css" rel="stylesheet" />


    <script src="~/Scripts/LibScripts/jquery.min.js"></script>
    <script src="~/Scripts/LibScripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/LibScripts/angular.min.js"></script>
    <script src="~/Scripts/LibScripts/smart-table.min.js"></script>
    <script src="~/Scripts/AppScripts/Home/Home.js"></script>
    @*<script src="~/Scripts/jquery-3.0.0.min.js"></script>*@


    <!--[if lt IE 9]>
        <script src="../assets/js/ie8-responsive-file-warning.js"></script>
    <![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")

    @*@Scripts.Render("~/bundles/js")*@

    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    @RenderSection("AddToHeadAnotherWay", required: false)

    <script src="~/Scripts/bootstrap.min.js"></script>

</head>
<body class="pace-done nav-md" ng-app="HomeApp" ng-controller="homeCtrl" ng-cloak>

    <div class="pace pace-inactive">
        <div class="pace-progress" data-progress-text="100%" data-progress="99" style="transform: translate3d(100%, 0px, 0px);">
            <div class="pace-progress-inner"></div>
        </div>
        <div class="pace-activity"></div>
    </div>

    <div class="container body">
        <div class="main_container">


            @Html.Partial("~/Views/Shared/OtherLayouts/_TopNavBar.cshtml")


            <div class="right_col" role="main" style="min-height: 677px;">

                @Html.Partial("~/Views/Shared/OtherLayouts/_Header.cshtml")
                @RenderBody()
                <div class="clearfix"></div>
            </div>
        </div>
        @Html.Partial("~/Views/Shared/OtherLayouts/_CustomNotification.cshtml")

        <script src="~/Scripts/AppScripts/App.js"></script>

        @*@Scripts.Render("~/bundles/js")*@

        @if (IsSectionDefined("AddToFooter"))
        {
            @RenderSection("AddToFooter", required: false)
        }

        @RenderSection("AddToFooterAnotherWay", required: false)

        @RenderSection("scripts", required: false)
    </div>
    <script src="~/Scripts/LibScripts/pace/pace.min.js"></script>
    <script src="~/Scripts/LibScripts/icheck/icheck.min.js"></script>

</body>
</html>
