<div class="col-lg-12">
    <div class="col-lg-3 col-md-3"  >
        <div class="dim tile-danger " >
            <span style="font-size:25px;align-content:center" >TOTAL CASES PER DAY</span>
            <hr style="margin-bottom:0px;margin-top:0px;"/><br/>
          <span class="col-lg-12" style="text-align: left; font-size: 14px;">
              <span class="pull-left col-lg-6">CM<br /> Open : 5 <br />Closed : 2</span>
              <span class="pull-left col-lg-6">DM <br />Open : 5<br />Closed : 2</span>
          </span>

        </div>
    </div>
    <div class="col-lg-3 col-md-3">
        <div class="dim tile-success ">
            <span style="font-size:25px;align-content:center">TOTAL CASES PER WEEK</span>
            <hr style="margin-bottom:0px;margin-top:0px;" /><br />
            <span class="col-lg-12 col-md-12" style="text-align: left; font-size: 14px;">
                <span class="pull-left col-lg-6 col-md-12">CM<br /> Open : 5 <br />Closed : 2</span>
                <span class="pull-left col-lg-6 col-md-12">DM <br />Open : 5<br />Closed : 2</span>
            </span>

        </div>
    </div>
    <div class="col-lg-3 col-md-3">
        <div class="dim tile-warning ">
            <span style="font-size:25px;align-content:center">TOTAL CASES PER MONTH</span>
            <hr style="margin-bottom:0px;margin-top:0px;" /><br />
            <span class="col-lg-12" style="text-align: left; font-size: 14px;">
                <span class="pull-left col-lg-6">CM<br /> Open : 5 <br />Closed : 2</span>
                <span class="pull-left col-lg-6">DM <br />Open : 5<br />Closed : 2</span>
            </span>

        </div>
    </div>
    <div class="col-lg-3 col-md-3">
        <div class="dim tile-primary ">
            <span style="font-size:25px;align-content:center">TOTAL CASES PER YEAR</span>
            <hr style="margin-bottom:0px;margin-top:0px;" /><br />
            <span class="col-lg-12" style="text-align: left; font-size: 14px;">
                <span class="pull-left col-lg-6">CM<br /> Open : 5 <br />Closed : 2</span>
                <span class="pull-left col-lg-6">DM <br />Open : 5<br />Closed : 2</span>
            </span>

        </div>
    </div>
</div>
<div class="col-lg-5" style="margin-top:45px;" >
    <div class="panel panel-default">
        <div class="panel-heading" style="height:60px;">
            <span  style="font-size: 18px;font-weight:bold">Request Distribution Summary</span>
            <div class="pull-right">
                <button class="btn btn-primary"><i class="fa fa-print"></i></button> &nbsp;<button class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i></button>&nbsp; <button class="btn btn-danger"><i class="fa fa-file-pdf-o" aria-hidden="true"></i></button>
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-offset-5">
                    <button id="weekly_RequestDistribution" class="btn btn-default" onclick="ChangeRequestDistribution('Week');"><b>Weekly</b></button>
                     &nbsp;<button id="monthly_RequestDistribution" onclick="ChangeRequestDistribution('Month');" class="btn btn-default"><b>Monthly</b></button>
                    &nbsp; <button onclick="ChangeRequestDistribution('Year');" class="btn btn-default" id="yearly_RequestDistribution"><b>Yearly</b></button>
                </div>
            </div>
            <div class="row" id="reqDistributionChart" style="height:450px;"></div>
        </div>
    </div>
    

</div>
<div class="col-lg-5" style="margin-top:45px;">
    <div class="panel panel-default">
        <div class="panel-heading" style="height:60px;">
            <span style="font-size: 18px;font-weight:bold">Request Conversion Summary</span>
            <div class="pull-right">
                <button class="btn btn-primary"><i class="fa fa-print"></i></button> &nbsp;<button class="btn btn-success"><i class="fa fa-file-excel-o" aria-hidden="true"></i></button>&nbsp; <button class="btn btn-danger"><i class="fa fa-file-pdf-o" aria-hidden="true"></i></button>
            </div>
        </div>
        <div class="panel-body">
            <div class="col-lg-offset-5">
                <button class="btn btn-default" onclick="ChangeRequestConversion('Week');"><b>Weekly</b></button> &nbsp;<button class="btn btn-default" onclick="ChangeRequestConversion('Month');"><b>Monthly</b></button>&nbsp; <button class="btn btn-default" onclick="ChangeRequestConversion('Year');"><b>Yearly</b></button>
            </div>
            <div class="row" id="reqConversionChart" style="height:450px;"></div>
        </div>
    </div>


</div>
