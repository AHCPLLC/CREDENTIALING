@*View Auth Tabs*@
<div class="x_panel ">
    <div class="x_content">
        <div class="width_13_percent viewAuthTabsContainer">
            <ul class="list-unstyled x-dtablist tabs-menu">
                <li class="tab-item current">
                    <a href="#tab-1" class="list-item no_underline">
                        <i class="fa fa-list-ul"></i>  Summary
                    </a>
                </li>
                <li>
                    <a href="#tab-2" class="list-item no_underline">
                        <i class="fa fa-comment-o"></i>  Review
                    </a>
                </li>
                <li>
                    <a href="#tab-3" class="list-item no_underline">
                        <i class="fa fa-sticky-note-o"></i>  Notes
                    </a>
                </li>
                <li>
                    <a href="#tab-4" class="list-item no_underline">
                        <i class="fa fa-phone-square"></i>  Contacts
                    </a>
                </li>
                <li>
                    <a href="#tab-5" class="list-item no_underline">
                        <i class="fa fa-paperclip"></i> Attachments
                    </a>
                </li>
                <li>
                    <a href="#tab-6" class="list-item no_underline">
                        <i class="fa fa-envelope-o"></i> Letters
                    </a>
                </li>
                <li>
                    <a href="#tab-7" class="list-item no_underline">
                        <i class="fa fa-check-square-o"></i> ODAG
                    </a>
                </li>
                <li>
                    <a href="#tab-8" class="list-item no_underline">
                        <i class="fa fa-flag-o"></i> Status
                    </a>
                </li>
            </ul>
        </div>
      
        <div class="tab">
            <div id="tab-1" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthSummary.cshtml")
            </div>

            <div id="tab-2" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthReview.cshtml")
            </div>

            <div id="tab-3" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthNotes.cshtml")
            </div>

            <div id="tab-4" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthContacts.cshtml")
            </div>

            <div id="tab-5" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthAttachments.cshtml")
            </div>

            <div id="tab-6" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthLetters.cshtml")
            </div>

            <div id="tab-7" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthODAG.cshtml")
            </div>

            <div id="tab-8" class="customtab-content">
                @Html.Partial("~/Views/ViewAuth/_AuthStatus.cshtml")
            </div>
        </div>
    </div>
</div>




