    <div class="clearfix"></div>
    <div ng-show="activeMessageDiv=='updatedID'" class="alert alert-success" role="alert">
        <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
        {{messageDesc}}
    </div>
    <div class="clearfix"></div>
    </div>




