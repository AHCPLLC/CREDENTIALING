@*Practice Location Facility Information Start*@
<fieldset>
    <legend>
        Practice Location / Facility Details
    </legend>  
    <div class="legend2 pull-right">
        <button ng-if="visibilityControl!=('editPracticeLocation{{$index}}')" ng-click="operateEditControl('editPracticeLocation{{$index}}', PracticeLocationDetail.Facility); assignCityComponentForFacility(); setFacilityPracticeDays(PracticeLocationDetail);" class="btn btn-xs btn-primary" title="Edit" tooltip><i class="fa fa-edit"></i></button>
        <button ng-if="visibilityControl==('editPracticeLocation{{$index}}')" ng-click="operateCancelControl(''); " class="btn btn-xs btn-default" title="Cancel" tooltip><i class="fa fa-close"></i></button>
        <button ng-if="visibilityControl!=('editPracticeLocation{{$index}}')" class="btn btn-default btn-xs" ng-click="ToggleScript()" title="View More" tooltip><i class="fa fa-th-list"></i></button>
        <button ng-if="visibilityControl==('editPracticeLocation{{$index}}')" class="btn btn-default btn-xs" ng-click="PanelToggle('practicefacilityedit');" title="Toggle View" tooltip><i class="fa fa-th-list"></i></button>
    </div>
    <div ng-if="visibilityControl!=('editPracticeLocation{{$index}}')">
        @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewFacilityInformation.cshtml")
    </div>
    <div id="practicefacilityedit" ng-if="visibilityControl==('editPracticeLocation{{$index}}')">
        <form id="newFacilityDataForm{{$index}}">
            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditFacilityData.cshtml")
        </form>
    </div>
    <div class="clearfix"></div>
</fieldset>
@*Practice Location Facility Information End*@

@*Practice Location Other Information Start*@
<div class="form-style">
    <fieldset>
        <legend>
            Other Information
        </legend>
        <div ng-hide="GeneralInformationEdit" class="legend2 pull-right">
            <button ng-click="editGeneralInfo(PracticeLocationDetail)" class="btn btn-xs btn-primary" title="Edit" tooltip><i class="fa fa-edit"></i></button>
        </div>
        <div ng-show="!GeneralInformationEdit">
            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewGeneralInformation.cshtml")
        </div>

        <div ng-show="GeneralInformationEdit">
            <form id="updatePracticeLocationDetails{{$index}}">
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditGeneralInformation.cshtml")
            </form>
            <div class="col-lg-12">
                <div class="control-group">
                    <button class="btn btn-warning btn-sm" ng-click="editGeneralCancel()">Cancel</button>
                    <button class="btn btn-success btn-sm pull-right" ng-click="updatePracticeLocationDetailInformaton($index)">Update</button>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div ng-if="activeMessageDiv=='UpdatePracticeLocation'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
        </div>
    </fieldset>
</div>
@*Practice Location Other Information End*@
