<div ng-show="loading" class="progress col-lg-12" ng-cloak>
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        Loading...Please wait...
    </div>
</div>
<div ng-show="!loading">
    <div class="col-lg-12">
        <label class="control-label small">Select Plan</label>
        <select data-ng-model="plan" ng-change="selectPlan(plan)" class="form-control input-sm" data-val="true" data-val-required="Please select plan.">
            <option selected hidden disabled value="">-select Plan-</option>
            <option ng-repeat="plan in Plans" value="{{plan}}">{{plan.PlanName}}</option>
        </select>
    </div>
    &nbsp;&nbsp;&nbsp;
    <div class="col-md-12">
        <button class="btn btn-primary btn-xs pull-right" type="submit" ng-click="CallView5();">Submit</button>
    </div>
</div>