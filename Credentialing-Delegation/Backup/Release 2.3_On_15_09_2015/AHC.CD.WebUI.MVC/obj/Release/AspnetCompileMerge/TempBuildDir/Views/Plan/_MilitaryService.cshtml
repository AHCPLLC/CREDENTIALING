<div class="panel-group" id="accordionmilitary" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="militaryHeading">
            <a data-toggle="collapse" data-parent="#accordionmilitary" href="#military" aria-expanded="false" aria-controls="military">
                Military Service Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="military" class="panel-collapse collapse" role="tabpanel" aria-labelledby="militaryHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">

                            <div class="form-group col-md-4">
                                <label class="control-label small">Military Branch</label>
                                <div>
                                    <select id="selectNameTitle" name="" class="form-control input-sm" required>
                                        <option>Branch1</option>
                                        <option>Branch2</option>
                                        <option>Branch3</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Start Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Discharge Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">Rank At Discharge</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>Rank1</option>
                                        <option>Rank2</option>
                                        <option>Rank3</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label small">Type Of Discharge</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>Discharge1</option>
                                        <option>Discharge2</option>
                                        <option>Discharge3</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="control-label small">Other Than Honorable, Please Explain</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">Present Duty Status/Assignment</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>Active</option>
                                        <option>Inactive</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label">Reserve Status</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group col-md-8">
                                <label for="Contact" class="control-label small">I Have No Military Obligation</label>
                                <div class="radio">
                                    <label class="radio-inline">
                                        <input type="radio" name="Contact"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="Contact"> No
                                    </label>
                                </div>
                            </div>
                        </div>
                        
                        
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




