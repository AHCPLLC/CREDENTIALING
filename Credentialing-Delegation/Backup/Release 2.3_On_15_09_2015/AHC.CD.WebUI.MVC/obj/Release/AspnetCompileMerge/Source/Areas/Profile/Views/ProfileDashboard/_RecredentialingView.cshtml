<div ng-show="errorInitiated">
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr ng-show="structuredata.length>0">
                <th class="col-lg-2">Plan Name</th>
                <th class="col-lg-2">Business Entity</th>
                <th class="col-lg-2">Line Of Business</th>
                <th class="col-lg-2">Specialty</th>
                <th class="col-lg-3">Location</th>
                @*<th class="col-lg-3">Status</th>*@
                <th class="col-lg-2" ng-show="credentialingInfo.IsDelegated == 'YES'">Initial Credentialing Date</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="c in structuredata track by $index" ng-class='{danger:c.CredentialingContractRequest.TableRowStatus ==false, success:c.CredentialingContractRequest.TableRowStatus ==true}'>
                <td>{{c.PlanObject.PlanName}}</td>
                <td>
                    <span ng-repeat="s in c.CredentialingContractRequest">
                        <i ng-if="$index>0">,</i><br ng-if="$index>0" />
                        <span>
                            {{s.BusinessEntity.GroupName}}
                        </span>
                    </span>
                </td>
                <td>
                    <span ng-repeat="q in c.CredentialingContractRequest">
                        <span ng-repeat="s in q.ContractLOBs">
                            <i ng-if="$index>0">,</i><br ng-if="$index>0" />
                            <span>
                                {{s.LOB.LOBName}}
                            </span>
                        </span>
                    </span>
                </td>
                <td>
                    <span ng-repeat="f in c.CredentialingContractRequest">
                        <span ng-repeat="d in f.ContractSpecialties">
                            <i ng-if="$index>0">,</i><br ng-if="$index>0" />
                            {{d.ProfileSpecialty.Specialty.Name}}
                        </span>
                    </span>
                </td>
                <td>
                    <span ng-repeat="g in c.CredentialingContractRequest">
                        <span ng-repeat="z in g.ContractPracticeLocations">
                            <i ng-if="$index>0">,</i><br ng-if="$index>0" />
                            {{z.ProfilePracticeLocation.Facility.FacilityName}} - {{z.ProfilePracticeLocation.Facility.Street}}, {{z.ProfilePracticeLocation.Facility.City}}, {{z.ProfilePracticeLocation.Facility.State}}, {{z.ProfilePracticeLocation.Facility.Country}}
                        </span>
                    </span>
                </td>
                <td ng-show="credentialingInfo.IsDelegated == 'YES'">{{c.InitialCredentialingDate || 'Not Available' | date:'MM-dd-yyyy'}}</td>
                @*<td>
                    <span class="label label-primary pull-left" ng-if="c.CredentialingContractRequest.TableRowStatus==true">Completed </span>
                    <span class="label label-danger pull-left" ng-if="c.CredentialingContractRequest.TableRowStatus==false">Not Completed </span>
                </td>*@
                
            </tr>
        </tbody>
    </table>
    @*<button class="btn btn-primary btn-sm pull-right" type="submit" ng-click="showPlanListReCred();">View All</button>*@
</div>