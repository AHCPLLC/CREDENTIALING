<div id="page-wrapper" ng-app="profileApp">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row" ng-cloak>
            <div class="col-lg-12" ng-controller="breadcrumCtrl">
                <ol class="breadcrumb">

                    <li class="active">
                        <i class="fa fa-list"></i>
                        <a href="~/Profile/SampleProfile/Index">Profiles</a>
                    </li>
                    <li class="active">
                        {{Provider.FirstName}} {{Provider.MiddleName}} {{Provider.LastName}}
                    </li>
                    <li class="text-capitalize active">
