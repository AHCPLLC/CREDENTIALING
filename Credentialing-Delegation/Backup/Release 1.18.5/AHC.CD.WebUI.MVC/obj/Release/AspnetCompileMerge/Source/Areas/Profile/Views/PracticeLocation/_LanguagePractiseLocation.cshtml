<div class="col-md-12">
    <label for="Contact" class=" control-label">Non English languages spoken by office personnel</label>
    <select multiple="multiple" size="10" name="duallistbox_demo1">
        <option value="option1">Afan</option>
        <option value="option2">Arabic</option>
        <option value="option3">Bengali</option>
        <option value="option4">Chinese</option>
        <option value="option5">Dutch</option>
        <option value="option6">Fiji</option>
        <option value="option7">French</option>
        <option value="option8">Greek</option>
        <option value="option9">Hindi</option>
        <option value="option10">Italian</option>
    </select>
</div>
<br /><br />
<div class="col-md-12">
    <div class="col-md-6">
        <label class="control-label">Do you communicate American Sign Language?*</label>
        <div>
            <label class="radio-inline">
                <input type="radio" name="Citizen" checked="checked"> Yes
            </label>
            <label class="radio-inline">
                <input type="radio" name="Citizen"> No
            </label>
        </div>
    </div>

    <div class="col-md-6">
        <label class="control-label">Interpreters Available?*</label>
        <div>
            <label class="radio-inline">
                <input type="radio" name="Citizen" ng-click="showLanguages()"> Yes
            </label>
            <label class="radio-inline">
                <input type="radio" name="Citizen" ng-click="hideLanguages()"> No
            </label>
        </div>
    </div>
</div><br /><br />

<div ng-show="languages" class="col-md-12">
    <label for="Contact" class=" control-label">Languages Interpreted</label>
    <select multiple="multiple" size="10" name="duallistbox_demo1">
        <option value="opt1">Afan</option>
        <option value="opt2">Arabic</option>
        <option value="opt3">Bengali</option>
        <option value="opt4">Chinese</option>
        <option value="opt5">Dutch</option>
        <option value="opt6">Fiji</option>
        <option value="opt7">French</option>
        <option value="opt8">Greek</option>
        <option value="opt9">Hindi</option>
        <option value="opt10">Italian</option>
    </select>
</div>
<div class="clearfix"></div>
<br />
    <div class="col-md-12">
        <button class="btn btn-warning btn-sm pull-left">Cancel</button>
        <button class="btn btn-success btn-sm pull-right">Save</button>
    </div>
