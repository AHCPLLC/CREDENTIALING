@model AHC.CD.WebUI.MVC.Areas.Profile.Models.WorkHistory.ProfessionalWorkExperienceViewModel

<div class="col-lg-12 zero-padding-left-right-div">
    <div class="col-md-12">
        <div class="form-group col-md-12">
            @Html.LabelFor(model => model.EmployerName, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{workExperience.EmployerName}}</i>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Number, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Number}}</i>
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Building, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Building}}</i>
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Street, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Street}}</i>
                </div>
            </div>

            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Country}}</i>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.State}}</i>
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.County}}</i>
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.City}}</i>
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Zipcode, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Zipcode}}</i>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.EmployerMobile, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.EmployerMobile}}</i>
                </div>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.EmployerFax, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <div>
                        <i class="data-label">{{workExperience.EmployerFax}}</i>
                    </div>
                </div>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.JobTitle, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.JobTitle}}</i>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.SupervisorName, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.SupervisorName}}</i>
                </div>
            </div>

            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.Department, htmlAttributes: new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.Department}}</i>
                </div>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.ProviderTypeID, new { @class = "control-label small" })
                <div>
                    <i class="data-label">{{workExperience.ProviderType.Title}}</i>
                </div>
            </div>
        </div>
        <div class="col-md-12">

            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.CanContactEmployerOption, htmlAttributes: new { @class = "control-label small" })
                <div class="radio">
                    <div>
                        <i ng-if="workExperience.CanContactEmployerOption" class="data-label">Yes</i>
                        <i ng-if="!workExperience.CanContactEmployerOption" class="data-label">No</i>
                    </div>
                </div>
            </div>

            <div class="form-group col-md-4">
                <label class="control-label small">Start Date *</label>
                <div>
                    <i class="data-label">{{workExperience.StartDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>

            <div class="form-group col-md-4">
                <label class="control-label small">End Date *</label>
                <div>
                    <i class="data-label">{{workExperience.EndDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-6">
                <label class="control-label small">Reason For Departure</label>
                <div>
                    <i class="data-label">{{workExperience.DepartureReason}}</i>
                </div>
            </div>
            <div class="form-group col-md-6">
                <label class="control-label small">Certificate</label>
                <div>
                    <i class="data-label"><a href="{{workExperience.WorkExperienceDocPath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="{{workExperience.WorkExperienceDocPath}}"><i class="fa fa-file">Supporting Document</i></a></i>
                </div>
            </div>
        </div>
        <div class="col-md-12 form-group">
            <div class="col-md-12">
                <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="operateEditControl($index+'_editProfessionalWorkExperience',workExperience)">Edit</button>
            </div>
        </div>
    </div>
</div>

