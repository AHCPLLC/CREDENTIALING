@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.OtherIdentificationNumberViewModel


<div ng-show="visibilityControl=='editOtherIdentificationNumbers'" class="panel-default">
    <div class="form-style">
        <div class="col-md-12 text-right" style="font-style:italic;font-size:x-small;color:black">
            <label>*If you have additional professional Id's report here.</label>
        </div>
        <div id="otherIdenNumbers" class="panel-body zero-padding-left-right-div">
            <form id="otherIdentificationNumbersForm">
                <div class="form-group col-md-12">
                    <span ng-if="tempObject.OtherIdentificationNumberID">
                        @Html.HiddenFor(model => model.OtherIdentificationNumberID, new { ng_value = "tempObject.OtherIdentificationNumberID" })
                        @Html.HiddenFor(model => model.CAQHAuthenticationDetail.OtherAuthenticationDetailID, new { ng_value = "tempObject.CAQHAuthenticationDetail.OtherAuthenticationDetailID" })
                        @Html.HiddenFor(model => model.NPIAuthenticationDetail.OtherAuthenticationDetailID, new { ng_value = "tempObject.NPIAuthenticationDetail.OtherAuthenticationDetailID" })
                    </span>
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.NPINumber, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.NPINumber, new { @class = "form-control input-sm", data_ng_model = "tempObject.NPINumber" })
                        </div>
                        @Html.ValidationMessageFor(model => model.NPINumber, "", new { @class = "text-danger" })

                    </div>
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.NPIAuthenticationDetail.UserName, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.NPIAuthenticationDetail.UserName, new { @class = "form-control input-sm", type = "password", data_ng_model = "tempObject.NPIAuthenticationDetail.UserName" })
                        </div>
                        @Html.ValidationMessageFor(model => model.NPIAuthenticationDetail.UserName, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.NPIAuthenticationDetail.Password, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.NPIAuthenticationDetail.Password, new { @class = "form-control input-sm", type = "password", data_ng_model = "tempObject.NPIAuthenticationDetail.Password" })
                        </div>
                        @Html.ValidationMessageFor(model => model.NPIAuthenticationDetail.Password, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group col-md-12">
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.CAQHNumber, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.CAQHNumber, new { @class = "form-control input-sm", data_ng_model = "tempObject.CAQHNumber" })
                        </div>
                        @Html.ValidationMessageFor(model => model.CAQHNumber, "", new { @class = "text-danger" })

                    </div>
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.CAQHAuthenticationDetail.UserName, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.CAQHAuthenticationDetail.UserName, new { @class = "form-control input-sm", type = "password", data_ng_model = "tempObject.CAQHAuthenticationDetail.UserName" })
                        </div>
                        @Html.ValidationMessageFor(model => model.CAQHAuthenticationDetail.UserName, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.CAQHAuthenticationDetail.Password, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.CAQHAuthenticationDetail.Password, new { @class = "form-control input-sm", type = "password", data_ng_model = "tempObject.CAQHAuthenticationDetail.Password" })
                        </div>
                        @Html.ValidationMessageFor(model => model.CAQHAuthenticationDetail.Password, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group col-md-12">
                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.UPINNumber, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.UPINNumber, new { @class = "form-control input-sm", data_ng_model = "tempObject.UPINNumber" })
                        </div>
                        @Html.ValidationMessageFor(model => model.UPINNumber, "", new { @class = "text-danger" })

                    </div>

                    <div class="form-group col-md-4 ">
                        @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.USMLENumber, new { @class = "form-control input-sm", data_ng_model = "tempObject.USMLENumber" })
                        </div>
                        @Html.ValidationMessageFor(model => model.USMLENumber, "", new { @class = "text-danger" })

                    </div>
                </div>
                <div ng-if="ErrorId" class="col-lg-12">
                    <div class="alert alert-danger" role="alert">
                        <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                        <span class="sr-only">Error:</span>
                        Error List : <br />
                        <span ng-repeat="err in ErrorId track by $index">{{err}}</span>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="col-md-12">
                        <button class="btn btn-warning pull-left" ng-click="operateCancelControl('')">Cancel</button>

                        <button class="btn btn-success pull-right" ng-click="saveOtherIdentificationNumber('otherIdentificationNumbersForm',tempObject)">Save</button>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>