@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.FederalDEAInformationViewModel

<form class="FederalDEALicenseForm">
    <div class="form-style">
        <fieldset>
            <legend>
                State License Information
            </legend>
            <div>
                <div id="licensedea" class="panel-body zero-padding-left-right-div">
                    <div class="zero-padding-left-right-div">
                        <div class="col-md-12">
                            <div class="form-group col-md-3">
                              @*  @Html.HiddenFor(model => model.FederalDEAInformationID, new { ng_value = "tempObject.FederalDEAInformationID" })*@


                                @Html.LabelFor(model => model.DEANumber, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.DEANumber}}</i>
                                </div>
                            </div>

                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.StateOfReg, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.StateOfReg}}</i>
                                </div>
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.IssueDate, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.IssueDate | date: "MM-dd-yyyy"}}</i>
                                </div>
                            </div>

                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.ExpiryDate, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.ExpiryDate | date: "MM-dd-yyyy"}}</i>
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>

                        <div class="clearfix"></div>
                        <div class="col-md-12">
                            <table class="table table-bordered table-condensed customtbodyStyle table-striped">
                                <thead>
                                    <tr>
                                        <th>Schedule</th>
                                        <th>Schedule Type</th>
                                       <th>Eligibility</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="sl in Schedules">
                                    <tr>
                                        <td>
                                            {{sl.ScheduleTitle}}
                                        </td>
                                        <td>
                                            <label>{{sl.ScheduleTypeTitle}}</label>
                                        </td>
                                        <td>
                                            <i ng-if="DEAScheduleInfoes[$index].IsEligibleYesNoOption=='Yes'" class="fa fa-check"></i>
                                            <i ng-if="DEAScheduleInfoes[$index].IsEligibleYesNoOption=='No'" class="fa fa-close"></i>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
    @*  @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewSchedules.cshtml")*@
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-md-12">
                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.IsInGoodStanding, new { @class = "control-label small" })
                                <div>
                                    <i ng-if="DEAInformation.GoodStandingYesNoOption" class="data-label">Yes</i>
                                    <i ng-if="!DEAInformation.GoodStandingYesNoOption" class="data-label">No</i>
                                </div>
                            </div>
                            <div class="form-group col-md-9">
                                <label class="control-label small">Certificate</label>
                                <div>
                                    <i class="data-label"><a href="{{DEAInformation.DEALicenceCertPath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="DEA License Certificate"><i class="fa fa-file"> DEA License Certificate</i></a></i>
                                </div>
                            </div>
                            </div>
                            @*<div class="col-md-3">
                                @Html.LabelFor(model => model.LimitedOrRestricted, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.LimitedOrRestricted}}</i>
                                </div>
                            </div>
                            <div class="col-md-3">
                                @Html.LabelFor(model => model.RestrictionExplanation, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.RestrictionExplanation}}</i>
                                </div>
                            </div>*@
                            @*<div class="form-group col-md-3">
                                @Html.LabelFor(model => model.HasStateControlledSubstanceRegCertificate, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{DEAInformation.HasStateControlledSubstanceRegCertificate}}</i>
                                </div>
                            </div>*@
                        </div>
                        <div class="clearfix"></div>
                        @*<div class="form-group col-md-12">
                            <label class="control-label small">Certificate</label>
                            <div>
                                <i class="data-label"><a href="{{DEAInformation.DEALicenceCertPath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="DEA License Certificate"><i class="fa fa-file"> DEA License Certificate</i></a></i>
                            </div>
                              @*<div class="col-md-2 form-group">
                                <span>@Html.LabelFor(model => model.DEALicenceCertPath, htmlAttributes: new { @class = "control-label small" })</span>
                            </div>
                            <div class="col-md-1 form-group">
                                <div>
                                    <a href="/Content/Document/ATTESTATIONOFSITEVISIT.pdf" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="Preview"><i class="fa fa-file"></i></a>
                                </div>
                            </div>*@
                  

                        <div class="clearfix"></div>
                        <div class="col-md-12">
                            <div class="col-md-12">
                                <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                                <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="operateEditControl($index+'_editDEAInformation',DEAInformation)">Edit</button>
                            </div>
                        </div>
                    </div>
                </div>

            </fieldset></div>
</form>


