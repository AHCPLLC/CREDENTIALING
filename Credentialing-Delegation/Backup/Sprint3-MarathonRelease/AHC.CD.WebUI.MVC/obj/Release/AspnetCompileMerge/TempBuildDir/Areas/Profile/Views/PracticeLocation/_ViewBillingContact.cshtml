<fieldset>
    <legend>
        Billing Contact
    </legend>
    <div id="practice3" class="panel-body">
        <div class="form-group">
            <div class="col-md-12">
                <div>
                    <i class="data-label">Use Office Manager Details</i>
                </div>
            </div>
            <div class="col-md-12">

                <div class="col-md-6">
                    <label class="control-label small ">Select Location</label>
                    <div>
                        <i class="data-label">{{billingContact.Location}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">First Name *</label>
                    <div>
                        <i class="data-label">{{billingContact.First}} </i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Middle Name</label>
                    <div>
                        <i class="data-label">{{billingContact.Middle}}</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Last Name *</label>
                    <div>
                        <i class="data-label">{{billingContact.Last}}</i>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-md-12">
                <div class="col-lg-3 form-group">
                    <label class="control-label small">Country *</label>
                    <div>
                        <i class="data-label">{{billingContact.Country}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">State *</label>
                    <div>
                        <i class="data-label">{{billingContact.State}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">City *</label>
                    <div>
                        <i class="data-label">{{billingContact.City}}</i>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-md-12">
                @*<div class="form-group col-md-3">
                    <label class="control-label small">Number *</label>
                    <div>
                        <i class="data-label">{{billingContact.Number}}</i>
                    </div>
                </div>*@

                <div class="form-group col-md-3">
                    <label class="control-label small">Street *</label>
                    <div>
                        <i class="data-label">{{billingContact.Street}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Suite/Building</label>
                    <div>
                        <i class="data-label">{{billingContact.Suite}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Zip Code *</label>
                    <div>
                        <i class="data-label">{{billingContact.ZIP}}</i>
                    </div>
                </div>
            </div>

            <div class="clearfix"></div>
            <div class="col-md-12">
                <div class="col-lg-3 form-group">
                    <label class="control-label small">P.O. Box Address *</label>
                    <div>
                        <i class="data-label">{{billingContact.BoxAddress}}</i>
                    </div>
                </div>

                <div class="form-group col-md-3">
                    <label for="MobileNo" class="control-label small">Telephone *</label>
                    <div>
                        <i class="data-label">{{billingContact.Telephone}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label for="MobileNo" class="control-label small">Fax </label>
                    <div>
                        <i class="data-label">{{billingContact.Fax}}</i>
                    </div>
                </div>

                <div class="col-lg-3 form-group">
                    <label class="control-label small">E-Mail Address</label>
                    <div>
                        <i class="data-label">{{billingContact.MailAddress}}</i>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="clearfix"></div>
            <div class="col-lg-12">
                <div class="control-group">
                    <button class="btn btn-warning btn-sm" ng-click="cancelBillingContact()">Cancel</button>
                    <button class="btn btn-success btn-sm pull-right" ng-click="editMidLevel($index,billingContact)">Edit</button>
                </div>
            </div>
        </div>

    </div>
</fieldset>
