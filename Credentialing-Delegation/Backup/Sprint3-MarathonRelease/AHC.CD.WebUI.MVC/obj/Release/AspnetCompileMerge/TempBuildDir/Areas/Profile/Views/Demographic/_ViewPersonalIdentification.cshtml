@model AHC.CD.WebUI.MVC.Areas.Profile.Models.Demographic.PersonalIdentificationViewModel

<div ng-if="!Provider.PersonalIdentification" class="col-md-12 zero-padding-left-right-div">
    No Data Available ! Please Provide Personal Identification...
    <button class="btn btn-xs btn-primary pull-right" ng-click="operateEditControl('_EditPersonalIdentification',Provider.PersonalIdentification)"><i class="fa fa-plus"></i> Personal Identification</button>
</div>
<div ng-if="Provider.PersonalIdentification">
    <div ng-if="Provider.PersonalIdentification.SSN" class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-6 form-group">
            @Html.LabelFor(model => model.SSN, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{Provider.PersonalIdentification.SSN}}</i>
            </div>
        </div>
        <div class="col-lg-6 form-group">
            <label class="control-label small">Certificate</label>
            <div>
                <i class="data-label">
                    <a ng-if="Provider.PersonalIdentification.SSNCertificatePath" href="{{Provider.PersonalIdentification.SSNCertificatePath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="SSN Certificate"><i class="fa fa-file"> SSN Certificate</i></a>
                    <span ng-if="!Provider.PersonalIdentification.SSNCertificatePath">Not Available</span>
                </i>
            </div>
        </div>
    </div>
    <div ng-if="Provider.PersonalIdentification.DL" class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-6 form-group">
            @Html.LabelFor(model => model.DL, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{Provider.PersonalIdentification.DL}}</i>
            </div>
        </div>
        <div class="col-lg-6 form-group">
            <label class="control-label small">Certificate</label>
            <div>
                <i class="data-label">
                    <a ng-if="Provider.PersonalIdentification.DLCertificatePath" href="{{Provider.PersonalIdentification.DLCertificatePath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="Driving Certificate"><i class="fa fa-file"> Driving Certificate</i></a>
                    <span ng-if="!Provider.PersonalIdentification.DLCertificatePath">Not Available</span>
                </i>
            </div>
        </div>
    </div>
</div>