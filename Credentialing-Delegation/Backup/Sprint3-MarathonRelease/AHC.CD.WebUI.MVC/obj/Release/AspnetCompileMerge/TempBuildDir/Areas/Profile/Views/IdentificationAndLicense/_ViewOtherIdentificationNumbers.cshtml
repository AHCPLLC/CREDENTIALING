                    <div ng-hide="showOtherId">
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPINumber, new { @class = "  control-label small" })
                                <div>

                                    <i class="data-label">{{OtherIdentificationNumbers.NPINumber}}</i>
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPIAuthenticationDetail.UserName, new { @class = "  control-label small", type = "password" })
                                <div>
                                
                                    <i class="data-label" ng-show="OtherIdentificationNumbers.NPIAuthenticationDetail.UserName">********</i>
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPIAuthenticationDetail.Password, new { @class = "  control-label small", type = "password" })
                                <div>
                                    <i class="data-label" ng-show="OtherIdentificationNumbers.NPIAuthenticationDetail.Password">********</i>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @*  @Html.HiddenFor(model => model.NPIDetailID, new { ng_value = "tempObject.NPIAuthenticationDetail.NPIDetailID" })*@
                                @Html.LabelFor(model => model.CAQHNumber, new { @class = "  control-label small" })
                                <div>

                                    <i class="data-label">{{OtherIdentificationNumbers.CAQHNumber}}</i>
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.CAQHAuthenticationDetail.UserName, new { @class = "  control-label small", type = "password" })
                                <div>
                                    <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHAuthenticationDetail.UserName">********</i>
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.CAQHAuthenticationDetail.Password, new { @class = "  control-label small", type = "password" })
                                <div>
                                    <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHAuthenticationDetail.Password">********</i>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.UPINNumber, new { @class = " control-label small" })
                                <div>
                                    <i class="data-label">{{OtherIdentificationNumbers.UPINNumber}}</i>
                                </div>
                            </div>

                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
                                <div>
                                    <i class="data-label">{{OtherIdentificationNumbers.USMLENumber}}</i>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*<div class="col-md-12">
                    <div class="col-md-12">
                        <button class="btn btn-warning pull-left" ng-click="saveOtherIdentificationNumber(otherIdentificationNumber)">Cancel</button>
                    </div>
                </div>*@

                    <div ng-show="showOtherId">
                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_OtherIdentificationNumbers.cshtml")


                    </div>
                </form>
            </div>
            @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_OtherIdentificationNumbers.cshtml")
            <div ng-show="visibilityControl=='updatedID'" class="alert alert-warning" role="alert">
                <button ng-click="operateCancelControl('')" class="close"><span>&times;</span></button>
                <i class="fa fa-2x fa-arrow-up"></i>Data Updated Successfully !!!!
            </div>
        </fieldset>
    </div>
