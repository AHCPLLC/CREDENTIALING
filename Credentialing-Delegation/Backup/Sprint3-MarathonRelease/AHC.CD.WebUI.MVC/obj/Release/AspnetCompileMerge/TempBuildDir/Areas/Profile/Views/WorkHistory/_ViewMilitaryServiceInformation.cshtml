@model AHC.CD.WebUI.MVC.Areas.Profile.Models.WorkHistory.MilitaryServiceInformationViewModel



<div class="col-lg-12 zero-padding-left-right-div">
    <div class="col-md-12">
        <div class="form-group col-md-4">
            @Html.LabelFor(model => model.MilitaryBranch, new { @class = "control-label small" })
            <div>
                <i class="data-label">{{militaryService.MilitaryBranch}}</i>
            </div>
        </div>
        <div class="form-group col-md-4">
            @Html.LabelFor(model => model.StartDate, new { @class = "control-label small" })
            <div>
                <div>
                    <i class="data-label">{{militaryService.StartDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>
        </div>
        <div class="form-group col-md-4">
            @Html.LabelFor(model => model.DischargeDate, new { @class = "control-label small" })
            <div>
                <div>
                    <i class="data-label">{{militaryService.DischargeDate | date:'MM-dd-yyyy'}}</i>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <div class="form-group col-md-6">
            @Html.LabelFor(model => model.MilitaryRank, new { @class = "control-label small" })
            <div>
                <div>
                    <i class="data-label">{{militaryService.MilitaryRank}}</i>
                </div>
            </div>
        </div>
        <div class="form-group col-md-6">
            @Html.LabelFor(model => model.MilitaryDischarge, new { @class = "control-label small" })
            <div>
                <div>
                    <i class="data-label">{{militaryService.MilitaryDischarge}}</i>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <div class="col-md-12">
            <div class="form-group">
                @Html.LabelFor(model => model.HonorableExplanation, new { @class = "control-label small" })
                <div>
                    <div>
                        <i class="data-label">{{militaryService.HonorableExplanation}}</i>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <div class="form-group col-md-6">
            @Html.LabelFor(model => model.MilitaryPresentDuty, new { @class = "control-label small" })
            <div>
                <div>
                    <i class="data-label">{{militaryService.MilitaryPresentDuty}}</i>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <br />
    <div class="col-md-12">
        <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
        <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="operateEditControl($index+'_editMilitaryServiceInformation',militaryService)">Edit</button>
    </div>
</div>
