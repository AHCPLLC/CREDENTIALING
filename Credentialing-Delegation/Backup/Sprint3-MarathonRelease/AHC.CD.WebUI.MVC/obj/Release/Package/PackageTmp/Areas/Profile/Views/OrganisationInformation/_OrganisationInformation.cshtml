
<br />
<div class="panel-group" ng-controller="OrgInfoAppCtrl">
    <div class="panel-default">
        <div class="form-style">
            <fieldset>
                <legend>
                    Organisation Information
                </legend>
                <div id="OrganisationInfo">
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <table class="table table-bordered table-condensed customtbodyStyle table-striped">
                            <thead>
                                <tr>
                                    <th>Provider Relationship</th>
                                    <th>Group Names</th>
                                    <th>CV</th>
                                    <th>Contract Certificate</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody ng-repeat="Oi in OrganisationInfos">
                                <tr>
                                    <td>{{Oi.ProviderRelationship}}</td>
                                    <td>{{Oi.GroupName}}</td>
                                    <td><a href="~/Content/Document/SINGH - CV.pdf" title="{{sl.Certificate}}" target="_blank"><i class="fa fa-file" title=""></i></a></td>
                                   
                                    <td><a href="~/Content/Document/ATTESTATIONOFSITEVISIT.pdf" title="{{sl.Certificate}}" target="_blank"><i class="fa fa-file" title=""></i></a></td>
