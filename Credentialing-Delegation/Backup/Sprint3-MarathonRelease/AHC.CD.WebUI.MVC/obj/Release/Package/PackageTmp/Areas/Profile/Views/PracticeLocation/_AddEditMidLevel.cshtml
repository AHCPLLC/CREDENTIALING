<div class="form-style">
    <fieldset>
        <legend>
            Mid-Level Practitioners
        </legend>
        <div class="form-group">
            <div class="col-md-12">
                <div class="form-group col-md-12">
                    <label class="control-label small">
                        Do Mid-Level Practitioners (Nurse Practitioners, Physician Assistants, etc.) Care For Patients In Your Practice?*
                    </label>
                    <div class="radio">
                        <label class="radio-inline">
                            <input type="radio" ng-click="showDetails()" name="StateStanding" value="Yes" data-ng-model="midLevel.MidLevelPractitioners"> Yes
                        </label>
                        <label class="radio-inline">
                            <input type="radio" ng-click="hideDetails()" name="StateStanding" value="No" data-ng-model="midLevel.MidLevelPractitioners"> No
                        </label>
                    </div>
                </div>
            </div>
            <div ng-show="details">
                <div class="col-md-12">
                    <div class="col-lg-4 form-group">
                        <label class="control-label small">Practitioner First Name</label>
                        <input type="text" class="form-control input-sm" id="first1" placeholder="first name" data-ng-model="midLevel.PractitionerFirstName">
                    </div>
                    <div class="col-lg-4 form-group">
                        <label class="control-label small">Practitioner Middle Name</label>
                        <input type="text" class="form-control input-sm" id="first2" placeholder="middle name" data-ng-model="midLevel.PractitionerMiddleName">
                    </div>
                    <div class="col-lg-4 form-group">
                        <label class="control-label small">Practitioner Last Name</label>
                        <input type="text" class="form-control input-sm" id="first3" placeholder="last name" data-ng-model="midLevel.PractitionerLastName">
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-md-12">
                    <div class="col-lg-12 form-group">
                        <label class="control-label small">Practitioner License/ Certificate Number</label>
                        <input type="text" class="form-control input-sm" id="license" placeholder="license/certificate number" data-ng-model="midLevel.PractitionerLicense">
                    </div>
                    <div class="col-lg-6 form-group">
                        <label class="control-label small">Practitioner State</label>
                        <input type="text" class="form-control input-sm" id="state" placeholder="state" data-ng-model="midLevel.PractitionerState">
                    </div>
                    <div class="col-md-6 form-group">
                        <label class="control-label small">Practitioner Type </label>
                        <select class="form-control input-sm" data-ng-model="midLevel.PractitionerType">
                            <option>type1</option>
                            <option>type2</option>
                        </select>
                    </div>
                </div>
            </div>
            <br />
            <div class="col-lg-12">
                <button class="btn btn-warning btn-sm" ng-click="cancelMidLevel()">Cancel</button>
                <button class="btn btn-success btn-sm pull-right">Save</button>
            </div>
        </div>
    </fieldset>
</div>