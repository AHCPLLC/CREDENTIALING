<script src="~/Areas/Profile/Scripts/DisclosureQuestions/DisclosureQuestions.js"></script>

<br />
<div class="panel-group" id="accordiondis" role="tablist" aria-multiselectable="true" ng-controller="DisclosureController">
    <div class="form-style">
        <fieldset>
            <legend>
                Disclosure Questions
            </legend>
            <div class="legend2 pull-right" ng-if="visibilityControl!='editcategory'">
                <button class="btn btn-xs btn-primary" ng-click="operateEditControl('editcategory')" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
            </div>
            <div ng-show="visibilityControl!='editcategory'">
                @Html.Partial("~/Areas/Profile/Views/DisclosureQuestion/_ViewDisclosureQuestions.cshtml")
            </div>
            <div ng-show="visibilityControl=='editcategory'">
                <div id="editShowDisclosureQuestions">
                    @Html.Partial("~/Areas/Profile/Views/DisclosureQuestion/_AddEditDisclosureQuestions.cshtml")
                </div>
            </div>
        </fieldset>
    </div>
</div> 

