@model AHC.CD.WebUI.MVC.Areas.Profile.Models.EducationHistory.ECFMGDetailsViewModel

<fieldset>
    <legend>
        ECFMG Details
    </legend>
    <div class="col-lg-3 form-group">
        
        @Html.LabelFor(model => model.ECFMGNumber, htmlAttributes: new { @class = "control-label small" })
        <div>
            <i class="data-label">04815080</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        
        @Html.LabelFor(model => model.ECFMGIssueDate, htmlAttributes: new { @class = "control-label small" })
        <div>
            <i class="data-label">11-07-1992</i>
        </div>
    </div>
    <div class="col-md-3 form-group">
        @Html.LabelFor(model => model.ECFMGCertPath, htmlAttributes: new { @class = "control-label small" })
        <div>
            <i class="data-label"><a href="~/Content/Document/SINGH - ECFMG.pdf" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="ECFMG Certificate"><i class="fa fa-file"> ECFMG Certificate</i></a></i>
        </div>
    </div>
</fieldset>

