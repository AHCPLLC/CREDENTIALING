@model AHC.CD.WebUI.MVC.Areas.Profile.Models.BoardSpecialty.PracticeInterestViewModel

<div class="legend2 pull-right" ng-show="(visibilityControl!='editpracticeInterest') && (practiceInterest != null)">
    <button class="btn btn-xs btn-primary" ng-click="operateEditControl('editpracticeInterest', practiceInterest)" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
</div>
<div>
    <div>
        <div ng-show="practiceInterest == null">No data available!! To Add New Click <button class="btn btn-xs btn-success" ng-click="operateViewAndAddControl('editpracticeInterest')" data-toggle="tooltip" data-placement="top" title="Add New"><i class="fa fa-plus"></i></button></div>
        <div ng-show="(practiceInterest != null) && (practiceInterest.Interest == null)">No data available!! Click on Edit button to provide data.</div>
        <i class="data-label">{{practiceInterest.Interest}}</i>
    </div>
</div>