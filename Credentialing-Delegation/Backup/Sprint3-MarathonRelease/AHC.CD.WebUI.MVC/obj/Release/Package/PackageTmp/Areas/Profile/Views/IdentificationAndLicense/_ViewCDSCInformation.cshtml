@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.CDSCInformationViewModel

@*<script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>*@

<form class="cDSCInformationForm">
    <div class="col-lg-12 zero-padding-left-right-div">
        <fieldset>
            <legend>
                CDS Information
            </legend>
           <div>
                <div class="col-md-12">
                  @*  @Html.HiddenFor(model => model.CDSCInformationID, new { ng_value = "tempObject.CDSCInformationID" })*@

                   
                        <div class="form-group col-md-3">
                            @Html.LabelFor(model => model.CertNumber, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{CDSCInformation.CertNumber}}</i>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            @Html.LabelFor(model => model.State, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{CDSCInformation.State}}</i>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            @Html.LabelFor(model => model.IssueDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{CDSCInformation.IssueDate | date: "MM-dd-yyyy"}}</i>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            @Html.LabelFor(model => model.ExpiryDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{CDSCInformation.ExpiryDate | date: "MM-dd-yyyy"}}</i>
                            </div>
                        </div>
                    </div>
                   <div class="col-md-12">
                       <div class="form-group col-lg-12">
                           <label class="control-label small">Certificate</label>
                           <div>
                               <i class="data-label"><a href="{{CDSCInformation.CDSCCerificatePath}}" class class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="{{CDSCInformation.CDSCCerificatePath}}"><i cl><i class="fa fa-file">CDS Document</i></a></i>
                           </div>
                       </div>
                   </div>
                    <div class="clearfix"></div>
               <br/>
                    <div class="col-md-12 form-group">
                        <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                        <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="operateEditControl($index+'_editCDSCInformation',CDSCInformation)">Edit</button>
                    </div>
                </div>
            </fieldset>
        </div>
</form>
