@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.CDSCInformationViewModel

<fieldset>
    <legend>
        CDS Information
    </legend>
    <div class="col-md-12 text-right" style="font-style:italic;font-size:x-small;color:black">
        <label>*Provide all current and previous licenses/certifications</label>
    </div>
    <div>
        <form class="cDSCInformationForm">
           
                <div class="col-md-12">
                    <span ng-if="tempObject.CDSCInformationID">
                        @Html.HiddenFor(model => model.CDSCInformationID, new { ng_value = "tempObject.CDSCInformationID" })
                    </span> 
                    <div class="form-group col-md-3">
                        @Html.LabelFor(model => model.CertNumber, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.CertNumber, new { @class = "form-control input-sm", data_ng_model = "tempObject.CertNumber" })
                        </div>
                        @Html.ValidationMessageFor(model => model.CertNumber, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group col-md-3">
                        @Html.LabelFor(model => model.State, new { @class = "control-label small" })
                        <div>
                         @*   @Html.DropDownListFor(model => model.State, new[] { new SelectListItem { Value = "Florida", Text = "Florida" }, new SelectListItem { Value = "Arizona", Text = "Arizona" } }, "-- select --", new { @class = "form-control input-sm", data_ng_model = "CDSCInformation.StateOfRegistration" })*@
                            <select id="state" class="form-control input-sm" data-ng-model="tempObject.State" name="State">
                                <option value="" disabled selected>--Select--</option>
                                <option ng-repeat="state in States | unique: 'State'" value="{{state.State}}">{{state.State}}</option>
                            </select>
                        </div>
                        @Html.ValidationMessageFor(model => model.State, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group col-md-3">
                        @Html.LabelFor(model => model.IssueDate, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.IssueDate, new { @class = "form-control input-sm", type = "date", data_ng_model = "tempObject.IssueDate" })
                        </div>
                        @Html.ValidationMessageFor(model => model.IssueDate, "", new { @class = "text-danger" })
                    </div>
                    <div class="form-group col-md-3">
                        @Html.LabelFor(model => model.ExpiryDate, new { @class = "control-label small" })
                        <div>
                            @Html.TextBoxFor(model => model.ExpiryDate, new { @class = "form-control input-sm", type = "date", data_ng_model = "tempObject.ExpiryDate" })
                        </div>
                        @Html.ValidationMessageFor(model => model.ExpiryDate, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="clearfix"></div>
            <div class="col-md-12">
                <div class="col-md-11">
                    @Html.HiddenFor(model => model.CDSCCerificatePath, new { ng_value = "tempObject.CDSCCerificatePath" })


                    <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                        <div class="form-control" data-trigger="fileinput">
                            <i class="glyphicon glyphicon-file fileinput-exists"></i>
                            <span class="fileinput-filename">{{tempObject.CDSCCerificateFile}}</span>
                        </div>
                        <span class="input-group-addon btn btn-default btn-sm btn-file">
                            <span class="fileinput-new small">CDS Document</span>
                            <span class="fileinput-exists">Change</span>
                            @*   @Html.TextBoxFor(model => model.StateLicenseDocumentFile, new { type = "file", data_ng_model = "stateLicense.StateLicenseDocumentFile" })*@
                            <input type="file" name="CDSCCerificateFile" ng-model="tempObject.CDSCCerificateFile" autocomplete="off">
                        </span>

                        <a href="#" class="input-group-addon btn btn-default btn-sm fileinput-exists" data-dismiss="fileinput">Remove</a>
                    </div>
                </div>
                <div ng-show="tempObject.CDSCCerificateFile" class="col-md-1">
                    <a href="{{tempObject.CDSCCerificateFile}}" targe target="_blank"><i class="fa fa-2x fa-file"></i></a>
                </div>
            </div>
            <div class="clearfix"></div>
            <div ng-if="ErrorCDS" class="col-lg-12">
                <div class="alert alert-danger" role="alert">
                    <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                    <span class="sr-only">Error:</span>
                    Error List : <br />
                    <span ng-repeat="err in CDSError track by $index">{{err}}</span>
                </div>
            </div>
                <div class="clearfix"></div>
            <div class="col-md-12 form-group">
                <div class="col-md-12" ng-if="visibilityControl=='addCDSCInformation'">
                    <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                    <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="saveCDSCInformation(tempObject,-1)">Add</button>
                </div>
                <div class="col-md-12" ng-if="visibilityControl==($index+'_editCDSCInformation')">
                    <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                    <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="saveCDSCInformation(tempObject,$index)">Update</button>
                </div>
            </div>
          
        </form>
</div></fieldset>