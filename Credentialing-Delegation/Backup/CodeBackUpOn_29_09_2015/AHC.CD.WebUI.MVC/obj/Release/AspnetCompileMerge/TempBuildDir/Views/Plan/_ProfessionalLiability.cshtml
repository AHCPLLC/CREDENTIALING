<div class="panel-group" id="accordionProfLiability" role="tablist" aria-multiselectable="true">

    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="ProfLiabilityHeading">
            <a data-toggle="collapse" data-parent="#accordionProfLiability" href="#ProfLiability" aria-expanded="false" aria-controls="ProfLiability">
                Professional Liability Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="ProfLiability" class="panel-collapse collapse" role="tabpanel" aria-labelledby="ProfLiabilityHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-4">
                            <label class="control-label small">Carrier Name *</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Address *</label>
                            <textarea class="form-control"></textarea>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Phone Number *</label>
                            <div class="input-group">
                                <div class="input-group-btn">
                                    <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                </div>
                                <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                    <div class="arrow"></div>
                                    <h3 class="popover-title">Select Country Dial Code</h3>
                                    <div class="popover-content">
                                        <select id="ContactContryCode">
                                            <option>+91</option>
                                        </select>
                                    </div>
                                </div>
                                <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">

                        <div class="col-md-4">
                            <label class="control-label small">Policy Number</label>
                            <input type="text" class="form-control input-sm" />
                        </div>

                        <div class="col-md-4">
                            <label class="control-label small">Effective Date</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Expiration Date</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br/>
                    <div class="col-md-12">
                       
                        <label class="col-md-6 control-label small">Have you ever denied professional liability Insurance?</label>
                        <div class="col-md-6 radio">
                            <label class="radio-inline">
                                <input type="radio" name="Medicare" checked="checked"> Yes
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="Medicare"> No
                            </label>
                        </div>
                    </div>
                    <div class="col-md-12">

                        <div class="col-md-4">
                            <label class="control-label small">If yes,Insurance Carrier</label>
                            <input type="text" class="form-control input-sm" />
                        </div>

                        <div class="col-md-4">
                            <label class="control-label small">Denial Date</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Denial Reason</label>
                            <textarea class="form-control"></textarea>
                        </div>
                    </div>


