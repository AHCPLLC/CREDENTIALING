@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.PracticePaymentAndRemittanceViewModel

<div class="form-style" ng-if="visibilitySecondControl != 'PaymentRemittanceEdit'">
    <fieldset class="wrap-words">
        <legend>
            Payment and Remittance
        </legend>
        <table ng-show="PracticeLocationDetailHistory.PaymentAndRemittance == null && visibilityControl != 'PaymentRemittanceEdit' " class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <td>
                        No History Available for Payment and Remittance for this Practice Location!
                    </td>
                </tr>
            </thead>
        </table>
        <div class="form-group" ng-if="PracticeLocationDetailHistory.PaymentAndRemittance">
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.ElectronicBillingCapabilityYesNoOption, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.ElectronicBillingCapability || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.BillingDepartment, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.BillingDepartment || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.CheckPayableTo, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.CheckPayableTo || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.Office, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.Office || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.FirstName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.FirstName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.MiddleName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.MiddleName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.LastName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.LastName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.Street, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Street || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.Building, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Building || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.City, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.City || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.State, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.State || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.ZipCode, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.ZipCode || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.Country, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Country || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.County, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.County || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.Telephone, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Telephone " class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.CountryCodeTelephone }}-{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Telephone }}</i>
                        <i ng-if="!PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Telephone " class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.Fax, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Fax " class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.CountryCodeFax }}-{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Fax}}</i>
                        <i ng-if="!PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.Fax " class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PaymentAndRemittancePerson.EmailAddress, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.PaymentAndRemittance.PaymentAndRemittancePerson.EmailAddress || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
    </fieldset>
</div>