
<fieldset>
    <legend>
        Address
    </legend>
    <div class="col-lg-12 ">
        <div class="col-lg-6 form-group">
            <label class="control-label small">Unit No</label>
            <input type="text" class="form-control input-sm" id="planCode" placeholder="unit number">
        </div>

        <div class="col-lg-6 form-group">
            <label class="control-label small">Street</label>
            <input type="text" class="form-control input-sm" id="planTitle" placeholder="street">
        </div>
    </div>
    <div class="col-lg-12">
        <div class="col-lg-6 form-group">
            <label class="control-label small">State</label>
            <select class="form-control input-sm">
                <option>Select State</option>
                <option>ALABAMA</option>
                <option>ALASKA</option>
                 <option>FLORIDA</option>
            </select>
        </div>
         <div class="col-lg-6 form-group">
            <label class="control-label small">City</label>
            <select class="form-control input-sm">
                <option>Select City</option>
                <option>MOODY</option> 
                <option>JUNEAU</option>
                <option>ANGOON</option>
                <option>HARVEST</option>
                <option>HOLLYTREE</option>
                <option>HAZEL GREEN</option>
            </select>
        </div>
      
    </div>

    <div class="col-lg-12">
        <div class="col-lg-6 form-group">
            <label class="control-label small">County</label>
            <select class="form-control input-sm">
                <option>Select County</option>
                <option>MADISON</option>
                <option>JACKSON</option>
                <option>SKAGWAY HOONAH ANGOON</option>
                <option>JUNEAU</option>

            </select>
        </div>
        <div class="col-lg-6 form-group">
            <label class="control-label small">Zip Code</label>
            <input type="text" class="form-control input-sm" id="planCode" placeholder="zipcode">
        </div>
    </div>
</fieldset>
