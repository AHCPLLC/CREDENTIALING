<div class="panel-group" id="accordionAmbulatory" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="AmbulatoryHeading">
            <a data-toggle="collapse" data-parent="#accordionAmbulatory" href="#Ambu" aria-expanded="false" aria-controls="Ambu">
                Ambulatory Surgical Centres/Procedures
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="Ambu" class="panel-collapse collapse" role="tabpanel" aria-labelledby="AmbulatoryHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label for="Contact" class="control-label small">Are you affiliated with any Ambulatory Surgical Centers?</label>
                            
                                <label class="radio-inline">
                                    <input type="radio" name="Ambulatory"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="Ambulatory"> No
                                </label>
                            
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Facility Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="form-group col-md-4">
                            <label for="MobileNo" class="control-label small">Phone</label>
                            <div>
                                <div class="input-group">
                                    <div class="input-group-btn">
                                        <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                    </div>
                                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                        <div class="arrow"></div>
                                        <h3 class="popover-title">Select Country Dial Code</h3>
                                        <div class="popover-content">
                                            <select id="ContactContryCode">
                                                <option>+91</option>
                                            </select>
                                        </div>
                                    </div>
                                    <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Address</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        </div>
                    <div class="col-md-12">
                        <div class="col-md-4">
                            <label class="control-label small">City</label>
                            <select class="form-control input-sm">
                                <option>Putnam</option>
                                <option>Raipur</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">State</label>
                            <select class="form-control input-sm">
                                <option>Florida</option>
                                <option>Alaska</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Zip</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label for="Contact" class="control-label small">Do you perform surgical or any other types of procedures in your office?</label>
                            
                                <label class="radio-inline">
                                    <input type="radio" name="surgical"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="surgical"> No
                                </label>
                        
                        </div>
                        <div class="col-md-12">
                            <label for="Contact" class="control-label small">List All</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        </div>
                        <div class="clearfix"></div>
                        <br />
                        <div class="col-md-12 panel-footer">
                            <div class="col-md-12">
                                <button class="btn btn-success pull-right">Initiate Verification</button>
                                <button class="btn btn-warning">Save</button>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>

