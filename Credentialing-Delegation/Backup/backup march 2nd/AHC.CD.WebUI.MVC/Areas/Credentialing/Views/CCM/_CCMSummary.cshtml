<script src="~/Scripts/Lib/Bootstrap/jasny-bootstrap.js"></script>
<link href="~/Content/SharedCss/jasny-bootstrap.css" rel="stylesheet" />


<div class="col-lg-12 text-right ng-scope">
    <button class="btn btn-xs btn-primary"><i class="fa fa-print"></i></button>
    <button class="btn btn-xs btn-primary"><i class="fa fa-download"></i></button>
    <button class="btn btn-primary btn-xs">Approve</button>
</div>
<div class="col-lg-12 col-md-12 zero-padding-left-right-div">
    <form class="form-horizontal">
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Name :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <label class="control-label">{{tempObject.FirstName}} {{tempObject.LastName}}</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Provider Type :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                    <label class="control-label">{{tempObject.SelectedProviderTypeArray}}</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">County :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <label class="control-label">{{tempObject.SelectedCounty}}</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Specialty :</label>
            <div class="col-lg-6 col-md-6 col-sm-6 radio">
               
                    <label class="control-label">{{tempObject.SelectedSpeciality}}</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Board Certification :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.Yes}}</label>

            </div>
            <div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForBoardCertification">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Hospital Privileges :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.Yes}}</label>

            </div>
            <div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForHospitalPrivileges">
            </div>
        </div>



        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Covering Physicians(s) :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <input class="form-control input-sm" ng-class="{'input-validation-error':CoveringPhysicians.length == 0 && isHasError}" ng-model="CoveringPhysiciansType" ng-focus="showContryCodeDiv('DemographicsProviderTypesSearchResultDiv')" placeholder="Select Covering Physicians" />

                <span ng-if="CoveringPhysicians.length == 0 && isHasError" class="text-danger field-validation-error">
                    <span>Please select the CoveringPhysicians.</span>
                </span>

                <div class="ProviderTypeSelectAutoList popover fade bottom in" role="tooltip" id="DemographicsProviderTypesSearchResultDiv">
                    <table class="table table-striped table-bordered table-hover table-condensed" style="margin-bottom: 0px;">
                        <tbody>
                            <tr ng-repeat="coveringPhysicians in filteredCoveringPhysiciansTypeResults = (CoveringPhysicians | filter:CoveringPhysiciansType)" ng-click="SelectCoveringPhysiciansType(coveringPhysicians)" class="pointer">
                                <td>{{coveringPhysicians.Name}}</td>
                            </tr>
                            <tr ng-if="CoveringPhysicians.length == 0">
                                <td>No data..... </td>
                            </tr>
                            <tr ng-if="CoveringPhysicians.length > 0 && filteredCoveringPhysiciansTypeResults.length == 0">
                                <td>Title not found..... </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div ng-if="tempObject.CoveringPhysicians1.length > 0" class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label small">Selected CoveringPhysicians : </label>
            <div class="col-lg-8 col-md-8 col-sm-8">
                <span ng-repeat="tpt in tempObject.CoveringPhysicians1 track by $index">

                    {{tpt.Name}}
                    <i class="fa fa-close pointer" ng-click="RemoveCoveringPhysiciansType(tpt)"></i>
                    <input type="hidden" name="tempObject.CoveringPhysicians1[{{$index}}].CoveringPhysiciansID" value="{{tpt.CoveringPhysiciansID}}" />
                    <input type="hidden" name="tempObject.CoveringPhysicians1[{{$index}}].Status" value="{{tpt.Status}}" />

                    &nbsp;&nbsp;
                </span>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Gaps in Practice :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.No}}</label>

            </div>
            <div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForGapsInPractice">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Clean license :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.Yes}}</label>

            </div>
            <div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForCleanLicense">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">NPDB Issues :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.No}}</label>

            </div>
            @*<div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForNPDBIssues">
            </div>*@
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Malpractice Issues :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.No}}</label>

            </div>
            @*<div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForMalpracticeIssues">
            </div>*@
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Years in Practice :</label>
            <div class="col-lg-6 col-md-6 col-sm-6 radio">
                <label class="control-label">5</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Site Visit Required. If yes, were there any issues :</label>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <label class="control-label">{{tempObject.No}}</label>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4">
                <input type="text" class="form-control input-sm" ng-model="tempObject.remarkForAnyOtherIssue">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Provider Level :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="radio">
                    <label ng-repeat="yn in tempObject.ProviderLevelArray">
                        <input type="radio" name="radioPL" ng-model="tempObject.ProviderLevelSelected">{{yn}}&nbsp;
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Approval Status :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="radio">
                    <label ng-repeat="yn in tempObject.ApprovalStatus">
                        <input type="radio" name="radioPL" ng-model="tempObject.ApprovalStatusSelected">{{yn}}&nbsp;
                    </label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Approved By :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="col-md-12 zero-padding-left-right-div">
                        <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                            <div class="form-control jancyFileWrapTexts" data-trigger=" fileinput">
                                <i class="glyphicon glyphicon-file fileinput-exists"></i>
                                <span class="fileinput-filename wraptext"></span>
                            </div>
                            <span class="input-group-addon btn btn-default btn-sm btn-file">
                                <span class="fileinput-new small">Select CDS Document</span>
                                <span class="fileinput-exists">Change</span>
                                <input type="text" ng-model="tempObject.SignaturePath">
                                @*@Html.TextBoxFor(model => model.CDSCCerificateFile, new { name = "CDSCCerificateFile", ng_data_model = "tempObject.CDSCCerificateFile", type = "file" })*@
                            </span>
                            <a href="#" class="input-group-addon btn btn-default btn-sm fileinput-exists" data-dismiss="fileinput">Remove</a>
                        </div>
                      
                  
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-4 col-md-4 col-sm-4 control-label">Date :</label>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="radio">
                    <label ng-repeat="yn in tempObject.ApprovalStatus">
                        <input type="radio" name="radioPL" ng-model="tempObject.SignedDate">{{yn}}&nbsp;
                    </label>
                </div>
            </div>
        </div>
        <br />
        <br />
    </form>
</div>




