
<div class="panel panel-default content-panel" style="border:none">
    @*<div class="panel-heading">
            <span class="badge bg-info pull-right" ng-show="ifFilter()">{{getIndexFirst()}} - {{getIndexLast()}} of {{data.length}}</span>
            <h3 class="panel-title">List of DeCredentialing Initiations</h3>
        </div>*@
    <div class="clearfix"></div>
    <div class="panel-body" ng-cloak>
        <div ng-show="loadingAjax" class="progress col-lg-12" ng-cloak>
            <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                Loading...Please wait...
            </div>
        </div>
        <div ng-show="data.length==0 && !loadingAjax">
            <h4>No Data Available...</h4>
        </div>
        <div ng-show="data.length!=0" class="col-lg-12" style="overflow-x:auto;">

            <table ng-table="tableParams3" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected">

                <!-- ngInclude: templates.header -->

                <tr ng-repeat="user in $data track by $index ">

                    <td class="col-md-2" data-title="'Title'" header-class="text-left" sortable="'Title'" filter="{ 'Title': 'text' }">
                        {{user.Title}}
                    </td>
                    <td data-title="'First Name'" header-class="text-left" sortable="'FirstName'" filter="{ 'FirstName': 'text' }">
                        {{user.FirstName}}
                    </td>
                    <td data-title="'Last Name'" header-class="text-left" sortable="'LastName'" filter="{ 'LastName': 'text' }">
                        {{user.LastName}}
                    </td>
                    <td data-title="'Plan'" header-class="text-left" sortable="'PlanName'" filter="{ 'PlanName': 'text' }">
                        {{user.PlanName}}
                    </td>

                    <td data-title="'De-Credentialing Date'" header-class="text-left" filter="{ 'LastModifiedDate': 'text' }" sortable="'LastModifiedDate'">
                        {{user.LastModifiedDate}}
                    </td>
                    <td data-title="'Action'" header-class="text-left"  data-title-text="Action" class="ng-binding">
                        <a class="btn btn-xs btn-info" href="/Credentialing/Initiation/DecredentialingSummary/{{user.CredentialingInfoID}}" tooltip data-toggle="tooltip" data-placement="top" title="Plan Contract"><i class="fa fa-caret-right"></i></a>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</div>
