<br />
<div class="progress col-lg-12" ng-show="progressbar" ng-cloak>
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        Loading...Please wait...
    </div>
</div>
<div ng-show="!progressbar">
    <div class="list-group" ng-show="$index > cnt-3" ng-repeat="DecredDetail in DecredInfo track by $index" ng-init="Count()" ng-cloak>
        <a href="#" class="list-group-item">
            <i class="data-label">{{ProfileData.PersonalDetail.Salutation}}&nbsp{{ProfileData.PersonalDetail.FirstName}}&nbsp;{{ProfileData.PersonalDetail.LastName}} <label>DeCredentialing Initiated</label> for Plan: {{DecredDetail.Plan.PlanName}}, LOB: {{DecredDetail.LOB.LOBName}}, on {{DecredDetail.Report.TerminationDate | date:sort}}</i>
        </a>
    </div>
</div>
