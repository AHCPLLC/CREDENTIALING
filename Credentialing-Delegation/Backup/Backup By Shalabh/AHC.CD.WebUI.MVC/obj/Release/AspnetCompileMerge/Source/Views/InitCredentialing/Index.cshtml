@{
    ViewBag.Title = "InitCreadentialing";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Lib/Angular/angular.min.js"></script>

<link href="~/Content/SharedCss/bootstrap-duallistbox.css" rel="stylesheet" />
<link href="~/Content/SharedCss/select2/select2.css" rel="stylesheet" />

<style>
    .vertical-align {
        display: flex;
        align-items: center;
    }

    .selectedProvider {
        background-color: rgb(150, 162, 225);
    }

    .imageStyle {
        padding: 0px;
        margin-bottom: 0px;
    }

    .alignTableStyle > thead > tr > th,
    .alignTableStyle > tbody > tr > th,
    .alignTableStyle > tfoot > tr > th,
    .alignTableStyle > thead > tr > td,
    .alignTableStyle > tbody > tr > td,
    .alignTableStyle > tfoot > tr > td {
        vertical-align: middle;
        text-align: center;
    }

    .marginTop {
        margin-top: 22px;
    }

    .marginBottom {
        margin-bottom: 22px;
    }

    .select2-container {
        display: block;
    }
</style>


<div id="page-wrapper">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="fa fa-dashboard"></i>  <a href="/Dashboard/Index">Dashboard</a>
                    </li>
                    <li class="active">
                        <i class="fa fa-search"></i> Initiate Credentialing
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <div class="row" ng-app="ProviderApp" ng-controller="ProviderCtrlNew" ng-cloak>
            <div class="col-lg-12">
                <div class="panel panel-default content-panel">
                    <div class="panel-heading">
                        <button class="pull-right" onclick="SearchProviderPanelToggle('SearchProviderPanel');"><i class="fa fa-th-list"></i></button>
                        <div class="panel-title">Search Providers</div>
                    </div>
                    <div id="SearchProviderPanel" class="panel-body">
                        <div class="col-lg-6">
                            <div class="form-style">
                                <fieldset>
                                    <legend>
                                        Search By Providers Detail
                                    </legend>
                                    <div class="col-lg-12">
                                        <div class="radio inline"><input type="radio" name="searchByProviderType" ng-model="searchByProviderType" ng-value="false" /></div>
                                        <div class="col-lg-11 form-group">
                                            <label class="col-lg-3 control-label">NPI Number</label>
                                            <div class="col-lg-9">
                                                <input type="text" class="form-control input-sm" ng-readonly="searchByProviderType" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="radio inline"><input type="radio" name="searchByProviderType" ng-model="searchByProviderType" ng-value="true" /></div>
                                        <div class="col-lg-11 form-group">
                                            <label class="col-lg-3 control-label">First name</label>
                                            <div class="col-lg-9">
                                                <input type="text" class="form-control input-sm" ng-readonly="!searchByProviderType" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="col-lg-11 form-group">
                                            <label class="col-lg-3 control-label">Last name</label>
                                            <div class="col-lg-9">
                                                <input type="text" class="form-control input-sm" ng-readonly="!searchByProviderType" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="col-lg-11 form-group">
                                            <label class="col-lg-3 control-label">Group</label>
                                            <div class="col-lg-9">
                                                <select id="groups" multiple>
                                                    <option>
                                                        American Medical Student Association
                                                    </option>
                                                    <option>
                                                        Archimedes Movement
                                                    </option>
                                                    <option>
                                                        California Nurses Association/National Nurses Organizing Committee
                                                    </option>
                                                    <option>
                                                        Campaign for Better Health Care
                                                    </option>
                                                    <option>
                                                        Families USA
                                                    </option>
                                                    <option>
                                                        Health Care for America NOW!
                                                    </option>
                                                    <option>
                                                        Healthcare-NOW!
                                                    </option>
                                                    <option>
                                                        Medicare Rights Center
                                                    </option>
                                                    <option>
                                                        National Physicians Alliance
                                                    </option>
                                                    <option>
                                                        Pennsylvania Medical Society
                                                    </option>
                                                    <option>
                                                        Physicians for a National Health Program
                                                    </option>
                                                    <option>
                                                        ShoutAmerica
                                                    </option>
                                                    <option>
                                                        Single Payer Now!
                                                    </option>
                                                    <option>  Universal Health Care Foundation of Connecticut</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="col-lg-11 form-group">
                                            <label class="col-lg-3 control-label">Specialty</label>
                                            <div class="col-lg-9">
                                                <select id="specialities" multiple>
                                                    <option>
                                                        UROLOGY
                                                    </option>
                                                    <option>
                                                        RHEUMATOLOGY
                                                    </option>
                                                    <option>
                                                        RADIATION ONCOLOGY
                                                    </option>
                                                    <option>
                                                        PULMONARY
                                                    </option>
                                                    <option>
                                                        PSYCHIATRY
                                                    </option>
                                                    <option>
                                                        PODIATRY
                                                    </option>
                                                    <option>  PAIN MANAGEMENT</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="col-lg-12 text-center" ng-click="new_search();"><button class="btn btn-primary btn-sm"><i class="fa fa-search"></i> Search</button></div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                        <div class="col-lg-6 form-horizontal">
                            <div class="form-style">
                                <fieldset>
                                    <legend>
                                        Search By Plan
                                    </legend>
                                    <div class="col-lg-12 form-group">
                                        <label class="col-lg-3 control-label">Company</label>
                                        <div class="col-lg-9">
                                            <select class="form-control input-sm">
                                                <option>Ultimate</option>
                                                <option>FREEDOM</option>
                                                <option>Wellcare</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 form-group">
                                        <label class="col-lg-3 control-label">Category</label>
                                        <div class="col-lg-9">
                                            <select class="form-control input-sm">
                                                <option>HMO</option>
                                                <option>HDHPs</option>
                                                <option>POS</option>
                                                <option>HSAs</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12  form-group">
                                        <label class="col-lg-3 control-label">Plan</label>
                                        <div class="col-lg-9">
                                            <select id="plans" multiple>
                                                <option>VIP SAVINGS</option>
                                                <option>SAVINGS PLAN RX</option>
                                                <option>SAVINGS COPD</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="col-lg-12 text-center pull-right" ng-click="new_search();"><button class="btn btn-primary btn-sm"><i class="fa fa-search"></i> Search</button></div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>
                @* Client side search start here *@
                <div class="panel panel-default">
                    <div class="panel panel-heading">
                        <span class="pull-right badge label-primary">Total {{allProviders.length}}</span>
                        Search Result
                    </div>
                    <div class="panel-body">
                        <table class="table table-bordered table-hover table-condensed alignTableStyle">
                            <thead>
                                <tr>
                                    <th rowspan="2">Profile Pic</th>
                                    <th>NPI Number</th>
                                    <th>First Name</th>
                                    <th>Middle Name</th>
                                    <th>Last Name</th>
                                    <th>Specialty</th>
                                    <th rowspan="2"><button class="btn btn-primary btn-sm" ng-click="addAllSelected()"><i class="fa fa-plus-circle"></i> Selected</button></th>
                                </tr>
                                <tr>
                                    <td><input type="text" placeholder="Filter NPI" class="form-control input-sm" ng-model="search.NPI" /></td>
                                    <td><input type="text" placeholder="Filter First Name" class="form-control input-sm" ng-model="search.FirstName" /></td>
                                    <td><input type="text" placeholder="Filter Middle Initial" class="form-control input-sm" ng-model="search.MiddleName" /></td>
                                    <td><input type="text" placeholder="Filter Last Name" class="form-control input-sm" ng-model="search.LastName" /></td>
                                    <td><input type="text" placeholder="Filter Specialty" class="form-control input-sm" ng-model="search.Specialty" /></td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-class="{selectedProvider: p.IsSelected == true}" ng-click="selectProvider($index)" ng-repeat="p in allProviders | filter:{Title: search.Title,NPI:search.NPI,FirstName:search.FirstName,MiddleName:search.MiddleName,LastName:search.LastName,Specialities:search.Specialities}">
                                    <td>
                                        <div class="thumbnail imageStyle">
                                            <img src="~/Content/Images/Providers/{{p.Image}}" class="img-rounded">
                                        </div>
                                    </td>
                                    <td>{{p.NPI}}</td>
                                    <td>{{p.FirstName}}</td>
                                    <td>{{p.MiddleName || 'N/A'}}</td>
                                    <td>{{p.LastName}}</td>
                                    <td><span ng-repeat="s in p.Specialities"><span ng-if="$index > 0">, </span>{{s}}</span></td>
                                    <td>
                                        <a class="btn btn-primary btn-sm" href="~/Profile/MasterProfile/Index" data-toggle="tooltip" data-placement="right" title="View Profile" target="_blank"><i class="fa fa-search-plus"></i></a>
                                        <a class="btn btn-primary btn-sm" ng-click="addSelectedProvider($index);$event.stopPropagation();"><i class="fa fa-plus-circle"></i></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                        <div class="form-style">
                            <div class="col-lg-3" style="padding-left: 0px;">
                                <fieldset>
                                    <legend>
                                        Selected Providers
                                    </legend>
                                    <div ng-repeat="p in providers">
                                        <div class="col-md-1">
                                            <input type="checkbox" />
                                            <i class="fa fa-times text-danger" ng-click="unselectProvider($index)"></i>
                                        </div>
                                        <div class="col-md-10" ng-click="setCurrentProvider($index)">
                                            <div class="col-md-6 thumbnail">
                                                <img src="~/Content/Images/Providers/{{p.Image}}" class="img-rounded">
                                            </div>
                                            <div class="col-md-6">
                                                <h5 class="small">{{p.Name}}</h5>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="col-lg-9" style="padding-left: 0px; padding-right: 0px;">
                                <fieldset>
                                    <legend>
                                        Plan Association
                                    </legend>
                                    <div class="col-lg-12 zero-padding-left-right-div">
                                        <fieldset>
                                            <legend>
                                                Existing Plans
                                            </legend>
                                            <table class="table">
                                                <tbody>
                                                    <tr ng-repeat="credPlan in currentProvider.relatedPlans">
                                                        <td>
                                                            <div class="thumbnail">
                                                                <img src="~/Content/Images/Plans/{{credPlan.Plan.Logo}}" class="img-rounded" height="60px" width="160px">
                                                            </div>
                                                        </td>
                                                        <td>
                                                            <label>{{credPlan.Plan.Title}}</label><br />
                                                            <span>{{credPlan.Plan.InsuranceCompany}}</span>
                                                        </td>
                                                        <td>
                                                            <ol class="progtrckr panel-info" data-progtrckr-steps="5">
                                                                <li class="progtrckr-done">Initiated</li>
                                                                <li class="progtrckr-done">Submitted</li>
                                                                <li class="progtrckr-done">Verified</li>
                                                                <li class="progtrckr-fail">Rejected</li>
                                                                <li class="progtrckr-todo">Completed</li>
                                                            </ol>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </fieldset>
                                    </div>
                                    <div class="col-lg-12 zero-padding-left-right-div">
                                        <fieldset>
                                            <legend>
                                                New Plans
                                            </legend>
                                            <div class="col-lg-12 zero-padding-left-right-div">
                                                <div class="col-lg-5">
                                                    <label class="col-lg-3 control-label">Company</label>
                                                    <select class="col-lg-9 form-control input-sm">
                                                        <option>Ultimate</option>
                                                        <option>FREEDOM</option>
                                                        <option>Wellcare</option>
                                                    </select>
                                                </div>
                                                <div class="col-lg-5">
                                                    <label class="col-lg-3 control-label">Category</label>
                                                    <select class="col-lg-9 form-control input-sm">
                                                        <option>HMO</option>
                                                        <option>HDHPs</option>
                                                        <option>POS</option>
                                                        <option>HSAs</option>
                                                    </select>
                                                </div>
                                                <div class="col-lg-2 text-center">
                                                    <button ng-click="planSearhed=true" class="btn btn-info btn-xs marginTop"><i class="fa fa-search"></i> Search </button>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 zero-padding-left-right-div marginBottom">
                                            </div>
                                            <div class="col-lg-12 zero-padding-left-right-div">
                                                <div class="col-lg-6">
                                                    <fieldset>
                                                        <legend>
                                                            Plans
                                                        </legend>
                                                        <table class="table" ng-show="planSearhed">
                                                            <tbody>
                                                                <tr ng-repeat="plan in allPlans">
                                                                    <td>
                                                                        <div class="thumbnail">
                                                                            <img src="~/Content/Images/Plans/{{plan.Logo}}" class="img-rounded" height="60px" width="160px">
                                                                        </div>
                                                                    </td>
                                                                    <td>
                                                                        <label>{{plan.Title}}</label><br />
                                                                        <span>{{plan.InsuranceCompany}}</span>
                                                                    </td>
                                                                    <td>
                                                                        <i class="fa fa-plus-circle pull-left" style="font-size:16px; cursor:pointer;" ng-click="addSelectedPlan($index);$event.stopPropagation();"></i>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </fieldset>
                                                </div>
                                                <div class="col-lg-6">
                                                    <fieldset>
                                                        <legend>
                                                            Selected Plans
                                                        </legend>
                                                        <table class="table" ng-show="planSearhed">
                                                            <tbody>
                                                                <tr ng-repeat="plan in selectedPlans">
                                                                    <td>
                                                                        <div class="thumbnail">
                                                                            <img src="~/Content/Images/Plans/{{plan.Logo}}" class="img-rounded" height="60px" width="160px">
                                                                        </div>
                                                                    </td>
                                                                    <td>
                                                                        <label>{{plan.Title}}</label><br />
                                                                        <span>{{plan.InsuranceCompany}}</span>
                                                                    </td>
                                                                    <td>
                                                                        <i class="fa fa-minus-circle pull-right" style="font-size:16px; cursor:pointer;" ng-click="deleteSelectedPlan($index);$event.stopPropagation();"></i>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </fieldset>
                                                </div>
                                                <div class="col-md-12 zero-padding-left-right-div">
                                                    <div class="col-md-4">
                                                        <label class="control-label">Group</label>
                                                        <select class="form-control input-sm">
                                                            <option>
                                                                Families USA
                                                            </option>
                                                            <option>
                                                                Health Care for America
                                                            </option>
                                                            <option>
                                                                Healthcare-NOW!
                                                            </option>
                                                            <option>
                                                                Medicare Rights Center
                                                            </option>
                                                            <option>
                                                                ShoutAmerica
                                                            </option>
                                                            <option>
                                                                Single Payer Now!
                                                            </option>
                                                        </select>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label class="control-label">Location</label>
                                                        <select class="form-control input-sm">
                                                            <option>5350 Spring Hill Drive</option>
                                                            <option>14690 Spring Hill Drive</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label class="control-label">Specialty</label>
                                                        <select class="form-control input-sm">
                                                            <option>
                                                                UROLOGY
                                                            </option>
                                                            <option>
                                                                RHEUMATOLOGY
                                                            </option>
                                                            <option>
                                                                RADIATION ONCOLOGY
                                                            </option>
                                                            <option>
                                                                PULMONARY
                                                            </option>
                                                            <option>
                                                                PSYCHIATRY
                                                            </option>
                                                            <option>
                                                                PODIATRY
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </fieldset>
                                <div class="col-md-12">
                                    <a class="btn btn-primary btn-sm pull-right" ng-click="initiated=true">Initiate Credentialing</a>
                                    <button class="btn btn-info btn-sm" ng-click="saved=true">Save</button>
                                </div>
                                <div class="col-md-12 clearfix">
                                    <br />
                                    <div class="col-lg-12">
                                        <div class="alert alert-success alert-dismissible" role="alert" ng-show="initiated">
                                            <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                            <strong>Success!</strong> Credentialing Initiated Successfully.
                                        </div>

                                        <div class="alert alert-success alert-dismissible" role="alert" ng-show="saved">
                                            <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                            <strong>Success!</strong> Credentialing Data Saved Successfully.
                                        </div>
                                    </div>
                                </div>
                                </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
    </div>
</div>
<!-- /#page-wrapper -->
<script src="~/Scripts/Lib/Jquery/jquery-1.11.0.js"></script>
<script src="~/Scripts/Custom/Profile/Util.js"></script>
<script src="~/Scripts/Lib/Jquery/select2.min.js"></script>
<script src="~/Scripts/Lib/Bootstrap/jquery.bootstrap-duallistbox.js"></script>
<script src="~/Scripts/Custom/Credentialing/CredentialingInitNew.js"></script>

