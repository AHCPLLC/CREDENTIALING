
@*<script src="~/Areas/Profile/Scripts/JavaScripts/PdfMappingJS.js"></script>*@

<div class="panel-group" ng-controller="PdfMappingController">

    

    <div class="form-style">
        <fieldset>
            <legend>
                PDF Mapping
            </legend>
            <div class="clearfix"></div><br />
            <div class="progress col-lg-12" ng-if="loadingAjax" ng-cloak>
                <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                    Generating...Please wait...
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="col-lg-4 col-md-4 col-sm-4">
                    <ul class="list-group">                       
                        <li class="list-group-item">                            
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th class="col-lg-4">Plan Template &nbsp;<input type="text" id="pdfText" ng-model="profileId" /></th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr><td><a ng-click="ViewTemplate(profileId,'AETNACOVENTRYTemplate')" class="pointer">AETNA</a></td></tr> 
                                    <tr><td><a ng-click="ViewTemplate(profileId,'TRICARE_PROVIDER_APPLICATION')" class="pointer">TRICARE PROVIDER APPLICATION</a></td></tr>
                                    <tr><td><a ng-click="ViewTemplate(profileId,'Letter_of_Intent')" class="pointer">LETTER OF INTENT</a></td></tr>
                                    <tr><td><a ng-click="ViewTemplate(profileId,'TRICARE_PA_APPLICATION')" class="pointer">TRICARE PA APPLICATION</a></td></tr>
                                    <tr><td><a ng-click="ViewTemplate(profileId,'ALLIED_CREDENTIALING_APPLICATION')" class="pointer">ALLIED CREDENTIALING APPLICATION</a></td></tr>                                    
                                    <tr><td><a ng-click="ViewTemplate(profileId,'UltimateCredentialingApplicationPractitioner_2015')" class="pointer">ULTIMATE CREDENTIALING APPLICATION PRACTIONER 2015</a></td></tr>
                                   
                                </tbody>
                            </table>
                        </li>                     
                    </ul>
                </div>

               
            </div>
            
        </fieldset>
    </div>
</div>
