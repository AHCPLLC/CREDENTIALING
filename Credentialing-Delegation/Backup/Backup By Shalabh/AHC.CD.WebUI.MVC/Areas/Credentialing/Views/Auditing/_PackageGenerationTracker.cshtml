

<div ng-if="!loading && data.length > 0">
    <table ng-table="tableParams2" class="table table-bordered table-hover table-condensed alignTableStyle">
        <thead>
            <tr>
                <th>Generated Package</th>
                <th>Generated By</th>
                <th>Generated For</th>
                <th>Generated Date</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="t in $data track by $index" ng-show="IsValidIndex($index)">

                <td class="wrap-text">
                    <a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{t.PackageFilePath}}" target="_blank" tooltip data-toggle="tooltip" data-placement="top" title="Package"><i class="fa fa-file" title=""></i></a>
                </td>
                <td class="wrap-text">{{t.GeneratedByName || 'Not Available'}}</td>
                <td class="wrap-text">{{t.GeneratedFor.PersonalDetail.FirstName}} {{t.GeneratedFor.PersonalDetail.MiddleName}} {{t.GeneratedFor.PersonalDetail.LastName}}</td>
                <td class="wrap-text">{{t.GeneratedDate | date:'MM-dd-yyyy'}}</td>

            </tr>
        </tbody>
    </table>
</div>
<div ng-if="!loading && data.length == 0">No Data Available</div>

<div ng-show="loading">
    <div class="progress">
        <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
            <span>Loading...Please wait...</span>
        </div>
    </div>
</div>