@*Add Section Of Mid-Level Practitioners Starts Here*@
<fieldset>
    <legend>
        Mid-Level Practitioners
    </legend>
    <div id="MidLevelPractitionerDiv">
        <div class="col-lg-12">
            <table ng-if="tempObject.FacilityDetail.FacilityPracticeProviders.length>0" class=" table table-bordered table-condensed customtbodystyle table-striped">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Middle name</th>
                        <th>Last Name</th>
                        <th>NPI Number </th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody ng-repeat="practitioner in tempObject.FacilityDetail.FacilityPracticeProviders track by $index">
                    <tr>
                        <td>{{practitioner.FirstName || 'Not Available'}}</td>
                        <td>{{practitioner.MiddleName || 'Not Available'}}</td>
                        <td>{{practitioner.LastName || 'Not Available'}}</td>
                        <td>{{practitioner.NPINumber || 'Not Available'}}</td>
                        <td class="col-lg-2 zero-padding-left-right-div">
                            <a class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Edit" ng-click="operateThirdEditControl('editMidlevel'+$index,practitioner);" tooltip><i class="fa fa-edit"></i></a>
                            <a class="btn btn-xs btn-danger" data-toggle="tooltip" data-placement="top" title="Remove" ng-click="removeMidLevelProviderPractitioner(practitioner,visibilityControlPracLoc);" tooltip><i class="fa fa-minus-circle"></i></a>
                            <a tooltip data-toggle="tooltip" data-placement="right" title="Add New" ng-hide="$index+1!=tempObject.FacilityDetail.FacilityPracticeProviders.length" class="btn btn-xs btn-success" ng-click="resetMidLevelProviderPractitioner();"><i class="fa fa-plus"></i></a>
                        </td>
                    </tr>
                    <tr ng-if="visibilityThirdControl==('editMidlevel'+$index)">
                        <td colspan="5">
                            <div class="col-lg-12 zero-padding-left-right-div">
                                <div class="col-lg-3">
                                    <div>
                                        <label class="control-label small">First Name*</label>
                                    </div>
                                    <input type="text" class="form-control input-sm" name="fname" ng-model="tempThirdObject.FirstName" />
                                    <span class="text-danger field-validation-error" id="FnameMsgInner"></span>
                                </div>
                                <div class="col-lg-3">
                                    <div>
                                        <label class="control-label small">Middle Name</label>
                                    </div>
                                    <input type="text" class="form-control input-sm" name="Mname" value="{{tempThirdObject.MiddleName}}" ng-model="tempThirdObject.MiddleName" />
                                    <span class="text-danger field-validation-error" id="MnameMsgInner"></span>
                                </div>
                                <div class="col-lg-3">
                                    <div>
                                        <label class="control-label small">Last Name*</label>
                                    </div>
                                    <input type="text" class="form-control input-sm" name="Lname" value="{{tempThirdObject.LastName}}" ng-model="tempThirdObject.LastName" />
                                    <span class="text-danger field-validation-error" id="LnameMsgInner"></span>
                                </div>
                                <div class="col-lg-3">
                                    <div>
                                        <label class="control-label small">NPI Number</label>
                                    </div>
                                    <input type="text" class="form-control input-sm" name="NPINumber" value="{{tempThirdObject.NPINumber}}" ng-model="tempThirdObject.NPINumber" />
                                    <span class="text-danger field-validation-error" id="NPINumberMsgInner"></span>
                                </div>
                                <div class="clearfix"></div><br />
                            </div>
                            <div class="col-md-12 form-group" ng-if="visibilityThirdControl==('editMidlevel'+$index)">
                                <a class="btn btn-warning btn-sm" ng-click="operateThirdCancelControl();">Cancel</a>
                                <a class="btn btn-success btn-sm pull-right" ng-click="updateFacilityMidlevelPractioners(tempObject.FacilityDetail.FacilityPracticeProviders,tempThirdObject, $index, 'inner');">Update</a>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
            <div ng-if="tempObject.FacilityDetail.FacilityPracticeProviders.length>0">
                <div ng-repeat="practitioner in tempObject.FacilityDetail.FacilityPracticeProviders">
                    <span ng-if="tempObject.FacilityDetail.FacilityPracticeProviders[$index].FacilityPracticeProviderID">
                        <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].FacilityPracticeProviderID" value="{{practitioner.FacilityPracticeProviderID}}" />
                    </span>
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].PracticeType" value="{{practitioner.PracticeType}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].RelationType" value="{{practitioner.RelationType}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].FirstName" value="{{practitioner.FirstName}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].MiddleName" value="{{practitioner.MiddleName}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].LastName" value="{{practitioner.LastName}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].NPINumber" value="{{practitioner.NPINumber}}" />
                    <input type="hidden" name="FacilityDetail.FacilityPracticeProviders[{{$index}}].StatusType" value="{{practitioner.StatusType}}" />
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <fieldset ng-if="tempObject.FacilityDetail.FacilityPracticeProviders.length<1 || addmidlevelNew">
            <legend>
                Add Mid-Level Practitioners
            </legend>
            <div class="col-md-12 form-group zero-padding-left-right-div">
                <div class="form-group col-md-6">
                    <label class="control-label small">Use Existing Mid-Level Practitioners?</label>
                    <div>
                        <label class="radio-inline"><input type="radio" name="RelationType" value="@AHC.CD.Entities.MasterData.Enums.RelationType.Internal" ng-change="clearData(MidLevelProviderPractitioner.RelationType)" ng-model="MidLevelProviderPractitioner.RelationType" /> Yes </label>
                        <label class="radio-inline"><input type="radio" name="RelationType" value="@AHC.CD.Entities.MasterData.Enums.RelationType.External" ng-change="clearData(MidLevelProviderPractitioner.RelationType)" ng-model="MidLevelProviderPractitioner.RelationType"  ng-click="cleartextboxes()"/> No </label>
                    </div>
                </div>
                <div class="form-group col-md-6 pull-right">
                    <label class="control-label small">Select Mid-Level Practitioner </label>
                    <input type="text" ng-disabled="MidLevelProviderPractitioner.RelationType != '@AHC.CD.Entities.MasterData.Enums.RelationType.Internal'" class="form-control input-sm" name="midLevelPractioner" data-practitioners="midLevelPractitioners" data-onselect="setFacilityMidLevelPractitioner" data-msg="Mid-Level Practitioners" data-autocomplete="practitioner" practitionersearch />
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div" ng-if="MidLevelProviderPractitioner.FirstName || MidLevelProviderPractitioner.LastName || MidLevelProviderPractitioner.NPINumber || MidLevelProviderPractitioner.RelationType == 'External'">
                <div class="col-lg-3">
                    <div>
                        <label class="control-label small" >First Name*</label>
                    </div>
                    <input type="text" class="form-control input-sm" name="fname" placeholder="Fname" value="{{MidLevelProviderPractitioner.FirstName}}" ng-model="MidLevelProviderPractitioner.FirstName" />
                    <span class="text-danger field-validation-error" id="FnameMsg"></span>
                </div>
                <div class="col-lg-3">
                    <div>
                        <label class="control-label small" >Middle Name</label>
                    </div>
                    <input type="text" class="form-control input-sm" name="Mname" placeholder="Mname" value="{{MidLevelProviderPractitioner.MiddleName}}" ng-model="MidLevelProviderPractitioner.MiddleName" />
                    <span class="text-danger field-validation-error" id="MnameMsg"></span>
                </div>
                <div class="col-lg-3">
                    <div>
                        <label class="control-label small" >Last Name*</label>
                    </div>
                    <input type="text" class="form-control input-sm" name="=Lname" placeholder="Lname" value="{{MidLevelProviderPractitioner.LastName}}" ng-model="MidLevelProviderPractitioner.LastName" />
                    <span class="text-danger field-validation-error" id="LnameMsg"></span>
                </div>
                <div class="col-lg-3">
                    <div>
                        <label class="control-label small" >NPI Number*</label>
                    </div>
                    <input  type="text" class="form-control input-sm" name="NPINumber" value="{{MidLevelProviderPractitioner.NPINumber}}" ng-model="MidLevelProviderPractitioner.NPINumber" placeholder="NPINumber" />
                    <span class="text-danger field-validation-error" id="NPINumberMsg"></span>
                </div>
                <div class="clearfix"></div><br />
            </div>
            <div class="col-md-12 form-group">

                <a class="btn btn-warning btn-sm" ng-click="resetMidLevelProviderPractitioner();" ng-if="tempObject.FacilityDetail.FacilityPracticeProviders">Cancel</a>
                <a class="btn btn-success btn-sm pull-right" ng-disabled="!(MidLevelProviderPractitioner.RelationType == 'External' || (MidLevelProviderPractitioner.RelationType == 'Internal' && disableBit))" ng-click="addFacilityMidlevelPractioners(MidLevelProviderPractitioner, 'outer');">Add</a>
              
            </div>
        </fieldset>
    </div>
</fieldset>
