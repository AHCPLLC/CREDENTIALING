        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
        {
            <label class="radio-inline">
                @Html.RadioButtonFor(model => model.FacilityDetail.Language.AmericanSignLanguageYesNoOption, (int)value, new { data_ng_model = "tempObject.FacilityDetail.Language.AmericanSignLanguageYesNoOption" }) @value.ToString()
