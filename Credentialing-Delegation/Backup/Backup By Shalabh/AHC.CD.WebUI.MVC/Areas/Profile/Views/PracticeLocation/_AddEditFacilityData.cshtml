    @Html.HiddenFor(model => model.FacilityID, new { data_ng_value = "tempObject.FacilityID" })
    @Html.HiddenFor(model => model.FacilityDetail.FacilityDetailID, new { data_ng_value = "tempObject.FacilityDetail.FacilityDetailID" })
    @Html.HiddenFor(model => model.FacilityDetail.Service.FacilityServiceID, new { data_ng_value = "tempObject.FacilityDetail.Service.FacilityServiceID" })
    @Html.HiddenFor(model => model.FacilityDetail.Accessibility.FacilityAccessibilityID, new { data_ng_value = "tempObject.FacilityDetail.Accessibility.FacilityAccessibilityID" })
    @Html.HiddenFor(model => model.FacilityDetail.Language.FacilityLanguageID, new { data_ng_value = "tempObject.FacilityDetail.Language.FacilityLanguageID" })
