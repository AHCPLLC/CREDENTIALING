<div ng-show="loading" class="progress col-lg-12" ng-cloak>
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        Loading...Please wait...
    </div>
</div>
<div ng-show="!loading && errorInitiated == true">
    <div class="zero-padding-left-right-div" ng-repeat="p in formattedDataForRecred1"><label class="control-label small">{{p.PlanName}} : {{p.Days}}&nbsp;days</label></div>

    <div class="col-md-12">
        <button class="btn btn-primary btn-xs pull-right" type="submit" ng-click="CallView3();">View All</button>
    </div>
</div>
<div ng-show="errorInitiated == false && !loading">
    <label class="control-label small">No Upcoming Re-Credentialing information available.</label>
</div>