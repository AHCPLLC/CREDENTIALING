<div ng-show="loading" class="progress col-lg-12" ng-cloak>
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        Loading...Please wait...
    </div>
</div>
<div ng-show="!loading && (!tempData[0].Licenses.length == '0' || !tempData[1].Licenses.length == '0' || !tempData[2].Licenses.length == '0' || !tempData[3].Licenses.length == '0' || !tempData[4].Licenses.length == '0' || !tempData[5].Licenses.length == '0' || !tempData[6].Licenses.length == '0')">
    <div class="zero-padding-left-right-div"><label class="control-label small">Upcoming Renewals : <label class="label label-info">{{GrandTotal.GrandTotalValidLicense}}</label></label></div>
    <div class="zero-padding-left-right-div"><label class="control-label small">Renewals Needed :<label class="label label-warning">{{GrandTotal.GrandTotalPendingDaylicense}}</label></label></div>
    <div class="zero-padding-left-right-div"><label class="control-label small">Expired : <label class="label label-danger">{{GrandTotal.GrandTotalExpiredLicense}}</label></label></div>
    <div class="col-md-12">
        <button class="btn btn-primary btn-xs pull-right" type="submit" ng-click="CallView2();">View All</button>
    </div>
</div>
<div ng-show="tempData[0].Licenses.length == '0' && tempData[1].Licenses.length == '0' && tempData[2].Licenses.length == '0' && tempData[3].Licenses.length == '0' && tempData[4].Licenses.length == '0' && tempData[5].Licenses.length == '0' && tempData[6].Licenses.length == '0'">
    <label class="control-label small">No Upcoming Renewal information available.</label>
</div>