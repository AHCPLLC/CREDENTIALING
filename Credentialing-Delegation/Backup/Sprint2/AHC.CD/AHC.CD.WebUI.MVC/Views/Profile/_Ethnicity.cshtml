<br />
<div class="panel-group" id="accordion3" role="tablist" aria-multiselectable="true" ng-controller="ethnicityController">
    <div class="panel panel-default">
        <div class="panel-heading text-right">
            <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-minus-square-o"></i></button>
            <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Expand All" onclick="expandAll(this)"><i class="fa fa-plus-square-o"></i></button>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="birth_heading">

            <a data-toggle="collapse" data-parent="#accordion3" href="#birth" aria-expanded="true" aria-controls="birth">
                Birth Information
                <span class="pull-right">
                    <span class="glyphicon glyphicon-minus"></span>
                </span>
            </a>

        </div>
        <div id="birth" class="panel-body panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
            <div class="form-group">
                <div class="col-md-12">
                    <div class="form-group col-md-4">
                        <label class="control-label small">Birth Date *</label>
                        <div>
                            <input type="date" class="form-control input-sm" />
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="control-label small">Birth Country *</label>
                        <div>
                            <select id="selectNameTitle" name="countryName" class="form-control input-sm" required>
                                <option selected disabled hidden>Select Country</option>
                                <option>USA</option>
                                <option>India</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label small">Birth State *</label>
                        <div>
                            <select id="selectNameTitle" name="stateName" class="form-control input-sm" required>
                                <option selected disabled hidden>Select State</option>
                                <option>Florida</option>
                                <option>New York</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>

                <div class="col-md-12">
                    <div class="form-group col-md-6">
                        <label class="control-label small">Birth County</label>
                        <div>
                            <select id="selectNameTitle" name="countyName" class="form-control input-sm">
                                <option selected disabled hidden>Select County</option>
                                <option>Alachua</option>
                                <option>Baker</option>
                                <option>Bay</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="control-label small">Birth City *</label>
                        <div>
                            <select class="col-md-12" style="padding:0" id="selectCity" ng-model="Provider.Address.City" name="ProviderAddressCity" required>
                                <option></option>
                                <option>Spring Hill</option>
                                <option>Largo</option>
                                <option>Lakeland</option>
                            </select>
                            @*<input type="text" class="form-control input-sm" />*@
                        </div>
                    </div>
                </div>

                <div class="col-md-12">

                    <div class="form-group col-md-12">
                        <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                            <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
                            <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new">Select Birth Certificate</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
                            <a href="#" class="input-group-addon btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
                        </div>
                    </div>

                </div>

                <div class="clearfix"></div>
                <div class="col-md-12 panel-footer">
                    @*<button class="btn btn-success pull-right">Initiate Verification</button>*@
                    <button class="btn btn-success btn-sm pull-right">Save</button>
                </div>

            </div>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="citizenship-heading">
            <a data-toggle="collapse" data-parent="#accordion3" href="#citizenship" aria-expanded="false" aria-controls="citizenship">
                Citizenship
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="citizenship" class="panel-body panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
            <div class="form-style">
                <fieldset>
                    <legend>
                        Visa Details
                    </legend>
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-lg-6">
                            <label class="control-label small">Are you a citizen of US? *</label>
                            &nbsp;&nbsp;
                            <label class="radio-inline">
                                <input type="radio" name="citizen" value="Y" checked="checked" required onclick="visaViewHide()"> Yes
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="citizen" value="N" required onclick="visaViewShow()"> No
                            </label>
                        </div>
                        @*<div class="col-lg-6">
                            <label class="control-label small">Are you authorised to work in US?*</label>
                            &nbsp;&nbsp;
                            <label class="radio-inline">
                                <input type="radio" name="authwork" value="Y" checked="checked" required> Yes
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="authwork" value="N" required> No
                            </label>
                        </div>*@
                    </div>
                    <div id="visaView" style="display:none;">
                        <div class="col-lg-12 clearfix" style="padding-bottom:15px;"></div>
                        <div class="col-lg-12 zero-padding-left-right-div">
                            <table class="table table-bordered table-condensed customtbodyStyle">
                                <thead>
                                    <tr>
                                        <th>Visa Number</th>
                                        <th>Visa type</th>
                                        <th>Visa Expiration</th>
                                        <th>Visa Status</th>
                                        <th>Certificate</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="vis in Visas">
                                    <tr>
                                        <td>{{vis.Number}}</td>
                                        <td>{{vis.Type}}</td>
                                        <td>{{vis.Expiration}}</td>
                                        <td>{{vis.Status}}</td>
                                        <td><a href="#" title="{{vis.Certificate}}"><i class="fa fa-file" title=""></i></a></td>
                                        <td>
                                            <button class="btn btn-xs btn-primary" ng-click="editVisa($index,vis)"><i class="fa fa-edit"></i></button>
                                        </td>
                                    </tr>
                                    <tr ng-show="showingDetails && vis==Visa">
                                        <td colspan="6" id="visaEditDiv{{$index}}" class="editTableDataStyle">Java Script or Jquery or Angular Error!!!!!!!!</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                        <div class="clearfix"></div>

                        <div id="visaForm" style="display:none;">
                            <div class=" col-lg-12 zero-padding-left-right-div">
                                <div class="col-md-4 form-group">
                                    <label class="control-label small">Visa number *</label>
                                    <div>
                                        <input type="text" ng-model="Visa.Number" class="form-control input-sm" />
                                    </div>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label small">Type of Visa *</label>
                                    <div>
                                        <select id="selectNameTitle" ng-model="Visa.Type" name="visatype" class="form-control input-sm" required>
                                            <option>Business</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label small">Visa status *</label>
                                    <div>
                                        <select id="selectNameTitle" ng-model="Visa.Status" name="visastatus" class="form-control input-sm" required>
                                            <option>Active</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="control-label small">Visa Sponsor *</label>
                                    <div>
                                        <input type="text" ng-model="Visa.Sponsor" class="form-control input-sm" />
                                    </div>
                                </div>

                                <div class="form-group col-md-6">
                                    <label class="control-label small">Visa Expiration *</label>
                                    <div>
                                        <input type="date" class="form-control input-sm" />
                                    </div>
                                </div>
                                <div class="col-md-12 form-group">
                                    <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                        <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
                                        <span class="input-group-addon btn btn-default btn-sm btn-file"><span class="fileinput-new">Select VISA Certificate</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
                                        <a href="#" class="input-group-addon btn btn-default btn-sm fileinput-exists" data-dismiss="fileinput">Remove</a>
                                    </div>
                                </div>
                                <div class="col-md-12 form-group">
                                    <button ng-show="visaFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelVisa('editCancel')">cancel</button>
                                    <button ng-show="!visaFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelVisa('saveCancel')">Cancel</button>
                                    <button ng-show="!visaFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="saveVisa(Visa)">Add</button>
                                    <button ng-show="visaFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="updateVisa(Visa)">Update</button>
                                </div>
                            </div>
                        </div>

                        <fieldset ng-show="newVisaForm && !visaFormStatus">
                            <legend>
                                Add Visa
                            </legend>
                            <div id="newVisaFormDiv">

                            </div>
                        </fieldset>

                        <div ng-hide="visaFormStatus || newVisaForm" class="col-md-12 text-right">
                            <button class="btn btn-xs btn-primary" ng-click="addVisa()"><i class="fa fa-plus"></i> Add Visa details</button>
                        </div>
                    </div>
                </fieldset>
            </div>

            <div class="form-style">
                <fieldset>
                    <legend>
                        Other Details
                    </legend>
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="form-group col-md-3">
                            <label class="control-label small">Green Card Number *</label>
                            <div>
                                <input type="text" class="form-control input-sm" />
                            </div>
                        </div>
                        <div class="col-md-8">
                            <label class="control-label small"></label>
                            <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
                                <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new">Select GreenCard Certificate</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
                                <a href="#" class="input-group-addon btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
                            </div>
                        </div>
                        <div class="col-md-1">
                            <label class="control-label"></label>
                            <div>
                                <a href="~/Content/Document/ATTESTATIONOFSITEVISIT.pdf" target="_blank"><i class="fa fa-3x fa-file"></i></a>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="form-group col-md-3">
                            <label class="control-label small">National Id. Number *</label>
                            <div>
                                <input type="text" class="form-control input-sm" />
                            </div>
                        </div>
                        <div class="col-md-8">
                            <label class="control-label small"></label>
                            <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
                                <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new">Select National Identification Certificate</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
                                <a href="#" class="input-group-addon btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
                            </div>
                        </div>
                        <div class="col-md-1">
                            <label class="control-label"></label>
                            <div>
                                <a href="~/Content/Document/ATTESTATIONOFSITEVISIT.pdf" target="_blank"><i class="fa fa-3x fa-file"></i></a>
                            </div>
                        </div>
                    </div>


                    <div class="form-group col-md-4">
                        <label class="control-label small">Country of Issue *</label>
                        <div>
                            <select id="selectNameTitle" name="countryName" class="form-control input-sm" required>
                                <option>USA</option>
                            </select>
                        </div>
                    </div>

                </fieldset>
            </div>
            <br />
            <div class="col-md-12 panel-footer">
                @*<button class="btn btn-success pull-right">Initiate Verification</button>*@
                <button class="btn btn-success btn-sm pull-right">Save</button>
            </div>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="language-heading">
            <a data-toggle="collapse" data-parent="#accordion3" href="#language" aria-expanded="false" aria-controls="language">
                Languages
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="language" class="panel-body panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
            @Html.Partial("_Language")
        </div>
    </div>
</div>

<script>
    function visaViewHide() {
        $("#visaView").css("display", "none");
    }

    function visaViewShow() {
        $("#visaView").css("display", "block");
    }
</script>