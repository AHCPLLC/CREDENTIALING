<div class="panel-group" id="accordiospecialty" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="specialtyHeading">
            <a data-toggle="collapse" data-parent="#accordiospecialty" href="#specialty" aria-expanded="false" aria-controls="specialty">
               Specialty Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="specialty" class="panel-collapse collapse" role="tabpanel" aria-labelledby="specialtyHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">

                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">Speciality Type</label>
                                <div>
                                    <label class="radio-inline">
                                        <input type="radio" name="Speciality" checked="checked"> Primary
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="Speciality"> Secondary
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Board Certified?</label>
                                <div>
                                    <label class="radio-inline">
                                        <input type="radio" name="Board" checked="checked"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="Board"> No
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Board Name</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group col-md-4">
                                <label class="control-label small">Initial Certification Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Last Re-cerification Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Expiration Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>                        
                        <div class="col-md-12">
                            <div class="form-group col-md-12">
                                <label class="control-label small">Reson Not Taking Boards</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>                            
                        </div>                        

                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






