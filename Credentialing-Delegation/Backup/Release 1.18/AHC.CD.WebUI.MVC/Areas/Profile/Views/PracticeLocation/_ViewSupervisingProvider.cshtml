<div class="form-style" id="SupervisingProviderViewDiv">
    <fieldset>
        <legend>
            Supervising Providers
        </legend>
        <div class="form-group">
            <div class="col-md-12 form-group">
                <div class="col-md-3">
                    <div>
                        <label class="control-label small">First Name</label>
                    </div>
                    <i>{{provider.FirstName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3">
                    <div>
                        <label class="control-label small">Middle Name</label>
                    </div>
                    <i>{{provider.MiddleName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3">
                    <div>
                        <label class="control-label small">Last Name</label>
                    </div>
                    <i>{{provider.LastName || 'Not Availabe'}}</i>
                </div>
                <div class="col-md-3">
                    <div>
                        <label class="control-label small">NPI Number</label>
                    </div>
                    <i>{{provider.NPINumber || 'Not Availabe'}}</i>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div ng-show="activeMessageDiv=='supervisingProvidersEditSuccessMsg'" class="alert alert-success" role="alert">
                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                {{messageDesc}}
            </div>
            <div class="col-md-12">
                <div class="col-md-12 form-group">
                    <button class="btn btn-warning btn-sm" ng-click="operateThirdCancelControl('SupervisingProviderViewDiv')">Close</button>
                    <button class="btn btn-sm btn-primary pull-right" ng-click="operateThirdEditControl('SupervisingProviderEdit'+$index, provider)">Edit</button>
                </div>
            </div>
        </div>
    </fieldset>
</div>