
<div class="form-style">
    <fieldset>
        <legend>
            Credentialing Contact
        </legend>

        <form>   

            <div class="legend2 pull-right">
                <button ng-click="operateEditControl('editCc',PracticeLocationDetails[$index].PrimaryCredentialingContactPerson)" class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Edit" tooltip><i class="fa fa-edit"></i></button>
            </div>

      

            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">First Name</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.FirstName}}</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Middle Name</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.MiddleName}}</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Last Name</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.LastName}}</i>
                    </div>
                </div>
            </div>

            <div class="col-md-12 zero-padding-left-right-div">
                <div class="form-group col-md-3">
                    <label for="MobileNo" class="control-label small">Telephone Number  *</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.Telephone}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">

                    <label for="Fax" class="control-label small">Fax</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.Fax}}</i>
                    </div>
                </div>
                <div class="col-md-3 form-group">
                    <label class="control-label small">Email ID</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.EmailAddress}}</i>
                    </div>
                </div>


                <div class="col-md-3 form-group">
                    <label class="control-label small">Street</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.Street}}</i>
                    </div>
                </div>
            </div>

            <div class="col-lg-12 zero-padding-left-right-div">

                <div class="col-lg-3 form-group">
                    <label class="control-label small">Suite/Building</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.Building}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">City</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.City}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">State</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.State}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">Zip Code</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.ZipCode}}</i>
                    </div>
                </div>
            </div>
            
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-3 form-group">
                    <label class="control-label small">Country</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.Country}}</i>
                    </div>
                </div>

                <div class="col-lg-3 form-group">
                    <label class="control-label small">County</label>
                    <div>
                        <i class="data-label">{{PracticeLocationDetails[$index].PrimaryCredentialingContactPerson.County}}</i>
                    </div>
                </div>

            </div>

        </form>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='updatedCredentialingContactInformation'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
        </div>

    </fieldset>
</div>
