    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="PIHeading">
            <a data-toggle="collapse" data-parent="#accordionProviderInfo" href="#PI" aria-expanded="false" aria-controls="PI">
              Provider Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="PI" class="panel-collapse collapse" role="tabpanel" aria-labelledby="PIHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label class="control-label small">First Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Middle Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Last Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        </div>
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label for="Contact" class="control-label small">Gender</label>
                            <div class="radio">
                                <label class="radio-inline">
                                    <input type="radio" name="gender1"> Male
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="gender1"> Female
                                </label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Maiden Name(If Applies)</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Degree</label>
                            <select class="form-control input-sm">
                                <option>Degree 1</option>
                                <option>Degree 2</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label class="control-label small">Date Of Birth</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Place Of Birth</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Country</label>
                            <select class="form-control input-sm">
                                <option>Florida</option>
                                <option>Alaska</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-12 zero-padding-left-right-div">
