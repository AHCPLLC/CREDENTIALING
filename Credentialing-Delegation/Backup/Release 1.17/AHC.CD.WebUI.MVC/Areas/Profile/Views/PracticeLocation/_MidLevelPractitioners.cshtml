@*ng-if="visibilitySecondControl != 'MidLevelPractitionerEdit'"*@

<div id="MidLevelPractitioner">
    <fieldset>
        <legend>
            Mid-Level Practitioner
        </legend>
        
        <div class="panel-body">
            <table ng-hide="PracticeLocationDetail.MidlevelPractioners.length > 0 || visibilityThirdControl == 'MidLevelPractitionerEdit'" class="table table-bordered table-condensed customtbodyStyle table-striped">
                <thead>
                    <tr>
                        <td>
                            No Data Available for Mid-Level Practitioner's!

                            <span>Please click on <button class="btn btn-xs btn-success" ng-click="operateThirdViewAndAddControl('MidLevelPractitionerEdit')" data-toggle="tooltip" data-placement="top" title="Add New" id="noDataAddNewPracticeInterest" tooltip><i class="fa fa-plus"></i></button> to add new Mid-Level Practitioner's information.</span>

                        </td>
                    </tr>
                </thead>
            </table>
            <table ng-if="PracticeLocationDetail.MidlevelPractioners.length>0" class="table table-bordered table-condensed customtbodyStyle table-striped">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Middle name</th>
                        <th>Last Name</th>
                        <th>NPI Number </th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody ng-repeat="practitioner in PracticeLocationDetail.MidlevelPractioners">
                    <tr>
                        <td>{{practitioner.Profile.PersonalDetail.FirstName || 'Not Available'}}</td>
                        <td>{{practitioner.Profile.PersonalDetail.MiddleName || 'Not Available'}}</td>
                        <td>{{practitioner.Profile.PersonalDetail.LastName || 'Not Available'}}</td>
                        <td>{{practitioner.Profile.OtherIdentificationNumber.NPINumber || 'Not Available'}}</td>
                        <td>{{practitioner.Profile.Status || 'Not Available'}}</td>
                        <td>
                            <button class="btn btn-xs btn-default" data-toggle="tooltip" data-placement="left" title="View / Edit" ng-click="operateThirdViewAndAddControl($index+'_ViewMidLevelPractitioner')" tooltip><i class="fa fa-eye"></i></button>
                            <button class="btn btn-xs btn-danger" data-toggle="tooltip" data-placement="left" title="Remove" ng-click="initWarning(practitioner);" tooltip><i class="fa fa-minus-circle"></i></button>
                            <button class="btn btn-xs btn-success" data-toggle="tooltip" data-placement="right" title="Add New" ng-click="operateThirdViewAndAddControl('MidLevelPractitionerEdit')" ng-if=" $index+1==PracticeLocationDetail.MidlevelPractioners.length" tooltip><i class="fa fa-plus"></i></button>
                        </td>
                    </tr>
                    <tr ng-show="visibilityThirdControl == ($index+'_ViewMidLevelPractitioner')">
                        <td colspan="6"> @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewMidLevelPractitioners.cshtml")</td>
                    </tr>
                </tbody>
            </table>

        </div>
        <div ng-show="visibilityThirdControl == 'MidLevelPractitionerEdit'">
            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditMidLevelPractitioner.cshtml")
        </div>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='midlevelPractionersSuccessMsg'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>

        <div ng-show="activeMessageDiv=='midlevelPractionersRemoveErrorMsg'" class="alert alert-danger" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>

        <div class="clearfix"></div>
    </fieldset>

    <div class="modal fade" id="warningModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form id="editMidlevelPractioner">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                    </div>
                    <div class="modal-body">
                        Do you really want to remove <b>{{tempMidLevelpractitioner.Profile.PersonalDetail.FirstName}} {{tempMidLevelpractitioner.Profile.PersonalDetail.LastName}}</b> from your Mid-Level practitioners list ?

                        <input type="hidden" name="PracticeLocationDetailID" value="{{PracticeLocationDetail.PracticeLocationDetailID}}" />
                        <input type="hidden" name="MidLevelPractitionerID" value="{{tempMidLevelpractitioner.MidLevelPractitionerID}}" />
                        <input type="hidden" name="ProfileID" value="{{tempMidLevelpractitioner.ProfileID}}" />
                        @*<input type="hidden" name="ActivationDate" value="{{tempMidLevelpractitioner.ActivationDate}}" />*@
                        <input type="hidden" name="StatusType" value="2" />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-primary" ng-click="updateMidLevelPractitioner(PracticeLocationDetail)">Remove</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

</div>