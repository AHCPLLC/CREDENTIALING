                <span>Select Languages:</span>
                <div>
                    <input class="form-control input-sm" ng-model="searchLang" id="searchLanguageKnow" ng-focus="showLanguageList('FacilityLanguageDiv')" />
                    <div class="LanguageSelectAutoList popover fade bottom in" role="tooltip" id="FacilityLanguageDiv">
                        <table class="table table-striped table-bordered table-hover table-condensed">
                            <tbody>
                                <tr ng-repeat="lang in filteredLangResults = (tempLanguages | filter:searchLang) track by $index" ng-click="SelectLanguage(lang)" class="pointer">
                                    <td>{{lang.name}} ({{lang.nativeName}})</td>
                                </tr>
                                <tr ng-if="tempLanguages.length == 0">
                                    <td>No data..... </td>
                                </tr>
                                <tr ng-if="tempLanguages.length > 0 && filteredLangResults.length == 0">
                                    <td>Match not found..... </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-5  remove-rigth-padding" ng-if="tempObject.FacilityDetail.Language.NonEnglishLanguages.length > 0">
            Selected Languages ({{tempObject.FacilityDetail.Language.NonEnglishLanguages.length}}) : <br />
            <table class="table table-striped table-bordered table-hover table-condensed">
                <thead>
                    <tr>
                        <th>Language</th>
                        <th>Interpreters Available? *</th>
                        <th>Remove</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="lang in tempObject.FacilityDetail.Language.NonEnglishLanguages track by $index" ng-click="selectedLanguage(kl,$index);">
                        <td>
                            <span>{{lang.Language}}</span>
                            <span ng-if="lang.KnownLanguageID"><input type="hidden" name="NonEnglishLanguages[{{$index}}].NonEnglishLanguageID" value="{{lang.NonEnglishLanguageID}}" /></span>
                            <input type="hidden" name="FacilityDetail.Language.NonEnglishLanguages[{{$index}}].Language" value="{{lang.Language}}" />
                        </td>
                        <td>
                            @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                            {
                               
                                <label class="radio-inline">
                                        <input type="radio" name="FacilityDetail.Language.NonEnglishLanguages[{{$index}}].InterpretersAvailableYesNoOption" value="@value" data-val="true" data-val-required="Please specify if Interpreter's Available." /> @value.GetType().GetMember(value.ToString())[0].GetCustomAttributesData()[0].ToString().Split('"')[1]
                                </label>

                            }
                            <br />
                            <span class="text-danger field-validation-error" data-valmsg-for="FacilityDetail.Language.NonEnglishLanguages[{{$index}}].InterpretersAvailableYesNoOption" data-valmsg-replace="true">
                                <span for="FacilityDetail.Language.NonEnglishLanguages[{{$index}}].InterpretersAvailableYesNoOption" class=""></span>
                            </span>
                        </td>
                        <td ng-class="{true:'label-warning'}[selectedIndex==$index]"><i ng-if="!lang.NonEnglishLanguageID" class="fa fa-minus-circle pointer" style="color:#d9534f;" ng-click="DeselectLanguage(lang)"></i></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="col-lg-12 clearfix" style="padding-bottom:15px;"></div>
