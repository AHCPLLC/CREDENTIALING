            @*<button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Upload Docs" onclick="openChildControlDiv('uploadDiv')"><i class="fa fa-upload"></i></button>*@
            @*<a href="~/Content/Document/DocPreview.pdf" download="Pariksith_Singh_Full_Profile.PDF" class="btn btn-xs btn-default" data-toggle="tooltip" data-placement="top" title="Download Full Profile"><i class="fa fa-file-pdf-o"></i></a>*@
            @*<div class="col-lg-2"><button class="btn btn-xs btn-warning" data-toggle="tooltip" data-placement="right" title="Print"><i class="fa fa-print"></i></button></div>*@
            @*</div>*@
        </div>
        <!-- /.row -->
        <div id="loadingData" class="loading-modal-wait">
            <div class="col-lg-12">
                <div class="progress" style="margin-top: 15%;">
                    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%;">
                        <span class="sr-only">Loading ....</span>
                    </div>
                </div>
                <div class="text-center">
                    <span class="h3" id="Loading_Message">Initializing Profile ...</span><span class="h3"> Please wait....</span>
                </div>
            </div>
        </div>
        <div id="loading" align="center" class="loading modal-wait" style="display:none">
            <p style="margin-top: 25%;">
                Please Wait
            </p>
        </div>
        <!-- Add Provider content form div -->
