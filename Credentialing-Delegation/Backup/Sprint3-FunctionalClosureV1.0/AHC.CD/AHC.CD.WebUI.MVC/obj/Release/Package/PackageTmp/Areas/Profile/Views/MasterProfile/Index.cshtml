<style>
    /* styles for validation helpers */
    .field-validation-error {
        color: #b94a48;
    }

    .field-validation-valid {
        display: none;
    }

    input.input-validation-error {
        border: 1px solid #b94a48;
    }

    select.input-validation-error {
        border: 1px solid #b94a48;
    }

    input[type="checkbox"].input-validation-error {
        border: 0 none;
    }

    .validation-summary-errors {
        color: #b94a48;
    }

    .validation-summary-valid {
        display: none;
    }

    .select2-container {
        display: block;
    }
</style>

<div id="page-wrapper" ng-app="profileApp">
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row" ng-cloak>
            <div class="col-lg-12" ng-controller="breadcrumCtrl">
                <ol class="breadcrumb">

                    <li class="active">
                        <i class="fa fa-list"></i>
                        <a href="~/Profile/SampleProfile/Index">Profiles</a>
                    </li>
                    <li class="active">
                        {{Provider.FirstName}} {{Provider.MiddleName}} {{Provider.LastName}}
                    </li>
                    <li class="text-capitalize active">
                        <span id="tabname">Demographics</span>
                    </li>
                </ol>
            </div>
            @*<div class="col-lg-2 text-right breadcrumb">*@
            @*<div class="col-lg-2"><a href="#" class="btn btn-xs btn-success" data-toggle="tooltip" data-placement="left" title="Save"><i class="fa fa-save"></i></a></div>*@
