
<div class="panel-group" id="accordionWork" role="tablist" aria-multiselectable="true" ng-controller="workhistoryController">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="WorkHistoryInfo">
            <a data-toggle="collapse" data-parent="#accordionWork" href="#workHistoryInformation" aria-expanded="false" aria-controls="workHistoryInformation">
                Work History Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="workHistoryInformation" class="panel-collapse collapse" role="tabpanel" aria-labelledby="WorkHistoryInfo">
            <div class="panel-body table-bordered">
            <div class="form-group">

                @*<div class="form-group">*@
                <div class="clearfix"></div>

                @*<div class="col-md-12"><i class="fa fa-times text-danger pull-right" ng-click="deleteWork($index)"></i></div>*@

                <div class="col-md-12">
                    <div class="col-md-6">
                        <label class="control-label small">Practice / Employer Name</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="col-md-6">
                        <label class="control-label small">Street Address</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12">
                    <div class="col-md-4">
                        <label class="control-label small">City</label>
                        <div>
                            <select class="form-control input-sm">
                                <option disabled hidden selected>Select City</option>
                                <option>City 1</option>
                                <option>City 2</option>
                                <option>City 3</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">State</label>
                        <div>
                            <select class="form-control input-sm">
                                <option disabled hidden selected>Select State</option>
                                <option>State 1</option>
                                <option>State 2</option>
                                <option>State 3</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">County</label>
                        <div>
                            <select class="form-control input-sm">
                                <option disabled hidden selected>Select County</option>
                                <option>County 1</option>
                                <option>County 2</option>
                                <option>County 3</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12">
                    <div class="col-md-4">
                        <label class="control-label small">ZIP</label>
                        <input type="number" class="form-control input-sm" />
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">Phone Number</label>
                        <div>
                            <div class="input-group">
                                <div class="input-group-btn">
                                    <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                </div>
                                <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                    <div class="arrow"></div>
                                    <h3 class="popover-title">Select Country Dial Code</h3>
                                    <div class="popover-content">
                                        <select id="ContactContryCode">
                                            <option>+91</option>
                                        </select>
                                    </div>
                                </div>
                                <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">Fax Number</label>
                        <div>
                            <div class="input-group">
                                <div class="input-group-btn">
                                    <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                </div>
                                <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                    <div class="arrow"></div>
                                    <h3 class="popover-title">Select Country Dial Code</h3>
                                    <div class="popover-content">
                                        <select id="ContactContryCode">
                                            <option>+91</option>
                                        </select>
                                    </div>
                                </div>
                                <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Fax Number" required>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12">
                    <div class="col-md-6">
                        <label class="control-label small">Start Date</label>
                        <input type="date" class="form-control input-sm" />
                    </div>
                    <div class="col-md-6">
                        <label class="control-label small">End Date</label>
                        <input type="date" class="form-control input-sm" />
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12">
                    <div class="col-md-4">
                        <label class="control-label small">Department</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">Supervisor Name</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="col-md-4">
                        <label class="control-label small">Supervisor Title</label>
                        <div>
                            <select class="form-control input-sm">
                                <option disabled hidden selected>Select Title</option>
                                <option>Title 1</option>
                                <option>Title 2</option>
                                <option>Title 3</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12">
                    <div class="col-md-6">
                        <label class="control-label small">Reason For Departure</label>
                        <textarea class="form-control input-sm"></textarea>
                    </div>
                    <div class="col-md-6">
                        <label class="control-label small">May We Contact Employer</label>
                        <div>
                            <label class="radio-inline">
                                <input type="radio" name="WHI" checked="checked"> Yes
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="WHI"> No
                            </label>
                        </div>
                    </div>
                </div>

                <div class="clearfix"></div>
                <br />

                <div class="col-md-12 panel-footer">
                    <div class="col-md-12">
                        <button class="btn btn-success pull-right">Cancel</button>
                        <button class="btn btn-warning">Save</button>
                    </div>
                </div>
                @*</div>*@
            </div>

            <div class="clearfix"></div>

            @*<div class="col-md-12 text-right">
                <button class="btn btn-xs btn-primary" ><i class="fa fa-plus"></i> Add Work History Information</button>
            </div>*@

        </div>
        </div>
    </div>
</div>

<script src="~/Scripts/Custom/Plan/workhistory.js"></script>