        <div class="col-lg-4 zero-padding-left-right-div" style="margin-bottom: 10px;">
            <div class="col-lg-12">
                <label class="control-label small">Email Id</label>
                <a ng-click="AddEmail(TempContactDetailsForEdit.EmailIDs)" class="label label-primary pull-right pointer" data-toggle="tooltip" data-placement="top" title="Add"><i class="fa fa-plus-circle"></i> Add</a>
                <div ng-repeat="email in TempContactDetailsForEdit.EmailIDs track by $index">
                    <div class="input-group">
                        <input ng-class="{'disabledField':email.StatusType==2}" class="form-control ng-pristine ng-untouched ng-valid" data-ng-model="email.EmailAddress" data-val="true" data-val-length="Email Id must be between 6 and 50 characters." data-val-length-max="50" data-val-length-min="6" data-val-regex="Please enter the valid email address." data-val-regex-pattern="^[a-z]+[a-z0-9._]+@@[a-z]+\.[a-z.]{2,5}$" data-val-required="Please enter the Email Id." id="EmailIDs_{{$index}}__EmailAddress" name="EmailIDs[{{$index}}].EmailAddress" placeholder="email address" type="email">
                        <span ng-if="email.EmailDetailID">
                            <input type="hidden" name="EmailIDs[{{$index}}].EmailDetailID" value="{{email.EmailDetailID}}" />
                        </span>
                        <input type="hidden" name="EmailIDs[{{$index}}].PreferenceType" value="{{email.PreferenceType}}" />
                        <input type="hidden" name="EmailIDs[{{$index}}].StatusType" value="{{email.StatusType}}" />

                        <span class="input-group-addon">
                            <a ng-if="email.PreferenceType == 2" class="text-primary pointer" ng-click="ContactDetailsEmailConditionFunction($index, TempContactDetailsForEdit.EmailIDs)">
                                <i ng-if="email.PreferenceType==1" class="fa fa-arrow-up text-success" data-toggle="tooltip" data-placement="top" title="This is Primary"></i>
                                <i ng-if="email.PreferenceType == 2" class="fa fa-arrow-down text-warning" data-toggle="tooltip" data-placement="top" title="This is Secondary"></i>
                            </a>
                            <a ng-if="email.EmailDetailID" ng-click="showConfirmation(TempContactDetailsForEdit.EmailIDs, $index, 'email')" data-toggle="tooltip" data-placement="top" title="Remove" class="text-danger pointer">
                                <i class="fa fa-hand-o-up"></i>
                            </a>
                            <a ng-if="!email.EmailDetailID" ng-click="RemoveContactDetails($index,TempContactDetailsForEdit.EmailIDs)" class="text-danger pointer" data-toggle="tooltip" data-placement="top" title="Remove"><i class="fa fa-minus-circle"></i></a>
                        </span>
                    </div>
                    <div>
                        <span class="field-validation-valid text-danger" data-valmsg-for="EmailIDs[{{$index}}].EmailAddress" data-valmsg-replace="true"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            @Html.LabelFor(model => model.PagerNumber, new { @class = "control-label small" })
            <div class="input-group">
                <span ng-if="!TempContactDetailsForEdit.CountryCode" ng-init="TempContactDetailsForEdit.CountryCode='+1'"></span>
                <div class="input-group-btn">
                    <button type="button" class="btn btn-default countryCodeClass" ng-click="showContryCodeDiv('PagerCountryCodeContactDetails')"> {{TempContactDetailsForEdit.CountryCode}} <span class="caret"></span></button>
                </div>
                <div class="countryDailCodeContainer popover fade bottom in" role="tooltip" id="PagerCountryCodeContactDetails">
                    <div class="arrow"></div>
                    <h3 class="popover-title">Select Country Dial Code</h3>
                    <div class="popover-content">
                        <select name="CountryCode" ng-model="TempContactDetailsForEdit.CountryCode">
                            <option ng-repeat="code in CountryDialCodes" value="{{code.dial_code}}" ng-selected="code.dial_code == TempContactDetailsForEdit.CountryCode">{{code.name}}({{code.dial_code}})</option>
                        </select>
                    </div>
                </div>
                @Html.TextBoxFor(model => model.PagerNumber, new { @class = "form-control", placeholder = "pager number", data_ng_model = "TempContactDetailsForEdit.PagerNumber" })
            </div>
            @Html.ValidationMessageFor(model => model.PagerNumber, "", new { @class = "text-danger" })
        </div>
    </div>

