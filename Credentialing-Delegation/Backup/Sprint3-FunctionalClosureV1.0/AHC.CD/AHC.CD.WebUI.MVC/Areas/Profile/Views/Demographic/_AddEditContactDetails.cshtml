                            <i ng-if="hm.PreferenceType == 2" class="fa fa-arrow-down text-warning" data-toggle="tooltip" data-placement="top" title="This is Secondary"></i>
                        </a>
                        <a ng-if="hm.PhoneDetailID" class="pointer" ng-click="showConfirmation(TempContactDetailsForEdit.PhoneDetails, TempContactDetailsForEdit.PhoneDetails.indexOf(hm), 'phone')">
                            <i ng-if="hm.StatusType==1" class="fa fa-hand-o-up text-success" data-toggle="tooltip" data-placement="top" title="click to diactivate"></i>
