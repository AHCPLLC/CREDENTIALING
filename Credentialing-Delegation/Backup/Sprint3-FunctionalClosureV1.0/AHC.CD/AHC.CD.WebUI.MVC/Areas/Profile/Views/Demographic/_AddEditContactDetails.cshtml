                        <div class="popover-content">
                            <select name="PhoneDetails[{{TempContactDetailsForEdit.PhoneDetails.indexOf(hm)}}].CountryCode" ng-model="hm.CountryCode" id="ContactContryCode">
                                <option ng-repeat="code in CountryDialCodes" value="{{code.dial_code}}" ng-selected="code.dial_code == hm.CountryCode">{{code.name}}({{code.dial_code}})</option>
                            </select>
                        </div>
                    </div>
                    <input ng-class="{'disabledField':hm.StatusType==2}" class="form-control text-box single-line ng-pristine ng-untouched ng-valid" id="PhoneDetails_{{TempContactDetailsForEdit.PhoneDetails.indexOf(hm)}}__Number" placeholder="phone number" type="text" name="PhoneDetails[{{TempContactDetailsForEdit.PhoneDetails.indexOf(hm)}}].Number" data-ng-model="hm.Number" data-val="true" data-val-regex="Mobile Number must be of 10 digits." data-val-regex-pattern="\d{10}" data-val-required="Please enter the Mobile number.">
                    <span ng-if="hm.PhoneDetailID">
                        <input type="hidden" name="PhoneDetails[{{TempContactDetailsForEdit.PhoneDetails.indexOf(hm)}}].PhoneDetailID" value="{{hm.PhoneDetailID}}" />
