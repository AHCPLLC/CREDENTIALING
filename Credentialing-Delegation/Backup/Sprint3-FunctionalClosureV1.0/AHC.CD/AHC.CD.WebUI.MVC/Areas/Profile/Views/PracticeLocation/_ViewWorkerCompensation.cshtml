@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocation.WorkersCompensationInformationViewModel

<div class="form-style" ng-hide="WorkingCompEdit">
    <fieldset>
        <legend>
            Workers Compensation Information
        </legend>
        <div ng-hide="WorkingCompEdit" class="legend2 pull-right">
            <button ng-click="WorkingCompEdit=true" class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
        </div>
        <div id="practice12" class="panel-body">


            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.WorkersCompensationNumber, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{WorkersCompensationInformation.WorkersCompensationNumber}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                   @Html.LabelFor(model => model.CertificationStatus, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{WorkersCompensationInformation.CertificationStatus}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.IssueDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{WorkersCompensationInformation.IssueDate}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.ExpirationDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{WorkersCompensationInformation.ExpirationDate}}</i>
                    </div>
                </div>
            </div>
        </div>

    </fieldset>
</div>
