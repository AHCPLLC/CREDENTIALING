<div class="form-style">
    <fieldset>
        <legend>
            Partners/Associates/Covering Colleagues
        </legend>
        <div class="form-group">
            <div class="col-md-12">
                <div class="col-md-6">
                    <label class="control-label small  ">Re-use information from another practice location </label>
                    <select class="form-control input-sm " data-ng-model="partner.Location">
                        <option></option>
                        <option></option>
                    </select>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-md-12">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">First Name </label>
                    <input type="text" class="form-control input-sm" id="name11" placeholder="first name" data-ng-model="partner.First">
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Middle Name</label>
                    <input type="text" class="form-control input-sm" id="name22" placeholder="middle name" data-ng-model="partner.Middle ">
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Last Name </label>
                    <input type="text" class="form-control input-sm" id="name33" placeholder="last name" data-ng-model="partner.Last">
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-md-12">
                @*<div class="form-group col-md-4">
                    <label class="control-label small">Number </label>
                    <div>
                        <input type="text" class="form-control input-sm" data-ng-model="partner.Number" />
                    </div>
                </div>*@
                <div class="form-group col-md-4">
                    <label class="control-label small">State </label>
                    <div>
                        <select id="selectNameTitle" name="" class="form-control input-sm" data-ng-model="partner.State" required>
                            <option>State1</option>

                        </select>
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label class="control-label small">City </label>
                    <div>
                        <select id="selectNameTitle" name="" class="form-control input-sm" data-ng-model="partner.City" required>
                            <option>City1</option>

                        </select>

                    </div>
                </div>

            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">Suite/Building</label>
                    <div>
                        <input type="text" class="form-control input-sm" data-ng-model="partner.Suite" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Street </label>
                    <div>
                        <input type="text" class="form-control input-sm" data-ng-model="partner.Street" />
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label class="control-label small">Zip Code </label>
                    <div>
                        <input type="text" class="form-control input-sm" data-ng-model="partner.ZIP" />
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="MobileNo" class="control-label small">Phone number</label>
                    <div>
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button type="button" class="btn btn-sm btn-default"><span class="caret"></span></button>
                            </div>
                            <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                <div class="arrow"></div>
                                <h3 class="popover-title">Select Country Dial Code</h3>
                                <div class="popover-content">
                                    <select id="ContactContryCode">
                                        <option>+91</option>
                                    </select>
                                </div>
                            </div>
                            <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" data-ng-model="partner.Phonenumber" required>
                        </div>

                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="control-label small">Specialty Code </label>
                    <select class="form-control input-sm" data-ng-model="partner.Specialty">
                        <option>code1</option>
                        <option>code2</option>
                    </select>
                </div>
                <div class="col-md-4 form-group">
                    <label class="control-label small">Provider Type</label>
                    <select class="form-control input-sm" data-ng-model="partner.ProviderType">
                        <option>type1</option>
                        <option>type2</option>
                    </select>
                </div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-lg-12">
                <button class="btn btn-warning btn-sm" ng-click="cancelPartners()">Cancel</button>
                <button class="btn btn-success btn-sm pull-right">Save</button>
            </div>
        </div>
    </fieldset>
</div>