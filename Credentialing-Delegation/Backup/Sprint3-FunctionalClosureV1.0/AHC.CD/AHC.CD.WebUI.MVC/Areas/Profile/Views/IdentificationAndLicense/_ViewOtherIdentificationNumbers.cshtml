@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.OtherIdentificationNumberViewModel
@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<div class="panel-default">
    <div class="form-style">
        <fieldset>
            <legend>
                Other Identification Numbers
            </legend>
            <div class="legend2 pull-right" ng-hide="visibilityControl=='editOtherIdentificationNumbers'">
                @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Edit, ViewBag.Roles, true))
                {
                    <button class="btn btn-xs btn-primary" ng-click="operateEditControl('editOtherIdentificationNumbers',OtherIdentificationNumbers)" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
                }
              
            </div>
            <div ng-hide="visibilityControl=='editOtherIdentificationNumbers'" id="otherIdenNumbers" class="panel-body zero-padding-left-right-div">
                <form>

                    <div ng-hide="showOtherId">
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPINumber, new { @class = "  control-label small" })
                                <div>
                                    @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                    {
                                     
    <i class="data-label" ng-show="OtherIdentificationNumbers.NPINumber">######</i>
                                    }
                                    else
                                    {
                                        <i class="data-label">{{OtherIdentificationNumbers.NPINumber}}</i>
                                    }
                                  
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPIUserName, new { @class = "  control-label small", type = "password" })
                                <div>
                                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                {
                                    <i class="data-label" ng-show="OtherIdentificationNumbers.NPIUserName">######</i>

                                }
                                   else
                                   {
                                     <i class="data-label" ng-show="OtherIdentificationNumbers.NPIUserName">######</i>
                                    }
                                </div>
                            </div>
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.NPIPassword, new { @class = "  control-label small", type = "password" })
                                <div>
                                    @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                    {
                                        <i class="data-label" ng-show="OtherIdentificationNumbers.NPIPassword">######</i>

                                    }
                                    else
                                    {
                                        <i class="data-label" ng-show="OtherIdentificationNumbers.NPIPassword">######</i>
                                    }
                                          </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.CAQHNumber, new { @class = "  control-label small" })
                                <div>
                                    @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                    {
                                        <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHNumber">######</i>

                                    }
                                    else
                                    {
                                       <i class="data-label">{{OtherIdentificationNumbers.CAQHNumber}}</i>
                                    }
                                   
                                </div>
                            </div>
                            <div ng-show="OtherIdentificationNumbers.CAQHNumber">
                                <div ng-show="OtherIdentificationNumbers.CAQHUserName" class="form-group col-md-4 zero-padding-left-right-div">
                                    @Html.LabelFor(model => model.CAQHUserName, new { @class = "  control-label small", type = "password" })
                                    <div>
                                        @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                        {
                                            <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHUserName">######</i>

                                        }
                                        else
                                        {
                                            <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHUserName">######</i>
                                        }
                                    </div>
                                </div>
                                <div ng-show="OtherIdentificationNumbers.CAQHPassword" class="form-group col-md-4 zero-padding-left-right-div">
                                    @Html.LabelFor(model => model.CAQHPassword, new { @class = "  control-label small", type = "password" })
                                    <div>
                                        @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                        {
                                            <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHPassword">######</i>

                                        }
                                        else
                                        {
                                            <i class="data-label" ng-show="OtherIdentificationNumbers.CAQHPassword">######</i>
                                        }
                                    </div>
                                </div>
                            </div>
                       
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.UPINNumber, new { @class = " control-label small" })
                                <div>
                                    @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                    {
                                       
    <i class="data-label" ng-show="OtherIdentificationNumbers.UPINNumber">######</i>
                                    }
                                    else
                                    {
                                        <i class="data-label">{{OtherIdentificationNumbers.UPINNumber}}</i>
                                    }  
                                </div>
                            </div>

                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
                                <div>
                                    @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                                    {
                                        <i class="data-label" ng-show="OtherIdentificationNumbers.USMLENumber">######</i>

                                    }
                                    else
                                    {
                                        <i class="data-label">{{OtherIdentificationNumbers.USMLENumber}}</i>
                                    }   
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <div ng-show="showOtherId">
                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_OtherIdentificationNumbers.cshtml")


                    </div>
                </form>
            </div>
            @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Edit, ViewBag.Roles, true))
            {
                @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_OtherIdentificationNumbers.cshtml")
            }
            <div ng-show="visibilityControl=='updatedID'" class="alert alert-success" role="alert">
                <button ng-click="operateCancelControl('')" class="close"><span>&times;</span></button>
                <i class="fa fa-2x fa-arrow-up"></i>Other identification information saved successfully.
            </div>
        </fieldset>
    </div>
</div>
