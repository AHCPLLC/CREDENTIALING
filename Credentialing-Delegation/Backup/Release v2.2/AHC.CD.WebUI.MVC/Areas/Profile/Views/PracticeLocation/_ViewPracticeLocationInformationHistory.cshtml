@*Practice Location Facility Information Start*@
<fieldset>
    <legend>
        Practice Location / Facility Details
    </legend>
    <div class="legend2 pull-right">
        <button ng-if="visibilityControl!=('editPracticeLocation{{$index}}')" class="btn btn-default btn-xs" ng-click="ToggleScript()" title="View More" tooltip><i class="fa fa-th-list"></i></button>
    </div>
    <div ng-if="visibilityControl!=('editPracticeLocation{{$index}}')">
        @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewFacilityInformationHistory.cshtml")
    </div>
    <div class="clearfix"></div>
</fieldset>
@*Practice Location Facility Information End*@

@*Practice Location Other Information Start*@
<div class="form-style">
    <fieldset>
        <legend>
            Other Information
        </legend>
        <div ng-show="!GeneralInformationEdit">
            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewGeneralInformationHistory.cshtml")
        </div>
    </fieldset>
</div>
@*Practice Location Other Information End*@
