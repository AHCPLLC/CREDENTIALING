    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="demoHeading">
            <a data-toggle="collapse" data-parent="#accordiondemo" href="#demo" aria-expanded="false" aria-controls="demo">
                Demographics
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="demo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="demoHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <div class="col-md-4 form-group">
                                <label class="control-label small">Title</label>
                                <select class="form-control input-sm">
                                    <option>Mr.</option>
                                    <option>Miss</option>
                                </select>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">First Name</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> Middle Name</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">Last Name</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Maiden Name</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> Date</label>
                                <input type="date" class="form-control input-sm" />
                            </div>
                        </div>


                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">Home Address</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> Apartment/Unit</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">City</label>
                                <select class="form-control input-sm">
                                    <option>Raipur</option>
                                    <option>jaipur</option>
                                </select>
                            </div>
                        </div>



                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">State</label>
                                <select class="form-control input-sm">
                                    <option>Florida</option>
                                    <option>Putnam</option>
                                </select>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> Zip</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> County</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group col-md-4">
                                <label for="MobileNo" class="control-label small">Home Phone</label>
                                <div>
                                    <div class="input-group">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                        </div>
                                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                            <div class="arrow"></div>
                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                            <div class="popover-content">
                                                <select id="ContactContryCode">
                                                    <option>+91</option>
                                                </select>
                                            </div>
                                        </div>
                                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group col-md-4">
                                <label for="MobileNo" class="control-label small">Home Fax</label>
                                <div>
                                    <div class="input-group">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                        </div>
                                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                            <div class="arrow"></div>
                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                            <div class="popover-content">
                                                <select id="ContactContryCode">
                                                    <option>+91</option>
                                                </select>
                                            </div>
                                        </div>
                                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Fax Number" required>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group col-md-4">
                                <label for="MobileNo" class="control-label small">Mobile</label>
                                <div>
                                    <div class="input-group">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                        </div>
                                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                            <div class="arrow"></div>
                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                            <div class="popover-content">
                                                <select id="ContactContryCode">
                                                    <option>+91</option>
                                                </select>
                                            </div>
                                        </div>
                                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Mobile Number" required>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">SSN</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Marital Status</label>
                                <select class="form-control input-sm">
                                    <option>Single</option>
                                    <option>Married</option>
                                    <option>Divorce</option>
                                </select>
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Gender</label>
                                <div>
                                    <label class="radio-inline">
                                        <input type="radio" ng-model="gender" name="Gender" id="Gender1" value="M" required> Male
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" ng-model="gender" name="Gender" id="Gender2" value="F" required> Female
                                    </label>
                                </div>
                            </div>
                        </div>


                        <div class="col-md-12">
                            <div class="col-md-4">
                                <label class="control-label small">Spouse</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small">Email</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-4">
                                <label class="control-label small"> Drivers License</label>
                                <input type="date" class="form-control input-sm" />
                            </div>
                        </div>


                        <div class="col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Time At Present Address</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="col-md-6">
                                <label class="control-label small">Other Legal Names You Have Used</label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


