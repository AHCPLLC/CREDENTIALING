    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="mamHeading">
            <a data-toggle="collapse" data-parent="#accordionmam" href="#mam" aria-expanded="false" aria-controls="mam">
               Medicare And Medicaid
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="mam" class="panel-collapse collapse" role="tabpanel" aria-labelledby="mamHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="form-group col-md-12">
                            <label for="Contact" class="col-md-3 control-label small">Medicare Approved</label>
                            <div class="col-md-9 radio">
                                <label class="radio-inline">
                                    <input type="radio" name="Medicare" checked="checked"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="Medicare"> No
                                </label>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Medicare Number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" required />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label small">State</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>State</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                       
                        <div class="form-group col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Issue Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" required />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label small">Expiration Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" required />
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <label for="Contact" class="col-md-5 control-label small">Any sanction imposed by Medicare?</label>
                            <div class="col-md-7 radio">
                                <label class="radio-inline">
                                    <input type="radio" name="sanctionMedicare" checked="checked"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="sanctionMedicare"> No
                                </label>
                            </div>
                        </div>        
                        
                        <div class="form-group col-md-12">
                            <label for="Contact" class="col-md-3 control-label small">Medicaid Approved</label>
                            <div class="col-md-9 radio">
                                <label class="radio-inline">
                                    <input type="radio" name="Medicaid" checked="checked"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="Medicaid"> No
                                </label>
                            </div>
                        </div>               

                        <div class="form-group col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Medicaid Number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" required />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label small">State</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>State</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Issue Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" required />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class="control-label small">Expiration Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" required />
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <label for="Contact" class="col-md-5 control-label small">Any sanction imposed by Medicaid?</label>
                            <div class="col-md-7 radio">
                                <label class="radio-inline">
                                    <input type="radio" name="sanctionMedicare" checked="checked"> Yes
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" name="sanctionMedicare"> No
                                </label>
                            </div>
                        </div>        
                        

                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





