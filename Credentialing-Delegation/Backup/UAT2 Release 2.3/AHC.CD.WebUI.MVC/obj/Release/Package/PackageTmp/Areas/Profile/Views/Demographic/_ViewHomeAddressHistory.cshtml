@model AHC.CD.WebUI.MVC.Areas.Profile.Models.Demographic.HomeAddressViewModel
@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<fieldset class="wrap-words">
    <legend>
        Home Address
    </legend>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.UnitNumber, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.UnitNumber">############</i>
                    <i class="data-label" ng-if="!haHistory.UnitNumber">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.UnitNumber">{{haHistory.UnitNumber}}</i>
                    <i class="data-label" ng-if="!haHistory.UnitNumber">Not Available</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.Street, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.Street">############</i>
                    <i class="data-label" ng-if="!haHistory.Street">Not Available</i>
                }
                else
                {
                    <i class="data-label">{{haHistory.Street}}</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.City">############</i>
                    <i class="data-label" ng-if="!haHistory.City">Not Available</i>
                }
                else
                {
                    <i class="data-label">{{haHistory.City}}</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.State">############</i>
                    <i class="data-label" ng-if="!haHistory.State">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.State">{{haHistory.State}}</i>
                    <i class="data-label" ng-if="!haHistory.State">Not Available</i>
                }
            </div>
        </div>
    </div>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.ZipCode, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.ZipCode">############</i>
                    <i class="data-label" ng-if="!haHistory.ZipCode">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.ZipCode">{{haHistory.ZipCode}}</i>
                    <i class="data-label" ng-if="!haHistory.ZipCode">Not Available</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            <label class="control-label small">Country</label>
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.Country">############</i>
                    <i class="data-label" ng-if="!haHistory.Country">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.Country">{{haHistory.Country}}</i>
                    <i class="data-label" ng-if="!haHistory.Country">Not Available</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.County">############</i>
                    <i class="data-label" ng-if="!haHistory.County">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.County">{{haHistory.County}}</i>
                    <i class="data-label" ng-if="!haHistory.County">Not Available</i>
                }
            </div>
        </div>
    </div>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.AddressPreferenceType, new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.AddressPreference">############</i>
                    <i class="data-label" ng-if="!haHistory.AddressPreference">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.AddressPreference">{{haHistory.AddressPreference}}</i>
                    <i class="data-label" ng-if="!haHistory.AddressPreference">Not Available</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.IsPresentlyStayingYesNoOption, new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.IsPresentlyStaying">############</i>
                    <i class="data-label" ng-if="!haHistory.IsPresentlyStaying">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.IsPresentlyStaying">{{haHistory.IsPresentlyStaying}}</i>
                    <i class="data-label" ng-if="!haHistory.IsPresentlyStaying">Not Available</i>
                }
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.LivingFromDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.LivingFromDate">##/##/####</i>
                    <i class="data-label" ng-if="!haHistory.LivingFromDate">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.LivingFromDate">{{haHistory.LivingFromDate | date: "MM/dd/yyyy"}}</i>
                    <i class="data-label" ng-if="!haHistory.LivingFromDate">Not Available</i>
                }
            </div>
        </div>
        <div ng-if="haHistory.IsPresentlyStayingYesNoOption==2" class="col-lg-3 form-group">
            @Html.LabelFor(model => model.LivingEndDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-if="haHistory.LivingEndDate">##/##/####</i>
                    <i class="data-label" ng-if="!haHistory.LivingEndDate">Not Available</i>
                }
                else
                {
                    <i class="data-label" ng-if="haHistory.LivingEndDate">{{haHistory.LivingEndDate | date: "MM/dd/yyyy"}}</i>
                    <i class="data-label" ng-if="!haHistory.LivingEndDate">Not Available</i>
                }
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div ng-if="activeMessageDiv=='alertHomeAddressSuccess'+$index" class="col-lg-12 alert alert-{{messageType}}" role="alert">
        <button ng-click="closeAlertMessage()" class="close"><span>&times;</span></button>
        {{messageDesc}}
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12 form-group">
        <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl()">Cancel</button>
    </div>
</fieldset>