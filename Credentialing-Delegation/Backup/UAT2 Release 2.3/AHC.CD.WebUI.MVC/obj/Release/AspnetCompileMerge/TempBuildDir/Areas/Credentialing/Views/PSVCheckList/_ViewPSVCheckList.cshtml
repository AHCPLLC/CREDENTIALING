
<table ng-if="!AddNewPSV" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th class="col-lg-1 col-md-1 col-sm-1 col-xs-1">S. No.</th>
            <th class="col-lg-6 col-md-6 col-sm-6 col-xs-6">PSV Checklist Name</th>
            <th class="col-lg-3 col-md-3 col-sm-3 col-xs-3">Code</th>
            <th class="col-lg-2 col-md-2 col-sm-2 col-xs-2">Action</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="psv in PSVCheckListMasterData track by $index">
            <td>{{$index+1}}</td>
            <td>{{psv.Name}}</td>
            <td>{{psv.Code}}</td>
            <td>
                <a class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="left" title="View"><i class="fa fa-eye"></i></a>
                <a ng-click="AddNewPSVData(psv)" class="btn btn-xs btn-primary" tooltip data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></a>
                <a ng-click="RemovePSV($index, psv)" class="btn btn-xs btn-danger" tooltip data-toggle="tooltip" data-placement="right" title="Remove"><i class="fa fa-close"></i></a>
            </td>
        </tr>
    </tbody>
</table>