<div class="panel-group" id="accordiores" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="resHeading">
            <a data-toggle="collapse" data-parent="#accordiores" href="#res" aria-expanded="false" aria-controls="res">
                Residency/Internship
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="res" class="panel-collapse collapse" role="tabpanel" aria-labelledby="resHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">

                        <div class="col-md-12">
                            <div class="form-group col-md-4">
                                <label class="control-label small">Type</label>
                                <div>
                                    <select class="form-control input-sm">
                                        <option>Primary</option>
                                        <option>Secondary</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">University Affiliation For Residency </label>
                                <input type="text" class="form-control input-sm" />
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Type Of Residency/Chairman</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">                           
                            <div class="form-group col-md-4">
                                <label class="control-label small">State</label>
                                <div>
                                    <select id="selectNameTitle" name="stateName" class="form-control input-sm" required>
                                        <option>State1</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">City</label>
                                <div>
                                    <select class="form-control input-sm">
                                        <option>city1</option>
                                        <option>city2</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Program Name</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group col-md-4">
                                <label class="control-label small">Preceptor/Chairman</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Title</label>
                                <div>
                                    <select class="form-control input-sm" >
                                        <option>Mr</option>
                                        <option>Mr</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Honors</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>


                        <div class="col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">Start Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                <label class="control-label small">End Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>
                       
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





