<div class="panel-group" id="accordionCOI" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default" ng-controller="conflict">
        <div class="panel-heading" role="tab" id="COIHeading">
            <a data-toggle="collapse" data-parent="#accordionCOI" href="#COI" aria-expanded="false" aria-controls="COI">
                 Conflict of Interest Statement
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="COI" class="panel-collapse collapse" role="tabpanel" aria-labelledby="COIHeading">
            <div class="panel-body table-bordered">


                <div class="form-group">
                    
                    <div class="clearfix"></div>
                    <div class="col-lg-12">

                        <label class="control-label small">
                            Do you or any member of your family own,have and investment in,or otherwise have a business
                            interest in clinical laboratory,diagnostic or testing center,hospital, surgery center or other business dealing with the provision
                            of ancilary health servives,equipment or supplies?
                        </label>

                        <input type="radio" /> Yes
                        <input type="radio" /> No

                    </div>

                    <div class="clearfix"></div><br>

                    <div class="col-lg-6">
                        <label class="control-label small">Name of the Organisation</label>
                        <input type="text" class="form-control input-sm" />
                    </div>

                    <div class="col-lg-6">
                        <label class="control-label small"> Percent of Investment/Ownership</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="clearfix"></div><br>
                    <div class="col-lg-6">
                        <label class="control-label small"> Address</label>
                        <input type="text" class="form-control input-sm" />
                    </div>

                    <div class="col-lg-6">
                        <label class="control-label small"> Phone</label>
                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="clearfix"></div>
                    <br />

                    <div class="col-lg-6">
                        <label class="control-label small"> Tax ID</label>
                        <input type="text" class="form-control input-sm" />

                    </div>

                    <div class="col-lg-6">
                        <label class="control-label small">Type Of Organisation</label>

                        <input type="text" class="form-control input-sm" />
                    </div>
                    <div class="clearfix"></div>
                    <br />

                    <div class="col-lg-6">
                        <label class="control-label small"> Nature of business interest(i.e partner,owner,investor)</label>

                        <input type="text" class="form-control input-sm" />

                    </div>

                    <div class="col-lg-6">
                        <label class="control-label small"> Size of organisation</label>

                        <input type="number" class="form-control input-sm" />

                    </div>
                    <div class="clearfix"></div>
                    <br />



                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>

                    <div class="clearfix"></div>
                    <br />
                </div>
                    <div class="col-md-12 text-right">
                        <button class="btn btn-xs btn-primary" ng-click="Conflict= Conflict+ 1"><i class="fa fa-plus"></i> Add more Conflict of interest</button>
                    </div>           
            </div>
        </div>
    </div>
</div>


