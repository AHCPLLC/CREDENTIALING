<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Access Healthcare</title>

    @Styles.Render("~/Content/commonCss")
    <!-- Bootstrap Core CSS 
    <link href="~/Content/SharedCss/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/SharedCss/plugins/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/SharedCss/bootstrap-theme.min.css" rel="stylesheet" />-->
    <!-- Custom CSS 
    <link href="~/Content/SharedCss/app.css" rel="stylesheet" />-->
    
    <!-- Custom Fonts -->
    <link href="~/Content/SharedFonts/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    @RenderSection("AddToHeadAnotherWay", required: false)

</head>
<body>
    <div id="wrapper">
        @* Header implementation with sidebar menu *@
        @Html.Partial("_Header")
        @* End of Header implementation *@
        @RenderSection("featured", required: false)
        @RenderBody()
        @Html.Partial("_Footer")

        <!-- scroll top button, Author : krglv-->
        <a class="btn btn-info scrollToTop"><i class="fa fa-arrow-circle-o-up fa-2x"></i></a>
        <!-- end-->
    </div>
    <!-- /#wrapper -->
    <!-- jQuery Version 1.11.0 -->
    @*<script src="~/Scripts/jquery-1.11.0.js"></script>*@

    <!-- Bootstrap Core JavaScript -->
    @*<script src="~/Scripts/bootstrap.min.js"></script>*@
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/metisMenu")
    @Scripts.Render("~/bundles/app")
    @RenderSection("scripts", required: false)

</body>
</html>
