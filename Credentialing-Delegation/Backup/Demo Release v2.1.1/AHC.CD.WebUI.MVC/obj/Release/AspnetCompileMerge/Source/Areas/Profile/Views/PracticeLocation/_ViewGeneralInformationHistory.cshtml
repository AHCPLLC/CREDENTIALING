@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.PracticeLocationDetailViewModel

<div class="wrap-words">
    <div class="col-md-12">
        <div class="form-group col-md-3">
            <label class="ontrol-label small">Facility Name</label>
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityName || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PracticeLocationCorporateName, new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.PracticeLocationCorporateName || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="ontrol-label small">IPA</label>
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Group.Group.Name || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="ontrol-label small">Group Name</label>
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.GroupName || 'Not Available'}}</i>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PrimaryYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.IsPrimary || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.CurrentlyPracticingYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.CurrentlyPracticingAtThisAddress || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label" ng-show="!PracticeLocationDetailHistory.StartDate">Not Available</i>
                <i class="data-label">{{ PracticeLocationDetailHistory.StartDate == (01-01-0001).value ? 'Not Available' : PracticeLocationDetailHistory.StartDate | date:'MM-dd-yyyy'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.GeneralCorrespondenceYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.SendGeneralCorrespondence || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PrimaryTax, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.PrimaryTaxId || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PracticeExclusivelyYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.PracticeExclusively || 'Not Available'}}</i>
            </div>
        </div>
    </div>
</div>