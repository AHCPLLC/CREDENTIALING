@model AHC.CD.WebUI.MVC.Areas.Profile.Models.WorkHistory.ProfessionalWorkExperienceViewModel

<div class="col-lg-12" style="word-wrap: break-word;">
    <fieldset class="wrap-words">
        <legend>
            Professional Work Experience Information
        </legend>
        <div class="col-md-12">
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.EmployerName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.EmployerName  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.Street, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.Street  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.Building, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.Building  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.City  || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.State  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.ZipCode, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.ZipCode  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.Country  || 'Not Available'}}</i>
                    </div>
                </div>

                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.County  || 'Not Available'}}</i>
                    </div>
                </div>

            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.EmployerMobile, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.MobileNumber  || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.EmployerFax, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <div>
                            <i class="data-label">{{pwxHistory.FaxNumber  || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.EmployerEmail, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <div>
                            <i class="data-label">{{pwxHistory.EmployerEmail || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.JobTitle, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.JobTitle || 'Not Available'}}</i>
                    </div>
                </div>

            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SupervisorName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.SupervisorName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.Department, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.Department || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.ProviderTypeID, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{pwxHistory.ProviderType.Title || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.CanContactEmployerOption, htmlAttributes: new { @class = "control-label small" })
                    <div class="radio">
                        <div>
                            <i class="data-label">{{pwxHistory.CanContactEmployer || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>

            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    <label class="control-label small">Currently Working Here?</label>
                    <div>
                        <i class="data-label">{{pwxHistory.CurrentlyWorking || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Start Date</label>
                    <div>
                        <i class="data-label">{{pwxHistory.StartDate || 'Not Available' | date:'MM-dd-yyyy'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3" ng-hide="workExperience.CurrentlyWorkingOption == 1 ">
                    <label class="control-label small">End Date</label>
                    <div>
                        <i class="data-label">{{pwxHistory.EndDate || 'Not Available' | date:'MM-dd-yyyy'}}</i>
                    </div>

                </div>
                <div class="form-group col-md-3" ng-hide="workExperience.CurrentlyWorkingOption == 1 ">
                    <label class="control-label small">Reason For Departure</label>
                    <div>
                        <i class="data-label">{{pwxHistory.DepartureReason || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Experience Document</label>
                    <div>
                        <i class="data-label" ng-show="pwxHistory.WorkExperienceDocPath"><a href="/Document/View?path={{pwxHistory.WorkExperienceDocPath}}" class="pointer" target="_blank" tooltip data-toggle="tooltip" data-placement="top" title="{{workExperience.WorkExperienceDocPath}}"><i class="fa fa-file center-block"></i></a></i>
                        <i class="data-label" ng-show="pwxHistory.WorkExperienceDocPath==null">Not Available</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 form-group">
                <div class="col-md-12">
                    <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                </div>
            </div>
        </div>
    </fieldset>
</div>


