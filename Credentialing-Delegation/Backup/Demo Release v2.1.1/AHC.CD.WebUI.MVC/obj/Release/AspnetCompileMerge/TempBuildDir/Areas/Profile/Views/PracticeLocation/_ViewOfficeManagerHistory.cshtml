@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.FacilityEmployeeViewModel

<div class="form-style" ng-if="visibilitySecondControl != 'BussinessManagerEdit'">
    <fieldset class="wrap-words">
        <legend>
            Office manager/Business Office Staff Contact
        </legend>
        <table ng-show="PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff == null && visibilityControl != 'BussinessManagerEdit' " class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <td>
                        No History Available for Office manager/Business Office Staff Contact for this Practice Location!
                    </td>
                </tr>
            </thead>
        </table>
        <div ng-if="PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff">
            <div class="col-md-12">
                <div class="col-lg-4 form-group">
                    @Html.LabelFor(model => model.FirstName, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.FirstName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    @Html.LabelFor(model => model.MiddleName, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.MiddleName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    @Html.LabelFor(model => model.LastName, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.LastName || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    @Html.LabelFor(model => model.Telephone, new { @class = "control-label small" })
                    <div>
                        <i ng-if="PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Telephone" class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.CountryCodeTelephone}}-{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Telephone}}</i>
                        <i ng-if="!PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Telephone" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    @Html.LabelFor(model => model.Fax, new { @class = "control-label small" })
                    <div>
                        <i ng-if="PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Fax" class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.CountryCodeFax}}-{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Fax}}</i>
                        <i ng-if="!PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.Fax" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    @Html.LabelFor(model => model.EmailAddress, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{PracticeLocationDetailHistory.BusinessOfficeManagerOrStaff.EmailAddress || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='businessManagerSuccessMsg'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
    </fieldset>
</div>