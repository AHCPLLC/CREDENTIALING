                                                    <option ng-repeat="code in CountryDialCodes" value="{{code.dial_code}}" ng-selected="code.dial_code == tempHCP.FaxCountryCode">{{code.name}}({{code.dial_code}})</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div>
                                            <input type="text" ng-model="tempHCP.Fax" class="form-group form-control input-sm" />
                                        </div>
                                    </div>
                                </td>
                                <td class="col-lg-2">{{tempHCP.Status}}</td>
                                <td class="col-lg-2">{{tempHCP.LastModifiedDate | date : 'MM-dd-yyyy'}}</td>
                                <td class="col-lg-2">
                                    <button ng-if="!tempHCP.HospitalContactPersonID" ng-click="saveHCP($index)" class="btn btn-xs btn-primary">Save</button>
                                    <button ng-if="tempHCP.HospitalContactPersonID" ng-click="updateHCP($index)" class="btn btn-xs btn-primary">Update</button>
                                    <button ng-if="tempHCP.HospitalContactPersonID" ng-click="reset()" class="btn btn-xs btn-warning">Cancel</button>
                                    <button ng-if="!tempHCP.HospitalContactPersonID" ng-click="cancelAdd()" class="btn btn-xs btn-warning">Cancel</button>
                                </td>
                            </script>
                           
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>