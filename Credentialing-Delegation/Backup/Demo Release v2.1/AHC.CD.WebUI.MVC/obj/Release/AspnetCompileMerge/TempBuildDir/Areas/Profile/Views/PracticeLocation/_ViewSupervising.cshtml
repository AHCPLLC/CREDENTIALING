<div class="form-style">
    <fieldset>
        <legend>
            Supervising Provider
        </legend>
        <div class="form-group">
            <div class="col-md-12">
                <div class="col-lg-3 form-group">
                    <label class="control-label small">State License Number</label>
                    <div>
                        {{supervisor.ProviderStateLicense}}
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">DEA Number</label>
                    <div>
                        {{supervisor.ProviderDEANumber}}
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">Specialty</label>
                    <div>
                        {{supervisor.ProviderSpecialty}}
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small">Supporting Document</label>
                    <div>
                        Not Available
                    </div>
                </div>
            </div>
        </div>

        <br />

        <div class="col-lg-12">
            <button class="btn btn-warning btn-sm" ng-click="cancelSupervisor()">Cancel</button>
            <button class="btn btn-success btn-sm pull-right" ng-click="cancelSupervisor()">Edit</button>
        </div>

    </fieldset>
</div>
