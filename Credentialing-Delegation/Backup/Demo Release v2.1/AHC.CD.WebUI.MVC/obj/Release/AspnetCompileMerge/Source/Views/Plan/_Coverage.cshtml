<div class="panel-group" id="accordioncover" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="CoverHeading">
            <a data-toggle="collapse" data-parent="#accordioncover" href="#CH" aria-expanded="false" aria-controls="CH">
               Coverage
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="CH" class="panel-collapse collapse" role="tabpanel" aria-labelledby="CoverHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label for="Contact" class="control-label small">Every physician must arrange for twenty-four (24) hour coverage.  It is required that your on-call coverage physician(s) be a participating provider fully credentialed or in the process of being credentialed with Ultimate Health Plans.</label>
                            
                        </div>
                        <div class="col-md-6">
                            <label class="control-label small">Name of Covering Physician</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="form-group col-md-6">
                            <label for="MobileNo" class="control-label small">Telephone</label>
                            <div>
                                <div class="input-group">
                                    <div class="input-group-btn">
                                        <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                    </div>
                                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                        <div class="arrow"></div>
                                        <h3 class="popover-title">Select Country Dial Code</h3>
                                        <div class="popover-content">
                                            <select id="ContactContryCode">
                                                <option>+91</option>
                                            </select>
                                        </div>
                                    </div>
                                    <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Fax Number" required>
                                </div>

                            </div>
                        </div> 
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

