@*ng-if="visibilitySecondControl != 'MidLevelPractitionerEdit'"*@

<div id="MidLevelPractitioner">
    <fieldset>
        <legend>
            Supervising Practitioner
        </legend>
        <table ng-if="!(PracticeLocationDetail.MidlevelPractioners.length > 0 || visibilityThirdControl == 'MidLevelPractitionerEdit')" class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <td>
                        No Data Available for Supervising Practitioner's!
                        <span>Please click on <button class="btn btn-xs btn-success" ng-click="operateThirdViewAndAddControl('MidLevelPractitionerEdit')" data-toggle="tooltip" data-placement="top" title="Add New" id="noDataAddNewPracticeInterest" tooltip><i class="fa fa-plus"></i></button> to add new Supervising Practitioner's information.</span>
                    </td>
                </tr>
            </thead>
        </table>
        <table ng-if="PracticeLocationDetail.MidlevelPractioners.length>0" class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Middle name</th>
                    <th>Last Name</th>
                    <th>NPI Number </th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody ng-repeat="practitioner in PracticeLocationDetail.MidlevelPractioners">
                <tr ng-if="practitioner.Status != 'Inactive'">
                    <td>{{practitioner.FirstName || 'Not Available'}}</td>
                    <td>{{practitioner.MiddleName || 'Not Available'}}</td>
                    <td>{{practitioner.LastName || 'Not Available'}}</td>
                    <td>{{practitioner.NPINumber || 'Not Available'}}</td>
                    <td class="col-lg-2 zero-padding-left-right-div">
                        <button class="btn btn-xs btn-default" data-toggle="tooltip" data-placement="left" title="View" ng-click="operateThirdViewAndAddControl($index+'_ViewMidLevelPractitioner')" tooltip><i class="fa fa-eye"></i></button>
                        <button class="btn btn-xs btn-danger" data-toggle="tooltip" data-placement="top" title="Remove" ng-click="initWarning(practitioner);" tooltip><i class="fa fa-minus-circle"></i></button>
                        <button class="btn btn-xs btn-success" data-toggle="tooltip" data-placement="right" title="Add New" ng-click="operateThirdViewAndAddControl('MidLevelPractitionerEdit')" ng-if=" $index+1==PracticeLocationDetail.MidlevelPractioners.length" tooltip><i class="fa fa-plus"></i></button>
                    </td>
                </tr>
                <tr ng-if="visibilityThirdControl == ($index+'_ViewMidLevelPractitioner')">
                    <td colspan="5">@Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewMidLevelPractitioners.cshtml")</td>
                </tr>
                <tr ng-if="visibilityThirdControl == ('MidLevelPractitionerEdit'+$index)">
                    <td colspan="5">
                        <fieldset>
                            <legend>
                                Edit Supervising Practitioner
                            </legend>
                            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditMidLevelPractitioner.cshtml")
                        </fieldset>
                    </td>
                </tr>
            </tbody>
        </table>        
        <div ng-if="visibilityThirdControl == 'MidLevelPractitionerEdit'">
            <div ng-if="!PracticeLocationDetail.Facility.FacilityDetail.FacilityPracticeProviders || PracticeLocationDetail.Facility.FacilityDetail.FacilityPracticeProviders.length<1">
                <table class="table table-bordered table-condensed customtbodyStyle table-striped">
                    <tbody>
                        <tr>
                            <td>No Supervising Practitioners Available For This Facility</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <fieldset ng-if="PracticeLocationDetail.Facility.FacilityDetail.FacilityPracticeProviders.length>0">
                <legend>
                    Add Supervising Practitioner
                </legend>
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditMidLevelPractitioner.cshtml")
            </fieldset>
        </div>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='midlevelPractionersSuccessMsg'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div ng-show="activeMessageDiv=='midlevelPractionersRemoveErrorMsg'" class="alert alert-danger" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div class="clearfix"></div>
        </fieldset>
    <div class="modal fade" id="warningModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form id="editMidlevelPractioner">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                    </div>
                    <div class="modal-body">
                        Do you really want to remove <b>{{tempMidLevelpractitioner.FirstName}} {{tempMidLevelpractitioner.LastName}}</b> from your Mid-Level practitioners list ?
                        <input type="hidden" name="PracticeLocationID" value="{{PracticeLocationDetail.PracticeLocationDetailID}}" />
                        <input type="hidden" name="PracticeProviderID" value="{{tempMidLevelpractitioner.PracticeProviderID}}" />
                        <input type="hidden" name="ProfileID" value="{{tempMidLevelpractitioner.ProfileID}}" />
                        <input type="hidden" name="StatusType" value="2" />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-primary" ng-click="removeMidLevelPractitioner(PracticeLocationDetail)">Remove</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>