<div class="panel-group" id="accordionOffice" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="OfficeHeading">
            <a data-toggle="collapse" data-parent="#accordionOffice" href="#OD" aria-expanded="false" aria-controls="OD">
               Office Demographics
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="OD" class="panel-collapse collapse" role="tabpanel" aria-labelledby="OfficeHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-lg-12 zero-padding-left-right-div">

                        <div class="col-md-12">
                    
                            <label for="Contact" class="control-label small">Practise Type</label>
                          
                              <select class="form-control input-sm">
                                <option>Solo Practise</option>
                                <option>Group Practise</option>
                            </select>
                      
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Group Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Country</label>
                            <select class="form-control input-sm">
                                <option>Country 1</option>
                                <option>Country 2</option>
                            </select>
                        </div>
                        <div class="col-lg-4">
                            <label class="control-label small">State *</label>
                            <select class="form-control input-sm">
                                <option>Florida</option>
                                <option>Alaska</option>
                            </select>
                        </div>
                      
                    </div>

                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-lg-4">
                            <label class="control-label small">City</label>
                            <select class="form-control input-sm">
                                <option>Putnam</option>
                                <option>Raipur</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Street</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Suite</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                    </div>

                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label class="control-label small">Zip Code</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="form-group col-md-4">
                            <label for="MobileNo" class="control-label small">Office Phone</label>
                            <div>
                                <div class="input-group">
                                    <div class="input-group-btn">
                                        <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                    </div>
                                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                        <div class="arrow"></div>
                                        <h3 class="popover-title">Select Country Dial Code</h3>
                                        <div class="popover-content">
                                            <select id="ContactContryCode">
                                                <option>+91</option>
                                            </select>
                                        </div>
                                    </div>
                                    <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                                </div>

                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="MobileNo" class="control-label small">Fax</label>
                            <div>
                                <div class="input-group">
                                    <div class="input-group-btn">
                                        <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                    </div>
                                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                        <div class="arrow"></div>
                                        <h3 class="popover-title">Select Country Dial Code</h3>
                                        <div class="popover-content">
                                            <select id="ContactContryCode">
                                                <option>+91</option>
                                            </select>
                                        </div>
                                    </div>
                                    <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Fax Number" required>
                                </div>

                            </div>
                        </div>
                    </div>                    
                    
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label class="control-label small">Office Hours</label>
                            <input type="time" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Credentialing Contact</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Email</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                    </div>
                                        
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <div class="col-md-4">
                            <label class="control-label small">Office Manager</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Email</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Group NPI</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

