<div id="finalscreen" style="display:none;">
    <div class="alert alert-success alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <strong>Success!</strong> Credentialing Initiated Successfully.
    </div>    
    <div class="panel panel-success">
        <div class="panel-heading">
            <div>Plans Selected</div>
        </div>
        <div class="panel-body">
            <div>
                <div class="col-lg-12">
                    <br />
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-3" ng-repeat="plan in getSelectedPlanObj()">
                    @*<img src="data:[<mediatype>][;base64],{{plan.Logo}}" class="insurance-logo" />*@
                    <div class="page-header text-capitalize text-center">
                        <div class="col-lg-12" style="height:60px;">
                            <img src="{{plan.Logo}}" class="insurance-logo" />
                        </div>
                        <div class="clearfix">
                        </div>
                        <br />
                        <div class="panel-footer" style="height:70px;">
                            <span class="h5">{{plan.Title}}</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-success">
        <div class="panel-heading">
            <div>Additional Info</div>
        </div>
        <div class="panel-body">
            <div class="form-group">
                <label class="col-md-3 control-label">Remarks :</label>
                <div class="col-md-9">
                    <p>{{credentialInit.Remarks}}</p>
                </div>
            </div>
        </div>        
    </div>
    <div class="panel panel-success">
        <div class="panel-heading">
            <div>Tracker</div>
        </div>
        <div class="panel-body">
            <div class="col-lg-12">
                <ol class="progtrckr col-lg-12" data-progtrckr-steps="5">
                    <li class="progtrckr-done">Initiated</li>
                    <li class="progtrckr-todo">Submitted</li>
                    <li class="progtrckr-todo">Verified</li>
                    <li class="progtrckr-todo">Approved/Rejected</li>
                    <li class="progtrckr-todo">Completed</li>
                </ol>
            </div>
            <div class="clearfix"></div>            
        </div>
    </div>
    <a href="/Provider/SearchProvider"> Click Here To Initiate Credentialing For Another Provider</a>
</div>


