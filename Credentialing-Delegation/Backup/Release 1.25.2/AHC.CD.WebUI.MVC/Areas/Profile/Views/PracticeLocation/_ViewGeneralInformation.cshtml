@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.PracticeLocationDetailViewModel

<div>
    <div class="col-md-12">
        @*<div class="form-group col-md-3">
                <label class="ontrol-label small">Organization Name</label>
                <div>
                    <i class="data-label">{{PracticeLocationDetail.Organization.Name || 'Not Available'}}</i>
                </div>
            </div>*@
        <div class="form-group col-md-3">
            <label class="ontrol-label small">Facility Name</label>
            <div class="wrap-words">
                <i class="data-label">{{PracticeLocationDetail.Facility.FacilityName || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PracticeLocationCorporateName, new { @class = "control-label small" })
            <div class="wrap-words">
                <i class="data-label">{{PracticeLocationDetail.PracticeLocationCorporateName || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="ontrol-label small">IPA</label>
            <div>
                <i class="data-label">{{PracticeLocationDetail.Group.Group.Name || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            <label class="ontrol-label small">Group Name</label>
            <div class="wrap-words">
                <i class="data-label">{{PracticeLocationDetail.GroupName || 'Not Available'}}</i>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PrimaryYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetail.IsPrimary || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.CurrentlyPracticingYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetail.CurrentlyPracticingAtThisAddress || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label" ng-show="!PracticeLocationDetail.StartDate">Not Available</i>
                <i class="data-label">{{ PracticeLocationDetail.StartDate == (01-01-0001).value ? 'Not Available' : PracticeLocationDetail.StartDate | date:'MM-dd-yyyy'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.GeneralCorrespondenceYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetail.SendGeneralCorrespondence || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PrimaryTax, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetail.PrimaryTaxId || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.PracticeExclusivelyYesNoOption, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetail.PracticeExclusively || 'Not Available'}}</i>
            </div>
        </div>
    </div>
</div>