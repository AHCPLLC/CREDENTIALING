<div id="conformscreen" style="display:none;">
    <div class="panel panel-default">
        <div class="panel-heading">
            <div>Plans Selected</div>
        </div>
        <div class="panel-body">
            <div class="col-lg-12">
                <div class="col-lg-12">
                    <br />
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-3" ng-repeat="plan in getSelectedPlanObj()">
                    @*<img src="data:[<mediatype>][;base64],{{plan.Logo}}" class="insurance-logo" />*@
                    <div class="page-header text-capitalize text-center">
                        <div class="col-lg-12" style="height:60px;">
                            <img src="{{plan.Logo}}" class="insurance-logo" />
                        </div>
                        <div class="clearfix">
                        </div>
                        <br />
                        <div class="panel-footer" style="height:70px;">
                            <span class="h5">{{plan.Title}}</span>
                        </div>
                    </div>
                </div>
            </div>            
        </div>        
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">
            <div>Additional Info</div>
        </div>
        <div class="panel-body">
            <div class="form-group">
                <label class="col-md-3 control-label">Remarks :</label>
                <div class="col-md-9">                    
                    <p>{{credentialInit.Remarks}}</p>
                </div>
            </div>
        </div>
        <div class="panel-footer clearfix">
            <input type="button" value="Confirm" class="btn btn-success btn-sm pull-right" ng-click="submitCredential()" />
            <input type="button" value="<< Prev" class="btn btn-primary btn-sm pull-left" ng-click="prev()" />
        </div>
    </div>
</div>