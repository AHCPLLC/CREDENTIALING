    <div class="col-lg-4">
        <label>Index : </label>
        <div ng-if="credentialingInfo.IsDelegated == 'YES'">
            <a style="cursor:pointer" ng-click="goToNextTab('psv')"><i class="data-label">PSV</i></a><i ng-show="CCMTabStatus" class="fa fa-check-circle text-success pull-right"></i>
        </div>
        <div ng-if="credentialingInfo.IsDelegated == 'YES'">
            <a style="cursor:pointer" ng-click="goToNextTab('credentialing_action')"><i class="data-label">CCM Action</i></a><i ng-show="PackageGeneratorTabStatus" class="fa fa-check-circle text-success pull-right"></i>
        </div>
        <div ng-if="credentialingInfo.IsDelegated == 'NO'">
            <a style="cursor:pointer" ng-click="goToNextTab('packagegenerator')"><i class="data-label">Package Generator</i></a><i ng-show="GetAllPackage.length > 0" class="fa fa-check-circle text-success pull-right"></i>
        </div>
        <div>
            <a style="cursor:pointer" ng-click="goToNextTab('load_to_plan')"><i class="data-label">Load to Plan</i></a><i ng-show="LoadedData.length > 0" class="fa fa-check-circle text-success pull-right"></i>
        </div>
        <div>
            <a style="cursor:pointer" ng-click="goToNextTab('PlanReport')"><i class="data-label">Plan Enrollment</i></a>@*<i ng-show="PlanReportStatus" class="fa fa-check-circle text-success pull-right"></i>*@
        </div>
        <br />
    </div>
</div>
