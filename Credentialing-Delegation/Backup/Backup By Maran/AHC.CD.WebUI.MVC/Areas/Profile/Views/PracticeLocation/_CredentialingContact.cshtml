@*<script src="~/Scripts/Custom/Profile/CredentialingContact.js"></script>*@
<div class="form-style">
    <fieldset>
        <legend>
            Credentialing Contact
        </legend>

        <div id="CredentialingContact">
            <div class="col-lg-12 zero-padding-left-right-div" ng-show="PracticeLocationDetails[$index].PrimaryCredentialingContactPerson == null && visibilityControl!='addCc'">
                <table class="table table-bordered table-condensed customtbodyStyle table-striped">
                    <thead>
                        <tr>
                            <td>
                                No Data Available for Credentialing Contact! 
                                <span>Please click on <button id="Add_Credentialing_Contact" class="btn btn-xs btn-success" ng-click="operateViewAndAddControl('addCc')" tooltip data-toggle="tooltip" data-placement="top" title="Add New"><i class="fa fa-plus"></i></button> to add new Credentialing Contact.</span>

                            </td>
                        </tr>
                    </thead>
                </table>
            </div>
            <div ng-if="PracticeLocationDetails[$index].PrimaryCredentialingContactPerson != null && visibilityControl!='addCc' && visibilityControl!='editCc'" class="col-lg-12 zero-padding-left-right-div">
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewCredentialingContact.cshtml")
            </div>
            <div class="clearfix"></div>
            <div ng-show="activeMessageDiv=='addedNewCredentialingContactInformation'" class="alert alert-success" role="alert">
                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
            </div>
            <div class="clearfix"></div>            
                <div ng-if="visibilityControl=='addCc'" id="newShowCredentialingContactDiv">
                    @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditCredentialingContact.cshtml")
                </div>
            <div ng-if="visibilityControl=='editCc'" id="CredentialingContactEditDiv">
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditCredentialingContact.cshtml")
            </div>
        </div>
    </fieldset>
</div>