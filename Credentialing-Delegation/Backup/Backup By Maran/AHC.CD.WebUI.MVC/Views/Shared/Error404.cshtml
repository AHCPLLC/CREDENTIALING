@*@model System.Web.Mvc.HandleErrorInfo*@

@{
    Layout = null;
    ViewBag.Title = "Error";
}



<link href="~/Content/SharedCss/bootstrap.min.css" rel="stylesheet" />
<!--[if lt IE 9]>
            <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

<link href="~/Content/SharedCss/app.css" rel="stylesheet" />
<!--login modal-->
<div class="col-lg-12" style="margin-top:10%;">
    <div class="col-lg-12">
        <div class="col-lg-12" style="padding-left:15%;padding-right:15%;">
            <div class="col-lg-12">
                <div class="col-lg-12">
                    <div class="col-lg-7">
                        <div class="clearfix"></div>
                        <br />
                        <div class="clearfix"></div>
                        <br />
                        <div class="col-lg-12">
                            <div class="col-lg-12 text-left zero-padding-left-right-div">
                                <a class="navbar-brand header-logo" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Dashboard/Index"><img src="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Content/Images/logo2.png" style="width: 180px;"></a>
                            </div>
                            <div class="clearfix"></div>
                            <br />
                            <div class="clearfix"></div>
                            <br />
                            <div class="col-lg-12">
                                <div class="col-lg-12 text-left">
                                    <h4 class="text-danger">Error 404.</h4>
                                    <h5 class="text-muted">We could not find the requested page on our server. Please contact your Admin or Please <a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Dashboard/Index">Click Here</a></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="text-center">
                            <img src="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Content/Images/error.jpg" style="width:90%;" />
                            <h4 class="text-muted">Page Not Found</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/Scripts/Lib/Jquery/jquery-1.11.0.js"></script>
<script src="~/Scripts/Lib/Bootstrap/bootstrap.min.js"></script>

