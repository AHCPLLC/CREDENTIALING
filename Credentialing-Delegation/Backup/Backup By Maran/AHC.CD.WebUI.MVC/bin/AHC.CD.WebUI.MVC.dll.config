<?xml version="1.0" encoding="utf-8"?>
<configuration>



  <configSections>
    <!-- Unity Configuration Section -->
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />

    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <sectionGroup name="elmah">
      <section name="security" requirePermission="false" type="Elmah.SecuritySectionHandler, Elmah" />
      <section name="errorLog" requirePermission="false" type="Elmah.ErrorLogSectionHandler, Elmah" />
      <section name="errorMail" requirePermission="false" type="Elmah.ErrorMailSectionHandler, Elmah" />
      <section name="errorFilter" requirePermission="false" type="Elmah.ErrorFilterSectionHandler, Elmah" />
    </sectionGroup>
    <sectionGroup name="bundleTransformer">
      <section name="core" type="BundleTransformer.Core.Configuration.CoreSettings, BundleTransformer.Core" />
    </sectionGroup>
    <sectionGroup name="jsEngineSwitcher">
      <section name="core" type="JavaScriptEngineSwitcher.Core.Configuration.CoreConfiguration, JavaScriptEngineSwitcher.Core" />
    </sectionGroup>
  </configSections>


  <unity configSource="UnityRegister.config" />
  <connectionStrings configSource="ConnectionStrings.config" />


  <appSettings file="PrivateSettings.config">
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.IgnoreDefaultRoute" value="false" />
    <add key="elmah.mvc.allowedRoles" value="*" />
    <add key="elmah.mvc.allowedUsers" value="*" />
    <add key="elmah.mvc.route" value="elmah" />
    <add key="redisHost" value="localhost" />
    <add key="owin:AppStartup" value="AHC.CD.WebUI.MVC.Startup,AHC.CD.WebUI.MVC" />
    <add key="AppRoot" value="" />
    <add key="ProductEmailID" value="support@credaxis.com" />
    <add key="DisclaimerEmailID" value="credentialing@accesshealthcarellc.net" />
    <add key="DisclaimerMessage" value="This is a system generated email, please do not respond. Send all inquiries directly to credentialing@accesshealthcarellc.net" />    
    <add key="CompanyName" value="Access Health Care" />
    <add key="CompanyContactNumber" value="" />
    <add key="DashBoardStatus" value="true"/>
    <!--controls the root settings use like "/cnd"-->




    <!-- Session Timeout (In minutes)-->
    <add key="sessionTimeout" value="60" />

    <!-- Beginning of License Expiry Email Notification Config Section -->

    <!-- seconds(0-59) minutes(0-59) hours(0-23) dayOfMonth(1-31) month(1-12) dayOfWeek(1-7) year (optional or 1970-2099) -->

    <!-- Run every day at 1:30 AM -->
    <add key="expiryNotificationSendCornScheduler" value="30 1 2 6 7 ?" />

    <!-- Run every day at 1:20 AM -->
    <add key="expiryNotificationDataCollectCornScheduler" value="30 1 3 6 7 ?" />

    <!-- For only testing Run every 1 minute 30 seconds once -->
    <add key="expiryNotificationSendCornScheduler" value="59 1 * * * ?" />

    <!-- For only testing Run every 1 minutes once -->
    <add key="expiryNotificationDataCollectCornScheduler" value="30 1 * * * ?" />
    <!-- For only testing Run every 1 minute 30 seconds once -->
    <add key="taskexpiryNotificationSendCornScheduler" value="0 2/5 7 1/1 * ? *" />

     <!--For only testing Run every 1 minutes once--> 
    <add key="taskexpiryNotificationDataCollectCornScheduler" value="0 0/5 7 1/1 * ? *" />
    <!-- End of License Expiry Email Notification Config Section -->

    <!-- Beginning of Profile Change Email Notification Config Section -->

    <!-- seconds(0-59) minutes(0-59) hours(0-23) dayOfMonth(1-31) month(1-12) dayOfWeek(1-7) year (optional or 1970-2099) -->

    <!-- Run at every 59 minutes once -->
    <!--<add key="changeNotificationCornScheduler" value="* 59 * * * ?" />-->

    <!-- For only testing  - Runs at every 30 seconds once -->

    <add key="changeNotificationForCCMCornScheduler" value="30 * * * * ?" />
    <add key="changeNotificationCornScheduler" value="0 30 2 * * ?" />
    <add key="emailNotificationSendingCornScheduler" value="10 * * 20 * ?" />

    <add key="changeNotificationForCCMCornScheduler" value="30 1 3 6 7 ?" />
    <add key="changeNotificationCornScheduler" value="30 1 3 6 7 ?" />
    <add key="emailNotificationSendingCornScheduler" value="30 1 3 6 7 ?" />
    <!-- End of Profile Change Email Notification Config Section -->



  </appSettings>

  <system.web>
    <!--<customErrors mode="RemoteOnly" defaultRedirect="/ErrorPage" />-->
    <globalization fileEncoding="utf-8" requestEncoding="utf-8" responseEncoding="utf-8" culture="en-US" uiCulture="de-DE" />
    <customErrors mode="RemoteOnly" defaultRedirect="~/ErrorPage">
      <error statusCode="404" redirect="~/ErrorPage/Error404" />
      <error statusCode="500" redirect="~/ErrorPage/Error500" />
    </customErrors>
    <!--<sessionState mode="InProc" timeout="1" />-->

    <!--<authentication mode="Forms">
      <forms loginUrl="/Account/Login" />
    </authentication>
    <roleManager enabled="true" />-->
    <sessionState mode="InProc" timeout="1" />
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime maxRequestLength="1547116567" targetFramework="4.5.1" />
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
  </system.web>



  <system.web.extensions>
    <scripting>
      <webServices>
        <jsonSerialization maxJsonLength="2147483647" />
      </webServices>
    </scripting>
  </system.web.extensions>

  <system.webServer>
    <urlCompression doDynamicCompression="true" doStaticCompression="true" dynamicCompressionBeforeCache="true" />
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" cacheControlCustom="public" />
    </staticContent>
    <modules>
      <remove name="FormsAuthenticationModule" />
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" />
    </modules>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1547116567" />
      </requestFiltering>
    </security>
    <validation validateIntegratedModeConfiguration="false" />

  </system.webServer>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Optimization" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-1.1.0.0" newVersion="1.1.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Practices.Unity" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.2.0" newVersion="5.2.2.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <entityFramework codeConfigurationType="AHC.CD.Data.EFRepository.EFCacheConfiguration, AHC.CD.Data.EFRepository">
    <!-->  For EF 2nd Level Cacheing  -->
    <!--<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />-->
    <!--</providers>-->
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v12.0" />
      </parameters>
    </defaultConnectionFactory>
    <contexts>
      <context type="AHC.CD.Data.EFRepository.EFEntityContext, AHC.CD.Data.EFRepository">
        <databaseInitializer type="System.Data.Entity.MigrateDatabaseToLatestVersion`2[[AHC.CD.Data.EFRepository.EFEntityContext, AHC.CD.Data.EFRepository], [AHC.CD.Data.EFRepository.Migrations.Configuration,AHC.CD.Data.EFRepository]], EntityFramework" />
      </context>
    </contexts>
  </entityFramework>

  <elmah>
    <security allowRemoteAccess="1" requirePermission="false" />

    <!-- EMail Logger Configuration 
    
          <errorMail from="venkat@pratian.com"
              to="kk@pratian.com,venkatshiva.reddy@gmail.com"
              subject="Testing ELMAH Error in AHC-CD Application"
              cc=""
              async="true"
              smtpPort="25"
              smtpServer="smtp.net4india.com"
              userName=""
              password=""/>  
  -->

    <!-- XML Logger Configuration -->
    <errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/App_Data" />
    <!-- SQL Server DB Errorl Log Configuration -->
    <!--<errorLog type="Elmah.SqlErrorLog, Elmah" connectionString="Data Source=103.229.5.131;Initial Catalog=ACDElmahDB1.10;User ID=ahc;Password=password@123" />-->

  </elmah>

  <!-- Email Configuraiton -->

  <system.net>
    <defaultProxy>

      <proxy usessystemdefault="False" proxyaddress="http://mail.accesshealthcarellc.net" bypassonlocal="True" autoDetect="False" />


    </defaultProxy>

    <mailSettings>

      <!--<smtp from="Access Health Care LLC - CredAxis &lt;credtestingaxis@inbox.lt&gt;">
            <network host="mail.inbox.lt" port="587" defaultCredentials="false" userName="credtestingaxis" password="CredAxis" enableSsl="true" />
      </smtp>-->

      <!--<smtp from="Access Health Care LLC - CredAxis &lt;support@credaxis.com&gt;">
        <network host="smtpout.secureserver.net" port="80" defaultCredentials="false" userName="support@credaxis.com" password="Welcome123" enableSsl="false" />
      </smtp>-->


      <smtp deliveryMethod="SpecifiedPickupDirectory" from="Pratian &lt;support@credaxis.com&gt;">
        <network host="ignored" />
        <specifiedPickupDirectory pickupDirectoryLocation="c:\temp" />
      </smtp>

    </mailSettings>
  </system.net>

  <bundleTransformer xmlns="http://tempuri.org/BundleTransformer.Configuration.xsd">
    <core>
      <css>
        <translators>
          <add name="NullTranslator" type="BundleTransformer.Core.Translators.NullTranslator, BundleTransformer.Core" enabled="false" />
        </translators>
        <postProcessors>
          <add name="UrlRewritingCssPostProcessor" type="BundleTransformer.Core.PostProcessors.UrlRewritingCssPostProcessor, BundleTransformer.Core" useInDebugMode="false" />
        </postProcessors>
        <minifiers>
          <add name="NullMinifier" type="BundleTransformer.Core.Minifiers.NullMinifier, BundleTransformer.Core" />
        </minifiers>
        <fileExtensions>
          <add fileExtension=".css" assetTypeCode="Css" />
        </fileExtensions>
      </css>
      <js>
        <translators>
          <add name="NullTranslator" type="BundleTransformer.Core.Translators.NullTranslator, BundleTransformer.Core" enabled="false" />
        </translators>
        <minifiers>
          <add name="NullMinifier" type="BundleTransformer.Core.Minifiers.NullMinifier, BundleTransformer.Core" />
        </minifiers>
        <fileExtensions>
          <add fileExtension=".js" assetTypeCode="JavaScript" />
        </fileExtensions>
      </js>
    </core>
  </bundleTransformer>
  <jsEngineSwitcher xmlns="http://tempuri.org/JavaScriptEngineSwitcher.Configuration.xsd"></jsEngineSwitcher>
</configuration>