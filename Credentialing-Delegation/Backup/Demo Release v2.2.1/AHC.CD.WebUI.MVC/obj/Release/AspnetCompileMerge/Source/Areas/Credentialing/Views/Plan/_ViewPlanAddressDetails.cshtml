@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.Plan.PlanAddressViewModel

<fieldset>
    <legend>
        Address Details
    </legend>
    <div ng-repeat="pl in tempViewObject.Locations track by $index" class="col-lg-12 zero-padding-right-right-div">

        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.Street, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.Street, new { @class = "form-control input-sm", data_ng_model = "pl.Street", placeholder = "street" })*@
            <label class="control-label small" for="SalutationType">Street/P.O Box:</label>
            <div>
                <i class="data-label" ng-if="pl.Street">{{pl.Street}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.Appartment, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.Appartment, new { @class = "form-control input-sm", data_ng_model = "pl.Appartment", placeholder = "appartment" })*@
            <label class="control-label small" for="SalutationType">Appartment No:</label>
            <div>
                <i class="data-label" ng-if="pl.Appartment">{{pl.Appartment}}</i>
            </div> 
        </div>
        <div class="col-lg-4 form-group">
            @*@Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.City, new { @class = "form-control input-sm", data_ng_model = "pl.City", placeholder = "city name", data_ng_change = "addressHomeAutocomplete(pl.City)", onfocus = "showLocationList(this)" })*@
            <label class="control-label small" for="SalutationType">City:</label>
            <div>
                <i class="data-label" ng-if="pl.City">{{pl.City}}</i>
            </div> 
            
            
            <div class="ProviderTypeSelectAutoList popover fade bottom in" role="tooltip">
                <table class="table table-striped table-bordered table-hover table-condensed marginBottomAutosearch">
                    <tbody>
                        <tr ng-repeat="location in Locations track by $index" ng-click="selectedLocation(location)" class="pointer">
                            <td>{{location.City}} - {{location.State}} - {{location.CountryCode}}</td>
                        </tr>
                        <tr ng-if="Locations.length == 0">
                            <td>Not Available..... </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            @Html.ValidationMessageFor(model => model.City, "", new { @class = "text-danger" })
        </div>
        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.ZipCode, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.ZipCode, new { @class = "form-control input-sm", data_ng_model = "pl.State", placeholder = "state" })
            @Html.ValidationMessageFor(model => model.ZipCode, "", new { @class = "text-danger" })*@
            <label class="control-label small" for="SalutationType">Zip Code:</label>
            <div>
                <i class="data-label" ng-if="pl.State">{{pl.State}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.State, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.State, new { @class = "form-control input-sm", data_ng_model = "pl.ZipCode", placeholder = "ZipCode" })
            @Html.ValidationMessageFor(model => model.State, "", new { @class = "text-danger" })*@
            <label class="control-label small" for="SalutationType">State:</label>
            <div>
                <i class="data-label" ng-if="pl.ZipCode">{{pl.ZipCode}}</i>
            </div> 
        </div>
        
        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.Country, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.Country, new { @class = "form-control input-sm", data_ng_model = "pl.Country", placeholder = "country" })
            @Html.ValidationMessageFor(model => model.Country, "", new { @class = "text-danger" })*@
            <label class="control-label small" for="SalutationType">Country:</label>
            <div>
                <i class="data-label" ng-if="pl.Country">{{pl.Country}}</i>
            </div> 
        </div>
        <div class="col-md-4 form-group">
            @*@Html.LabelFor(model => model.County, new { @class = "control-label small" })
            @Html.TextBoxFor(model => model.County, new { @class = "form-control input-sm", data_ng_model = "pl.County", placeholder = "county" })
            @Html.ValidationMessageFor(model => model.County, "", new { @class = "text-danger" })*@
            <label class="control-label small" for="SalutationType">County:</label>
            <div>
                <i class="data-label" ng-if="pl.County">{{pl.County}}</i>
            </div> 
        </div>
    </div>
</fieldset>


