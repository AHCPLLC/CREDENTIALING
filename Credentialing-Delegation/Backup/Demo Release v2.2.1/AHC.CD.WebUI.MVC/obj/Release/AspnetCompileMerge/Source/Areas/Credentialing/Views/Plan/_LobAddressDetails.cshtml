@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.Plan.LOBAddressDetailViewModel

<fieldset>
    <legend>
        Address Details
    </legend>
    @*<div class="pull-right legend2">
        <button type="button" class="btn btn-xs btn-success" tooltip data-toggle="tooltip" data-placement="top" title="Add More" ng-click="AddLobAddress();"><i class="fa fa-plus"></i></button>
    </div>*@
    <div ng-repeat="lobadd in tempObject1.AddressDetails track by $index" class="col-lg-12 zero-padding-right-right-div">
        @*<fieldset>
    <div ng-if="$index > 0" class="pull-right legend2">
        <button type="button" class="btn btn-xs btn-danger" tooltip data-toggle="tooltip" data-placement="top" title="Remove" ng-click="RemoveLobAddress($index);"><i class=" fa fa-minus"></i></button>
    </div>*@
        <div class="col-lg-12">
            <input type="checkbox" ng-init="lobadd.IsSameAddress=true" ng-model="lobadd.IsSameAddress" ng-change="GetAddress(lobadd.IsSameAddress)" /><label class="control-label">&nbsp;Same As Plan Address Detail</label>
        </div>
        <div class="col-lg-12">
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.Street, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.Street, new { @class = "form-control input-sm", data_ng_model = "lobadd.Street", placeholder = "street" })
                @Html.ValidationMessageFor(model => model.Street, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.Appartment, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.Appartment, new { @class = "form-control input-sm", data_ng_model = "lobadd.Appartment", placeholder = "appartment" })
                @Html.ValidationMessageFor(model => model.Appartment, "", new { @class = "text-danger" })
            </div>
            <div class="col-lg-4 form-group">
                @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.City, new { @class = "form-control input-sm", data_ng_model = "lobadd.City", placeholder = "city name", data_ng_change = "addressHomeAutocomplete(lobadd.City , lobadd)", onfocus = "showLocationList(this)" })
                <div class="ProviderTypeSelectAutoList popover fade bottom in" role="tooltip">
                    <table class="table table-striped table-bordered table-hover table-condensed marginBottomAutosearch">
                        <tbody>
                            <tr ng-repeat="location in Locations track by $index" ng-click="selectedLocation(location , lobadd)" class="pointer">
                                <td>{{location.City}} - {{location.State}} - {{location.CountryCode}}</td>
                            </tr>
                            <tr ng-if="Locations.length == 0">
                                <td>Not Available..... </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                @Html.ValidationMessageFor(model => model.City, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="col-lg-12">
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.State, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.State, new { @class = "form-control input-sm", data_ng_model = "lobadd.State", placeholder = "state" })
                @Html.ValidationMessageFor(model => model.State, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.ZipCode, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.ZipCode, new { @class = "form-control input-sm", data_ng_model = "lobadd.ZipCode", placeholder = "ZipCode" })
                @Html.ValidationMessageFor(model => model.ZipCode, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.Country, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.Country, new { @class = "form-control input-sm", data_ng_model = "lobadd.Country", placeholder = "country" })
                @Html.ValidationMessageFor(model => model.Country, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="col-lg-12">
            <div class="col-md-4 form-group">
                @Html.LabelFor(model => model.County, new { @class = "control-label small" })
                @Html.TextBoxFor(model => model.County, new { @class = "form-control input-sm", data_ng_model = "lobadd.County", placeholder = "county" })
                @Html.ValidationMessageFor(model => model.County, "", new { @class = "text-danger" })
            </div>
        </div>
        @*</fieldset>*@
    </div>
</fieldset>

