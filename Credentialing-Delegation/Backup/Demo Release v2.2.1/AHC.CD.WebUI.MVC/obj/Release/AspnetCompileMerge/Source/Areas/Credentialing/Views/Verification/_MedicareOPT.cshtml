
<div class="panel-body">
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="MedicareOPT.ViewDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_ViewMedicareOPTVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="MedicareOPT.AddDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditMedicareOPTVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="MedicareOPT.EditDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditMedicareOPTVerificationResult.cshtml")
    </div>
    <div class="clearfix"></div><br />
    <div ng-show="activeMessageDiv=='optPSV'" class="alert alert-success" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
    <div ng-show="activeMessageDiv=='optPSVError'" class="alert alert-danger" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
</div>


