<fieldset>
    <legend>
        @*{{lisence.name}}*@
        <strong>Verification Result</strong>
    </legend>
    <div class="col-lg-12">
        <table class="table table-bordered table-condensed">
            <thead>
                <tr>
                    <th>Status</th>
                    <th>Source</th>
                    <th>Remarks</th>
                    <th>Verified Document</th>
                    <th>Action</th>
            <tbody>
                <tr>
                    <td>
                        <div>
                            <label class="control-label small">{{lisence.ProfileVerificationDetail.VerificationResult.VerificationResultStatus || 'Not Available'}}</label>
                        </div>
                    </td>
                    <td>
                        <div>
                            <label class="control-label small">{{lisence.ProfileVerificationDetail.VerificationResult.Source || 'Not Available'}}</label>
                        </div>
                    </td>
                    <td>
                        <div>
                            <label class="control-label small">{{lisence.ProfileVerificationDetail.VerificationResult.Remark || 'Not Available'}}</label>
                        </div>
                    </td>
                    <td>
                        <div>
                            <a ng-if="lisence.ProfileVerificationDetail.VerificationResult.VerificationDocumentPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="{{lisence.ProfileVerificationDetail.VerificationResult.VerificationDocumentPath}}" target="_blank"><i class="fa fa-file"></i></a>
                            <label ng-if="!lisence.ProfileVerificationDetail.VerificationResult.VerificationDocumentPath">Not Available</label>
                        </div>
                    </td>
                    <td>
                        <div>
                            <a class="btn btn-xs btn-primary" tooltip data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit" ng-click="lisence.EditDiv=true;lisence.ViewDiv=false"></i></a>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</fieldset>

