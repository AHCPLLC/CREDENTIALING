<div class="panel-group" id="accordionedu" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="educationHeading">
            <a data-toggle="collapse" data-parent="#accordionedu" href="#edu" aria-expanded="false" aria-controls="edu">
                Education
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="edu" class="panel-collapse collapse" role="tabpanel" aria-labelledby="educationHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label class="control-label small">Type Of Education</label>
                            <select class="form-control input-sm">
                                <option>Medical School</option>
                                <option>Internship</option>
                                <option>Residency</option>
                                <option>Fellowship</option>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                       
                        <div class="col-md-4">
                            <label class="control-label small">State</label>
                            <select class="form-control input-sm">
                                <option>State 1</option>
                                <option>State 2</option>
                            </select>

                        </div>
                        <div class="col-md-4">
                            <label class="control-label small">Degree</label>
                            
                            <select class="form-control input-sm">
                                <option>Degree 1</option>
                                <option>Degree 2</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label class="control-label small">Year From</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-6">
                            <label class="control-label small">Year To</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


