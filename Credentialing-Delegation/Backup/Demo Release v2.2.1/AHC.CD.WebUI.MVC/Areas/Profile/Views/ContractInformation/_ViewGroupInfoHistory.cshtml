<fieldset>
    <legend>
        Group Information
    </legend>
    @Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
    <div class="col-md-12 zero-padding-left-right-div">
        <div class="col-md-3 form-group">
            <label class="control-label small">IPA Name *</label>
            <div>
                <i class="data-label">{{groupHistory.PracticingGroup.Group.Name}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">Start Date *</label>
            <div>
                <i class="data-label">{{groupHistory.JoiningDate| date:'MM-dd-yyyy'}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">End Date</label>
            <div>
                <i ng-show="gi.ExpiryDate" class="data-label">{{groupHistory.ExpiryDate| date:'MM-dd-yyyy'}}</i>
                <i ng-show="!gi.ExpiryDate" class="data-label">Not Available</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">Status *</label>
            <div>
                <i class="data-label">{{groupHistory.ContractGroupStatus}}</i>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="clearfix"></div>
    <div class="col-md-12 form-group">
        <div>
            <button class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
        </div>
    </div>
</fieldset>