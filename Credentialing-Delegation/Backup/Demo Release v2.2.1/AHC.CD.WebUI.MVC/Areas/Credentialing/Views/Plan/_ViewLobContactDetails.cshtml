@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.Plan.LOBContactDetailViewModel

<fieldset>
    <legend>
        Contact Details
    </legend>
    <div ng-repeat="lobcon in tempObjectPLOB.ContactDetails track by $index" class="col-lg-12 zero-padding-right-right-div">
        <div class="col-lg-3 form-group">
            <label class="control-label small" for="ContactPersonName">Contact Person Name:</label>
            <div>
                <i class="data-label" ng-if="lobcon.ContactPersonName">{{lobcon.ContactPersonName}}</i>
            </div> 
            

        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small" for="PhoneNumber">Phone Number:</label>
            <div>
                <i class="data-label" ng-if="lobcon.PhoneNumber">{{lobcon.PhoneCountryCode}}-{{lobcon.PhoneNumber}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small" for="FaxNumber">Fax Number:</label>
            <div>
                <i class="data-label" ng-if="lobcon.FaxNumber">{{lobcon.FaxCountryCode}}-{{lobcon.FaxNumber}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small" for="EmailAddress">Email Address:</label>
            <div>
                <i class="data-label" ng-if="lobcon.EmailAddress">{{lobcon.EmailAddress}}</i>
            </div>
        </div>
    </div>
</fieldset>

