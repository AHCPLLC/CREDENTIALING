@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.SubPlanViewModel

<fieldset>
    <legend>
        Sub Plan
    </legend>
    <div ng-repeat="lobsub in tempObjectPLOB.SubPlans track by $index" class="col-lg-12 zero-padding-right-right-div">
        <fieldset>
            <div ng-if="$index > 0" class="pull-right legend2">
                <button type="button" class="btn btn-xs btn-danger" tooltip data-toggle="tooltip" data-placement="top" title="Remove" ng-click="RemoveSubPlan($index);"><i class=" fa fa-minus"></i></button>
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small" for="SubPlanName">SubPlanName:</label>
                <div>
                    <i class="data-label" ng-if="lobsub.SubPlanName">{{lobsub.SubPlanName}}</i>
                </div>
            </div>
            <div class="col-md-4 form-group">
                <div class="col-lg-4 form-group">
                    <label class="control-label small" for="SubPlanCode">SubPlanCode:</label>
                    <div>
                        <i class="data-label" ng-if="lobsub.SubPlanCode">{{lobsub.SubPlanCode}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-4 form-group">
                <div class="col-lg-4 form-group">
                    <label class="control-label small" for="SubPlanDescription">SubPlanDescription:</label>
                    <div>
                        <i class="data-label" ng-if="lobsub.SubPlanDescription">{{lobsub.SubPlanDescription}}</i>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
</fieldset>


