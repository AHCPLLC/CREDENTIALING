@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.Plan.LOBAddressDetailViewModel

<fieldset>
    <legend>
        Address Details
    </legend>
    <div ng-repeat="lobadd in tempObjectPLOB.Locations track by $index" class="col-lg-12 zero-padding-right-right-div">
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Street">Street:</label>
            <div>
                <i class="data-label" ng-if="lobadd.Street">{{lobadd.Street}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Appartment">Appartment:</label>
            <div>
                <i class="data-label" ng-if="lobadd.Appartment">{{lobadd.Appartment}}</i>
            </div>
        </div>
        <div class="col-lg-4 form-group">
            <label class="control-label small" for="City">City:</label>
            <div>
                <i class="data-label" ng-if="lobadd.City">{{lobadd.City}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="State">State:</label>
            <div>
                <i class="data-label" ng-if="lobadd.State">{{lobadd.State}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="State">ZipCode:</label>
            <div>
                <i class="data-label" ng-if="lobadd.State">{{lobadd.ZipCode}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Country">Country:</label>
            <div>
                <i class="data-label" ng-if="lobadd.Country">{{lobadd.Country}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="County">County:</label>
            <div>
                <i class="data-label" ng-if="lobadd.County">{{lobadd.County}}</i>
            </div>
        </div>
    </div>
</fieldset>


