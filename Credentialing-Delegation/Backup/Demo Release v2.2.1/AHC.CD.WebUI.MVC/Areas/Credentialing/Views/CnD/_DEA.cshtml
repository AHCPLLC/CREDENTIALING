<div class="panel-body" ng-repeat="lisence in tab.Info">
    <form id="PlanForm" class="form-style">
        <fieldset>
            <legend>
                @*{{lisence.name}}*@
                <strong>{{tab.Info[0].ProfileVerficationParameterObj.Title}}-{{$index+1}}</strong>
            </legend>

            <div class="col-lg-12">
                <table class="table table-bordered table-condensed">
                    <thead>
                        <tr>
                            <th>License Number</th>
                            <th>Issue Date</th>
                            <th>Expiration Date</th>
                            <th>Document</th>
                    <tbody>
                        <tr>
                            <td>
                                <div>
                                    <label class="control-label small">{{lisence.VerificationData.DEANumber}}</label>
                                </div>
                            </td>
                            <td>
                                <div>
                                    <label class="control-label small" ng-bind="reformatDate(lisence.VerificationData.IssueDate)"></label>
                                </div>
                            </td>
                            <td>
                                <div>
                                    <label class="control-label small" ng-bind="reformatDate(lisence.VerificationData.ExpiryDate)"></label>
                                </div>
                            </td>
                            <td>
                                <div>
                                    <a ng-if="lisence.VerificationData.DEALicenceCertPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="{{lisence.VerificationData.DEALicenceCertPath}}" targ targe target="_blank"><i class="fa fa-file"></i></a>
                                    <p ng-if="!lisence.VerificationData.DEALicenceCertPath">Not Available</p>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-1 form-group">
                    <label class="control-label small" for="SalutationType">Status:</label>
                    <div>
                        <i class="data-label">{{lisence.VerificationResultObj.VerificationResultStatus || 'Not Available'}}</i>
                    </div>
                </div>

                <div class="col-lg-2 form-group">
                    @*<input type="text" class="form-control input-sm" id="exampleInputEmail1" placeholder="Verification Source" ng-model="lisence.source">*@

                    <label class="control-label small" for="SalutationType">Source:</label>
                    <div>
                        <i class="data-label">{{lisence.VerificationResultObj.Source || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="form-group">
                        <label class="control-label small" for="SalutationType">Remarks:</label>
                        <div>
                            <i class="data-label">{{lisence.VerificationResultObj.Remark || 'Not Available'}}</i>
                        </div>
                        @*<input type="text" class="form-control input-sm" id="exampleInputEmail1" placeholder="Remarks" ng-model="lisence.remarks">*@
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="form-group">
                        <label class="control-label small" for="SalutationType">Verified Document:</label>
                        <div>
                            <a ng-if="lisence.VerificationResultObj.VerificationDocumentPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="{{lisence.VerificationResultObj.VerificationDocumentPath}}" targ target="_blank"><i class="fa fa-file"></i></a>
                            <label ng-if="!lisence.VerificationResultObj.VerificationDocumentPath">Not Available</label>
                        </div>
                        @*<input type="text" class="form-control input-sm" id="exampleInputEmail1" placeholder="Remarks" ng-model="lisence.remarks">*@
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    <label class="control-label small" for="SalutationType">Verified Date:</label>
                    <div>
                        <i class="data-label" ng-if="lisence.VerificationResultObj.LastModifiedDate" ng-bind="ConvertDateFormat(lisence.VerificationResultObj.LastModifiedDate)">{{lisence.VerificationResultObj.LastModifiedDate}}</i>
                        <label ng-if="!lisence.VerificationResultObj.LastModifiedDate">Not Available</label>
                    </div>
                </div>
            </div>

        </fieldset>
    </form>
</div>
