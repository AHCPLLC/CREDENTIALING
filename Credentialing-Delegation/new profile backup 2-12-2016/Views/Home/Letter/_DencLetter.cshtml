<div class="modal fade authPreviewModal" tabindex="-1" role="dialog" id="DENCPreviewModal" style="overflow: auto; top: 8%;">
    <div class="modal-dialog modal-lg ">
        <form class="modal-content">
            <div class="modal-header zero_padding_bottom">
                <div class="row">

                    <h4 class="modal-title">DENC Letter</h4>

                </div>
            </div>

            <div class="modal-body modalbodyheight uppercase previewModalStyle text-uppercase">
                <div id="LetterGeneration">
                    <div class="col-lg-4"><button class="btn btn-danger" data-dismiss="modal" id="cancelLetterBtn" @*title="Cancel letter"*@><i class="fa fa-times"></i>Close</button></div>
                    <div class="col-lg-2"></div>
                    <div class="col-lg-2">
                        <button ng-click="GetallfalseForLetter();printLetterfunc('printletterdenial',modalTitle );" ng-if="TempLet.LetterTemplateName=='Denial Letter' || modalTitle=='Denial Letter'" class="btn btn-primary" ng-disabled="IsHistoryPage" style="margin-left:-40%;" @*title="Print letter"*@><i class="fa fa-print" @*title="Print letter"*@></i> Print</button>
                        <button ng-click="GetallfalseForLetter();printLetterfunc('printletterdenc',modalTitle );" ng-if="TempLet.LetterTemplateName=='DENC Letter' || modalTitle=='DENC Letter'" class="btn btn-primary" ng-disabled="IsHistoryPage" style="margin-left:-40%;" @*title="Print letter"*@><i class="fa fa-print" @*title="Print letter"*@></i> Print</button>
                        <button ng-click="GetallfalseForLetter();printLetterfunc('printletternomnc',modalTitle );" ng-if="TempLet.LetterTemplateName=='NOMNC Letter' || modalTitle=='NOMNC Letter'" class="btn btn-primary" ng-disabled="IsHistoryPage" style="margin-left:-40%;" @*title="Print letter"*@><i class="fa fa-print" @*title="Print letter"*@></i> Print</button>
                        <button ng-click="printLetter('letter',modalTitle );" ng-if="TempLet.LetterTemplateName!='NOMNC Letter' && TempLet.LetterTemplateName!='Denial Letter' && modalTitle!='Denial Letter' &&  modalTitle!='NOMNC Letter' && modalTitle!='DENC Letter' && TempLet.LetterTemplateName!='DENC Letter' " class="btn btn-primary" ng-disabled="IsHistoryPage" style="margin-left:-40%;" @*title="Print letter"*@><i class="fa fa-print" @*title="Print letter"*@></i> Print</button>
                    </div>
                    <div class="col-lg-4">
                        <button ng-click="saveTemplate('',Authorization.AuthorizationID, TempLet.LetterTemplateName, TempLet.Entity, Reason, '', '',TempObject.ServicesNOMNCLetter,LOSIDForNomncLetter,TempObject.Facts,TempObject.DetailedExplanation,TempObject.Policy);" ng-if="TempLet.LetterTemplateName=='DENC Letter'" data-dismiss="modal" class="btn btn-info pull-right" ng-disabled="!isNew"><i class="fa fa-save"></i> Save</button>
                        <button ng-click="saveTemplate('',Authorization.AuthorizationID, TempLet.LetterTemplateName, TempLet.Entity, Reason,TempObject.ToDateForNomc, TempObject.FromDateForNomc, TempObject.ServicesNOMNCLetter,LOSIDForNomncLetter,'','','');" ng-if="TempLet.LetterTemplateName=='NOMNC Letter'" data-dismiss="modal" class="btn btn-info pull-right" ng-disabled="!isNew"><i class="fa fa-save"></i> Save</button>
                        <button ng-click="saveTemplate('',Authorization.AuthorizationID, TempLet.LetterTemplateName, TempLet.Entity, Reason,'','','','','','','');" ng-if="TempLet.LetterTemplateName!='NOMNC Letter' && TempLet.LetterTemplateName!='DENC Letter'" data-dismiss="modal" class="btn btn-info pull-right" ng-disabled="!isNew" @*title="Save letter"*@><i class="fa fa-save" @*title="Save letter"*@></i> Save</button>
                    </div>
                    <div class="col-lg-12" style="margin-top:20px; border-top: 1px solid #e5e5e5;">
                        <div class="row-fluid overflow-visible">

                            <!-- DENC LETTER -->
                            <div id="letter">
                                <div style="margin-top:-40px;" id="">
                                    <div>
                                        <div style="" id="">
                                            <div id="letter_content">
                                                <div id="">
                                                    <div style='text-align:center;'>
                                                        <p style="font-family:'Courier New';font-size:14px">
                                                            <b class="TimesNewRoman-17FontSize">
                                                                {{Authorization.Facility.Name||""}}
                                                            </b>
                                                            <br />
                                                            {{Authorization.Facility.Address1||""}}  {{Authorization.Facility.City||""}}<br />
                                                            {{Authorization.Facility.PhoneNumber||""}}
                                                        </p>
                                                        <p class='TimesNewRoman-19FontSize removeBottomMargin'>
                                                            <b>Detailed Explanation of Non-coverage</b>
                                                        </p>
                                                    </div>
                                                    <div class="pSpacing" id="" style="border-top: solid #020202; margin-top:5px;">
                                                        <div id='' class="ArialFontStyle-12FontSize" style='overflow:auto;'>
                                                            <div style='width:90%;'>
                                                                <p style='float:left;margin-top:0px;margin-bottom:0px;' class="removeBottomMargin">
                                                                    <br />
                                                                    Date:
                                                                    <b>
                                                                        <span class="text-uppercase">{{currentDt||""}}</span>
                                                                    </b>
                                                                    <b>
                                                                        <span class="text-uppercase"></span>
                                                                    </b>
                                                                </p>
                                                                <br />
                                                                <br />
                                                                <br />
                                                                <p style='float:left;margin-top:0px;margin-bottom:0px;' class="removeBottomMargin">
                                                                    Patient Name:
                                                                    <b>
                                                                        <span class="text-uppercase">{{Authorization.Member.Name||""}}</span>
                                                                    </b>
                                                                </p>
                                                                <p style='float:right;margin-top:0px;margin-bottom:0px;' class="removeBottomMargin">
                                                                    Patient Number:
                                                                    <b>
                                                                        {{memberSubscriberIdForLetter||""}}
                                                                    </b>
                                                                </p>
                                                            </div>
                                                            <br />
                                                            <br />
                                                        </div>
                                                    </div>
                                                    <div class="pSpacing ArialFontStyle-12FontSize" id="" style="border-top:solid #020202;">
                                                        <br />
                                                        <br />
                                                        <p style="width: 93%">
                                                            This notice gives a detailed explanation of why your Medicare provider and/or health plan has determined Medicare coverage for your current services should end. <b><i>This notice is not the decision on your appeal</i></b>.  The decision on your appeal will come from your Quality Improvement Organization (QIO).
                                                        </p>
                                                        <br />
                                                    </div>
                                                    <div class="pSpacing ArialFontStyle-12FontSize" id="" style="width: 93%">
                                                        <p class=''>
                                                            <b>We have reviewed your case and decided that Medicare coverage of your current {{ServiceForNomncletter||""}} services should end.</b>
                                                        </p>
                                                        <ul>
                                                            <li style="margin-left:-20px">
                                                                <p><b>The facts used to make this decision:</b></p>
                                                                <p style="min-height: 50px;" class="wrap-words"><b>{{DencFacts}}</b></p>
                                                            </li>
                                                            <li style="margin-left:-20px">
                                                                <p><b>Detailed explanation of why your current services are no longer covered, and the specific Medicare coverage rules and policy used to make this decision:</b></p>
                                                                <p style="min-height: 50px;" class="wrap-words"><b>{{DencExp}}</b></p>
                                                            </li>
                                                            <li style="margin-left:-20px">
                                                                <p><b>Plan policy, provision, or rationale used in making the decision (health plans only):</b></p>
                                                                <p style="min-height: 50px; " class="wrap-words"><b>{{DencPolicies}}</b></p>
                                                            </li>
                                                        </ul><br /><br />
                                                    </div>
                                                    <div class="pSpacing ArialFontStyle-12FontSize" id="" style="width: 93%">
                                                        <p class=''>
                                                            If you would like a copy of the policy or coverage guidelines used to make this decision, or a copy of the documents sent to the QIO, please call us at: <b>Ultimate Health Plans 1-855-858-3567</b>
                                                        </p><br /><br />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer" style="border-top:0px;">
                                        <footer style="font-family:Arial;font-size:12px">
                                            <p style='float:left;' id="footertoleft">Form CMS-10124-DENC (Approved 12/31/2011)</p>
                                            <p style='float:right;' id="footertoright">OMB Approval No. 0938-0953</p>
                                        </footer>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>




            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-danger pull-left" data-dismiss="modal">Cancel</button>
                <button class="btn btn-sm btn-success pull-right" data-dismiss="modal" id="trigger-overlay" onclick="toggleQueueList();">Confirm</button>
            </div>
        </form>
    </div>
</div>
