<link href="~/Content/AppCss/Search/searchOverlay.css" rel="stylesheet" />
<div class="searchResult" hidden>
    <div class="search-content">
        <div class="x_title-custom theme_thead">
            <p class="margin_left_15px"><i class="fa fa-search"></i> SEARCH RESULT</p>

            <ul class="nav navbar-right panel_toolbox cursor-pointer" id="close">
                <li>
                    <span>Press Esc or <span class="btn custom-cls-btn"><span class="custombadge outertabclose-red custom-cls"><i class="fa fa-times fa-2x"></i></span></span>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        @*<div class="overlay-content" id="serch-default-message">
                <h1>Enter Search Keyword</h1>
            </div>*@
        <div class="overlay-content col-lg-12" id="search-results">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right font-size-18">
                You searched for <b>&nbsp;"<span class="search-word"></span>"</b>
                &nbsp;&nbsp;|&nbsp;&nbsp;<span class="result-message"><b>35</b> results found in <b>Member</b></span>
                &nbsp;&nbsp;|
                <span>More Results Found in : </span>
                <span class="more-result">UM (10)</span>
                <span class="more-result">Claims (10)</span>
                <span class="more-result">Census (10)</span>
                <span class="more-result">Others (5)</span>


                @*<a href="javascript:void(0)" class="custom-button text-cyan pull-right" data-toggle="tooltip" data-placement="top" title="" data-original-title="Email">

                        <img src="~/Resources/Images/Icons/Mail1.ico" />
                    </a>

                    <a href="javascript:void(0)" class="custom-button text-red pull-right" data-toggle="tooltip" data-placement="top" title="" data-original-title="Pdf">
                        <img src="~/Resources/Images/Icons/pdf1.png" />
                    </a>


                    <a href="javascript:void(0)" class="custom-button text-success pull-right" data-toggle="tooltip" data-placement="top" title="" data-original-title="Excel">
                        <img src="~/Resources/Images/Icons/excel.PNG" />
                    </a>*@
              
               @Html.ExportOptions()
            </div>
            <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped" style="margin-bottom: 0px;" cellspacing="0">
                <thead class="theme_thead">
                    <tr>
                        <th class="withfilter colHead-1">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>MBR ID</label>
                                <i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-2">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>LAST NAME</label><i class="fa fa-sort"></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-3">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>FIRST NAME</label><i class="fa fa-sort"></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-4">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>DOB</label><i class="fa fa-sort"></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-5 gender">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>SEX</label><i class="fa fa-sort"></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-6 pcp">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>PCP</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-7">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>PCP PHONE</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-8">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>EFF DATE</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-9">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>ADDRESS</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-10">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>CITY</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-11">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>ST</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                        <th class="withfilter colHead-12">
                            <span class="styled-input">
                                <input class="filer-search" type="text" required /><label>ZIP</label><i class="fa fa-sort "></i>
                            </span>
                        </th>
                    </tr>
                </thead>

            </table>
            <div id="searchBodyDiv">
                <table class="table table-striped custom-table-striped  margin_bottom_zero margin_left_zero tableLayer  custom-thead-back custom-thead-font custom-tbody" cellspacing="0" cellpadding="0">
                    <tbody id="searhcResultBody"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="~/Resources/Data/SearchData.js"></script>
