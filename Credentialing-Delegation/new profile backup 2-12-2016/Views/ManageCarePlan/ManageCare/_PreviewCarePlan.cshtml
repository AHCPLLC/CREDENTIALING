<style>



    /*---------------------------------Tree Structure----------------------------------------*/
    .tree {
        min-height: 20px;
        /*padding:19px;*/
        margin-bottom: 20px;
        /*background-color:#fbfbfb;*/
        /*border-right:1px solid #999;*/
        /*-webkit-border-radius:4px;
    -moz-border-radius:4px;
    border-radius:4px;
    -webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);
    -moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);
    box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05)*/;
    }

    @@media (min-width: 1200px) {
        .marginleft {
            margin-left: -30px;
        }
    }

    .tree > ul {
        margin-left: -17px;
    }

   
    .tree li {
        list-style-type: none;
        margin: 0;
        padding: 10px 5px 0 5px;
        position: relative;
    }

        .tree li::before, .tree li::after {
            content: '';
            left: -20px;
            position: absolute;
            right: auto;
        }

        .tree li::before {
            border-left: 1px solid #999;
            bottom: 50px;
            height: 100%;
            top: 0;
            width: 1px;
        }

        .tree li::after {
            border-top: 1px solid #999;
            height: 20px;
            top: 25px;
            width: 25px;
        }


        .tree li span {
            -moz-border-radius: 5px;
            -webkit-border-radius: 5px;
            display: inline-block;
            padding: 3px 8px;
            text-decoration: none;
            cursor: pointer;
        }

        .tree li.parent_li > span {
            cursor: pointer;
        }

    .tree > ul > li::before, .tree > ul > li::after {
        border: 0;
    }

    .tree li:last-child::before {
        height: 30px;
    }

    /*.tree li.parent_li > span:hover, .tree li.parent_li > span:hover + ul li span {
    background: palegoldenrod;
    border: 1px solid #94a0b4;
    color: #000;
}*/
    /*---------------------------------End Tree Structure----------------------------------------*/







    .inline-tree-element {
        display: inline-flex !important;
    }

    .width-tree-element-problem {
        width: 85% !important;
    }

    .width-tree-element-intervention {
        width: 90% !important;
    }

    .width-tree-element-goal {
        width: 95% !important;
    }

    .width-tree-element-outcome {
        width: 100% !important;
    }

    .highlight-tree-elements {
        background: #eee;
        border: 1px solid #94a0b4;
        color: #000;
        /*display: inline-block;*/
        padding: 0px 8px;
    }

    /*.removeFloat {
        float: none !important;
    }*/

    .position-tree {
        /*padding-left: 20%;*/
        /*float: none;*/
    }

    .tree-span-accomidate {
        border: 1px solid grey;
        font-weight: bold;
        background-color: white;
        padding-top: 2px !important;
        padding-right: 5px !important;
        word-break: break-all;
    }

    .tree-span-button {
        height: 25%;
        padding-left: 2px;
        padding-right: 2px;
    }
</style>



@*<div class="removeFloat">*@
<div class="x_panel tree position-tree">
    <ul>
        <li>


            &nbsp;<span class="tree-span-accomidate" style="padding-right: 20px !important; padding-left: 20px !important; background-color: gold">CARE MANAGEMENT CATEGORY : <span ng-if="Template.selectedCat">{{Template.selectedCat}}</span><span ng-if="!Template.selectedCat">CM</span>{{Template.viewTemplateIterations.Category}} &nbsp;&nbsp;&nbsp; PROGRAM NAME : <span ng-if="Template.selectedProg">{{Template.selectedProg}}</span><span ng-if="!Template.selectedProg">CCM</span>{{Template.viewTemplateIterations.Program}}</span>


            <ul @*ng-show="conditionVisibility.ConditionVisibility"*@>
                <li ng-repeat="p in previewTemplateIterations.Level1 track by $index">
                    <span class="inline-tree-element width-tree-element-problem">
                        <button class="btn btn-xs btn-primary tree-span-button" ng-show="previewTemplateIterations.Level1[$index].Level2.length > 0" ng-click="previewTemplateIterations.Level1[$index].Visibility = !previewTemplateIterations.Level1[$index].Visibility">
                            <i ng-class="{'fa fa-chevron-down': !previewTemplateIterations.Level1[$index].Visibility , 'fa fa-chevron-up': previewTemplateIterations.Level1[$index].Visibility }" @*ng-click="expandCollapseFeature(null,null,null,$index)"*@></i>
                        </button>
                        &nbsp;<span class="tree-span-accomidate"><span ng-if="previewTemplateIterations.Level1[$index].PriorityFlagID==1" class="label label-success ng-scope">STG</span><span ng-if="previewTemplateIterations.Level1[$index].PriorityFlagID==2" class="label label-success ng-scope">LTG</span> {{previewTemplateIterations.Level1[$index].Description}}</span>

                    </span>
                    <ul ng-show="previewTemplateIterations.Level1[$index].Visibility">
                        <li ng-repeat="i in previewTemplateIterations.Level1[$index].Level2  track by $index">
                            <span class="inline-tree-element width-tree-element-intervention">
                                <button class="btn btn-xs btn-primary tree-span-button" ng-show="previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3.length > 0" ng-click="previewTemplateIterations.Level1[$parent.$index].Level2[$index].Visibility = !previewTemplateIterations.Level1[$parent.$index].Level2[$index].Visibility">
                                    <i ng-class="{'fa fa-chevron-down': !previewTemplateIterations.Level1[$parent.$index].Level2[$index].Visibility ,'fa fa-chevron-up': previewTemplateIterations.Level1[$parent.$index].Level2[$index].Visibility}" ng-click="expandCollapseFeature(null,null,$index,$parent.$index);"></i>
                                </button>
                                &nbsp;<span class="tree-span-accomidate"><span ng-if="previewTemplateIterations.Level1[$parent.$index].Level2[$index].PriorityFlagID==1" class="label label-success ng-scope">STG</span><span ng-if="previewTemplateIterations.Level1[$parent.$index].Level2[$index].PriorityFlagID==2" class="label label-success ng-scope">LTG</span> {{previewTemplateIterations.Level1[$parent.$index].Level2[$index].Description}}</span>

                            </span>
                            <ul ng-show="previewTemplateIterations.Level1[$parent.$index].Level2[$index].Visibility">
                                <li ng-repeat="g in previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3 track by $index">
                                    <span class="inline-tree-element width-tree-element-goal">
                                        <button class="btn btn-xs btn-primary tree-span-button" ng-show="previewTemplateIterations.Level1[$parent.$parent.$index].Level2[$parent.$index].Level3[$index].Level4.length > 0" ng-click="previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3[$index].Visibility = !previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3[$index].Visibility">
                                            <i ng-class="{'fa fa-chevron-down': !previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3[$index].Visibility, 'fa fa-chevron-up' : previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3[$index].Visibility}" ng-click="expandCollapseFeature(null,$index,$parent.$index,$parent.$parent.$index);"></i>
                                        </button>
                                        &nbsp;<span class="tree-span-accomidate"><span ng-if="previewTemplateIterations.Level1[$parent.$parent.$index].Level2[$parent.$index].Level3[$index].PriorityFlagID==1" class="label label-success ng-scope">STG</span><span ng-if="previewTemplateIterations.Level1[$parent.$parent.$index].Level2[$parent.$index].Level3[$index].PriorityFlagID==2" class="label label-success ng-scope">LTG</span> {{previewTemplateIterations.Level1[$parent.$parent.$index].Level2[$parent.$index].Level3[$index].Description}}</span>

                                    </span>
                                    <ul ng-show="previewTemplateIterations.Level1[$parent.$index].Level2[$index].Level3[$index].Visibility">
                                        <li ng-repeat="o in previewTemplateIterations.Level1[$parent.$parent.$index].Level2[$parent.$index].Level3[$index].Level4 track by $index">
                                            <span class="inline-tree-element width-tree-element-outcome">
                                                &nbsp;<span class="tree-span-accomidate">{{previewTemplateIterations.Level1[$parent.$parent.$parent.$index].Level2[$parent.$parent.$index].Level3[$parent.$index].Level4[$index].Description}}</span>

                                            </span>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                    </ul>

                </li>
            </ul>
        </li>

    </ul>
</div>
@*</div>*@


