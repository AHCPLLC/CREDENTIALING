@model List<PortalTemplate.Models.Claims_Dashboard.FilterHistory>

@foreach (var item in Model)
{
    <li class="clearfix">
        <div class="left_desc" id="@item.DivId" draggable="true" ondragstart="dragStart(event)">
            <i class="fa fa-ellipsis-v"></i>
        </div>
        <div class="right_desc">
            <label>@item.FilterName</label>

            <a class="btn btn-xs btn-success pull-right" onclick="ApplyFilteredHistory('@item.ID')">Apply</a>
            <a class="btn btn-xs btn-warning pull-right" onclick="ViewFilteredHistory('@item.ID')">View</a>


            @*@foreach (var ele in item.Elements)
                {
                    <span class="badge badge-success filter_text_badge">@ele.By:</span>
                    <b>
                        @foreach (var e in ele.On)
                        {
                            @e<span>,</span>
                        }
                    </b>
                }*@

        </div>
    </li>

}
