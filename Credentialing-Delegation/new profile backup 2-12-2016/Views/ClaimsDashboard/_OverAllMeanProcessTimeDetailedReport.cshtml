<div class="detailed_view">

    <div class="clearfix">
        <h4 class="margin_left_10px pull-left">
            OVERALL MEAN PROCESS TIME FOR YEAR -
            <select class="financial_year_dropdown">
                <option>2013</option>
                <option>2014</option>
                <option>2015</option>
                <option selected>2016</option>
            </select>
        </h4>
        <a class="btn pull-right btn-sm zeroPaddingTop margin_top_5" id="Close_Detailed_Report" onclick="CloseFullScreen('OverAllMeanProcessTimeId')" data-toggle="tooltip" data-placement="bottom" title="Close">
            <i class="fa fa-2x fa-close"></i>
        </a>

        @Html.ExportOptions("print","pdf")

        <a href="javascript:void(0)" class="export-button  pull-right" data-toggle="tooltip" data-placement="bottom" title="Save as Image">
            <img src="~/Resources/Images/Icons/image%20download%20new.png" />
        </a>
        <a href="javascript:void(0)" class="export-button  pull-right" id="master_filter_overall_filter" data-toggle="tooltip" data-placement="bottom" title="Filter">
            <img src="~/Resources/Images/Icons/filter10.png" />
        </a>
    </div>

        @Html.Partial("~/Views/ClaimsDashboard/_FilterForCharts.cshtml")

    <div class="row">
        <div class="col-lg-12">
            @*<div class="col-lg-1 width_3"><a class="btn make_center btn-primary btn-sm" id="showPrevious"><i class="fa fa-angle-double-left"></i></a></div>*@

            <div class="col-lg-1 width_3 left_chev" id="showPrevious"><a class="make_center"><i class="fa fa-chevron-left"></i></a></div>
            <div class="col-lg-10 width_94">
                <div id="OverAllMeanProcessingTimeDetailedGraph"></div>
                <div class="table-responsive fix_height margin_left_fix">
                    <table class="table table-striped custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                        <thead class="table_hearder_DR">
                            <tr>
                                <th rowspan="2">Payer ID</th>
                                <th rowspan="2" style="width:300px">Plan Name</th>
                                <th rowspan="1" colspan="5" class="text-center">Overall Mean Process Time(in days)</th>
                            </tr>
                            <tr>
                                <th>Initiated  </th>
                                <th>Accepted by CH  </th>
                                <th>Accepted by Payer </th>
                                <th>Payment Received </th>
                                <th>Overall</th>
                            </tr>
                        </thead>
                        <tbody id="overAllMeanTimeInDetail"></tbody>
                    </table>
                </div>
            </div>
            <div class="col-lg-1 width_3 right_chev" id="showNext"><a class="make_center"><i class="fa fa-chevron-right"></i></a></div>
            @*<div class="col-lg-1 width_3"><a class="btn make_center btn-primary btn-sm" id="showNext"><i class="fa fa-angle-double-right"></i></a></div>*@
        </div>
    </div>
    <div class="">
  
    </div>


</div>
<script src="~/Scripts/AppScripts/ClaimsDashboard/DetailedGraphs/OverAllMeanProcessTIme.js"></script>