<div class="detailed_view">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="clearfix">
                <h4 class="pull-left margin_left_10px">
                    MOST COMMON DISEASE FOR YEAR -
                    <select class="financial_year_dropdown">
                        <option>2013</option>
                        <option>2014</option>
                        <option>2015</option>
                        <option selected>2016</option>
                    </select>
                </h4>
                <a class="btn pull-right  btn-sm zeroPaddingTop margin_top_5" id="Close_Detailed_Report" onclick="CloseFullScreen('MostCommonDisease')"><i class="fa fa-2x fa-close"></i></a>
                @Html.ExportOptions("print", "pdf")


                <a href="javascript:void(0)" class="export-button  pull-right" data-toggle="tooltip" data-placement="bottom" title="Save as Image">
                    <img src="~/Resources/Images/Icons/image%20download%20new.png" />
                </a>
                <a href="javascript:void(0)" class="export-button  pull-right" id="master_filter_overall_filter" data-toggle="tooltip" data-placement="bottom" title="Filter">
                    <img src="~/Resources/Images/Icons/filter10.png" />
                </a>
            </div>
            <!--Filter-->
            @Html.Partial("~/Views/ClaimsDashboard/_FilterForCharts.cshtml")

            <div class="clearfix"></div>
            <div class="col-lg-1 width_3 left_chev" id="showPrevious"><a class="make_center"><i class="fa fa-chevron-left"></i></a></div>
            <div class="col-lg-10 width_94">
                <div id="OverAllMeanProcessingTimeDetailedGraph"></div>
                <div class="table-responsive fix_height margin_left_fix">
                    <table class="table table-striped custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                        <thead class="table_hearder_DR">
                            <tr>
                                <th>Disease Code</th>
                                <th>Disease Description</th>
                                <th>Disease Count</th>
                                <th>Disease Percentage</th>
                                <th>Provider Count</th>
                                <th>Patient Count</th>
                            </tr>
                        </thead>
                        <tbody id="MostCommonDiseasesDRTable"></tbody>
                    </table>
                </div>
            </div>
            <div class="col-lg-1 width_3 right_chev" id="showNext"><a class="make_center"><i class="fa fa-chevron-right"></i></a></div>
        </div>
    </div>
</div>
<script src="~/Scripts/AppScripts/ClaimsDashboard/DetailedGraphs/MostCommonDiseasesDR.js"></script>