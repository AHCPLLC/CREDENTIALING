@model List<PortalTemplate.Areas.Auditing.Models.AuditingList.InactiveQueueViewModel>


<div class="x_panel border_on_panel">
    <div class="x_content">

        <div class="col-lg-12">
            <label class="title_text">Inactive Queue</label>
            <div class="pull-right">
                @Html.ExportOptions("print", "pdf", "excel")
            </div>
        </div>
        <div class="col-lg-12 table-responsive zero-padding-left">
            <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped p_body_data_background" style="margin-bottom: 0px;" cellspacing="0">
                <thead class="theme_thead">
                    <tr>
                        <th>
                            <label>Enctr Id</label>
                        </th>
                        <th>
                            <label>Mbr Id</label>
                        </th>
                        <th>
                            <label>Mbr LName</label>
                        </th>
                        <th>
                            <label>Mbr FName</label>
                        </th>
                        <th>
                            <label>Prv NPI</label>
                        </th>
                        <th>
                            <label>Prv LName</label>
                        </th>
                        <th>
                            <label>Prv FName</label>
                        </th>
                        <th>
                            <label>Facility</label>
                        </th>
                        <th>
                            <label>DOS</label>
                        </th>
                        <th>
                            <label>DOC</label>
                        </th>
                        <th>
                            <label>Created By</label>
                        </th>
                        <th>
                            <label>Enctr Type</label>
                        </th>
                        <th>
                            <label>Status</label>
                        </th>
                        <th>
                            <label>Action</label>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.EncounterId</td>
                                <td>@item.MemberId</td>
                                <td>@item.MemberLastName</td>
                                <td>@item.MemberFirstName</td>
                                <td>@item.ProviderNPI</td>
                                <td>@item.ProviderLastName</td>
                                <td>@item.ProviderFirstName</td>
                                <td>@item.Facility</td>
                                <td>@item.DOS.Value.ToShortDateString()</td>
                                <td>@item.DOC.Value.ToShortDateString()</td>
                                <td>@item.CreatedBy</td>
                                <td>@item.EncounterType</td>
                                <td>@item.Status</td>
                                <td>
                                    <a data-toggle="tooltip" data-placement="top" title="" class="p_view_btn btn btn-xs tab-navigation member-actions" data-tab-val="ViewEncounter" data-tab-action="View Encounter" data-tab-title="View Audit #@item.EncounterId " data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="/Auditing/Auditing/ViewAuditing" data-original-title="View Auditing" aria-describedby="tooltip510268"><i class="fa fa-eye"></i></a>
                                    @*<a data-toggle="tooltip" title="" class="auditedit p_edit_btn btn btn-xs tab-navigation member-actions" data-tab-val="Audit Encounter" data-tab-action="Audit Encounter" data-tab-title="Audit #@item.EncounterId " data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="/Auditing/Auditing/EditAuditing" data-original-title="Audit"><i class="fa fa-edit"></i></a>*@
                                    @*<a data-toggle="tooltip" title="" class="auditcancel p_delete_btn btn btn-xs" onclick="showCancelModal(116244951 )" data-original-title="Cancel Auditing"><i class="fa fa-close"></i></a>*@
                                    <a class="btn btn-xs tab-navigation member-actions" data-toggle="tooltip" title="Re-Activate"><i class="fa fa-undo"></i></a>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
