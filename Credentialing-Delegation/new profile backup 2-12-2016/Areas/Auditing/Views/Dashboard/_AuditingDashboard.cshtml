<p-link filepath="~/Areas/Auditing/Contents/CSS/AppCss/Dashboard/NonMinified/_AuditingDashboard.css" rel="stylesheet"></p-link>
<div class="row" id="AuditingDashboard">
    <div class="row dashboardTopBar">
        <div class="pull-right">
            <a class="btn btn-xs btn-info pull-right" id="filterData">
                <i class="fa fa-filter"> Filter</i>
            </a>
            <a class="btn btn-xs btn-primary  tab-navigation member-actions" data-toggle="tooltip" title="View Auditing List" data-tab-val="Auditing List" data-tab-action="Auditing List" data-tab-title="Auditing List" data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="/Auditing/Auditing/GetAuditingList"><span class="fa fa-eye"></span> View Auditing List</a>
        </div>
    </div>

    <div class="row Auditing_dashboard_filter" id="showfilter">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <fieldset class="fieldsetStyle">
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label>Member</label>
                    <div>
                        <input list="members" class="form-control input-xs non_mandatory_field_halo">
                        <datalist id="members">
                            <option value="Jill Crawley">
                            <option value="John Satterfield">
                            <option value="Smith Lucas">
                        </datalist>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label>PCP</label>
                    <div>
                        <input list="pcp" class="form-control input-xs non_mandatory_field_halo">
                        <datalist id="pcp">
                            <option value="Pariksith Singh">
                            <option value="Nishat Seema">
                            <option value="Ali Syed">
                        </datalist>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                    <label>Facility</label>
                    <div>
                        <input list="facility" class="form-control input-xs non_mandatory_field_halo">
                        <datalist id="facility">
                            <option value="1094 North Cliffe Dlve">
                            <option value="10441 Quality Dr">
                            <option value="10494 Internal Med">
                            <option value="10045 Cortez Blvd">
                            <option value="13235 State Road Blvd">
                            <option value="14690 Spring Hill Dr">
                        </datalist>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label>DOS  From</label>
                    <div>
                        <input type="text" placeholder="MM/DD/YYYY" class="input-xs non_mandatory_field_halo">
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label>DOS  To</label>
                    <div>
                        <input type="text" placeholder="MM/DD/YYYY" class="input-xs non_mandatory_field_halo">
                    </div>
                </div>
            </fieldset>
        </div>
    </div>

    <div class="row P_zero-padding-left-right" style="margin-top:10px;">
        <div class="col-lg-12 col-md-12 col-xs-12 ">
            <div class="col-lg-6 col-md-6 col-xs-6" style="padding-right:0px;">
                <div class="DivHeading pacCoordinatorsHeader zero-padding-left-right" style="text-transform: capitalize; margin-bottom: 6px;">
                    Encounter Accuracy
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_ngreen">
                        <div class="biscuit_heading auth">
                            <b>Accuracy</b>
                        </div>
                        <div class="biscuit_body">
                            @*<div class="row">
                                <div class="col-lg-6">
                                    86.9%
                                    </div>
                                <div class="col-lg-6">
                                    220
                                </div>
                                </div>*@
                            <b>86.9%</b>
                        </div>
                    </button>
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_yellow">
                        <div class="biscuit_heading auth">
                            <b>UnderCoding</b>
                        </div>
                        <div class="biscuit_body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <b># 110</b>
                                </div>
                                <div class="col-lg-6">
                                    <b> $ 65.8K</b>
                                </div>
                            </div>
                        </div>
                    </button>
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_red">
                        <div class="biscuit_heading auth">
                            <b>OverCoding</b>
                        </div>
                        <div class="biscuit_body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <b># 70</b>
                                </div>
                                <div class="col-lg-6">
                                    <b>$ 79.8K</b>
                                </div>
                            </div>
                        </div>
                    </button>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-xs-6" style="padding-left:0px;">
                <div class="DivHeading pacCoordinatorsHeader zero-padding-left-right" style="text-transform: capitalize; margin-bottom: 6px;">
                    Encounters Count
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_ngreen">
                        <div class="biscuit_heading auth">
                            <b>Open</b>
                        </div>
                        <div class="biscuit_body">
                            <b>40</b>
                        </div>
                    </button>
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_orange">
                        <div class="biscuit_heading auth">
                            <b>Rejected</b>
                        </div>
                        <div class="biscuit_body">
                            <b>12</b>
                        </div>
                    </button>
                </div>
                <div class="col-lg-4 zero-padding-left-right">
                    <button class="biscuit p_biscit_outerColor_rblue">
                        <div class="biscuit_heading auth">
                            <b>Ready to Bill</b>
                        </div>
                        <div class="biscuit_body">
                            <b>80</b>
                        </div>
                    </button>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="col-lg-6 col-md-6 col-xs-6" style="padding-right:0px;">
                <div class="p_panel" id="ReadytoCodeid" style="border-top: 2px solid rgba(50,165,151,1) !important">
                    <div class="small_view">
                        <div class="p_panel_header" style="text-align:center">
                            <label class="labelheader" style="color: rgb(67, 102, 160); padding-left: 0px;">Overall Accuracy Report</label>
                        </div>
                        <div class="p_panel_body">
                            <div id="Auditing_organisationreport"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-xs-6" style="padding-right:0px;">
                <div class="p_panel" id="ReadytoCodeid" style="border-top: 2px solid rgba(50,165,151,1) !important">
                    <div class="small_view">
                        <div class="p_panel_header" style="text-align:center">
                            <label class="labelheader" style="color: rgb(67, 102, 160);padding-left:0px;">Provider Accuracy Report</label>
                        </div>
                        <div class="p_panel_body" style="height:320px;overflow-x:scroll">
                            <table class="table table-striped table-hover table-bordered custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                                <thead class="theme_thead">
                                    <tr>
                                        <th>Provider Name</th>
                                        <th>Accuracy</th>
                                    </tr>
                                </thead>
                                <tbody id="Auditing_reasonsforRejection"></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<p-script filepath="~/Areas/Auditing/Scripts/AppJs/Dashboard/NonMinified/_AuditingDashboard.js"></p-script>
