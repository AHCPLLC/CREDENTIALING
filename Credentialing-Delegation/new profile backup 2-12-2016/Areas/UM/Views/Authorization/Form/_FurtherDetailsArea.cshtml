@model PortalTemplate.Areas.UM.Models.ViewModels.Authorization.AuthorizationViewModel
@{
    List<SelectListItem> UMSvcGroupList = new List<SelectListItem>()
    {
    };

    List<SelectListItem> RoomTypeList = new List<SelectListItem>()
    {
    };

    List<SelectListItem> LevelOfCareList = new List<SelectListItem>()
    {
    };


}
<div class="x-panel border_on_panel zero-padding-left-right" style="margin-top: 4px;">
    <div class="x_content">

        @*<div class="inline_block margin_right_1">
                @Html.Label("TOS")<br />
                @Html.TextBoxFor(e => e.TypeOfService, new { @class = "form-control input-xs", @readonly = "readonly" })
            </div>*@
        <div class="inline_block margin_right_1">
            <nobr><span class="theme_label">@Html.Label("UM SVC GRP")</span></nobr>
            @Html.DropDownListFor(e => e.UMServiceGroup, UMSvcGroupList, new { @class = " form-control input-xs non_mandatory_field_halo variable_width_select", @tabindex = "-1" })
            <a class="label label-success pull-right in_label" style="visibility: hidden;">Oops!</a>
        </div>
        <div class="inline_block margin_right_1">
            <span class="theme_label">@Html.Label("LEVEL OF CARE")</span>
            @Html.DropDownListFor(e => e.LevelOfCare, LevelOfCareList, new { @class = "form-control input-xs mandatory_field_halo variable_width_select", @tabindex = "-1" })
        </div>
        <div class="inline_block margin_right_1">
            <span class="theme_label">@Html.Label("EXPECTED CHARGES $")</span>
            @Html.TextBoxFor(e => e.ExpectedCharge, new { @class = "form-control input-xs text-uppercase non_mandatory_field_halo expected_charges_field", @placeholder = "Expected charges" })
        </div>
        <div class="inline_block margin_right_1">
            <span class="theme_label">@Html.Label("EXPECTED DOS")</span>
            @Html.TextBoxFor(e => e.ExpectedDOS, new { @class = "form-control input-xs text-uppercase important_field_halo variable_width_date datePicker", @placeholder = "MM/DD/YYYY" })
            <a class="label label-success pull-right in_label" style="visibility: hidden;">Oops!</a>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
<div class="clearfix"></div>
