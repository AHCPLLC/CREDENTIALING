                                    <input type="text" class="form-control input-xs text-uppercase" />
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-5 col-xs-3 theme_label zero-padding-left-right">
                                    <input type="text" class="form-control input-xs text-uppercase" />
                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-5 col-xs-3 theme_label zero-padding-left-right">
                                    @Html.PratianUiRadioButtonFor(e => e.OutcomeType, "SUCCESSFUL", new { @checked = "checked", @id = "success", @Value = "SUCCESS" })
                                    @Html.PratianUiRadioButtonFor(e => e.OutcomeType, "UNSUCCESSFUL", new { @checked = "checked", @id = "unsuccess", @Value = "UNSUCCESS" })
                                </div>
                            </div>
                            <div class="x_content zero-padding-left-right">
