                                </div>
                                <div class="col-lg-4 col-md-4 col-sm-5 col-xs-3 theme_label zero-padding-left-right">
                                    @Html.PratianUiRadioButtonFor(e => e.OutcomeType, "Active", new { @checked = "checked", @id = "Active", @Value = "Active" })
                                    @Html.PratianUiRadioButtonFor(e => e.OutcomeType, "In Active", new { @checked = "checked", @id = "In Active", @Value = "In Active" })
                                </div>
                            </div>
