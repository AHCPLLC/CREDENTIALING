<form enctype="multipart/form-data" id="PreAuthRequestForm" class="zero-padding-left-right createForm form_11 auth_form" onsubmit="return false;">
    <div class="x_panel col-lg-12  col-md-12 col-sm-12 col-xs-12 zero-padding-left-right topPadding">
        <div class="x_content zero-padding-left-right">
            <div class="col-md-12 text-center theme_label">
                <img src="/Resources/Images/Common/ahc_logo.png" class="pull-left" style=" width: 140px; height: 68px;">
                <h3>
                    <b style="">PRIOR AUTHORIZATION</b>
                </h3>
            </div>
            <div class="row col-lg-12">
                <!--Authorization Type-->
                <div class="x_panel border_on_panel zero-padding-left-right">
                    <div class="x_content zero-padding-left-right">
                        <div class="col-lg-5">
                            <span class="col-lg-5 zero-padding-left-right theme_label">
                                <b>@Html.LabelFor(e=>e.AuthorizationType)</b>
                            </span>
                            <div class="col-lg-4 ">
                                <input type="radio" name="RequestType" id="standardId" value="Standard" class="checkbox-radio form-control AuthTypeCheck" checked>
                                <label for="standardId" class="theme_label_data"><span></span>STANDARD</label>
                            </div>
                            <div class="col-lg-3 zero-padding-left-right">
                                <input type="radio" name="RequestType" id="expediteId" value="Expedited" class="checkbox-radio form-control AuthTypeCheck">
                                <label for="expediteId" class="theme_label_data"><span></span>EXPEDITED</label>
                            </div>
                        </div>
                        <div id="expediteMessageId" class="col-lg-7 zero-padding-left-right hidden">
                            <span class="ExpeditedMsg"> Select Expedited Only if the member's life, health or ability to regain maximum function is jeopardized.</span>
                        </div>
                    </div>
                </div>
                <!--Authorization Type Ends-->
                <!--Member Information-->
                <fieldset class="fsStyle">
                    <legend class="legendStyle">
                        <span class="theme_depend text-uppercase">
                            <span class="transparent-font-color">_____.</span>
                            MEMBER INFORMATION
                            <span class="transparent-font-color">_____.</span>
                        </span>
                    </legend>
                    <div class="x_panel zero-padding-left-right" id="PreAuthMemberArea">
                        <div class="x_content zero-padding-left-right">
                            <div class="col-lg-12">
                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e=>e.Member.MemberID)</span><br />
                                    @Html.TextBoxFor(e => e.Member.SubscriberID, new { @class = "form-control input-xs read_only_field", @placeholder = "MEMBER ID", @readonly = "readonly", @id = "preAuthMBRID" })
                                </div>
                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e=>e.Member.LastName)</span><br />
                                    @Html.TextBoxFor(e => e.Member.LastName, new { @class = "form-control input-xs read_only_field", @placeholder = "LAST NAME", @readonly = "readonly", @id = "preAuthMBRLastNameID" })
                                </div>

                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e => e.Member.FirstName)</span><br />
                                    @Html.TextBoxFor(e => e.Member.FirstName, new { @class = "form-control input-xs read_only_field", @placeholder = "FIRST NAME", @readonly = "readonly", @id = "preAuthMBRFirstNameID" })
                                </div>
                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e => e.ReceivedDate)</span><br />
                                    @Html.TextBoxFor(e => e.ReceivedDate,
                            new
                            {
                                @class = "form-control input-xs width-120 dateTimePicker",
                                placeholder = "MM/DD/YYYY hh:mm:ss",
                                @name="received",
                                Value = "",
                                id = "requestDateID"
                            })
                                </div>
                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e => e.Member.MemberContact.PrimaryContact)</span><br />
                                    @Html.TextBoxFor(e => e.Member.MemberContact.PrimaryContact, new { @class = "form-control input-xs read_only_field", @placeholder = "PHONE NUMBER", @readonly = "readonly", @id = "preAuthMBRPhoneNumberID" })
                                </div>
                                <div class="col-lg-2">
                                    <span class="theme_label">@Html.LabelFor(e => e.Member.DateOfBirth)</span><br />
                                    @Html.TextBoxFor(e => e.Member.DateOfBirth,
                                new
                                {
                                    @class = "form-control input-xs read_only_field datePicker",
                                    @name="DOB",
                                    @placeholder = "MM/DD/YYYY",
                                    @readonly = "readonly",
                                    @id = "preAuthMBRDOBID"
                                })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </fieldset>
                <!--Member Information Ends-->
                <!--Service Requested Starts-->
                <fieldset class="fsStyle">
                    <legend class="legendStyle">
                        <span class="theme_depend text-uppercase">
                            <span class="transparent-font-color">_______</span>
                            SERVICE REQUESTED
                            <span class="transparent-font-color">_______</span>
                        </span>
                    </legend>
                    <div class="x_panel zero-padding-left-right">
                        <div class="x_content zero-padding-left-right" id="ServiceRequested">
                            <div class="col-lg-9">
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="InOffice"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="11(a)-Office" checked>
                                    <label class="theme_label_data" for="InOffice"><span class="theme_label "></span>IN-OFFICE</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="FreeStandingFacility"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="24-Ambulatory Surgical Center">
                                    <label class="theme_label_data" for="FreeStandingFacility"><span class="theme_label "></span>FREE STANDING FACILITY</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="Dialysis"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="65-End-Stage Renal Disease Treatment Facility">
                                    <label class="theme_label_data" for="Dialysis"><span></span>DIALYSIS</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="Observation"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="22-OutPatient Hospital">
                                    <label class="theme_label_data" for="Observation"><span></span>OBSERVATION</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="RehabFacility"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="11(b)-OP Rehab">
                                    <label class="theme_label_data" for="RehabFacility"><span></span>REHAB FACILITY</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="Hospice"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="34-Hospice">
                                    <label class="theme_label_data" for="Hospice"><span></span>HOSPICE</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="InpatientHospital"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="21-InPatient Hospital">
                                    <label class="theme_label_data" for="InpatientHospital"><span></span>INPATIENT HOSPITAL</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="SNF"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="31-Skilled Nursing Facility" />
                                    <label class="theme_label_data" for="SNF"><span></span>SNF</label>
                                </div>
                                <div class="col-lg-4" ng-hide="true">
                                    <input type="radio"
                                           id="DME"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="12-Patient Home" />
                                    <label class="theme_label_data" for="DME"><span></span>DME</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="OutPatientHospital"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="22-OutPatient Hospital" />
                                    <label class="theme_label_data" for="OutPatientHospital"><span></span>OUTPATIENT HOSPITAL</label>
                                </div>
                                <div class="col-lg-4">
                                    <input type="radio"
                                           id="HomeHealth"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="12-Patient Home" />
                                    <label class="theme_label_data" for="HomeHealth"><span></span>HOME HEALTH</label>
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="col-lg-12">
                                    <input type="radio"
                                           id="Therapy"
                                           class="checkbox-radio servicereqCheck form-control"
                                           name="PlaceOfService"
                                           value="11(b)-OP Rehab" />
                                    <label class="theme_label_data" for="Therapy"><span></span>THERAPY</label>
                                </div>
                                <div class="col-lg-12 therapychecks hidden">
                                    <div class="col-lg-12">
                                        <input type="checkbox"
                                               id="Physical"
                                               class="checkbox-radio servicereqCheck form-control"
                                               value="PHYSICAL"
                                               name="PHYSICAL" />
                                        <label class="theme_label_data" for="Physical"><span></span>PHYSICAL</label>
                                    </div>
                                    <div class="col-lg-12">
                                        <input type="checkbox"
                                               id="Occupational"
                                               class="checkbox-radio servicereqCheck form-control"
                                               value="OCCUPATIONAL"
                                               name="OCCUPATIONAL" />
                                        <label class="theme_label_data" for="Occupational"><span></span>OCCUPATIONAL</label>
                                    </div>
                                    <div class="col-lg-12">
                                        <input type="checkbox"
                                               id="Speech"
                                               class="checkbox-radio servicereqCheck form-control"
                                               value="SPEECH"
                                               name="SPEECH" />
                                        <label class="theme_label_data" for="Speech"><span></span>SPEECH</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </fieldset>
                <!--Service Requested Ends-->
                <!--REQUESTING PROVIDER-->
                <fieldset class="fsStyle">
                    <legend class="legendStyle">
                        <span class="theme_depend text-uppercase">
                            <span class="transparent-font-color">_____.</span>
                            REQUESTING PROVIDER
                            <span class="transparent-font-color">_____.</span>
                        </span>
                    </legend>
                    <div class="x_panel zero-padding-left-right" id="PreAuthReqProvider">
                        <div class="col-lg-12">
                            <div class="col-lg-2">
                                <span>
                                    <span class="theme_label">@Html.LabelFor(e=>e.RequestingProvider.FirstName)</span>
                                    @*<span class="label label-success">IN</span>*@
                                </span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.FirstName, new { @class = "form-control input-xs read_only_field", @placeholder = "FIRST NAME", @id = "ReqProviderFirstName" })
                            </div>
                            <div class="col-lg-2">
                                <span><span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.LastName)</span></span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.LastName, new { @class = "form-control input-xs read_only_field", @placeholder = "LAST NAME", @id = "ReqProviderLastName" })
                            </div>

                            <div class="col-lg-2">
                                <span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.TaxID)</span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.TaxID, new { @class = "form-control input-xs", @placeholder = "TAX ID", @id = "ReqProviderTaxID" })
                            </div>
                            <div class="col-lg-2">
                                <span>
                                    <span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.NPI)</span>
                                </span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.NPI, new { @class = "form-control input-xs read_only_field", @placeholder = "NPI", @id = "ReqProviderNPI" })
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <span class="col-lg-12">
                                    <span class="theme_label">@Html.LabelFor(e => e.IsPCP)</span>
                                </span>
                                <div class="col-lg-4 zero-padding-left-right">
                                    @*<input type="radio"
                                           id="Pcp"
                                           class="checkbox-radio form-control IsPcpType"
                                           name="IsPCP"
                                           value="" checked="">
                                    <label class="theme_label_data"><span></span>PCP</label>*@
                                    <input type="radio" name="IsPCP" id="Pcp" value="" class="checkbox-radio form-control IsPcpType" checked>
                                    <label for="Pcp" class="theme_label_data"><span></span>PCP</label>
                                </div>
                                <div class="col-lg-6 zero-padding-left-right">
                                    <input type="radio" name="IsPCP" id="Specialist" value="" class="checkbox-radio form-control IsPcpType">
                                    <label for="Specialist" class="theme_label_data"><span></span>Specialist</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.ContactName)</span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.ContactName, new { @class = "form-control input-xs", @placeholder = "CONTACT NAME", @id = "ReqProviderContactName" })
                            </div>
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.PhoneNumber)</span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.PhoneNumber, new { @class = "form-control input-xs", @placeholder = "PHONE NUMBER", @id = "ReqProviderPhoneNumber" })
                            </div>
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.RequestingProvider.FaxNumber)</span><br>
                                @Html.TextBoxFor(e => e.RequestingProvider.FaxNumber, new { @class = "form-control input-xs", @placeholder = "FAX NUMBER", @id = "ReqProviderFaxNumber" })
                            </div>
                        </div>
                        <div class="col-md-6 " id="showSpecialistOptions" style="display:none">
                            <span class="col-lg-12">
                                <span class="theme_label"><label>HAS PCP APPROVED THIS REQUEST?</label></span>
                            </span>
                            <div class="col-lg-3">
                                <input type="radio" class="checkbox-radio form-control" id="isPCPApproved" name="approvedPCP" value="true" checked><label><span></span>YES</label>
                            </div>
                            <div class="col-lg-3">
                               <input type="radio" class="checkbox-radio form-control" id="isNotPCPApproved" name="approvedPCP" value="false"><label><span></span>NO</label>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </fieldset>
                <!--REQUESTING PROVIDER Ends-->
                <!--REFERRED TO/SERVICING PROVIDER-->
                <fieldset class="fsStyle">
                    <legend class="legendStyle">
                        <span class="theme_depend text-uppercase">REFERRED TO/SERVICING PROVIDER</span>
                    </legend>
                    <div class="x_panel zero-padding-left-right" id="PreAuthSvcProvider">
                        <div class="col-lg-12">
                            <div class="col-lg-2">
                                <span>
                                    <span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.FirstName)</span>
                                  </span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.FirstName, new { @class = "form-control input-xs", @placeholder = "FIRST NAME", @id = "ServicingProviderFirstName" })
                            </div>
                            <div class="col-lg-2">
                                <span><span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.LastName)</span></span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.LastName, new { @class = "form-control input-xs", @placeholder = "LAST NAME", @id = "ServicingProviderLastName" })
                            </div>
                            <div class="col-lg-2">
                                <span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.TaxID)</span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.TaxID, new { @class = "form-control input-xs", @placeholder = "TAX ID", @id = "ServicingProviderTaxID" })
                            </div>
                            <div class="col-lg-2">
                                <span><span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.NPI)</span></span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.NPI, new { @class = "form-control input-xs", @placeholder = "NPI", @id = "ServicingProviderNPI" })
                            </div>
                            <div class="col-lg-4 col-md-4">
                                <span><span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.Specialty)</span></span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.Specialty, new { @class = "form-control input-xs", @placeholder = "SPECIALITY", @id = "ServicingProviderSpeciality" })
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.ContactName)</span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.ContactName, new { @class = "form-control input-xs", @placeholder = "CONTACT NAME", @id = "ServicingProviderContactName" })
                            </div>
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.PhoneNumber)</span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.PhoneNumber, new { @class = "form-control input-xs", @placeholder = "PHONE NUMBER", @id = "ServicingProviderPhoneNumber" })
                            </div>
                            <div class="col-lg-4">
                                <span class="theme_label">@Html.LabelFor(e => e.ServicingProvider.FaxNumber)</span><br>
                                @Html.TextBoxFor(e => e.ServicingProvider.FaxNumber, new { @class = "form-control input-xs", @placeholder = "FAX NUMBER", @id = "ServicingProviderFaxNumber" })
                            </div>
                        </div>
                        <div class="col-md-6 " id="showSpecialistOptions" style="display:none">
                            <span class="col-lg-12"><span class="theme_label"><label>HAS PCP APPROVED THIS REQUEST?</label></span></span>
                            <div class="col-lg-3">
                                <label><input type="radio" class="checkbox-radio form-control" id="isPCPApproved" name="approvedPCP" value="true" checked=""><span></span>YES</label>
                            </div>
                            <div class="col-lg-3">
                                <label><input type="radio" class="checkbox-radio form-control" id="isNotPCPApproved" name="approvedPCP" value="false"><span></span>NO</label>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </fieldset>
                <!--REFERRED TO/SERVICING PROVIDER Ends-->
                <!--FACILITY-->
                <fieldset class="fsStyle">
                    <legend class="legendStyle">
                        <span class="theme_depend text-uppercase">
                            <span class="transparent-font-color">_____________.</span>
                            FACILITY
                            <span class="transparent-font-color">_____________.</span>
                        </span>
                    </legend>
                    <div class="x_panel zero-padding-left-right" id="PreAuthFacility">
                        <div class="x_content zero-padding-left-right">
                            <div class="col-lg-12">
                                <div class="col-lg-4">
                                    <span class="theme_label">@Html.LabelFor(e => e.Facility.FullName)</span><br>
                                    @Html.TextBoxFor(e => e.Facility.FullName, new { @class = "form-control input-xs", @placeholder = "FACILITY NAME", @id = "FacilityFullName" })
