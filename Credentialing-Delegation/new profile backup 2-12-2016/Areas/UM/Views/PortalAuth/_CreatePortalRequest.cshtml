                                </div>
                                <div class="clearfix"></div>
                                <div class="col-lg-4">
                                    <span class="theme_label">@Html.LabelFor(e => e.Facility.ContactName)</span><br>
                                    @Html.TextBoxFor(e => e.Facility.ContactName, new { @class = "form-control input-xs", @placeholder = "CONTACT NAME", @id = "FacilityContactName" })
                                </div>
                                <div class="col-lg-4">
                                    <span class="theme_label">@Html.LabelFor(e => e.Facility.PhoneNumber)</span><br>
                                    @Html.TextBoxFor(e => e.Facility.PhoneNumber, new { @class = "form-control input-xs", @placeholder = "PHONE NUMBER", @id = "FacilityPhoneNumber" })
