@model  IEnumerable<PortalTemplate.Areas.UM.Models.ViewModels.QueueBucket.UserViewModel>

<div class="col-lg-12 col-md-12 theInnerCard">
    <div class="this_box_chart">
        <div class="col-lg-12 col-md-12" id="theRoleUserBucket">
            @*<input name="UsersData" value="@ViewBag.Users" hidden />*@
            @{int i = 0;}
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-3 theCol">
                    <div class="theBox" data-path="/UM/AuthorizationAction/SaveAuthorization" data-container="QueueBuckets" data-selecteduserrole="@item.UserRole" data-currentuserrole="Intake" data-currentuser="B Joy" data-selecteduser="@item.UserName" data-expeditedcount="@item.ExpeditedCount" data-standardcount="@item.StandardCount">
                        <span class="standard-text">Standard-@item.StandardCount</span>
                        <div id="@i" class="row"></div> @*queueUser_*@
                        <span class="expedited-text">Expedited-@item.ExpeditedCount</span>
                        <div class="row">
                            <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 centreIt move_name mnsojfdigid">
                                <div class="theText"><span>@item.UserName</span></div>
                            </div>
                        </div>
                    </div>
                </div>
                i++;
            }
        </div>
    </div>
</div>
