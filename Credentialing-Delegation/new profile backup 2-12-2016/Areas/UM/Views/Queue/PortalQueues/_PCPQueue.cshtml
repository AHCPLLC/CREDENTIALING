<!-- FILES TO BE LOADED -->
<link href="~/Areas/UM/Styles/Queue/IntakeQueue.css" rel="stylesheet" />
@using PortalTemplate.Helper
<style>
    .globalQueueSelectList {
        float: left;
        padding: 0 !important;
    }

    .height29 {
        height: 29px !important;
    }

    .moveWork {
        display: none !important;
    }

    .assignedTo, .entry {
        display: none;
    }

    .legend-container {
        display: none;
    }

    .header_dropdown {
        display: none;
    }

    #CheckBoxDivOwn {
        display: none !important;
    }
</style>
<p-script filepath="~/Areas/UM/Scripts/Queue/PCPQueue/urlConfigPCPQueue.js"></p-script>
<ul class="innerTabsArea queueTabs" style="width:100.7%;">
    <li id="workmenu" class="ipaQueueMenu active tabs innerTabs">
        <a class="innerTabsLinks topminus3"><span class="text-uppercase">Work</span><label class="label queueUppertab_label">2</label></a><span class="pointing-caret"></span>
    </li>
    <li id="completedmenu" class="ipaQueueMenu tabs innerTabs">
        <a class="innerTabsLinks"><span class="text-uppercase ng-binding">Completed</span><label class="label queueUppertab_label">2</label></a><span class="pointing-caret"></span>
    </li>
</ul>
<div id="queueBody">
    <!-- TOP TOOLBAR -->
    <div class="fixed-toolbar marginTop5px">
        <button class="btn btn-xs btn-primary moveWork leftPanelButtons QueueAggregateButtons">Move Work</button>
        <button class="btn btn-xs btn-success completeCTFU leftPanelButtons QueueAggregateButtons">Complete</button>
        <div class="col-lg-3 completedDiv leftPanelButtons" style="width:25.4%;">
            <span class="col-lg-2"><label class="text-primary">FROM:</label></span>
            <span class="col-lg-4">
                <input type="text" class="form form-control input-xs" id="CompletedFromDate" placeholder="FROM" value="09/29/16">
            </span>
            <span class="col-lg-2"><label class="text-primary">TO:</label></span>
            <span class="col-lg-4">
                <input type="text" class="form form-control input-xs" id="CompletedToDate" placeholder="TO" value="09/29/16">
            </span>
        </div>

        

        <div class="btn btn-primary btn-xs total-count QueueAggregateButtons">All <span id="total-count" class="label queueUppertab_label"></span><span class="pointing-arrow-all"></span></div>
        <div class="btn btn-danger btn-xs total-expedited QueueAggregateButtons">Expedited <span id="total-expedited" class="label queueUppertab_label"></span><span class="pointing-arrow-expedited"></span></div>
        <div class="btn btn-success btn-xs total-standard QueueAggregateButtons">Standard <span id="total-standard" class="label queueUppertab_label"></span><span class="pointing-arrow-standard"></span></div>   
        <span class="filterButton">
            @Html.ExportOptions("filter")
        </span>
        <span class="excelButtonQueue">
            @Html.ExportOptions("excel")
        </span>
    </div>


    <div class="tableBodyDivision">
        <!-- SPINNER -->
        <div class="spinner-container">
            <img class="spinner-image" src="~/Resources/Images/Common/spinner.gif" />
        </div>
        <!-- END SPINNER -->
        <table id="facilityQueueTable" class="table table-striped table-hover table-responsive" @*border="1"*@>
            <thead class="theme_thead custom-thead-back theme1_table custom-thead-font custom-tbody">
                <tr>
                    
                    <th class="maxWidthtdSM" id="CheckBoxDivOwn">
                        <div><input id="mainCheckBox" type="checkbox" name="mainCheckBox" class="normal-checkbox" value="all"><label for="mainCheckBox"><span></span></label></div>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">ABV &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">

                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">AUTH NO &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">

                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">REF # &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">MBR ID  &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdXL">
                        <label class="singleElemQueue bgFieldsQueue">MBR NAME &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">FROM &nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">TO&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>

                    <th class="maxWidthtdXL">
                        <label class="singleElemQueue bgFieldsQueue">PROVIDER&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdXL">
                        <label class="singleElemQueue bgFieldsQueue">FACILITY&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdXL">
                        <label class="singleElemQueue bgFieldsQueue">SVC PROVIDER&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">REQ&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">AUTH&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">TOC&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">DURATION&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">DOS&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">UNITS&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">STATUS&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdSM">
                        <label class="singleElemQueue bgFieldsQueue">POS&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">DX&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">

                            <span></span>

                        </span>
                    </th>
             
                    <th class="maxWidthtdX">
                        <label class="singleElemQueue bgFieldsQueue">AUTH NOTE&nbsp;<i class="fa fa-sort facility-queue-sort"></i></label>
                        <span class="styled-input">


                            <span></span>

                        </span>
                    </th>
                    @*<th style="max-width:0;" class="width_1"></th>*@
                </tr>
            </thead>
            <tbody id="intakeQueueTable">
                <!-- DATA ROWS -->
            </tbody>
        </table>
    </div>
</div>
<p-script filepath="~/Areas/UM/Scripts/Queue/PCPQueue/PCPQueue.js"></p-script>