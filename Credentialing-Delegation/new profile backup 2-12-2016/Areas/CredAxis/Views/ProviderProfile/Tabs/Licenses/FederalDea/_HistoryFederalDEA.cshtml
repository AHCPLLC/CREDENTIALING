@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.LicensesViewModel.FederalDea>

    <span class="theme_depend text-uppercase TableHeading">FEDERAL DEA History</span>
    <span><button type="button" @*onclick="clearDiv(this)"*@ data-toggle="collapse" data-target="#FederalDeaHistory" class="btn btn-xs btn-danger closeBtnCSS pull-right"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left text-uppercase">

        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>DEA #</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>State of Registration</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Issue Date</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Expiration Date</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Schedule</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
            {

                    <tr>
                        @*<td class="col-lg-2">1</td>*@
                        <td class="col-lg-2">@item.DEA</td>
                        <td class="col-lg-2">@item.StateOfRegistration</td>
                        <td class="col-lg-2">@item.IssueDate</td>
                        <td class="col-lg-2">@item.ExpirationDate</td>
                        <td class="col-lg-2">
                            <span:if class="@item.SCHEDULE.One == false ? : hidden">@*<input type="checkbox"        checked="@item.SCHEDULE.One" name="IPA" class="normal-checkbox" disabled value="1">*@<label for="IPA" class="font-bold"><span></span>1</label></span:if>&nbsp;&nbsp;
                            <span class="@item.SCHEDULE.Two == false ? : hidden">@*<input type="checkbox" name="2" checked="@item.SCHEDULE.Two" class="normal-checkbox" disabled value="2">*@<label for="2" class="font-bold"><span></span>2</label></span>&nbsp;&nbsp;
                            <span class="@item.SCHEDULE.TwoN == false ? : hidden">@*<input type="checkbox" name="2N" checked="@item.SCHEDULE.TwoN" class="normal-checkbox" disabled value="2N">*@<label for="2N" class="font-bold"><span></span>2N</label></span>&nbsp;&nbsp;
                            <span>@*<input type="checkbox" name="3" checked="@item.SCHEDULE.Three" class="normal-checkbox" disabled value="3">*@<label for="3" class="font-bold"><span></span>3</label></span>&nbsp;&nbsp;
                            <span>@*<input type="checkbox" name="3N" checked="@item.SCHEDULE.ThreeN" class="normal-checkbox" disabled value="3N">*@<label for="3N" class="font-bold"><span></span>3N</label></span>&nbsp;&nbsp;
                            <span>@*<input type="checkbox" name="4N" checked="@item.SCHEDULE.FourN" class="normal-checkbox" disabled value="4N">*@<label for="4N" class="font-bold"><span></span>4N</label></span>&nbsp;&nbsp;
                            <span>@*<input type="checkbox" name="5N" checked="@item.SCHEDULE.FiveN" class="normal-checkbox" disabled value="5N">*@<label for="5N" class="font-bold"><span></span>5N</label></span>
                        </td>
                        <td class="col-lg-2"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On June 13,2014,Eduffin Made Changes -(1),Added <br /><b>Issue Date:-</b> 04-11-2012<br /><b>Expiration Date:-</b> 04-11-2014<br /><b>Schedule:-</b> I,II,III,IV,V">...</a></td>
                    </tr>

            }
            </tbody>
        </table>
    </div>
