@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.LicensesViewModel.MedicaidInfo>

    <span class="theme_depend text-uppercase TableHeading">Medicaid Information History</span>
    <span><button type="button" @*onclick="clearDiv(this)"*@ class="btn btn-xs btn-danger closeBtnCSS pull-right" data-toggle="collapse" data-target="#medicaidInfoHistory"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">
        @*<table class="table table-striped table-bordered claimlist_table dummy_table"><thead><tr></tr></thead></table>*@
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead text-uppercase">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Medicaid #</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Issue Date</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>Expiry Date</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>


                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
{
                    <tr>
                        <td class="col-lg-2">@item.MedicaidID</td>
                        <td class="col-lg-1">@item.IssueDate</td>
                        <td class="col-lg-2">@item.ExpirationDate</td>
                        <td class="col-lg-1">
                            <a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On Jan 12 2015, Elizabeth Made Changes -(3),Changed <br /> <b>Medicaid #:-</b> ME45125, <br /><b>ISSUE DATE:-</b> 04-11-2012, <br /><b>EXPIRY DATE:-</b> 04-11-2013.">...</a>
                        </td>

                    </tr>

}

            </tbody>
        </table>
    </div>
