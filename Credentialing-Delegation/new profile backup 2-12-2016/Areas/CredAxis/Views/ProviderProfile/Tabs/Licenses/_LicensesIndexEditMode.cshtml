@model PortalTemplate.Areas.CredAxis.Models.LicensesViewModel.LicenseMainViewModel
<div class="clearfix"></div>
<div class="target profilediv col-lg-12" id="IDLicenses">
    <div class="col-lg-12 padding_top_down_5px nopadding" id="StateLicense">
        @if (Model.stateLicense != null)
        {
            @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/StateLicense/_AddEditStateLicense.cshtml", Model.stateLicense)
        }
        else
        {
            @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "STATE LICENCE", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/StateLicense/_AddEditStateLicense.cshtml", SectionId = "StateLicense" })
        }


    </div>
    <div class="col-lg-12 padding_top_down_5px nopadding" id="FederalDEA">
        @if (Model.federalDEA != null)
        {
            @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/FederalDea/_AddEditFederalDEAInfo.cshtml", Model.federalDEA)
        }
        else
        {
            @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "FEDERAL DEA", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/StateLicense/_AddEditStateLicense.cshtml", SectionId = "FederalDEA" })
        }

    </div>
    <div class="col-lg-12 nopadding">
        <div class="col-lg-6 padding_top_down_5px zero-padding-left" id="MedicarePanel">
            @if (Model.medicare != null)
            {
                @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/MedicareInfo/_AddEditMedicareInformation.cshtml", Model.medicare)
            }
            else
            {
                @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "MEDICARE INFORMATION", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/MedicareInfo/_AddEditMedicareInformation.cshtml", SectionId = "MedicarePanel" })
            }


        </div>
        <div class="col-lg-6 padding_top_down_5px zero-padding-right" id="Medicaid">
            @if (Model.mediciad != null)
            {
                @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/MedicaidInfo/_AddEditMedicaidInfo.cshtml", Model.mediciad)
            }
            else
            {
                @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "MEDIACAID INFORMATION", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/MedicareInfo/_AddEditMedicareInformation.cshtml", SectionId = "MedicarePanel" })
            }

        </div>
    </div>
    <div class="col-lg-12 padding_top_down_5px nopadding" id="OtherIdentification">
        @if (Model.otherIdentification != null)
        {
            @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/OtherIdentification/_AddEditOtherIdentification.cshtml", Model.otherIdentification)
        }
        else
        {
            @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "OTHER IDENTIFICATIONS", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/MedicareInfo/_AddEditMedicareInformation.cshtml", SectionId = "OtherIdentification" })
        }




    </div>
    <div class="col-lg-12 padding_top_down_5px nopadding" id="CDSInfo">
        @if (Model.CDS != null)
        {
            @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/CDSInfo/_AddEditCDSInformation.cshtml", Model.CDS)
        }
        else
        {
            @Html.Action("GetNoDataAvailablePartial", "CredAxis", new { SectionName = "CDS IDENTIFICATION", RedirectUrl = "~/Areas/CredAxis/Views/ProviderProfile/Tabs/Licenses/CDSInfo/_AddEditCDSInformation.cshtml", SectionId = "CDSInfo" })
        }

    </div>

</div>
<div class="clearfix"></div>