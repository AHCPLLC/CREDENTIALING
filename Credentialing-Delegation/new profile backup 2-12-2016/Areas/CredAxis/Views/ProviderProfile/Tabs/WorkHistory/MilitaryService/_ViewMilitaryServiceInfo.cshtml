@model  IEnumerable<PortalTemplate.Areas.CredAxisProduct.Models.ProviderProfileViewModel.WorkHistoryViewModel.MilitaryServiceViewModel>
@{
    var MilitaryServiceInfoCount = 0;
}
<div id="MilitaryServiceID">
    <fieldset class="fsStyle" id="ViewMilitaryServiceInfo">
        <legend class="legendStyle">
            <span class="theme_depend text-uppercase create-auth-legends">
                <span class="transparent-font-color">__________</span>
                MILITARY SERVICE INFORMATION
                <span class="transparent-font-color">__________</span>
            </span>
            &nbsp;&nbsp; <span class="text-uppercase ">
                <span class="fstyleBtnStyle">
                    <button class="btn btn-success btn-xs add_note_btn list-item" data-tab-path="/CredAxis/WorkHistory/GetMilitaryServiceAddEditPartial?model=@Model&ActionType=Add" data-tab-container="MilitaryServiceDiv">
                        <i class="fa fa-plus"></i>
                    </button>
                    <button class="btn btn-primary btn-xs list-item" data-tab-path="/CredAxis/WorkHistory/GetMilitaryServiceAddEditPartial?model=@Model&ActionType=Edit" data-tab-container="MilitaryServiceDiv">
                        <i class="fa fa-pencil"></i>
                    </button>
                    <button class="btn btn-warning btn-xs historyBtnCSS" data-toggle="collapse" data-target="#MilitaryServiceHistory">
                        <i class="fa fa-history"></i>
                    </button>
                    <a class="print_button_for_MilitarySrvc hover_effect pull-right" data-toggle="tooltip" data-placement="top" title="" data-original-title="Print" tabindex="-1">
                        <img class="printIcon_image" src="/Resources/Images/Icons/print_icon.png">
                    </a>
                </span>
            </span>
        </legend>

@foreach (var item in Model)
{
    MilitaryServiceInfoCount = MilitaryServiceInfoCount + 1;
    if (MilitaryServiceInfoCount <= 2)
    {
        <div class="x_content zero-padding-left mutiple-data">
            <div class="SubHeaderSpacing1">
                <span class="text-uppercase TableHeading"> Military Service Information-@item.ID</span>&nbsp;
                <button onclick="groupmodalhistory('WorkHistory','RemoveMilitaryServiceInfo','@item.ID','MilitaryServiceDiv')" class="btn btn-danger btn-xs RemoveButtonStyleModal"><i class="fa fa-minus"></i></button>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.MilitaryBranch):</label>
                <span class="theme_label_data">@item.MilitaryBranch</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.StartDate):</label>
                <span class="theme_label_data">@item.StartDate</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.EndDate):</label>
                <span class="theme_label_data">@item.EndDate</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.RankAtDischarge):</label>
                <span class="theme_label_data">@item.RankAtDischarge</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.DischargeType):</label>
                <span class="theme_label_data">@item.DischargeType</span>
            </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                        <label class="theme_label">PRESENT DUTY STATUS:</label>
                        <span class="theme_label_data">MAJOR</span>
                    </div>
        </div>
        if (MilitaryServiceInfoCount == 2)
        {
            <span class="view-more pull-right" id="moreMilitaryService" data-toggle="collapse" data-target=".ViewMoreMilitaryServiceInfo" onclick="ShowHideAddView('lessMilitaryService', 'moreMilitaryService')">View More</span>
        }
    }
    else
    {

        <div class="x_content zero-padding-left mutiple-data collapse ViewMoreMilitaryServiceInfo">
            <div class="SubHeaderSpacing1">
                <span class="text-uppercase TableHeading"> Military Service Information-@item.ID</span>&nbsp; <button data-toggle="modal" data-target="#BhaukalId" class="btn btn-danger btn-xs RemoveButtonStyleModal"><i class="fa fa-minus"></i></button>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.MilitaryBranch):</label>
                <span class="theme_label_data">@item.MilitaryBranch</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.StartDate):</label>
                <span class="theme_label_data">@item.StartDate</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.EndDate):</label>
                <span class="theme_label_data">@item.EndDate</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.RankAtDischarge):</label>
                <span class="theme_label_data">@item.RankAtDischarge</span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                <label class="theme_label">@Html.DisplayNameFor(e => e.DischargeType):</label>
                <span class="theme_label_data">@item.DischargeType</span>
            </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle text_adjust">
                        <label class="theme_label">PRESENT DUTY STATUS/ASSIGNMENT:</label>
                        <span class="theme_label_data">MAJOR</span>
        </div>
                </div>
    }
    if (MilitaryServiceInfoCount == Model.Count())
    {
        <span class="view-more hidden pull-right" id="lessMilitaryService" data-toggle="collapse" data-target=".ViewMoreMilitaryServiceInfo" onclick="ShowHideAddView('lessMilitaryService', 'moreMilitaryService')">View Less</span>
    }

}
        <fieldset class="fsStyle collapse remove_shadow width-100" id="MilitaryServiceHistory">
            @Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/WorkHistory/MilitaryService/_HistoryMilitaryServiceInfo.cshtml")
        </fieldset>
    </fieldset>

</div>
@Html.Partial("~/Areas/CredAxis/Views/ProviderProfile/Tabs/_RemoveModal.cshtml")