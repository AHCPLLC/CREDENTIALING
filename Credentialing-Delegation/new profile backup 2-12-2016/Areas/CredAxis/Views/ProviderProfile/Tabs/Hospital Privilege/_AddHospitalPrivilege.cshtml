                    </span>
                    <span class="col-lg-1"></span>
                    <span class="col-lg-6 nopadding">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "HAVE", Value = "YES", @class = "HostPrevAdd" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "HAVE", Value = "NO", @class = "HostPrevAdd" })
