@model  IEnumerable<PortalTemplate.Areas.CredAxis.Models.ProfessionalAffiliationViewModel.ProfessionalAffiliationViewModel>


<fieldset class="fsStyle  width-100"  style="box-shadow: none !important;" aria-expanded="true">
    <span class="theme_depend text-uppercase TableHeading">Professional Affiliation History</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)"  data-toggle="collapse" data-target="#ProfessionalAffiliationHistory"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left text-uppercase">

        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>ORGANIZATION NAME</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>POSITION OFFICE HELD</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>START DATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>END DATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>MEMBER/APPLICANT</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required=""><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class=" col-lg-1" rowspan="1" colspan="1">
                        <span class="">
                            <input type="text" class="filer-search" required /><label>ACTION</label><span></span>
                        </span>
                    </th>

                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>

                        <td class="col-lg-2">@item.OrganizationName</td>
                        <td class="col-lg-2">@item.PositionOfficeHeld</td>
                        <td class="col-lg-2">@item.StartDate</td>
                        <td class="col-lg-2">@item.EndDate</td>
                        <td class="col-lg-2">@item.MemberApplicant</td>
                        <td class="col-lg-2"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On May 22,2014,Eduffin Made Changes -(1),Deleted <br /><b>Start Date</b>:-@item.StartDate,<br /><b>End Date</b>:- @item.EndDate" >...</a></td>
                        <td class="col-lg-1"><button class="btn btn-info btn-xs" @*data-toggle="modal"*@ data-target="#@item.ProfessionalAffiliationID"><i class="fa fa-eye"></i></button></td>
                    </tr>
                }


