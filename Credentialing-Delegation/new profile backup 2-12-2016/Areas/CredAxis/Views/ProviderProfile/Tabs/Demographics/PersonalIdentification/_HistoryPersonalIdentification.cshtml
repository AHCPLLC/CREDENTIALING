@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.DemographisViewModels.PersonalIdentificationViewModel>
<fieldset class="fsStyle remove_shadow" id="PersonalInfoHistory">
    <span class="theme_depend text-uppercase TableHeading">Personal Identification History</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left text-uppercase">
        @*<table class="table table-striped table-bordered claimlist_table dummy_table"><thead><tr></tr></thead></table>*@
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.SocialSecurityNumber) NUMBER</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.DriversLicense)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>STATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        @*<td class="col-lg-2">1</td>*@
                        <td class="col-lg-2">@item.SocialSecurityNumber</td>
                        <td class="col-lg-2">@item.DriversLicense</td>
                        <td class="col-lg-2">@item.IssueStateOfDL</td>
                        <td class="col-lg-2"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On June 13,2014,Eduffin Made Changes -(1),Added <br /><b>State:-</b> FLORIDA">...</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</fieldset>