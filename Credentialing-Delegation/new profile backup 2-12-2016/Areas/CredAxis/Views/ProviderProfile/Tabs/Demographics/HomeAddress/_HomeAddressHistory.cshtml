@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.DemographisViewModels.HomeAddressViewModel>
@***************Home Address History *********************************@
<fieldset class="fsStyle remove_shadow width-100" id="HomeAddress">
    <span class="theme_depend text-uppercase TableHeading">Home Address History</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>APARTMENT / UNITS #</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-3" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>STREET / P.O. BOX #</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>CITY</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>STATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>COUNTY</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>COUNTRY</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>ACTION</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-lg-2">@item.ApartmentOrUnit</td>
                        <td class="col-lg-3">@item.StreetOrPOBox</td>
                        <td class="col-lg-1">@item.City</td>
                        <td class="col-lg-1">@item.State</td>
                        <td class="col-lg-2">@item.County</td>
                        <td class="col-lg-1">@item.Country</td>
                        <td class="col-lg-1"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On Dec 12 2014, jennine Made Changes -(3),Changed <br /> <b>APARTMENT / UNITS:-</b> @item.ApartmentOrUnit, <br /><b>STREET / P.O. BOX:-</b> @item.StreetOrPOBox">...</a></td>
                        <td class="col-lg-1"><button class="btn btn-default btn-xs styleForViewEyeButton" data-toggle="modal" data-target="#HomeAddressHistoryView"><i class="fa fa-eye"></i></button></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</fieldset>
<!--Home Address History Modal-->
<div class="modal fade " tabindex="-1" role="dialog" id="HomeAddressHistoryView" style="overflow: auto; top: 8%;">
    <div class="modal-dialog modal-lg ">
        <form class="modal-content">
            <div class="modal-header zero_padding_bottom">
                <div class="row" style="text-align: center;">
                    <span class="modalTitle">Home Address History</span>
                    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button></span>
                    @*<span data-dismiss="modal" class="pull-right"><i class="fa fa-times btn btn-danger"></i></span>*@
                </div>
            </div>

            <div class="modal-body modalbodyheight  ">
                <div class="x_content zero-padding-left">
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Primary Home Address? :</label>
                        <span class="theme_label_data">Primary</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Apartment/Unit # :</label>
                        <span class="theme_label_data">SUITE 304</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Street/P. O. Box # :</label>
                        <span class="theme_label_data">11012 N DALE MABRY HWY.</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">City :</label>
                        <span class="theme_label_data">Tampa</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">State :</label>
                        <span class="theme_label_data">Florida</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">County :</label>
                        <span class="theme_label_data">HILLSBOROUGH</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Country :</label>
                        <span class="theme_label_data">United States</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Zip Code :</label>
                        <span class="theme_label_data">33618-3821</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Present Home Address ? :</label>
                        <span class="theme_label_data">Yes</span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Living Here From :</label>
                        <span class="theme_label_data">N/A</span>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
