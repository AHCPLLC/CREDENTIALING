@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.DemographisViewModels.BirthInformationViewModel>
@***************Birth Information History *********************************@
<fieldset class="fsStyle remove_shadow" id="BirthInfoHistory">
    <span class="theme_depend text-uppercase TableHeading">BIRTH INFORMATION History</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.DateOfBirth)</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>CITY</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>STATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.County)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.Country)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.BirthCertificate)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-lg-2">@item.DateOfBirth</td>
                        <td class="col-lg-1">@item.CityOfBirth</td>
                        <td class="col-lg-1">@item.StateOfBirth</td>
                        <td class="col-lg-2">@item.County</td>
                        <td class="col-lg-2">@item.Country</td>
                        <td class="col-lg-2">@item.BirthCertificate</td>
                        <td class="col-lg-1"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On Jan 12 2015, Elizabeth Made Changes -(1),Changed <br /> <b>CITY:-</b> ORLANDO">...</a></td>
                        @*<td class="col-lg-1"><span class="btn btn-default btn-xs"><i class="fa fa-eye"></i></span></td>*@
                    </tr>
                }
            </tbody>
        </table>
    </div>
</fieldset>
