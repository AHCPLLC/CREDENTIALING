@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.DemographisViewModels.PersonalDetailsViewModel>
<!--Personal Details History View-->
<fieldset class="fsStyle remove_shadow" id="PersonalDetails">
    <span class="theme_depend text-uppercase TableHeading">PERSONAL DETAILS HISTORY</span>

    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right list-item" onclick="clearDiv(this)"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.FirstName)</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.MiddleName)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.LastName)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.Suffix)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.Gender)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>@Html.DisplayNameFor(e => e.ProviderLevel)</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>ACTION</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-lg-2">@item.FirstName</td>
                        <td class="col-lg-2">@item.MiddleName</td>
                        <td class="col-lg-2">@item.LastName</td>
                        <td class="col-lg-1">@item.Suffix</td>
                        <td class="col-lg-1">@item.Gender</td>
                        <td class="col-lg-2">-</td>
                        <td class="col-lg-1" align="center">
                            <a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On Jan 12 2015, Elizabeth Made Changes -(3),Changed <br /> <b>First NAME:-</b> JEAN, <br /><b>Middle NAME:-</b> ROBERT, <br /><b>LAST NAME:-</b> KELLY.">...</a>
                        </td>
                        <td class="col-lg-1" align="center">
                            <button data-toggle="modal" data-target="#PersonalDetailsHistoryView" class="btn btn-default btn-xs styleForViewEyeButton"><i class="fa fa-eye"></i></button>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</fieldset>

<div class="modal fade " tabindex="-1" role="dialog" id="PersonalDetailsHistoryView" style="overflow: auto; top: 8%;">
    <div class="modal-dialog modal-lg ">
        <form class="modal-content">
            <div class="modal-header zero_padding_bottom">
                <div class="row" style="text-align: center;">
                    <span class="modalTitle">PERSONAL DETAILS HISTORY</span>
                    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button></span>
                    @*<span data-dismiss="modal" class="pull-right"><i class="fa fa-times btn btn-xs btn-danger"></i></span>*@
                </div>
            </div>

            <div class="modal-body modalbodyheight  ">
                <div class="x_content zero-padding-left">
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Salutation :</label>
                        <span class="theme_label_data">MR.</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">First Name :</label>
                        <span class="theme_label_data">JOHN</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Middle Name :</label>
                        <span class="theme_label_data">N/A</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Last Name  :</label>
                        <span class="theme_label_data">SMITH</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Gender :</label>
                        <span class="theme_label_data">MALE</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Suffix :</label>
                        <span class="theme_label_data">JOHN</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Marital Status :</label>
                        <span class="theme_label_data">MARRIED</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Provider Level :</label>
                        <span class="theme_label_data">DOCTOR</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Provider Type :</label>
                        <span class="theme_label_data">ARNP</span>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Have You Used Other Legal Names ?</label>
                        <span class="theme_label_data">NO</span>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>