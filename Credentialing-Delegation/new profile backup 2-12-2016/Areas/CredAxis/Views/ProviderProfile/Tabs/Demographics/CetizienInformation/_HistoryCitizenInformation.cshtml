@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.DemographisViewModels.CitizenshipInformationViewModel>
<style>
    #scrolldummybody {
        text-transform: uppercase;
    }
</style>
<fieldset id="citizenInfoHistory" class="fsStyle remove_shadow">
    <span class="theme_depend text-uppercase TableHeading">Citizenship Information History</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">

        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>VISA #</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>VISA TYPE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>

                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>VISA STATUS</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>VISA SPONSOR</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>VISA EXP.</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>GREEN CARD #</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>ACTION</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-lg-1">@item.VisaNumber</td>
                        <td class="col-lg-1">@item.VisaType</td>
                        <td class="col-lg-1">@item.VisaStatus</td>
                        <td class="col-lg-1">@item.VisaSponsor</td>
                        <td class="col-lg-1">@item.VisaExpiration</td>
                        <td class="col-lg-1">@item.GreenCardNumber</td>
                        <td class="col-lg-1"><a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On June 13,2014,Eduffin Made Changes -(1),Deleted<br /><b>VISA:-</b> 23578">...</a></td>
                        <td class="col-lg-1"><button class="btn btn-default btn-xs styleForViewEyeButton" id="viewBtn" data-toggle="modal" data-target="#CitizenshipInformationHistoryView"><i class="fa fa-eye"></i></button></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</fieldset>
<div class="modal fade " tabindex="-1" role="dialog" id="CitizenshipInformationHistoryView" style="overflow: auto; top: 8%;">
    <div class="modal-dialog modal-lg ">
        <form class="modal-content">
            <div class="modal-header zero_padding_bottom">
                <div class="row" style="text-align: center;">
                    <span class="modalTitle">Citizenship Information History</span>
                    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button></span>
                    @*<span data-dismiss="modal" class="pull-right"><i class="fa fa-times btn btn-xs btn-danger"></i></span>*@
                </div>
            </div>

            <div class="modal-body modalbodyheight  ">
                <div class="x_content zero-padding-left">
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Are You Citizen Of US?</label>
                        <span class="radioValueStyling">
                            <span><label class="radio-inline">Yes</label></span>
                        </span>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                        <label class="theme_label">Are you authorized to work in US?</label>
                        <span class="radioValueStyling">
                            <span><label class="radio-inline">Yes</label></span>
                        </span>
                    </div>
                    <div class="x_content zero-padding-left">
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Visa # :</label>
                            <span class="theme_label_data">213213</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Visa Type :</label>
                            <span class="theme_label_data">P1</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Visa Status :</label>
                            <span class="theme_label_data">Approved</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Visa Sponsor :</label>
                            <span class="theme_label_data">N/A</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Visa Expiration :</label>
                            <span class="theme_label_data">06/07/2017</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Green Card # :</label>
                            <span class="theme_label_data">N/A</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">National Id. # :</label>
                            <span class="theme_label_data">N/A</span>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                            <label class="theme_label">Country of Issue :</label>
                            <span class="theme_label_data">Florida</span>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
