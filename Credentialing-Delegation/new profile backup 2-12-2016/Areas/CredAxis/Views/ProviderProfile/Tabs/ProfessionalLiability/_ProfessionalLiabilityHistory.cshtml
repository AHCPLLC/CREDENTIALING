@model IEnumerable<PortalTemplate.Areas.CredAxis.Models.ProfessionalLiabilityViewModel.ProfessionalLiabilityViewModel>



<fieldset class="fsStyle  remove_shadow width-100">
    <span class="theme_depend text-uppercase TableHeading">Professional Liability HISTORY</span>
    <span><button type="button" class="btn btn-xs btn-danger closeBtnCSS pull-right" onclick="clearDiv(this)" id="ProfessionalLiability"><i class="fa fa-times" aria-hidden="true"></i></button></span>
    <div class="x_content zero-padding-left">
        <table class="table table-striped credAxisList_table custom-thead-back custom-thead-font custom-tbody custom-table-striped custom_margin" cellspacing="0">
            <thead class="theme_thead">
                <tr>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>INSURANCE CARRIER</label><span></span>
                            <i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-4" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>UNLIMITED COVERAGE WITH INSURANCE CARRIER</label><span></span>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>EXPIRATION DATE</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-2" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>POLICY NUMBER</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>NOTES</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                    <th class="withfilter col-lg-1" rowspan="1" colspan="1">
                        <span class="styled-input">
                            <input type="text" class="filer-search" required /><label>ACTION</label><span></span><i class="fa fa-sort"></i>
                        </span>
                    </th>
                </tr>
            </thead>
            <tbody id="scrolldummybody">
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-lg-2">@item.InsuranceCarrier</td>
                        <td class="col-lg-4">@item.InsuranceCarrierCoverage</td>
                        <td class="col-lg-2">@item.ExpirationDate</td>
                        <td class="col-lg-2">@item.PolicyNumber</td>
                        <td class="col-lg-1" align="center">  <a href="#" data-toggle="popover" data-trigger="hover" data-placement="left" data-html="true" data-content="On Jan 12 2015, Elizabeth Made Changes -(3),Changed <br /> <b>POLICY NUMBER:-</b>@item.PolicyNumber">...</a></td>
                        <td class="col-lg-1"><button class="btn btn-info btn-xs" @*data-toggle="modal"*@ data-target="#@item.ProfessionalLiabilityID"><i class="fa fa-eye"></i></button></td>
                    </tr>

                }



