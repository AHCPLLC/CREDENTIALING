<script>
    $("#speciltytypes").select2({
        tags: true,
        placeholder: 'SPECIALTY',
        allowClear: true
    })
    $("#Providertypes").select2({
        tags: true,
        placeholder: 'PROVIDER TYPE',
        allowClear: true
    })
</script>
@using PratianUI
<div class="col-lg-12 nopadding">
    <fieldset class="fsStyle" id="EditPersonalInfo">
        <legend class="legendStyle">
            <span class="theme_depend text-uppercase create-auth-legends">Edit Partners/Associates/Covering Colleagues</span>
        </legend>

        <div class="x_content zero-padding-left">
            <div class="col-lg-7 col-md-7 col-sm-7 col-xs-12 boxstyle">
                <span class="theme_label col-lg-8 nopadding">
                    <label class="">USE EXISTING PARTNER/ASSOCIATES/COVERING COLLEAGUE DETAILS ? </label>
                </span>
                <span class="col-lg-4 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })
                    </span>
                </span>
            </div>
            <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12 boxstyle">
                <span class="col-lg-8 theme_label nopadding">
                    <label class="">SELECT PARTNER/ASSOCIATE/COVERING COLLEAGUE</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-4 nopadding">
                    @Html.TextBox("PARTNER", "", new { @class = "form-control input-xs" })
                </span>
            </div>
            <br />
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-4 theme_label nopadding">
                    <label class="">FIRST NAME</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-7 nopadding">
                    @Html.TextBox("FIRST", "JOHN", new { @class = "form-control input-xs" })
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-4 theme_label nopadding">
                    <label class="">LAST NAME</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-7 nopadding">
                    @Html.TextBox("LAST", "SMITH", new { @class = "form-control input-xs" })
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-3 theme_label nopadding">
                    <label class="">SPECIALTY</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-8 nopadding">
                    @*@Html.TextBox("SPECIALTY", "Internal Medicine", new { @class = "form-control input-xs" })*@
                    <select id="speciltytypes" class="form-control  text-uppercase  " multiple="multiple">
                        <option selected>Internal Medicine</option>
                        <option>Anesthesiology</option>
                        <option>Chiropractor, Nutrition</option>
                        <option>Allergy and Immunology</option>
                    </select>
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 boxstyle">
                <span class="col-lg-4 theme_label nopadding">
                    <label class="">PROVIDER TYPE</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-7 nopadding">
                    <select id="Providertypes" class="form-control  text-uppercase  " multiple="multiple">
                        <option selected>MD</option>
                        <option>NURSE</option>
                        <option>MID LEVEL</option>
                    </select>
                    @*@Html.TextBox("PROVIDER", "MD", new { @class = "form-control input-xs" })*@
                </span>
            </div>
        </div>
        <div class="row col-lg-12 zero-padding-left-right">
            @*<input class="btn btn-success btn-xs  buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/Demographics/GetPersonalIdentificationInfoEditPartial?ActionType=View" data-tab-container="PersonalIdentificationDiv" type="submit" name="submit" value="Save">*@
            <button class="btn btn-success btn-xs yahanClickKar  list-item buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/PracticeLocation/ViewPartnerInformation" data-tab-container="PLPartnersColleaguesDetails" value="">Save</button>
            <button class="btn btn-danger btn-xs yahanClickKar list-item buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/PracticeLocation/ViewPartnerInformation" data-tab-container="PLPartnersColleaguesDetails" value="">Cancel</button>
        </div>
    </fieldset>
</div>

