@using PratianUI


<div class="col-lg-12 nopadding">
    <fieldset class="fsStyle" id="EditPersonalInfo">
        <legend class="legendStyle">
            <span class="theme_depend text-uppercase create-auth-legends">Edit Other Information</span>
        </legend>

        <div class="x_content zero-padding-left">
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-5 theme_label nopadding">
                    <label class="">FACILITY NAME</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-6 nopadding">
                    @Html.TextBox("facilityName", "ALLERGY ASTHMA AND IMMUNOLOGY", new { @class = "form-control input-xs", placeholder = "FACILITY NAME" })
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-6 theme_label nopadding">
                    <label class="">CORPORATE/PRACTICE NAME</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-5 nopadding">
                    @Html.TextBox("CORPORATE", "ALLERGY ASTHMA AND IMMUNOLOGY", new { @class = "form-control input-xs", @readonly = "readonly" })
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-5 theme_label nopadding">
                    <label class="">GROUP NAME</label>
                </span><span class="col-lg-1"></span>
                <span class="col-lg-6 nopadding">
                    @Html.TextBox("GROUP", "Access Healthcare Physician LLC", new { @class = "form-control input-xs", placeholder = "GROUP NAME" })
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 boxstyle">
                <span class="theme_label col-lg-8 nopadding">
                    <label class="">IS IT YOUR PRIMARY PRACTICE LOCATION ? </label>
                </span>
                <span class="col-lg-4 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })
                    </span>
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 boxstyle">
                <span class="theme_label col-lg-8 nopadding">
                    <label class="">CURRENTLY PRACTICING AT THIS ADDRESS ? </label>
                </span>
                <span class="col-lg-4 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })

                    </span>
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="col-lg-6 theme_label nopadding">
                    <label class="">START DATE</label>
                </span>
                <span class="col-lg-6 nopadding">
                    <span class="theme_label_data">@Html.PratianUiDatePickerFor(m => m, new { @class = "form-control input-xs", placeholder = "MM/DD/YYYY" })</span>
                </span>
            </div>
            <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12 boxstyle">
                <span class="theme_label col-lg-8 nopadding">
                    <label class="">SEND GENERAL CORRESPONDENCE HERE ? </label>
                </span>
                <span class="col-lg-4 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })

                    </span>
                </span>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 boxstyle">
                <span class="theme_label col-lg-5 nopadding">
                    <label class="">SELECT PRIMARY TAX ID </label>
                </span>
                <span class="col-lg-1"></span>
                <span class="col-lg-6 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })

                    </span>
                </span>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12 boxstyle">
                <span class="theme_label col-lg-7 nopadding">
                    <label class="">DO YOU PRACTICE EXCLUSIVELY WITHING INPATIENT SETTING ? </label>
                </span>
                <span class="col-lg-4 nopadding">
                    <span class="theme_label_data">
                        @Html.PratianUiRadioButton("YES", new { color = "green", @name = "type" })
                        @Html.PratianUiRadioButton("NO", new { color = "red", @name = "type" })

                    </span>
                </span>
            </div>
        </div>
        <div class="row col-lg-12 zero-padding-left-right">
            @*<input class="btn btn-success btn-xs  buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/Demographics/GetPersonalIdentificationInfoEditPartial?ActionType=View" data-tab-container="PersonalIdentificationDiv" type="submit" name="submit" value="Save">*@
            <button class="btn btn-success btn-xs yahanClickKar  list-item buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/PracticeLocation/ViewOtherInformation" data-tab-container="PLOtherInformationDetails" value="">Save</button>
            <button class="btn btn-danger btn-xs yahanClickKar list-item buttonmargin pull-right margin-right-5px" data-tab-path="/CredAxis/PracticeLocation/ViewOtherInformation" data-tab-container="PLOtherInformationDetails" value="">Cancel</button>
        </div>
    </fieldset>
</div>
