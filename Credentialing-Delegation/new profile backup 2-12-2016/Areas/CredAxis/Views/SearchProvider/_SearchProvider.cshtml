        </div>
        <div class="col-lg-3">
            <label class="theme_label_data1 labelselectspace">PLAN</label><br />
            <select id="planInput" class="form-control  text-uppercase planMutiple " multiple="multiple">
                <option>ULTIMATE</option>
                <option>WELLCARE</option>
                <option>HUMANA</option>
                <option>FREEDOM</option>
            </select>
            @*@Html.PratianUiDropDownListFor(e => e.Name, "/Portal/MasterData/GetAllPlanName", 1, "100%", "PLAN", "500", "CompanyID", "CompanyName", new { @class = "form-control input-xs text-uppercase", id = "planInput" })*@
        </div>
        <div class="col-lg-2 " id="searchActionItems">

            <button class="btn btn-primary btn-sm labelselectspace" id="searchProviderButton" disabled><i class="fa fa-search"></i></button>
            <button class="btn btn-cancel btn-sm labelselectspace" id="clearSearchButton" disabled><i class="fa fa-2x fa-remove removebutton"></i></button>
        </div>
    </div>

