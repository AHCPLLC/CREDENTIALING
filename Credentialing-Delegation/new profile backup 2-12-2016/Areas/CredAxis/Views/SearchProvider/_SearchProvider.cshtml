            <label class="theme_label_data1">NPI</label>
            <input id="providernpiInput" class="form-control input-sm text-uppercase labelinputspace" type="text" placeholder="NPI" />
        </div>
        <div class="col-lg-3">
            <label class="theme_label_data1">LAST NAME</label>
            <input id="lastNameInput" class="form-control input-sm text-uppercase labelinputspace" type="text" placeholder="Last Name" />
        </div>
        <div class="col-lg-3">
            <label class="theme_label_data1">FIRST NAME</label>
            <input id="firstNameInput" class="form-control input-sm text-uppercase labelinputspace" type="text" placeholder="First Name" />
        </div>
        <div class="col-lg-2">
            <label class="theme_label_data1">LEVEL</label>
            @*<input type="text" placeholder="Level" />*@
            <select id="providerlevelInput" class="form-control input-sm text-uppercase labelinputspace">
                <option value="" selected disabled>--Select--</option>
                <option value="DOCTOR">DOCTOR</option>
                <option value="NURSE">NURSE</option>
                <option value="MID LEVEL">MID LEVEL</option>
            </select>
        </div>
        <div class="col-lg-2">
            <label class="theme_label_data1">RELATIONSHIP</label>
            @*<input  />*@
            <select id="RelationshipInput" class="form-control input-sm text-uppercase labelinputspace">
                <option value="" selected disabled>--Select--</option>
                <option value="EMPLOYEE">EMPLOYEE</option>
                <option value="AFFILIATE">AFFILIATE</option>
            </select>
        </div>
    </div>

    <div class="col-lg-12 Divspace">

        <div class="col-lg-4">
            <label class="theme_label_data1 labelselectspace">SPECIALTY</label><br />
            <select id="specialtyInput" class="form-control  text-uppercase typeMutiple " multiple="multiple">
                <option>Cardiology</option>
                <option>Anesthesiology</option>
                <option>Chiropractor, Nutrition</option>
                <option>Allergy and Immunology</option>
            </select>
