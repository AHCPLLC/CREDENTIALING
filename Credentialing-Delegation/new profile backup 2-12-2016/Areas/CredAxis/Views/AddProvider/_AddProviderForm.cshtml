<p-script filepath="~/Areas/CredAxis/Scripts/ProviderProfile/AddProvider/AddProvider.js"></p-script>
<style>
    .req {
    display:none
    }
</style>
<div id="AddProvider">
    <div class="containerArea">
        <form class="form-horizontal">
            <div class="SubTabHeader">
                <div class="headerText">
                    <span class="headerTextCSS">
                        Add Provider
                    </span>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    @Html.Partial("~/Areas/CredAxis/Views/AddProvider/_AdditionalInformation.cshtml")

                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                   @Html.Partial("~/Areas/CredAxis/Views/AddProvider/_ProviderSection.cshtml")
                  
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    @Html.Partial("~/Areas/CredAxis/Views/AddProvider/_LocationSection.cshtml")

                </div>
            </div>
            <div class="row" id="AddNewLocationSection" hidden>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    @Html.Partial("~/Areas/CredAxis/Views/AddProvider/_AdditionalLocationSection.cshtml")

                </div>
            </div>
          
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    @Html.Partial("~/Areas/CredAxis/Views/AddProvider/_InsuranceInformation.cshtml")

                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 button-styles text-right margin-top-bottom" style="margin-top:10px">
                <span><button data-dismiss="modal" class="btn btn-danger close_modal_btn">CANCEL</button></span>
                <button class="btn btn-success Proceed" data-toggle="modal" data-target="#myModal">ADD</button>
            </div>
        </form>
    </div>
</div>


<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header" style="color:white">
                <button type="button" class="close" data-dismiss="modal" style="color:white">&times;</button>
                <h4 class="modal-title">DR.TARA GUNTER</h4>
            </div>
            <div class="modal-body">
                <h4 style="color:green"> PROVIDER ADDED SUCCESSFULLY..!!! </h4>
            </div>
            
        </div>

    </div>
</div>

