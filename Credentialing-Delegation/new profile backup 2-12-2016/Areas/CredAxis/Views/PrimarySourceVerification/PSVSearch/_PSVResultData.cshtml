        /*background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ffffff), color-stop(100%, #f4f8ff));
        background-image: -moz-linear-gradient(#ffffff, #f4f8ff);
        background-image: -webkit-linear-gradient(#ffffff, #f4f8ff);
        background-image: linear-gradient(#ffffff, #f4f8ff);*/
        text-shadow: 0 1px rgba(255, 255, 255, 0.5);
        width: 100%;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.2), inset 0 0 40px rgba(0, 0, 0, 0.1);
        border: 1px solid rgba(0, 0, 0, 0.2);
        border-radius: 0 0 4px 4px;
    }
</style>
<script>
    $("dd").hide();

    //$("<button></button>", {
    //    text: "Click to see answer",
    //    class: "faqControl"
    //}).appendTo("dt");

    $(".mainData")
      .click(function () {
          $(this).next("dd").slideToggle();
      });

    $(".faqControl")
        .focus(function () {
            $(this).parent().next("dd").slideDown();
        });

    $(".faqControl")
        .blur(function () {
            $(this).parent().next("dd").slideUp();
        });
</script>

