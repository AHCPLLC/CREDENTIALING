        overflow: hidden;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
    }

    .mainData:after {
        content: "";
        display: table;
        clear: both;
    }

    .faqControl {
        float: right;
        line-height: 30px;
        border: none;
        padding: 0 20px;
        color: white;
        text-shadow: 0 -1px rgba(0, 0, 0, 0.5);
        background: none;
        font-size: 0.8em;
        border-left: 1px solid rgba(255, 255, 255, 0.5);
    }

        .faqControl:hover, .faqControl:focus {
            outline: none;
            /*background-image: url('data:image/svg+xml;base64,');*/
            background-size: 100%;
            /*background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #9999ff), color-stop(100%, #8585ff));
