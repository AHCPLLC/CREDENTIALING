            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3 zero-padding-left-right">
                    <input type="radio" name="DateType" class="normal-radio" value="Date Of Service" id="DateOfSeviceDateType" Checked>
                    <label for="DateOfSeviceDateType"><span></span>Date Of Service</label>
                </div>
                <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                    <input type="radio" name="DateType" class="normal-radio" value="Payment Date" id="paymentDate">
                    <label for="paymentDate"><span> </span>Payment Date</label>
                </div>
                <div class="DOSContainer">
                    <div class="col-lg-2 zero-padding-left-right">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <label for="fullname" class="text-initial">DOS From</label>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <input class="form-control input-xs non_mandatory_field_halo" name="DOSFromEOB" placeholder="From">
                        </div>
                    </div>

                    <div class="col-lg-2 zero-padding-left-right">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <label class="text-initial">DOS To</label>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <input class="form-control input-xs non_mandatory_field_halo" name="DOSToEOB" placeholder="To">
                        </div>
                    </div>
                </div>

                <div class="PaymentDateContainer" hidden>
                    <div class="col-lg-2 zero-padding-left-right">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <label for="fullname" class="text-initial">Payment Date From</label>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <input class="form-control input-xs non_mandatory_field_halo" name="PDFromEOB" placeholder="From">
                        </div>
                    </div>

                    <div class="col-lg-2 zero-padding-left-right">
