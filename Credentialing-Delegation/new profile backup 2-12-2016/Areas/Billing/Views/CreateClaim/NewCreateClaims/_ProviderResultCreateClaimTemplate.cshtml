@model List<PortalTemplate.Areas.Billing.Models.CreateClaim.CreateClaimTemplate.ProviderResult>
<div id ="providers_title">

</div>
<table id="ProvidersResult" class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
    <thead class="theme_thead">
        <tr>
            <th>NPI</th>
            <th>Name</th>
            <th>Address</th>
            <th>Tax ID</th>
        </tr>

    </thead>
    <!-- ngIf: selectedMemberList.length>0 -->
    <tbody>
        @foreach (var item in Model)
        {
            <tr data-container="@item.ProviderId">
                <td>
                    @item.ProviderNPI
                </td>
                <td class="ProviderName">
                   <a href="javascript:void(0)"> @item.ProviderFirstName @item.ProviderMiddleName @item.ProviderLastOrOrganizationName</a>
                </td>
                <td class="ProviderAddress">
                    @item.ProviderFirstAddress @item.ProviderSecondAddress
                </td>
                <td>
                    @item.ProviderTaxonomy
                </td>
            </tr>
        }
    </tbody>

</table>

<script src="~/Areas/Billing/Scripts/AppJs/CreateClaims/Unminified/new/ProviderResultCreateClaimTemplate.js"></script>