@model PortalTemplate.Areas.Billing.Models.CreateClaim.ProviderMemberSelectViewModel
<div class="x_panel border_on_panel" id="providerMemberSelect">
    <div class="x_content">
        <div class="col-lg-12">
            <label class="title_text">Select Member</label>
        </div>
        @Html.Partial("~/Areas/Billing/Views/CreateClaim/NewCreateClaims/_RenderingProviderInfo.cshtml", Model.ProviderResult)

        <fieldset class="fsStyle createClaim">
            <legend class="legendStyle">
                <span class="text-initial title_heading">Members</span>
            </legend>
            <div class="panel-body col-lg-12">
                @Html.Partial("~/Areas/Billing/Views/CreateClaim/_MemberResult.cshtml", Model.MemberResult)
            </div>
        </fieldset>
    </div>

</div>
<p-script filepath="~/Areas/Billing/Scripts/AppJs/CreateClaims/Unminified/new/RenderingProviderInfo.js"></p-script>
