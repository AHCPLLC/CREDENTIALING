@model List<PortalTemplate.Areas.Billing.Models.CreateClaim.MemberResultViewModel>


        <table id="MemberResult" class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
            <thead class="theme_thead">
                <tr>
                    <th>Subscriber ID</th>
                    <th>Name</th>
                    <th>DOB</th>
                    <th>Address</th>
                    <th>PCP</th>
                    <th>Plan Name</th>
                    <th>Start Date</th>
                    <th>End Date</th>
                </tr>
            </thead>
            <tbody>
                @for (var i = 0; i < Model.Count; i++)
                {
                    <tr data-container="@Model[i].SubscriberID">
                        <td>@Model[i].SubscriberID </td>
                        <td><a href="javascript:void(0)">@Model[i].MemberFullName</a>  </td>
                        <td>@Html.DisplayFor(x => Model[i].DateOfBirth)</td>
                        <td>@Model[i].MemberFullAddress</td>
                        <td>@Model[i].PCPFullName</td>
                        <td>@Model[i].PlanName</td>
                        <td>@Html.DisplayFor(x => Model[i].PlanEffectiveDate)</td>
                        <td>@Html.DisplayFor(x => Model[i].PlanTerminationDate)</td>
                    </tr>
                }
            </tbody>
        </table>
<script src="~/Areas/Billing/Scripts/AppJs/CreateClaims/Unminified/MemberResult.js"></script>
