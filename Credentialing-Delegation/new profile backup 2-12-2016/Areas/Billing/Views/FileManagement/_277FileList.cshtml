@using PratianUI
@model List<PortalTemplate.Areas.Billing.Models.File_Management.File277ViewModel>


<div class="x_panel">
    <div class="x_content">
        <div class="col-lg-12 clearfix file277HeaderPart">
            <label class="title_text pull-left file277HeaderLabel">Claim Acknowledged & Claims Pending 277 Files</label>

            <span class="customCheckBoxFLoat">
                @Html.PratianUiCheckbox("Claim Acknowledged", new {@name = "ClaimAkg277", @value="Claim Acknowledged", @checked = "checked" })
                @Html.PratianUiCheckbox("Claims Pending", new { @class = "Input277", @name = "ClaimPending277", @value = "Claims Pending", @checked = "checked" })
            </span>
        </div>
    </div>
    <div class="x_content border_on_panel">
        <div class="x_content">

            <table id="277FileList" class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                <thead>
                    <tr class="theme_thead">
                        <th>Control Number </th>
                        <th>File Name</th>
                        <th>Sender</th>
                        <th>Receiver</th>
                        <th>Date of Creation</th>
                        <th>Received Date</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @Html.Partial("~/Areas/Billing/Views/FileManagement/_TBody277FileList.cshtml", Model)
                </tbody>
            </table>
        </div>
    </div>
</div>

<script src="~/Areas/Billing/Scripts/AppJs/FileManagement/Unminified/File277.js"></script>