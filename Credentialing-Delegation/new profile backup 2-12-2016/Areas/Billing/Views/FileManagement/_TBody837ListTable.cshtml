@model List<PortalTemplate.Areas.Billing.Models.File_Management.File837ViewModel>

@foreach (var item in Model)
{
    <tr data-container="@item.FileName">
        @*<td>@( ((Int32)1) + @Model.IndexOf(item))</td>*@
        <td>@item.ControlNumber</td>
        <td>@item.FileName</td>
        <td>@item.Sender</td>
        <td>@item.Receiver</td>
        <td>@item.DateOfCreation.Value.ToShortDateString()</td>
        <td>@item.AgeInDays</td>
        <td>@item.Status</td>
        <td>
            <button class="btn btn-info btn-xs 837Viewbtn" data-toggle="tooltip" title="View"><i class="fa fa-eye"></i></button>
            <a target="_blank" class="btn btn-info btn-xs BillingFMDownload" data-toggle="tooltip" title="Download" href="/Billing/FileManagement/DownloadFile?FileKey=@item.FileKey&ApplicaionOrGroupName=@item.ApplicaionOrGroupName&RegisterUserName=@item.RegisterUserName&FileName=@item.FileName"><i class="fa fa-download"></i></a>
            @*<button class="copy-button margin_left_12px pull-right 837Viewbtn">View</button>
                <button class="copy-button margin_left_12px  download_button_width_60 pull-right">Download</button>*@
        </td>
    </tr>
}
