@using PratianUI
@model List<PortalTemplate.Areas.Billing.Models.File_Management.File837ViewModel>
<div class="x_panel ">
    <div id="Table837List">
        <div class="x_content">
            <div class="col-lg-12 clearfix file837HeaderPart">
                <label class="title_text pull-left file837HeaderLabel">Received & Dispatched 837 Files</label>

                <span class="customCheckBoxFLoat">
                    @Html.PratianUiCheckbox("Received", new { name = "ReceivedList837", value = "Received", @checked = "checked" })
                    @Html.PratianUiCheckbox("Dispatched", new { name = "DispatchedList837", value = "Dispatched", @checked = "checked" })
                </span>
            </div>
        </div>
        <div class="x_content border_on_panel">
            <div class="x_content">
                <div class="table-responsive">
                    <table id="837FileList" class="table table-striped table-hover  custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                        <thead class="theme_thead">
                            <tr>
                                <th>Control Number </th>
                                <th>File Name</th>
                                <th>Sender</th>
                                <th>Receiver</th>
                                <th>Date of Creation</th>
                                <th>Age(days)</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @Html.Partial("~/Areas/Billing/Views/FileManagement/_TBody837ListTable.cshtml", Model)
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div id="837claimList">

    </div>
</div>
<script src="~/Areas/Billing/Scripts/AppJs/FileManagement/Unminified/tableEventfor837.js"></script>
