@using PratianUI
@model List<PortalTemplate.Areas.Billing.Models.File_Management.File835>
<div class="x_panel ">
    <div id="File835ListDiv">
        <div class="x_content">
            <div class="col-lg-12 clearfix file835HeaderPart">
                <label class="title_text pull-left file835HeaderLabel">Received & Dispatched 835 Files</label>

                <span class="customCheckBoxFLoat">
                    @Html.PratianUiCheckbox("Received", new { name = "Received835", value = "Received", @checked = "checked" })
                    @Html.PratianUiCheckbox("Dispatched", new { name = "Generated835", value = "Dispatched", @checked = "checked" })
                </span>
            </div>
        </div>
        <div class="x_content border_on_panel">
            <div class="x_content">
                <div id="835fileTable" class="table-responsive">
                    <table id="835ListfileTable" class="table table-striped table-hover custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                        <thead class="theme_thead">
                            <tr>
                                <th>
                                    Payer Name
                                </th>
                                <th>
                                    Payee Name
                                </th>
                                <th>
                                    Payee ID
                                </th>
                                <th>
                                    Check Issue Date
                                </th>
                                <th>
                                    Total Payment Amount
                                </th>
                                <th>
                                    Check Number
                                </th>
                                <th>
                                    File Name
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @Html.Partial("~/Areas/Billing/Views/FileManagement/_TBody835ListTable.cshtml", Model)
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
    <div id="835ProviderList">

    </div>

    <div id="835EOBList">

    </div>
</div>
<script src="~/Areas/Billing/Scripts/AppJs/FileManagement/Unminified/TableEventFor835.js"></script>

