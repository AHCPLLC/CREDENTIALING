@using PortalTemplate.Areas.ETL.Models.EdwLogger;
<a href="#" class="pull-right" style="text-decoration:none;" onclick="EdwDataBaseExportToExcel()" tooltip="" data-toggle="tooltip" data-placement="top" title="Export to Excel" data-original-title="Export to Excel">
    <img src="~/Areas/ETL/Resources/Images/ssis-export-excel-file-task.png" style="width:40px; height:40px;" />
</a>
<h5>Total Number of databases :@ViewData["_EdwDatabaseLength"] </h5>
<br />
<table id="EdwDatabaseTable" class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom table-striped">
    <thead class="theme_thead">
        <tr>
            <th>
                S No.
            </th>
            <th>
                Data Base Name
            </th>
            <th>
                Size (MB)
            </th>
            <th>
                Size (GB)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var EdwData in (List<EdwDatabases>)ViewData["_EdwDatabases"])
        {
            <tr>
                <td>@EdwData.SNo</td>
                <td style="word-wrap: break-word">@EdwData.DataBaseName</td>
                <td>@EdwData.SizeMb</td>
                <td>@EdwData.SizeGb</td>
            </tr>
        }

    </tbody>
</table>


