@model PortalTemplate.Areas.Encounters.Models.ScheduleCRUD.ViewScheduleViewModel
<div class="margin-top-5px" id="ViewSchedule">
    <div class="col-lg-12">
        <div class="x_panel border_on_panel">
            <div class="x_content">
                <div class="col-lg-12">
                    <label class="title_text">Schedule Details</label>
                </div>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 margin_top_-3px">
                        <fieldset class="fieldsetStyle">
                            <legend class="legendstyle">
                                Member
                            </legend>
                            <div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                    @Html.LabelFor(m => m.SubscriberID, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.SubscriberID</label></div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                    @Html.LabelFor(m => m.MemberFirstName, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.MemberFirstName</label></div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                    @Html.LabelFor(m => m.MemberLastName, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.MemberLastName</label></div>
                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                                    @Html.LabelFor(m => m.Gender, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.Gender</label></div>
                                </div>
                                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                                    @Html.LabelFor(m => m.DateOfBirth, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.DateOfBirth.Value.ToShortDateString()</label></div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                    @Html.LabelFor(m => m.Address, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.Address</label></div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                    @Html.LabelFor(m => m.PlanName, new { @class = "label-font-normal" })
                                    <div><label class="uppercase">@Model.PlanName</label></div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <fieldset class="fieldsetStyle">
                            <legend class="legendstyle">Service Information</legend>
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        Rendering Provider
                                        <div class="ProviderLabel">
                                            <label class="uppercase" id="SelectedReferingProvider">@Model.RenderingProviderName</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        Referring Provider
                                        <div class="ProviderLabel">
                                            <label class="uppercase" id="SelectedReferingProvider">@Model.ReferringProviderName</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        Billing Provider
                                        <div class="ProviderLabel">
                                            <label class="uppercase" id="SelectedBillingProvider">@Model.BillingProviderName</label>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        Service Facility
                                        <div class="ProviderLabel">
                                            <label class="uppercase" id="SelectedFacility">@Model.ServiceFacilityName</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row" style="margin-top:5px;">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        @Html.LabelFor(m => m.ScheduleDate, new { @class = "label-font-normal" })
                                        <div class="editelements">
                                            @Model.ScheduleDate.Value.ToShortDateString()
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        @Html.LabelFor(m => m.ScheduleTime, new { @class = "label-font-normal" })
                                        <div class="editelements">
                                            @Model.ScheduleTime
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        @Html.LabelFor(m => m.VisitCategory, new { @class = "label-font-normal" })
                                        <div class="editelements">
                                            @Model.VisitCategory
                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                        @Html.LabelFor(m => m.VisitLength, new { @class = "label-font-normal" })
                                        <div class="editelements">
                                            @Model.VisitLength
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row" style="margin-top:5px;">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <div class="col-lg-6 col-md-3 col-sm-3 col-xs-3">
                                        @Html.LabelFor(m => m.VisitReason, new { @class = "label-font-normal" })
                                        <div class="editelements">
                                            @Model.VisitReason
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12">
        <a class="btn btn-sm btn-warning pull-left tab-navigation member-actions encounterTemplateItem" id="BackScheduleDetails">Back</a>  
        <button type="submit" class="btn btn-sm btn-primary pull-right member-actions editelements encounterTemplateItem proceedToEncounter">Edit</button>
    </div>
</div>
