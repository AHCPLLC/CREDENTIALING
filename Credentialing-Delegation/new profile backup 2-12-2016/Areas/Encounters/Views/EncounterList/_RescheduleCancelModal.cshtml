@*Reschedule Cancel Modal*@
<div class="modal fade" tabindex="-1" role="dialog" id="reschedule_cancelModal">
    <div class="modal-dialog modal-md">
        <form class="modal-content">
            <div class="modal-header bg-primary">
                <div class="col-md-12 col-lg-12">
                    <i class="fa fa-question-circle"></i>
                    <label> Do You Want to Re_Schedule this Encounter</label>
                    <button type="button" class="btn btn-xs btn-danger pull-right" data-dismiss="modal"><i class="fa fa-close"></i></button>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            Re-Schedule Date
                            <div>
                                <input type="text" placeholder="MM/DD/YYYY HH:MM:SS" class="form-control input-xs non_mandatory_field_halo">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-danger pull-left" data-dismiss="modal">No</button>
                <button class="btn btn-sm btn-success pull-right" data-dismiss="modal">Save</button>
            </div>
        </form>
    </div>
</div>
