@model IEnumerable<PortalTemplate.Areas.Encounters.Models.EncounterList.EncounterRejectionListViewModel>

<div class="x_panel border_on_panel">
    <div class="x_panel form_Create">
        <div class="x_content">
            <div class="col-lg-12">
                <label class="title_text">Inactive Encounters</label>
            </div>
            <div class="col-lg-12 table-responsive">
                <table id="InactiveEncounterTable" class="table table-striped table-hover custom-thead-back custom-thead-font custom-tbody custom-table-striped p_body_data_background">
                    <thead class="theme_thead">
                        <tr>
                            <th>
                                <label>Enctr Id</label>
                            </th>
                            <th>
                                <label>Mbr Id</label>
                            </th>
                            <th>
                                <label>Mbr Name</label>
                            </th>
                            <th>
                                <label>Prv NPI</label>
                            </th>
                            <th>
                                <label>Prv Name</label>
                            </th>
                            <th>
                                <label>Facility</label>
                            </th>
                            <th>
                                <label>DOS</label>
                            </th>
                            <th>
                                <label>DOC</label>
                            </th>
                            <th>
                                <label>Created By</label>
                            </th>
                            <th>
                                <label>Enctr Type</label>
                            </th>
                            <th>
                                <label>Reason</label>
                            </th>
                            <th>
                                <label>Status</label>
                            </th>
                            <th>
                                <label>Action</label>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.EncounterID </td>
                                <td>@item.SubscriberID</td>
                                <td>@item.MemberLastName @item.MemberFirstName</td>
                                <td>@item.ProviderNPI</td>
                                <td>@item.ProviderLastName @item.ProviderFirstName</td>
                                <td>@item.FacilityName</td>
                                <td>@item.DateOfCreation.Value.ToShortDateString()</td>
                                <td>@item.DateOfService.Value.ToShortDateString()</td>
                                <td>@item.CreatedBy</td>
                                <td>@item.EncounterType</td>
                                <td>@item.Reason</td>
                                <td>@item.EncounterStatus</td>
                                <td class="displaytd">
                                    <a data-toggle="tooltip" data-placement="top" title="" class="p_view_btn btn btn-xs tab-navigation member-actions" data-tab-val="ViewEncounter" data-tab-action="View Encounter" data-tab-title="View #@item.EncounterID" data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="/Encounters/EncounterCRUD/ViewEncounter" data-original-title="View Encounter"><i class="fa fa-eye"></i></a>
                                    @*<a data-toggle="tooltip" title="" class="p_edit_btn btn btn-xs tab-navigation member-actions" data-tab-val="EditEncounter" data-tab-action="Edit Encounter" data-tab-title="Edit #116244951 " data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="/Encounters/EncounterCRUD/EditEncounter" data-original-title="Edit Encounter"><i class="fa fa-edit"></i></a>
                                        <a data-toggle="tooltip" title="" class="p_delete_btn btn btn-xs" onclick="showCancelModal(116244951 )" data-original-title="Cancel Encounter"><i class="fa fa-close"></i></a>*@
                                    <a data-toggle="tooltip" title="" class="p_cancel_btn btn btn-xs" onclick="showRescheduleModal(116244951 )" data-original-title="ReSchedule Encounter"><i class="fa fa-history"></i></a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script>
    $('#InactiveEncounterTable').prtGrid({
        url: "/EncounterList/GetEncounterRejectionListByIndex",
        dataLength: 50,
        columns: [
            { type: 'text', name: 'EncounterID', text: 'Enctr Id', widthPercentage: 10, sortable: false },
        { type: 'text', name: 'SubscriberID', text: 'Mbr Id', widthPercentage: 10, sortable: false },
        { type: 'text', name: 'MemberLastName', text: 'Mbr Name', widthPercentage: 10, sortable: true },
        { type: 'text', name: 'ProviderNPI', text: 'Prv NPI', widthPercentage: 10, sortable: true },
        { type: 'text', name: 'ProviderLastName', text: 'Prv Name', widthPercentage: 10, sortable: true },
        { type: 'text', name: 'FacilityName', text: 'Facility', widthPercentage: 10, sortable: true },
         { type: 'date', name: 'DateOfCreation', text: 'DOC', widthPercentage: 5, sortable: true },
        { type: 'date', name: 'DateOfService', text: 'DOS', widthPercentage: 5, sortable: true },
        { type: 'text', name: 'CreatedBy', text: 'Created By', widthPercentage: 5, sortable: true },
        { type: 'text', name: 'EncounterType', text: 'Enctr Type', widthPercentage: 5, sortable: true },
         { type: 'text', name: 'Reason', text: 'Reason', widthPercentage: 5, sortable: true },
        { type: 'text', name: 'EncounterStatus', text: 'Status', widthPercentage: 5, sortable: true },
        { type: 'none', name: '', text: '', widthPercentage: 10, sortable: false }]
    });
</script>