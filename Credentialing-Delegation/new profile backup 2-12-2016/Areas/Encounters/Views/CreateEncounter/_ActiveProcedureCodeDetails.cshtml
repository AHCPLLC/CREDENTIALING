@model List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel>

@{
    var CountPerList = (int)(Model.Count() / 3);

    List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel> leftList = new List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel>();
    List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel> middleList = new List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel>();
    List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel> rightList = new List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.ActiveProcedureCodeViewModel>();

    foreach (var item in Model.Select((value, i) => new { i, value }))
    {
        if (item.i <= (CountPerList-1))
        {
            leftList.Add(item.value);
        }
        else if (item.i > (CountPerList-1) && item.i <= ((2 *CountPerList)-1))
        {
            middleList.Add(item.value);
        }
        else
        {
            rightList.Add(item.value);
        }
    }
}
<fieldset class="fieldsetStyle">
    <legend class="legendstyle">Member Active Procedure Codes</legend>
    <div class="row">
        <div class="col-lg-12">
            <div class="col-lg-1 pull-right">
                <span class="custombadge outertabclose outertabclose-red activeCptCodes pull-right"><i class="fa fa-times"></i></span>
            </div>
            <div class=" col-lg-2 col-md-4 col-sm-6 col-xs-12  pull-right">
                <button class="btn btn-xs btn-primary margin_left_12px" id="cptCodeDescription">Search</button>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12  pull-right">
                <input type="text" class="form-control input-xs " placeholder="Search By CPT Code/Description" name="icdCodeDescription">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="col-lg-4 zero-padding-left-right">
                <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                    <thead class="theme_thead">
                        <tr>
                            <th></th>
                            <th>Code </th>
                            <th>Description </th>

                        </tr>
                    </thead>
                    <tbody id="cptCodeSearch1">
                        @foreach (var item in leftList)
                        {
                            <tr>
                                <td><input class="normal-checkbox cptCode" name="icdCode" type="checkbox" value="86152"><label><span></span></label></td>
                                <td>@item.CPTCode</td>
                                <td>@item.CPTDescription</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div class="col-lg-4 zero-padding-left-right">
                <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                    <thead class="theme_thead">
                        <tr>
                            <th></th>
                            <th>Code </th>
                            <th>Description </th>

                        </tr>
                    </thead>
                    <tbody id="cptCodeSearch2">
                        @foreach (var item in middleList)
                        {
                            <tr>
                                <td><input class="normal-checkbox cptCode" name="icdCode" type="checkbox" value="86152"><label><span></span></label></td>
                                <td>@item.CPTCode</td>
                                <td>@item.CPTDescription</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
            <div class="col-lg-4 zero-padding-left-right">
                <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                    <thead class="theme_thead">
                        <tr>
                            <th></th>
                            <th>Code </th>
                            <th>Description </th>

                        </tr>
                    </thead>
                    <tbody id="cptCodeSearch3">
                        @foreach (var item in rightList)
                        {
                            <tr>
                                <td><input class="normal-checkbox cptCode" name="icdCode" type="checkbox" value="86152"><label><span></span></label></td>
                                <td>@item.CPTCode</td>
                                <td>@item.CPTDescription</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</fieldset>