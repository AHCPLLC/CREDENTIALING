@model List<PortalTemplate.Areas.Encounters.Models.CreateEncounter.FacilityListViewModel>
<div id="providers_title"></div>
<table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped p_body_data_background" id="readytocodeid" style="margin-bottom: 0px;" cellspacing="0">
    <thead class="theme_thead">
        <tr>
            <th>Facility ID</th>
            <th>Facility Name</th>
            <th>Facility Address</th>           
        </tr>
    </thead>
    <tbody id="Facilities">

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FacilityID</td>
                <td class="ProviderName">@item.FacilityName</td>
                <td>@item.FacilityAddress</td>
            </tr>
        }


    </tbody>
</table>


<script src="~/Areas/Encounters/Scripts/AppJs/CreateEncounter/Unminified/FacilityList.js"></script>