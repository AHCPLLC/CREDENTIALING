@model PortalTemplate.Areas.Encounters.Models.EncounterViewModel

@{
    List<SelectListItem> Status = new List<SelectListItem>();
    Status.Add(new SelectListItem
      {
          Text = "Open",
          Value = "Open",
          Selected = true
      });
    Status.Add(new SelectListItem
    {
        Text = "Active",
        Value = "Active"

    });
    Status.Add(new SelectListItem
    {
        Text = "No Show",
        Value = "NoShow"
    });
    Status.Add(new SelectListItem
    {
        Text = "Re-Schedule",
        Value = "ReSchedule"
    });
    Status.Add(new SelectListItem
    {
        Text = "Ready To Code",
        Value = "ReadyToCode"
    });
}
<div class="x_panel border_on_panel">
    <div class="x_content">

        <div class="col-lg-12">
            <label class="title_text">Encounter Details</label>
            @*<a onclick="toggleCollapseEncounter()" class="btn btn-xs btn-info pull-right encounterCollapseShow"><span class="fa fa-list"></span></a>*@
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 marginTop_-3px">
                <fieldset class="fieldsetStyle">
                    <legend class="legendstyle">Member</legend>
                    <div>
                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                            Subscriber Id
                            <div><label class="uppercase">@Model.MemberInfo.SubscriberID</label></div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                            Member First Name
                            <div><label class="uppercase">@Model.MemberInfo.MemberFirstName</label></div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                            Member Last Name
                            <div><label class="uppercase">@Model.MemberInfo.MemberLastName</label></div>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                            Gender
                            <div><label class="uppercase">@Model.MemberInfo.Gender</label></div>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                            DOB
                            <div><label class="uppercase">@Model.MemberInfo.DateOfBirth</label></div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                            Address
                            <div><label class="uppercase">@Model.MemberInfo.Address</label></div>
                        </div>
                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                            Plan Name
                            <div><label class="uppercase">@Model.MemberInfo.Plan.PlanName</label></div>
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 marginTop_-3px">
                <fieldset class="fieldsetStyle paddingBottom_12px">
                    <legend class="legendstyle">Service Information</legend>
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Rendering Provider
                            <div>
                                <label class="uppercase">Pariksith Singh</label>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Referring Provider
                            <div>
                                <label class="uppercase">Pariksith Singh</label>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Billing Provider
                            <div>
                                <label class="uppercase">Pariksith Singh</label>
                            </div>
                        </div>

                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Service Facility
                            <div><label class="uppercase">1094 North Cliffe Dlve</label></div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            DOS From
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.DateOfServiceFrom, new { @class = "form-control input-xs non_mandatory_field_halo uppercase", placeholder = "mm/dd/yyyy" })
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            DOS To
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.DateOfServiceTo, new { @class = "form-control input-xs non_mandatory_field_halo uppercase", placeholder = "mm/dd/yyyy" })
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Place of Service
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.FacilityInfo.FacilityName, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })
                                <datalist id="pos" class="uppercase">
                                    <option value="21-Inpatient Hospital">
                                    <option value="11-Office">
                                    <option value="22-Outpatient Hospital">
                                    <option value="31-SNF">
                                </datalist>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Next Appointment Date
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.ServiceInfo.NextAppointmentDate, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Check In Time
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.ServiceInfo.CheckInTime, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })

                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Check Out Time
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.ServiceInfo.CheckOutTime, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })

                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Visit Length
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.ServiceInfo.VisitLength, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })
                                <datalist id="Visitlength" class="uppercase">
                                    <option value="15 minutes">
                                    <option value="30 minutes">
                                    <option value="45 minutes">
                                    <option value="60 minutes">
                                </datalist>
                            </div>
                        </div>

                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                            Visit Reason
                            <div class="editelements">
                                @Html.TextBoxFor(model => model.ServiceInfo.ReasonForVisit, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <fieldset class="fieldsetStyle">
                                <div class="row">
                                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                        Encounter Type
                                        <div>
                                            @*@Html.PratianUiMinimalRadioFor(model => model.EncounterType, new { color = "green", @id="HMO", @value = "HMO" })*@
                                            @Html.RadioButtonFor(model => model.EncounterType, "HMO", new { @class = "normal-radio", @id = "HMO" })
                                            <label for="HMO"><span></span>HMO</label>
                                            @Html.RadioButtonFor(model => model.EncounterType, "Medicare", new { @class = "normal-radio", @id = "Medicare", @checked = "checked" })
                                            <label for="Medicare"><span></span>Medicare</label>
                                        </div>
                                    </div>
                                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                        Status
                                        <div>
                                            @Html.DropDownListFor(model => model.EncounterStatus, Status, new { @class = "form-control input-xs non_mandatory_field_halo dropdown_selector text-uppercase" })


                                        </div>
                                    </div>
                                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
                                        Notes
                                        <div>
                                            @Html.TextAreaFor(model => model.EncounterNotes, new { @class = "form-control input-xs non_mandatory_field_halo uppercase" })

                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </div>
                    </div>
                </fieldset>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
                @Html.Partial("~/Areas/Encounters/Views/Documents/_DocumentsAdd.cshtml", Model.Documents)
            </div>
        </div>
        <a class="view-button pull-right tab-navigation member-actions" data-tab-val="Edit Encounter" data-tab-action="Edit Encounter" data-tab-title="Edit #116244951" data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="~/Areas/Encounters/Views/EncounterCRUD/_EditEncounter.cshtml">Edit</a>
        <a class="btn btn-sm btn-warning pull-left tab-navigation member-actions encounterTemplateItem">Back</a>
    </div>
</div>
