                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    @Html.LabelFor(m => m.RenderingProviderLastOrOrganizationName, new { @class = "font-normal" })
                    <div><label class="uppercase">@Model.RenderingProviderLastOrOrganizationName</label></div>
                </div>
