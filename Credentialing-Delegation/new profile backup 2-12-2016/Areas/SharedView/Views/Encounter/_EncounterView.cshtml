@model PortalTemplate.Areas.SharedView.Models.Encounter.EncounterDetailsViewModel
@{
    List<SelectListItem> Status = new List<SelectListItem>();
    Status.Add(new SelectListItem
      {
          Text = "Open",
          Value = "Open",
          Selected = true
      });
    Status.Add(new SelectListItem
    {
        Text = "Active",
        Value = "Active"

    });
    Status.Add(new SelectListItem
    {
        Text = "No Show",
        Value = "NoShow"
    });
    Status.Add(new SelectListItem
    {
        Text = "Re-Schedule",
        Value = "ReSchedule"
    });
    Status.Add(new SelectListItem
    {
        Text = "Ready To Code",
        Value = "ReadyToCode"
    });
}
@Html.Partial("~/Areas/SharedView/Views/Common/_DocumentViewer.cshtml")
