@model PortalTemplate.Areas.Coding.Models.CreateCoding.NotesViewModel
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
            <thead class="theme_thead">
                <tr>
                    <th>SNO </th>
                    <th>Category </th>
                    <th>Title</th>
                    <th>Description</th>
                    <th>Module</th>
                    <th>Notify</th>
                    <th>Added By</th>
                    <th>Added On</th>
                </tr>
            </thead>
            <tbody>
                @{
                    for (var i = 0; i < Model.CodingNotes.Count; i++)
                    {
                        <tr>
                            <td>@((@i) + 1)</td>
                            <td>
                                @Model.CodingNotes[i].Category
                            </td>
                            <td>
                               @Model.CodingNotes[i].Title
                            </td>
                            <td>
                                @Model.CodingNotes[i].Description
                            </td>
                            <td>
                                @Model.CodingNotes[i].Module
                            </td>
                            <td>
                                @Model.CodingNotes[i].Notify
                            </td>
                            <td>
                                @Model.CodingNotes[i].AddedBy
                            </td>
                            <td>
                                @Model.CodingNotes[i].AddedOn
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>