@model PortalTemplate.Areas.Coding.Models.CreateCoding.CPTCodeDetailsViewModel

<div class="col-lg-12 CPTCodesMainPanel_Create">
    <fieldset class="fieldsetStyle">
        <legend class="legendstyle">Procedure Codes</legend>
        <div class="row">
            <div class="col-lg-12">
                <button type="button" class="pull-right btn btn-xs btn-info activeCptCodes">Active Procedure Codes</button>
            </div>
        </div>

        <div class="col-lg-12 padding_icd" hidden id="historyCptCodes">
        </div>

        <div class="row">
            <div class="col-lg-12">
                <table class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped">
                    <thead class="theme_thead">
                        <tr>
                            <th>Code </th>
                            <th>Description </th>
                            <th>Modifier</th>
                            <th>Diagnosis Pointers</th>
                            <th>Fee </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody id="cptCodeBody">
                        <tr>
                            <td>
                                @Html.TextBoxFor(model => model.CPTCodes[0].Code, new { @class = "form-control input-xs text-uppercase" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Code)
                            </td>
                            <td>
                                @Html.TextBoxFor(model => model.CPTCodes[0].Description, new { @class = "form-control input-xs text-uppercase" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Description)
                            </td>
                            <td>
                                @Html.TextBoxFor(model => model.CPTCodes[0].Modifier1, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Modifier1)
                                @Html.TextBoxFor(model => model.CPTCodes[0].Modifier2, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Modifier2)
                                @Html.TextBoxFor(model => model.CPTCodes[0].Modifier3, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Modifier3)
                                @Html.TextBoxFor(model => model.CPTCodes[0].Modifier4, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].Modifier4)
                            </td>
                            <td>
                                @Html.TextBoxFor(model => model.CPTCodes[0].DiagnosisPointer1, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].DiagnosisPointer1)
                                @Html.TextBoxFor(model => model.CPTCodes[0].DiagnosisPointer2, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].DiagnosisPointer2)
                                @Html.TextBoxFor(model => model.CPTCodes[0].DiagnosisPointer3, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].DiagnosisPointer3)
                                @Html.TextBoxFor(model => model.CPTCodes[0].DiagnosisPointer4, new { @class = "form-control input-xs text-uppercase modifier_width" })
                                @Html.ValidationMessageFor(model => model.CPTCodes[0].DiagnosisPointer4)
                                <input class="form-control input-xs text-uppercase modifier_width JustForDP1" placeholder="" type="text" value="">                             
                                <a class="btn btn-xs openDiagnosisModal"><i class=" fa fa-forward"></i></a>
                            </td>
                            <td><input class="form-control input-xs text-uppercase" placeholder="" type="text" value=""></td>
                            <td></td>
                        </tr>
                    </tbody>
                </table>
                <button type="button" class="light-green-button pull-right" id="AddNewRowInCPTTable"><i class="fa fa-plus"></i></button>
            </div>
        </div>
    </fieldset>
</div>


<div class="modal fade" tabindex="-1" role="dialog" id="DiagnosisModal_createCoding" style="overflow: auto; top: 8%;">
        <div class="modal-dialog modal-md ">
            <form class="modal-content">
                <div class="modal-header bg-primary">
                    <div class="col-md-12 col-lg-12">
                        <label>Diagnosis Pointers</label>
                        <button type="button" class="btn btn-xs btn-danger pull-right" data-dismiss="modal"><i class="fa fa-close"></i></button>
                    </div>
                </div>
                <div class="modal-body">
                    <div class="row" id="ICDModalvalues">
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-sm btn-danger pull-left" data-dismiss="modal">Cancel</button>
                    <button class="btn btn-sm btn-success pull-right" data-dismiss="modal">Save</button>
                </div>
            </form>
        </div>
    </div>
<p-script filepath="~/Areas/Coding/Scripts/AppJs/CreateCoding/NonMinified/CPTCodesInfo.js"></p-script>