@model List<PortalTemplate.Areas.Coding.Models.CodingList.CodingListViewModel>

<script>
    var OpenEncountersJsonData = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model));
</script>

<div class="x_panel border_on_panel">
    <div class="x_content">
        <div class="col-lg-12">
            <label class="title_text">Open Encounters</label>
            <div class="pull-right">
                @*@Html.ExportOptions("print", "pdf", "excel")*@
            </div>
        </div>
        <table id="OpenCodingList" class="table table-striped table-hover table-responsive custom-thead-back custom-thead-font custom-tbody custom-table-striped p_body_data_background">
            <thead class="theme_thead">
                <tr>
                    <th>
                        <label>Enctr Id</label>
                    </th>
                    <th>
                        <label>Mbr Id</label>
                    </th>
                    <th>
                        <label>Mbr LName</label>
                    </th>
                    <th>
                        <label>Mbr FName</label>
                    </th>
                    <th>
                        <label>Prv NPI</label>
                    </th>
                    <th>
                        <label>Prv LName</label>
                    </th>
                    <th>
                        <label>Prv FName</label>
                    </th>
                    <th>
                        <label>Facility</label>
                    </th>
                    <th>
                        <label>DOS</label>
                    </th>
                    <th>
                        <label>DOC</label>
                    </th>
                    <th>
                        <label>Created By</label>
                    </th>
                    <th>
                        <label>Enctr Type</label>
                    </th>
                    <th>
                        <label>Status</label>
                    </th>
                    <th>
                        <label>Action</label>
                    </th>
                </tr>
            </thead>

            <tbody>

                @for (var i = 0; i < @Model.Count; i++)
                {
                    <tr>
                        <td>@Model[i].EncounterID</td>
                        <td>@Model[i].MemberID</td>
                        <td>@Model[i].MemberLastName</td>
                        <td>@Model[i].MemberFirstName</td>
                        <td>@Model[i].ProviderNPI</td>
                        <td>@Model[i].ProviderLastName</td>
                        <td>@Model[i].ProviderFirstName</td>
                        <td>@Model[i].Facility</td>
                        <td>@Html.DisplayFor(x => @Model[i].DateOfService)</td>
                        <td>@Html.DisplayFor(x => @Model[i].DateOfCreation)</td>
                        <td>@Model[i].CreatedBy</td>
                        <td>@Model[i].EncounterType</td>
                        <td>@Model[i].Status</td>
                        <td>
                            <a data-toggle="tooltip" title="View Encounter" class="p_view_btn btn btn-xs tab-navigation member-actions" data-tab-val="ViewEncounter" data-tab-action="View Encounter" data-tab-title="Code View #@Model[i].EncounterID" data-tab-container="fullBodyContainer" data-tab-autoflush="true" data-tab-path="/Coding/Coding/ViewDetails"><i class="fa fa-eye"></i></a>
                            <a data-toggle="tooltip" title="Edit Encounter" class="p_edit_btn btn  btn-xs tab-navigation member-actions" data-tab-val="EditEncounter" data-tab-action="Edit Encounter" data-tab-title="Code #@Model[i].EncounterID" data-tab-container="fullBodyContainer" data-tab-autoflush="true" data-tab-path="/Coding/Coding/Create"><i class="fa fa-edit"></i></a>
                            <a data-toggle="tooltip" title="Delete Encounter" class="p_delete_btn btn btn-xs" onclick="showcodingDeleteModal(OpenEncountersJsonData[@i])"><i class="fa fa-trash"></i></a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script>
    $('#OpenCodingList').prtGrid({
        url: "/Coding/GetOpenEncounters",
        dataLength: 50,
        columns: [
        { type: 'text', name: 'EncounterID', text: 'Enctr Id', widthPercentage: 5, sortable: false },
        { type: 'text', name: 'MemberID', text: 'Mbr Id', widthPercentage: 5, sortable: false },
        { type: 'text', name: 'MemberLastName', text: 'Mbr LName', widthPercentage: 5, sortable: true },
         { type: 'text', name: 'MemberFirstName', text: 'Mbr FName', widthPercentage: 7, sortable: true },
        { type: 'text', name: 'ProviderNPI', text: 'Prv NPI', widthPercentage: 5, sortable: true },
        { type: 'text', name: 'ProviderLastName', text: 'Prv LName', widthPercentage: 7, sortable: true },
        { type: 'text', name: 'ProviderFirstName', text: 'Prv FName', widthPercentage: 7, sortable: true },
        { type: 'text', name: 'FacilityName', text: 'Facility', widthPercentage: 8, sortable: true },
        { type: 'date', name: 'DateOfService', text: 'DOS', widthPercentage: 5, sortable: true },

        { type: 'date', name: 'DateOfCreation', text: 'DOC', widthPercentage: 5, sortable: true },
        { type: 'text', name: 'CreatedBy', text: 'Created By', widthPercentage: 5, sortable: true },
         { type: 'text', name: 'EncounterType', text: 'Enctr Type', widthPercentage: 5, sortable: true },
       
        { type: 'text', name: 'EncounterStatus', text: 'Status', widthPercentage: 5, sortable: true },
        { type: 'none', name: '', text: '', widthPercentage: 5, sortable: false }]
    });

</script>