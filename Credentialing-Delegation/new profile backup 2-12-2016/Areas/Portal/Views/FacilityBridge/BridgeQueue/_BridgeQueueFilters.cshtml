@using PortalTemplate.Helper
<div class="fixed-toolbar margin-top-5px">


    <div class="btn btn-primary btn-xs total-count QueueAggregateButtons">All <span id="total-count" class="label queueUppertab_label">9</span><span class="pointing-arrow-all"></span></div>
    <div class="btn btn-danger btn-xs total-expedited QueueAggregateButtons">Expedited <span id="total-expedited" class="label queueUppertab_label">5</span><span class="pointing-arrow-expedited"></span></div>
    <div class="btn btn-success btn-xs total-standard QueueAggregateButtons">Standard <span id="total-standard" class="label queueUppertab_label">4</span><span class="pointing-arrow-standard"></span></div>

    <span class="filterButton">
        @Html.ExportOptions("filter")
