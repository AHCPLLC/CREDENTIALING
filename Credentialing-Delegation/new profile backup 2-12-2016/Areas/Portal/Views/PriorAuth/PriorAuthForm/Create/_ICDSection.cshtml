@model PortalTemplate.Areas.Portal.Models.PriorAuth.PriorAuthorization.PriorAuthorizationViewModel
<div class="x_panel border_on_panel zero-padding-left-right">
    <div class="x_content zero-padding-left-right">
        <div class="col-lg-12">
            <div>
                <div class="col-lg-3 col-md-2 col-sm-8 col-xs-4">
                    <nobr><span class="theme_label">@Html.LabelFor(e => e.ICDCodes[0].ICDCode)</span></nobr>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-7">
                    <span class="theme_label">@Html.LabelFor(e => e.ICDCodes[0].ICDCodeDescription)</span> <br />
                </div>
                <div class="col-lg-3 col-md-3 col-sm-2 col-xs-1 button-styles-xs">
                    <span class="theme_label"><label>ACTION</label></span> <br />
                </div>
                <div class="clearfix"></div>
            </div>
            <div id="PrimaryDXArea">
                <div class="PrimaryDXRow">
                    <div class="clearfix"></div>
                    <div class="col-lg-3 col-md-3 col-sm-8 col-xs-4">
                        @Html.TextBoxFor(e => e.ICDCodes[0].ICDCode, new { @class = "form-control input-xs icd_search_cum_dropdown icdCode mandatory_field_halo ", @placeholder = "ICD CODE" })
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-7">
                        @Html.TextBoxFor(e => e.ICDCodes[0].ICDCodeDescription, new { @class = "form-control input-xs icd_search_cum_dropdown icdDescription mandatory_field_halo ", @placeholder = "DESCRIPTION" })
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-2 col-xs-1 button-styles-xs">
                        <button type="button" class="btn btn-danger btn-xs icd_delete_btn hidden"><i class="fa fa-minus"></i></button>
                        <button type="button" class="btn btn-success btn-xs icd_add_btn"><i class="fa fa-plus"></i></button>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
</div>
@*<p-script filepath="~/Areas/Portal/Scripts/PriorAuth/PriorAuthForm/Common/NonMinified/ICDSection.js"></p-script>*@
<script async src="~/Areas/Portal/Scripts/PriorAuth/PriorAuthForm/Common/NonMinified/ICDSection.js"></script>