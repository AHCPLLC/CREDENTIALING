</style>
<script>
    $('#bridgeQueueTableBody').off('click', '.moveToCA').on('click', '.moveToCA', function () {
        TabManager.openFloatingModal("~/Areas/Portal/Views/ProviderProfile/ApprovedConfirmationModal/_MoveToCABody.cshtml", "~/Areas/Portal/Views/ProviderProfile/ApprovedConfirmationModal/_MoveToCAHeader.cshtml", "~/Areas/Portal/Views/ProviderProfile/ApprovedConfirmationModal/_MoveToCAFooter.cshtml");
    });

    $('body').off('click', '.empyesbtn').on('click', '.tempCAyesbtn', function () {
        $('.temmessage').hide();
        $('#tempsuccess').removeClass('hidden');
        //$('.tempyesbtn').hide();
        $('.tempfooter').hide
        $('#closetemmodal').click();
       // setTimeout(searchpage, 2000);
    })
    function searchpage() {

        $('#closetemmodal').click();
        TabManager.navigateToTab(tabnavigation);
    }

    var tabnavigation =
        {
            "tabAction": "Bridge REQUESTS",
            "tabTitle": "Bridge REQUEST",
            "tabPath": "/BridgeQueue/Index",
            "tabContainer": "fullBodyContainer"
        }
</script>
<p-script filepath="~/Areas/UM/Scripts/SearchMember/NonMinified/SearchMemberResultGrid.js"></p-script>

