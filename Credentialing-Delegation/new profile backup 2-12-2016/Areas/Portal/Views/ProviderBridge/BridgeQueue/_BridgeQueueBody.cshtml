@model IEnumerable<PortalTemplate.Areas.Portal.Models.ProviderBridge.BridgeQueue.BridgeQueueViewModel>
@using PortalTemplate.Helper
@using PratianUI
<style>
    input.normal-checkbox:checked + label > span, input.normal-radio:checked + label > span, input.checkbox-radio:not(:disabled):checked + label > span {
        background: #6a5a8c;
        margin-right: 0px;
        margin-bottom: 0px;
    }

    input.normal-checkbox + label > span, input.normal-radio + label > span, input.checkbox-radio + label > span {
        display: inline-block;
        width: 1.25em;
        height: 1.25em;
        margin: 0.25em 0.75em 0.25em 0.25em;
        border: 0.0625em solid rgb(192,192,192);
        vertical-align: bottom;
        cursor: pointer;
        margin-right: 0px;
        margin-bottom: 0px;
    }

    legend.legendStyle {
        font-size: 12px !important;
        background-color: transparent;
        font-weight: bold !important;
    }

    legend {
        display: block;
        width: auto !important;
        margin-bottom: 3px !important;
        border: 0;
    }

    fieldset.fsStyle {
        margin-top: 7px !important;
    }

    label {
        display: inline-block;
        max-width: 100%;
        margin-bottom: 5px;
        font-weight: bold !important;
    }

    .theCity .filer-search {
        width: 60%;
    }

    .theState .filer-search {
        width: 40%;
    }

    .theLastName .filer-search {
        width: 79%;
    }

    .theAssigned {
        padding: 8px 0px !important;
    }

    /*.theReqBy .filer-search {
        width: 81%;
    }*/

    .NPWidth {
        width: 6.5em;
    }
    /*#queueTable thead, #queueTable tbody {
        display: block;
    }*/
    @@media screen and (min-width: 1821px) {
        #queueTable {
            max-height: 585px;
            overflow-y: auto;
        }
        /*#queueTable tbody {
                height: 530px;
                overflow-y: auto;
                overflow-x: hidden;
            }*/
    }

    @@media screen and (min-width: 1707px) and (max-width: 1820px) {
        #queueTable {
            max-height: 540px;
            overflow-y: auto;
        }
        /*#queueTable tbody {
                height: 475px;
                overflow-y: auto;
                overflow-x: hidden;
            }*/
    }

    @@media screen and (min-width: 1517px) and (max-width: 1706px) {
        #queueTable {
            max-height: 470px;
            overflow-y: auto;
        }
        /*#queueTable tbody {
                height: 390px;
                overflow-y: auto;
                overflow-x: hidden;
            }*/
    }

    @@media screen and (min-width: 1366px) and (max-width: 1516px) {
        #queueTable {
            max-height: 380px;
            overflow-y: auto;
        }
        /*#queueTable tbody {
                height: 300px;
                overflow-y: auto;
                overflow-x: hidden;
            }*/
    }

