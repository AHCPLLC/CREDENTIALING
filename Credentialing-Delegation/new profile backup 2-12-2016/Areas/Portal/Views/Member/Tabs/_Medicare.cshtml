@model IEnumerable<PortalTemplate.Areas.Portal.Models.Member.MemberMedicareViewModel>
<div class="x_panel border_on_panel">
    <table class="table margin_bottom_zero table-white alignLeftTable noBorderTable boldBody zeroPaddingBottomTh theadBorder width_99 custom-thead-back custom-thead-font custom-tbody">
        <thead class="">
            <tr class="text-uppercase">
                <th class="theme_label">@Html.DisplayNameFor(e => e.Event)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.EffectiveDate)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.HICN)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.State)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.County)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.PBPID)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.FromDate)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.VerifiedDate)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.SegmentID)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Input)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Result)</th>
            </tr>
        </thead>
        <tbody >
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.Event)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.EffectiveDate)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.HICN)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.State)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.County)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.PBPID)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.FromDate)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.VerifiedDate)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.SegmentID)</td>
                        <td  class="theme_label_data">@Html.DisplayFor(e => item.Input)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Result)</td>
                    </tr>
                }
            }
            @if (Model == null || Model.Count() <= 0)
            {
                <tr id="no-data-available-row">
                    <td colspan="11" class="theme_label_data text-center">NO DATA AVAILABLE</td>
                </tr>
            }
        </tbody>
    </table>
</div>
