@model IEnumerable<PortalTemplate.Areas.Portal.Models.Member.MemberEligibilityViewModel>
<div class="x_panel border_on_panel">
    <table class="table margin_bottom_zero table-white alignLeftTable noBorderTable boldBody zeroPaddingBottomTh theadBorder width_99 custom-thead-back custom-thead-font custom-tbody">
        <thead class="">
            <tr class="text-uppercase">
                <th class="theme_label">@Html.DisplayNameFor(e => e.EffectiveDate)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.TermDate)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Category)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.PlanID)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.PlanName)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Reason)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Subsidy)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Explanation)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Event)</th>
                <th class="theme_label">@Html.DisplayNameFor(e => e.Type)</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.EffectiveDate)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.TermDate)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Category)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.PlanID)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.PlanName)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Reason)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Subsidy)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Explanation)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Event)</td>
                        <td class="theme_label_data">@Html.DisplayFor(e => item.Type)</td>
                    </tr>
                }
            }
            @if (Model == null || Model.Count() <= 0)
            {
                <tr id="no-data-available-row">
                    <td colspan="10" class="theme_label_data text-center">NO DATA AVAILABLE</td>
                </tr>
            }
         
        </tbody>
    </table>
</div>


