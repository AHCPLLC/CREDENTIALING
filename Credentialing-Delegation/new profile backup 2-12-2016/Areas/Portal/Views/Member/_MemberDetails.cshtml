@model PortalTemplate.Areas.Portal.Models.Member.MemberViewModel
<div class="col-lg-12 zero-padding-left-right">
    <div class="col-lg-12 zero-padding-left-right">
        <fieldset class="member-Style" style="margin-left:0px">
            <legend class="legendStyle">
                <span class="theme_depend text-uppercase">@Html.DisplayNameFor(e => e.MemberInformation.MemberInformationTitle)</span>
            </legend>
            @if (Model != null && Model.MemberInformation != null) { 
                        @Html.Partial("~/Areas/Portal/Views/Member/Tabs/_MemberInformation.cshtml",Model.MemberInformation)
            }
         </fieldset>
    </div>
</div>

