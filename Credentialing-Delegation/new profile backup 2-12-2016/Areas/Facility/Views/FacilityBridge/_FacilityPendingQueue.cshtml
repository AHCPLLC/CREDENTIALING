@model IEnumerable<PortalTemplate.Areas.Facility.Models.FacilityBridgeQueueViewModel>
@using PortalTemplate.Helper
@using PratianUI
<table id="" class="table table-striped table-hover table-responsive text-uppercase" style="margin-top: 4px;margin-left:-3px">
    <thead class="theme_thead custom-thead-back theme1_table custom-thead-font custom-tbody" id="">
        <tr>
            <th class="maxWidthtdSM">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Name &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdX theLastName">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Type &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdX">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Corporate &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>

            @*<th class="maxWidthtdXL">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Address &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>
                <th class="maxWidthtdXL">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">City &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>
                <th class="maxWidthtdXL theCity">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">State &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>
                <th class="maxWidthtdX theState">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">County &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>*@
            <th class="maxWidthtdXL theReqBy">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req By &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdX">

                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req From &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>

            <th class="maxWidthtdX">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Time &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdXL">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req Date &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdXL">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Assigned &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="SelectAllProviderClass">
                @Html.PratianUiCheckbox(new { @id = "SelectAllPendingQueue", onchange = "CheckForAllCheckboxSelection(this,'PendingQueueCheckbox','SelectAllPendingQueue')" })
            </th>
            <th class="maxWidthtdSM">
                <label class="singleElemQueue bgFieldsQueue">Action&nbsp;</label>
                <span class="styled-input">
                    <span></span>
                </span>
            </th>
        </tr>
    </thead>
    <tbody id="" class="theme_label_data">
        @foreach (var item in Model)
        {
            
                <tr>
                    <td>@item.FacilityName</td>
                    <td>@item.FacilityType</td>
                    <td>@item.CorporateName</td>
                    @*<td>@item.AddressLine1</td>
                        <td>@item.City</td>
                        <td>@item.State</td>
                        <td>@item.County</td>*@
                    <td>@item.ReqBy</td>
                    <td>@item.ReqFrom</td>
                    <td>@item.Time</td>
                    <td>@item.ReqDate</td>
                    <td>@item.Assigned</td>
                    <td class="pendingfacility">@Html.PratianUiCheckbox(new { @class = "PendingQueueCheckbox", @name = "PendingQueueCheckbox", onchange = "CheckForCheckboxSelection(this,'PendingQueueCheckbox','SelectAllPendingQueue')" })
                    <td><a class="btn btn-primary btn-xs" onclick="ViewFacility('@item.FacilityID')"><i class="fa fa-eye"></i></a> <a class="btn btn-primary btn-xs" onclick="EditFacility('@item.FacilityID')"><i class="fa fa-edit"></i></a> </td>
                </tr>
        }
    </tbody>
</table>
