                </th>
                <th class="maxWidthtdXL">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">City &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>
                <th class="maxWidthtdXL theCity">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">State &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>
                <th class="maxWidthtdX theState">
                    <span class="styled-input">
                        <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">County &nbsp;</label>
                        <span></span>
                        <i class="fa fa-sort facility-queue-sort"></i>
                    </span>
                </th>*@
            <th class="maxWidthtdXL theReqBy">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req By &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdX">

                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req From &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>

            <th class="maxWidthtdX">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Time &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="maxWidthtdXL">
                <span class="styled-input">
                    <input class="filer-search" type="text" required /><label class="singleElemQueue bgFieldsQueue">Req Date &nbsp;</label>
                    <span></span>
                    <i class="fa fa-sort facility-queue-sort"></i>
                </span>
            </th>
            <th class="SelectAllProviderClass">
                @Html.PratianUiCheckbox(new { @id = "SelectAllOpenedQueue", onchange = "CheckForAllCheckboxSelection(this,'OpenedQueueCheckbox','SelectAllOpenedQueue')" })
            </th>
            <th class="maxWidthtdSM">
                <label class="singleElemQueue bgFieldsQueue">Action&nbsp;</label>
                <span class="styled-input">
                    <span></span>
                </span>
            </th>
        </tr>
    </thead>
    <tbody class="theme_label_data" id="">
        @foreach (var item in Model)
        {
            <tr data-toggle="collapse" data-target="#@item.FacilityID">
                <td>@item.FacilityName</td>
                <td>@item.FacilityType</td>
                <td>@item.CorporateName</td>
                @*<td>@item.AddressLine1</td>
                    <td>@item.City</td>
                    <td>@item.State</td>
                    <td>@item.County</td>*@
                <td>@item.ReqBy</td>
                <td>@item.ReqFrom</td>
                <td>@item.Time</td>
                <td>@item.ReqDate</td>
                <td class="">@Html.PratianUiCheckbox(new { @class = "OpenedQueueCheckbox", @name = "OpenedQueueCheckbox", onchange = "CheckForCheckboxSelection(this,'OpenedQueueCheckbox','SelectAllOpenedQueue')" })</td>
                <td><a class="btn btn-primary btn-xs" onclick="ViewFacility('@item.FacilityID')"><i class="fa fa-eye"></i></a> <a class="btn btn-primary btn-xs" onclick="EditFacility('@item.FacilityID')"><i class="fa fa-edit"></i></a> </td>
            </tr>
