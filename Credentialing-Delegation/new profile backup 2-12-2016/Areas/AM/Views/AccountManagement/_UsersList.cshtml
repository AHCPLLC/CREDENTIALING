@using PortalTemplate.Helper
<style>
    .btn-ssm {
        padding: 1px 8px;
        font-size: 12px;
    }

    .bootstrap-switch.bootstrap-switch-small {
        height: 22px;
    }

    label.title_text {
        font-weight: 900;
        font-size: 15px;
        background: #5b83d5;
        color: #ffffff;
        padding: 1px 9px;
        position: relative;
        margin-left: -25px;
    }
</style>

<div id="ListAllUsersView">
    <div class="col-lg-12" style="margin-top: 22px; background-color: #f7f7f7">
        <fieldset class="fieldsetStyle" >
            <legend class="legendStyle noBorder">
                <span class="theme_depend title_text text-uppercase ">All Users</span>
            </legend>
            <div style="margin-top:-38px">
                <div class="col-lg-12" >
                    <div class="pull-right">@Html.ExportOptions("excel", "print", "PDF", "filter")</div>
                    <div class="pull-right" style="margin-top: 6px;">
                        <a class="btn btn-xs btn-success  tab-navigation member-actions" id="" @*onclick="AddUserModal()"*@ data-toggle="tooltip" data-placement="bottom" title="Add New User" data-tab-val="Add New User" data-tab-action="Add New User" data-tab-title="Add New User" data-tab-container="fullBodyContainer" data-tab-autoflush="false" data-tab-path="~/Areas/AM/Views/AccountManagement/_AddNewUser.cshtml" data-tab-floatmenu="true" data-tab-float-menu-path="~/Areas/AM/Views/Shared/_AMFloatMenu.cshtml"><i class="fa fa-plus"></i> Add New User</a>
                    </div>
                </div>
                    <div class="c_panel_body">
                        <table class="table table-striped table-hover table-responsive custom-thead-back queue_table queue_table_head custom-thead-font custom-tbody custom-table-striped" id="AccountsTableView">
                            <thead class="theme_thead">
                                <tr>
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>User ID</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>First Name</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>Last Name</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>User Name</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    @*<th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>User Role</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>Team</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>*@
                                    <th class="withfilter sm-mbrid">
                                        <span class="styled-input">
                                            <input class="filer-search" type="text" required="">
                                            <label>Status</label>
                                            <span></span>
                                            <i class="fa fa-sort" style="color: rgb(255, 255, 255);"></i>
                                        </span>
                                    </th>
                                    <th class="text-center">
                                        <label>Action</label>
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="UsersListTableData" class=" theme_label_data"></tbody>

                        </table>
                        <br />
                        <br />
                    </div>
                </div>
        </fieldset>
    </div>
</div>
<div id="IndividualUserView" hidden>
@Html.Partial("~/Areas/AM/Views/AccountManagement/_ViewUserDetails.cshtml")
</div>
@Html.Partial("~/Areas/AM/Views/AccountManagement/_AddNewUserModal.cshtml")
@Html.Partial("~/Areas/AM/Views/AccountManagement/_MapUserModal.cshtml")
@Html.Partial("~/Areas/AM/Views/AccountManagement/_MapPrivilegesModal.cshtml")

@Html.Partial("~/Areas/AM/Views/AccountManagement/_ActivateUserModal.cshtml")
@Html.Partial("~/Areas/AM/Views/AccountManagement/_DeactivateUserModal.cshtml")


