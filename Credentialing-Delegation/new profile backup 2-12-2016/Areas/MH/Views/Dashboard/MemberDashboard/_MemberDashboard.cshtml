
<link href="~/Areas/MH/Styles/Dashboard/MemberDashboard.css" rel="stylesheet" />
<p-script filepath="~/Areas/MH/Scripts/Dashboard/NonMinified/MemberDashboard.js"></p-script>

<div class="clearfix"></div><br />
<div class="">
    <div class="col-lg-3 col-xs-6">
        <!-- small box -->
        <div class="small-box bg-aqua">
            <div class="inner">
                <h3>150</h3>

                <p>Total Members</p>
            </div>
            <div class="icon">
                <i class="fa fa-user" aria-hidden="true"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-xs-6">
        <!-- small box -->
        <div class="small-box bg-green">
            <div class="inner">
                <h3>100</h3>
                <p>Enrolled Members</p>
            </div>
            <div class="icon">
                <i class="fa fa-thumbs-o-up" aria-hidden="true"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-xs-6">
        <!-- small box -->
        <div class="small-box bg-red">
            <div class="inner">
                <h3>50</h3>
                <p>Disenrolled Members</p>
            </div>
            <div class="icon">
                <i class="fa fa-thumbs-o-down" aria-hidden="true"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-3 col-xs-6">
        <!-- small box -->
        <div class="small-box bg-yellow">
            <div class="inner">
                <h3>65</h3>

                <p>Total Providers</p>
            </div>
            <div class="icon">
                <i class="fa fa-user-md" aria-hidden="true"></i>
            </div>
            <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
        </div>
    </div>
    <!-- ./col -->
</div>
<div class="clearfix"></div><br />
<div class="col-lg-12 zero-padding-left-right">
    <div class="col-lg-6 zero-padding-left-right">
        <fieldset class="fsStyle">
            <legend class="legendStyle">
                <span class="theme_depend text-uppercase">
                    Members Enrollment/Dis-enrollment Report
                </span>
            </legend>

            <div id="chart1"></div>

        </fieldset>
    </div>
    <div class="col-lg-6 zero-padding-left-right">
        <fieldset class="fsStyle">
            <legend class="legendStyle">
                <span class="theme_depend text-uppercase">
                    Top 5 Providers-Members Distribution
                </span>
            </legend>

            <div id="chart2"></div>

        </fieldset>
    </div>
</div>
