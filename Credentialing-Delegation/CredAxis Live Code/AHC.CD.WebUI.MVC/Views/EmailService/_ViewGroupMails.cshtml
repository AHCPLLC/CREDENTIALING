

<div>
    <div>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-4 form-group">
                <label class="control-label small">Group Name</label>
                <div>
                    <i class="data-label wrap-words">{{tempObject.EmailGroupName || 'Not Available'}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">Description</label>
                <div>
                    <i class="data-label wrap-words">{{tempObject.Description || 'Not Available'}}</i>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">Created By</label>
                <div>
                    <i class="data-label wrap-words">{{tempObject.CreatedBy.EmailId || 'Not Available'}}</i>
                </div>
            </div>
        </div>

        <div class="col-lg-12 zero-padding-left-right-div">
           
            <div class="col-lg-6 form-control" style="height: 200px; overflow-y: scroll; overflow: auto; margin-top: 1%; width: 50%;">
                <table class="table table-hover table-responsive table-striped">
                    <thead>
                        <tr style="color: white; background-color: rgba(66, 123, 225, 0.78);">
                            <th>Emails</th>
                        </tr>
                        <tr>
                            <th><input class="form-control input-sm" style="margin-left:-2%;width:104%" ng-model="AddedEmailId" placeholder="Filter Email" /></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="data in filteredtempdata = (tempObject.EmailObj | filter:AddedEmailId) track by $index">
                            <td><p class="label label-info"><h4>{{data.EmailIds}}</h4></p></td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="col-lg-2 form-group"></div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">No of Emails</label>
                <div>
                    <i class="data-label wrap-words">{{tempObject.EmailObj.length || 'Not Available'}}</i>
                </div>
            </div>
        </div>
    </div>
    <br /><br /><br />
    <div class="clearfix"></div>
