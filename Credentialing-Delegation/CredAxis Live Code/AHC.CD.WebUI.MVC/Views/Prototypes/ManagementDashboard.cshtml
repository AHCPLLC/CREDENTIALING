                                            <a ng-click="reverse=!reverse;order('NPINumber',reverse,1)" class="pointer">NPI <i ng-if="selectedSection==1 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==1 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('FirstName',reverse,2)" class="pointer">Name <i ng-if="selectedSection==2 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==2 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.PlanName',reverse,3)" class="pointer">Plan<i ng-if="selectedSection==3 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==3 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.LOBName',reverse,4)" class="pointer">LOB<i ng-if="selectedSection==4 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==4 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.ParticipatingStatus',reverse,12)" class="pointer">Participating Status<i ng-if="selectedSection==12 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==12 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.GroupID',reverse,6)" class="pointer">GroupID<i ng-if="selectedSection==6 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==6 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.ProviderID',reverse,7)" class="pointer">ProviderID<i ng-if="selectedSection==7 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==7 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.InitiatedDate',reverse,8)" class="pointer">Effective Date <i ng-if="selectedSection==8 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==8 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.ReCredentialingDate',reverse,10)" class="pointer">Recredentialing Date <i ng-if="selectedSection==5 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==5 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                            <a ng-click="reverse=!reverse;order('License.dayLeft',reverse,6)" class="pointer">Day Left <i ng-if="selectedSection==6 && !reverse" class="fa fa-caret-up"></i><i ng-if="selectedSection==6 && reverse" class="fa fa-caret-down"></i></a>
                                        </th>
                                        <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>Action</th>
                                    </tr>
                                    <tr class="skip">
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.NPINumber" placeholder="NPI" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.Fullname" placeholder="Name" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.Plan" placeholder="Plan Name" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.LOBName" placeholder="LOB Name" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.ParticipatingStatus" placeholder="Participating Status" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.GroupID" placeholder="GroupID" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.ProviderID" placeholder="ProviderID" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.InitiatedDate" placeholder="Effective Date" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.ReCredentialingDate" placeholder="ReCredentialingDate" /></td>
                                        <td><input class="form-control input-sm textalign " type="text" ng-model="filterData.dayLeft" placeholder="Days Left" /></td>
                                        <td></td>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="p in Licenses  track  by $index | orderBy:'License.dayLeft'" ng-show="IsValidIndex($index)" ng-class='{danger:p.License.dayLeft <= 0, warning:p.License.dayLeft <= 30 && p.License.dayLeft > 0,lightorange:p.License.dayLeft <= 60 && p.License.dayLeft > 30, lightpurple:p.License.dayLeft <= 90 && p.License.dayLeft > 60}'>
                                        <td>{{p.NPINumber}}</td>
                                        <td>{{p.Fullname}}</td>
                                        <td>{{p.License.PlanName}}</td>
                                        <td>{{p.License.LOBName}}</td>
                                        <td>{{p.License.ParticipatingStatus|| 'Not Available'}}</td>
                                        <td>{{p.License.GroupID || 'Not Available'}}</td>
                                        <td>{{p.License.ProviderID || 'Not Available'}}</td>
                                        <td>{{p.License.InitiatedDate|| 'Not Available' | date:'MM/dd/yyyy'}}</td>
                                        <td>{{p.License.ReCredentialingDate|| 'Not Available' | date:'MM/dd/yyyy'}}</td>
                                        <td>{{p.License.dayLeft}}</td>
                                        <td><a ng-click=UpCome(p,'UpComingRecredentials') class="btn btn-primary btn-xs" data-toggle='tooltip' data-placement='bottom' title='Initiate Re-Credentialing'><i class='fa fa-hand-o-up'></i></a></td>
                                    </tr>
                                </tbody>
                                <tbody ng-show="Licenses.length==0">
                                    <tr>
                                        <td colspan="11" style="text-align:center;font-weight:bolder">No Records Available for {{LicenseType}} expiries</td>
                                    </tr>
                                </tbody>
                            </table>
                        </script>
                    </div>
                </div>
            </div>
        </div>

        <div class="clearfix"></div>

        <table id="ProviderDirectoryTable" ng-table-dynamic="tableParams with Columns" ng-table="tableParams" class="table table-condensed table-bordered table-striped" show-filter="true" style="margin-bottom:0px;">
            <tr ng-repeat="row in $data">
                <td ng-repeat="col in $columns" data-key="{{col.field}}">{{row[col.field]}}</td>
            </tr>
        </table>
    </div>
    <div class="col-md-4 zero-padding-left-right">
        <div class="form-style">
            <fieldset style="padding-bottom: 10px;">
                <legend>
                    RANK OF CCO
                </legend>
                <div class="col-lg-12 zero-padding-left-right-div">
                    <table id="ProviderDirectoryTable" ng-table-dynamic="CCORankTableParams with CCORankColumns" class="table table-condensed table-bordered table-striped" show-filter="false" style="margin-bottom:0px;">
                        <tr ng-repeat="row in $data">
                            <td ng-repeat="col in $columns" data-key="{{col.field}}">{{row[col.field]}}</td>
                        </tr>
                    </table>
                </div>
            </fieldset>
            <fieldset style="padding-bottom: 10px;">
                <legend>
                    Provider Profile Completion %
                </legend>
                <div class="col-lg-12 zero-padding-left-right-div">
                    <div id="ProfileReportChart"></div>
                </div>
            </fieldset>
        </div>
    </div>
    <div class="col-md-6 zero-padding-left-right">
        <div class="form-style">
            <fieldset style="padding-bottom: 10px;min-height:313px;">
                <legend>
                    New Providers Report
                </legend>
                <div class="col-lg-12 zero-padding-left-right-div">
                    <table ng-table-dynamic="NewAddedProviderTableParams with NAPColumns" class="table table-condensed table-bordered table-striped" show-filter="false" style="margin-bottom:0px;">
                        <tr ng-repeat="row in $data">
                            <td ng-repeat="col in $columns" data-key="{{col.field}}">{{row[col.field]}}</td>
                        </tr>
                    </table>
                </div>
            </fieldset>
        </div>
    </div>
    <div class="col-md-6 zero-padding-left-right">
        
        <div class="form-style">
            <fieldset style="padding-bottom: 10px;">
                <legend>
                    CCO REPORT
                </legend>
                <div class="col-lg-12 zero-padding-left-right-div">
                    <div id="CCOReportChart" style="margin-bottom: -50px;"></div>
                </div>
            </fieldset>
        </div>
    </div>
</div>