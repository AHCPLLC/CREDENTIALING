<div ng-show="NoPlan">
    <div class="col-lg-12">
            <div ng-show="activeMessageDiv=='PlanError'" class="alert alert-danger" role="alert">
                <span class="glyphicon glyphicon-exclamation-sign" ariea-hiddn="true"></span>
                <button ng-click="closeAlertMessage('')" class="close"> <span>&times;</span></button>
                {{messageDesc}}
            </div>
    </div>
</div>
<div ng-hide="NoPlan" id="InitiateCredentialingPanal">
    <label ng-if="ccoprofile" class="control-label small">
        Do you want to initiate {{ProviderName}} for {{selectedPlan.PlanName}} plan?
    </label>
    <label ng-if="!ccoprofile" class="control-label small">
        Do you want to initiate for {{selectedPlan.PlanName}} plan?
    </label>
    
    <div class="col-md-12 pull-right">
        <button class="btn btn-primary btn-sm pull-right" type="submit" ng-click="InitiateCredentialing();">Initiate</button>
    </div>
</div>
