@model AHC.CD.WebUI.MVC.Areas.Profile.Models.HospitalPrivilege.HospitalPrivilegeDetailViewModel
@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<div class="col-lg-12 zero-padding-left-right-div">
    <fieldset>
        <legend>
            Hospital Privilege Information
        </legend>
        <div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PreferenceType, new { @class = "control-label small" })
                    <div ng-hide="hospitalPrivilegeInformationHistory.PreferenceType == ''">
                        <i class="data-label " ng-class="{'1':'fa fa-check', '2':'fa fa-times'}[hospitalPrivilegeInformationHistory.PreferenceType]"></i>
                    </div>
                    <div ng-show="hospitalPrivilegeInformationHistory.PreferenceType == '' || hospitalPrivilegeInformationHistory.PreferenceType == null">
                        <i>Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.HospitalID, htmlAttributes: new { @class = "control-label small" })
                    <div class="radio">
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.Hospital.HospitalName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.HospitalContactInfoID, htmlAttributes: new { @class = "control-label small" })
                    <div class="radio">
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.LocationName || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    <label class="control-label small">Street/P. O. Box No:</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.Street || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Suite/Building</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.UnitNumber || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">City</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.City || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">State</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.State || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    <label class="control-label small">Zip Code</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.ZipCode || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Country</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.Country || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">County</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.County || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    <label class="control-label small">Phone Number</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.Phone || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Fax</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.Fax || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">E-mail Address</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactInfo.Email || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.HospitalContactPersonID, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactPerson.ContactPersonName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Contact Person Phone Number</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactPerson.ContactPersonPhone || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <label class="control-label small">Contact Person Fax Number</label>
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.HospitalContactPerson.ContactPersonFax || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.DepartmentName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{hospitalPrivilegeInformationHistory.DepartmentName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.DepartmentChief, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{hospitalPrivilegeInformationHistory.DepartmentChief || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.AffilicationStartDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.AffilicationStartDate || 'Not Available' | date:'MM/dd/yyyy'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.AffiliationEndDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.AffiliationEndDate || 'Not Available' | date:'MM/dd/yyyy' }}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.PrevilegesTemporaryYesNoOption, new { @class = "control-label small" })
                    <div ng-hide="!hospitalPrivilegeInformationHistory.PrevilegesTemporaryYesNoOption">
                        <i class="data-label " ng-class="{'1':'fa fa-check', '2':'fa fa-times'}[hospitalPrivilegeInformationHistory.PrevilegesTemporaryYesNoOption]"></i>
                    </div>
                    <div ng-show="!hospitalPrivilegeInformationHistory.PrevilegesTemporaryYesNoOption">
                        <i>Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.FullUnrestrictedPrevilagesYesNoOption, new { @class = "control-label small" })
                    <div ng-hide="!hospitalPrivilegeInformationHistory.FullUnrestrictedPrevilagesYesNoOption">
                        <i class="data-label" ng-class="{'1':'fa fa-check', '2':'fa fa-times'}[hospitalPrivilegeInformationHistory.FullUnrestrictedPrevilagesYesNoOption]"></i>
                    </div>
                    <div ng-show="!hospitalPrivilegeInformationHistory.FullUnrestrictedPrevilagesYesNoOption">
                        <i>Not Available</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus != 'Renewed'">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.AdmittingPrivilegeID, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label ">{{hospitalPrivilegeInformationHistory.AdmittingPrivilege.Title || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.AnnualAdmisionPercentage, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.AnnualAdmisionPercentage || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SpecialtyID, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{getSpecialityForProvider(hospitalPrivilegeInformationHistory.SpecialtyID).Specialty.Name || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.StaffCategoryID, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.StaffCategory.Title || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="form-group col-md-3" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus == 'Renewed'">
                    @Html.LabelFor(model => model.AffilicationStartDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.IssueDate || 'Not Available' | date:'MM/dd/yyyy'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3" ng-if="hospitalPrivilegeInformationHistory.HistoryStatus == 'Renewed'">
                    @Html.LabelFor(model => model.AffiliationEndDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{hospitalPrivilegeInformationHistory.ExpiryDate || 'Not Available' | date:'MM/dd/yyyy' }}</i>
                    </div>
                </div>
                <div class="col-md-3 form-group">
                    <span class="fileinput-new small">@Html.LabelFor(model => model.HospitalPrevilegeLetterPath, htmlAttributes: new { @class = "control-label" })</span>
                    <div>
                        <i ng-show="hospitalPrivilegeInformationHistory.HospitalPrevilegeLetterPath==null">Not Available</i><a ng-hide="hospitalPrivilegeInformationHistory.HospitalPrevilegeLetterPath==null" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{hospitalPrivilegeInformationHistory.HospitalPrevilegeLetterPath}}" tooltip data-toggle="tooltip" data-placement="top" title="Hospital Privilege Document" target="_blank"><i class="fa fa-file"></i></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12 zero-padding-left-right-div">
            <div class="col-md-12">
                <button id="CloseViewHistory_HP" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
            </div>
        </div>
    </fieldset>
</div>