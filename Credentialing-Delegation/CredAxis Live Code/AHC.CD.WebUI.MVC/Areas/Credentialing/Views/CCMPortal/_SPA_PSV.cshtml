        @* last 3 *@
        <table class="table table-striped table-bordered small wrap-words" ng-if="tab.Id==tempObject.NPDBParameterID || tab.Id==tempObject.MOPTParameterID || tab.Id==tempObject.OIGParameterID">
            <thead>
                <tr style="background-color:#ddd">
                    <th colspan="5">{{tab.Info[0].ProfileVerficationParameterObj.Title}}</th>
                </tr>
                @*<tr ng-show="tempData[3].Licenses.length == '0'">
                        <th colspan="6" class="danger"><span class="control-label danger">No Specialty/Board Available !!!</span></th>
                    </tr>*@
                <tr @*ng-show="tempData[3].Licenses.length > '0'"*@>
                    <th>
                        Source
                    </th>
                    <th>
                        Remarks
                    </th>
                    <th>
