<style>
    .table {
        width: 100%;
        max-width: 100%;
        margin-bottom: 0px;
    }
</style>

<div class="col-lg-12 zero-padding-left-right-div form-style" ng-if="tempObject.PSVdata != undefined || tempObject.PSVdata.length !=0">
    <div ng-repeat="tab in tempObject.PSVdata track by $index" style="margin:10px">
        @* state license *@
        <table class='table table-striped table-bordered small wrap-words' ng-if="tab.Id==tempObject.StateLicenseParameterID">
            <thead>
                <tr style="background-color:#ddd">
                    <th colspan="6">{{tab.Info[0].ProfileVerficationParameterObj.Title}}</th>
                </tr>
                @*<tr ng-show="tempData[0].Licenses.length == '0'">
                        <th colspan="6" class="danger"><span class="control-label danger">No State Licenses Available !!!</span></th>
                    </tr>*@
                <tr @*ng-show="tempData[0].Licenses.length > '0'"*@>
                    <th>
                        License Number
                    </th>
                    <th class="col-lg-3">
                        Source
                    </th>
                    <th>
                        Remarks
                    </th>
                    <th>
                        Document
                    </th>
                    <th>
                        Verified Date
                    </th>
                    <th>
                        Verified Document
                    </th>
                    @*<th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>Action</th>*@
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat='lisence in tab.Info track by $index' ng-class='{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}'>
                    <td class="wrap-words">{{lisence.VerificationData.LicenseNumber}}</td>
                    <td class="wrap-words"><a style="cursor:pointer" class="data-label wrap-words" ng-if="lisence.VerificationResultObj.Source" ng-click="checkForHttp(lisence.VerificationResultObj.Source)">{{lisence.VerificationResultObj.Source}}</a></td>
                    <td class="wrap-words">{{lisence.VerificationResultObj.Remark}}</td>
                    <td class="wrap-words"><a ng-if="lisence.VerificationData.StateLicenseDocumentPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{lisence.VerificationData.StateLicenseDocumentPath}}" targ target="_blank"><i class="fa fa-file"></i></a></td>
                    <td class="wrap-words">{{lisence.VerificationDate | date:'MM/dd/yyyy'}}</td>
                    <td class="wrap-words"><a ng-if="lisence.VerificationResultObj.VerificationDocumentPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{lisence.VerificationResultObj.VerificationDocumentPath}}" targ target="_blank"><i class="fa fa-file"></i></a></td>
                    @*<td><a class='btn btn-xs btn-default' tooltip data-toggle='tooltip' data-placement='right' title='Perform PSV'><i class='fa fa-repeat'></i></a></td>*@
                </tr>
            </tbody>
        </table>

        @* Board certificates *@
        <table class='table table-striped table-bordered small wrap-words' ng-if="tab.Id==tempObject.BoardCertificationParameterID">
            <thead>
                <tr style="background-color:#ddd">
                    <th colspan="6">{{tab.Info[0].ProfileVerficationParameterObj.Title}}</th>
                </tr>
                @*<tr ng-show="tempData[3].Licenses.length == '0'">
                        <th colspan="6" class="danger"><span class="control-label danger">No Specialty/Board Available !!!</span></th>
                    </tr>*@
                <tr @*ng-show="tempData[3].Licenses.length > '0'"*@>
                    <th class="col-lg-3">
                        Specialty Board Name
                    </th>
                    <th class="col-lg-3">
                        Source
                    </th>
                    <th>
                        Remarks
                    </th>
                    <th>
                        Document
                    </th>
                    <th>
                        Verified Date
                    </th>
                    <th>
                        Verified Document
                    </th>
                    @*<th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>Action</th>*@
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat='lisence in tab.Info track by $index' ng-class='{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}'>
                    <td class="wrap-words">{{lisence.VerificationData.SpecialtyBoardCertifiedDetail.SpecialtyBoard.Name}}</td>
                    <td class="wrap-words"><a style="cursor:pointer" class="data-label wrap-words" ng-if="lisence.VerificationResultObj.Source" ng-click="checkForHttp(lisence.VerificationResultObj.Source)">{{lisence.VerificationResultObj.Source}}</a></td>
                    <td class="wrap-words">{{lisence.VerificationResultObj.Remark}}</td>
                    <td class="wrap-words"><a ng-if="lisence.VerificationData.SpecialtyBoardCertifiedDetail.BoardCertificatePath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{lisence.VerificationData.SpecialtyBoardCertifiedDetail.BoardCertificatePath}}" tar target="_blank"><i class="fa fa-file"></i></a></td>
                    <td class="wrap-words">{{lisence.VerificationDate | date:'MM/dd/yyyy'}}</td>
                    <td class="wrap-words"><a ng-if="lisence.VerificationResultObj.VerificationDocumentPath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{lisence.VerificationResultObj.VerificationDocumentPath}}" targ target="_blank"><i class="fa fa-file"></i></a></td>
                    @*<td><a class='btn btn-xs btn-default' tooltip data-toggle='tooltip' data-placement='right' title='Perform PSV'><i class='fa fa-repeat'></i></a></td>*@
                </tr>
            </tbody>
        </table>

