                    <td class="wrap-words">{{lisence.VerificationData.CertNumber}}</td>
                    <td class="wrap-words"><a style="cursor:pointer" class="data-label wrap-words" ng-if="lisence.VerificationResultObj.Source" ng-click="checkForHttp(lisence.VerificationResultObj.Source)">{{lisence.VerificationResultObj.Source}}</a></td>
                    <td class="wrap-words">{{lisence.VerificationResultObj.Remark}}</td>
                    <td class="wrap-words"><a ng-if="lisence.VerificationData.CDSCCerificatePath" toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{lisence.VerificationData.CDSCCerificatePath}}" targ targe target="_blank"><i class="fa fa-file"></i></a></td>
                    <td class="wrap-words">{{lisence.VerificationDate | date:'MM/dd/yyyy'}}</td>
