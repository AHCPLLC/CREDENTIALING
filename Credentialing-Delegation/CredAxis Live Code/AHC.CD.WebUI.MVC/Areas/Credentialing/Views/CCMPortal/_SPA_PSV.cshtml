<div style="margin:10px" class="ng-scope">
    <style>
        .table {
            width: 100%;
            max-width: 100%;
            margin-bottom: 0px;
        }
    </style>
    <table class="table table-striped table-bordered small wrap-words ng-scope" @*ng-if="tab.Id==StateLicenseParameterID"*@>
        <thead>
            <tr style="background-color:#ddd">
                <th colspan="6" class="ng-binding"></th>
            </tr>

            <tr>
                <th>
                    License Number
                </th>
                <th class="col-lg-3">
                    Source
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Document
                </th>
                <th>
                    Verified Date
                </th>
                <th>
                    Verified Document
                </th>

            </tr>
        </thead>
        <tbody>
            <tr ng-class="{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}" class="ng-scope">
                <td class="wrap-words ng-binding"></td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">aedre</td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">03/02/2017</td>
                <td class="wrap-words"></td>

            </tr>
        </tbody>
    </table>
    <table class="table table-striped table-bordered small wrap-words ng-scope" @*ng-if="tab.Id==BoardCertificationParameterID"*@>
        <thead>
            <tr style="background-color:#ddd">
                <th colspan="6" class="ng-binding"></th>
            </tr>

            <tr>
                <th class="col-lg-3">
                    Speciality Board Name
                </th>
                <th class="col-lg-3">
                    Source
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Document
                </th>
                <th>
                    Verified Date
                </th>
                <th>
                    Verified Document
                </th>

            </tr>
        </thead>
        <tbody>
            <tr @*ng-repeat="lisence in tab.Info track by $index"*@ ng-class="{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}" class="ng-scope">
                <td class="wrap-words ng-binding"></td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">aedre</td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">03/02/2017</td>
                <td class="wrap-words"></td>
            </tr>
        </tbody>
    </table>
    <table class="table table-striped table-bordered small wrap-words ng-scope" @*ng-if="tab.Id==DEAParameterID"*@>
        <thead>
            <tr style="background-color:#ddd">
                <th colspan="6" class="ng-binding"></th>
            </tr>

            <tr>
                <th>
                    DEA Number
                </th>
                <th class="col-lg-3">
                    Source
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Document
                </th>
                <th>
                    Verified Date
                </th>
                <th>
                    Verified Document
                </th>

            </tr>
        </thead>
        <tbody>
            <tr @*ng-repeat="lisence in tab.Info track by $index"*@ ng-class="{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}" class="ng-scope">
                <td class="wrap-words ng-binding"></td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">aedre</td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">03/02/2017</td>
                <td class="wrap-words"></td>

            </tr>
        </tbody>
    </table>
    <table class="table table-striped table-bordered small wrap-words ng-scope" @*ng-if="tab.Id==CDSParameterID"*@>
        <thead>
            <tr style="background-color:#ddd">
                <th colspan="6" class="ng-binding"></th>
            </tr>

            <tr>
                <th>
                    CDS Number
                </th>
                <th>
                    Source
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Document
                </th>
                <th>
                    Verified Date
                </th>
                <th>
                    Verified Document
                </th>

            </tr>
        </thead>
        <tbody>
            <tr @*ng-repeat="lisence in tab.Info track by $index"*@ ng-class="{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}" class="ng-scope">
                <td class="wrap-words ng-binding"></td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">aedre</td>
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">03/02/2017</td>
                <td class="wrap-words"></td>
            </tr>
        </tbody>
    </table>
    <table class="table table-striped table-bordered small wrap-words ng-scope" @*ng-if="tab.Id==NPDBParameterID || tab.Id==MOPTParameterID || tab.Id==OIGParameterID"*@>
        <thead>
            <tr style="background-color:#ddd">
                <th colspan="5" class="ng-binding"></th>
            </tr>

            <tr>
                <th>
                    Source
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Verified Date
                </th>
                <th>
                    Verified Document
                </th>

            </tr>
        </thead>
        <tbody>
            <tr @*ng-repeat="lisence in tab.Info track by $index"*@ ng-class="{danger:license.dayLeft < -1, warning:license.dayLeft < 90, info:license.dayLeft < 180}" class="ng-scope">
                <td class="wrap-words"></td>
                <td class="wrap-words ng-binding">aedre</td>
                <td class="wrap-words ng-binding">03/02/2017</td>
                <td class="wrap-words"></td>
            </tr>
        </tbody>
    </table>
</div>