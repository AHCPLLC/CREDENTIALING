<div class="col-lg-12 zero-padding-left-right-div" style="background-color: white; padding-top: 8px;">
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="SalutationType">Name</label>
        <div>
            <span>Kellie Duffy</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="FirstName">Type of practice</label>
        <div>
            <span>Solo Practice</span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="MiddleName">County</label>
        <div>
            <span>Hernando</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="LastName">Speciality</label>
        <div>
            <span>Family Practitioner</span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small">Board Certificate</label>
        <div>
            <span>Yes</span>
        </div>
    </div>
</div>
<div class="col-lg-12 zero-padding-left-right-div" style="background-color: white; padding-top: 8px;">
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="SalutationType">Hospital Privileges</label>
        <div>
            <span>St. Oak Hill Hospital</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="FirstName">Covering Physician</label>
        <div>
            <span></span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="MiddleName">Gaps in Practice</label>
        <div>
            <span>No</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="LastName">Clean License</label>
        <div>
            <span></span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small">NPDB Issues</label>
        <div>
            <span>No</span>
        </div>
    </div>
</div>
<div class="col-lg-12 zero-padding-left-right-div" style="background-color: white; padding-top: 8px;">
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="SalutationType">Malpractice issues</label>
        <div>
            <span>No</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="FirstName">Years In practice</label>
        <div>
            <span>10 Years</span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small" for="MiddleName">Site Visit Done ?</label>
        <div>
            <span>Yes</span>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small" for="LastName">If yes, Were there any issues</label>
        <div>
            <span>Yes</span>
        </div>
    </div>
    <div class="col-lg-2 form-group">
        <label class="control-label small">Attached document</label>
        <div>
            <span><a data-toggle="tooltip" data-placement="right" title="" href="" target="_blank" data-original-title="Document"><i class="fa fa-2x fa-file"></i></a></span>
        </div>
    </div>
</div>
<div class="col-lg-12 zero-padding-left-right-div" style="background-color: white; padding-top: 8px;">
    <div class="col-lg-4 form-group">
        <label class="control-label small" for="ProviderLevel">Date</label>
        <div>
            <span>
                <input data-bs-datepicker="bs-datepicker" data-date-format="MM/dd/yyyy" data-ng-model="AppointmentDate" data-val="true" data-val-date="Please enter date in mm/dd/yyyy format." id="SignedDate" name="SignedDate" placeholder="mm/dd/yyyy" style="width:61%" type="text" value="" class="ng-pristine ng-valid valid ng-touched">
            </span>
        </div>
        <label class="control-label small" for="ProviderLevel" style="margin-top: 36px;">Provider Level</label>
        <div>
            <span>
                <label class="radio-inline control-label">
                    <input data-ng-model="ProviderLevel" id="" name="ProviderLevel" type="radio" value="1" class="ng-pristine ng-untouched ng-valid"> Level1
                </label>
                <label class="radio-inline control-label">
                    <input data-ng-model="ProviderLevel" id="" name="ProviderLevel" type="radio" value="2" class="ng-pristine ng-untouched ng-valid"> Level2
                </label>
                <label class="radio-inline control-label">
                    <input data-ng-model="ProviderLevel" id="" name="ProviderLevel" type="radio" value="3" class="ng-pristine ng-untouched ng-valid"> Level3
                </label>
            </span>
        </div>
    </div>
    <div class="col-lg-4 form-group">
        <label class="control-label small">Remark </label>
        <div>
            <span>
                <textarea cols="20" id="RemarkForApprovalStatus" name="RemarkForApprovalStatus" rows="5" style="width:100%"></textarea>
            </span>
        </div>
    </div>
    <div class="col-lg-4 form-group">
        <label class="control-label small" for="FirstName">
            Signature : <select class="input-xs" style="color:black">
                <option value="Digital Signature">Digital Signature</option>
                <option value="Digital Signature">Use Previous Signature</option>
                <option value="Digital Signature">Upload Signature</option>
            </select>
        </label>
        <div>
            <span>
                <canvas id="SignatureCanvas" height="98" width="248" style="border: solid;" ng-signature-pad="signature"></canvas>
                <button id="clear" ng-click="cleardigitalsignature()" class="btn btn-primary btn-xs" style=" margin-bottom: 14px;">
                    Clear
                </button>
            </span>
        </div>
    </div>
</div>
<div class="col-lg-12 zero-padding-left-right-div" style="background-color: white; padding-bottom: 8px;">
    <span class="col-lg-4"><button get-siblings ng-model="VisibilityControl" class="ApprovalSubmission btn btn-danger btn-xs pull-left " ng-click="">Reject</button></span>
    <span class="col-lg-4"><button get-siblings ng-model="VisibilityControl" class="ApprovalSubmission btn btn-warning btn-xs" ng-click="" style="margin-left: 160px;">Pending</button></span>
    <span class="col-lg-4"><button get-siblings ng-model="VisibilityControl" class="ApprovalSubmission btn btn-primary btn-xs pull-right" ng-click="">Accept</button> </span>
</div>