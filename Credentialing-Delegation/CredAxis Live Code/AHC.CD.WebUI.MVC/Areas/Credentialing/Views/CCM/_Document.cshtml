<script>
    var showAttachMore = function () {
        $("#ATTACHmore").show();
        $("#SubmitDone").hide();
        $("#AddDone").hide();

    };
    var showSubmit = function () {
        $("#ATTACHmore").hide();
        $("#SubmitDone").show();
        $("#AddDone").hide();

    };
    var showAdd = function () {
        $("#ATTACHmore").hide();
        $("#SubmitDone").hide();
        $("#AddDone").show();
    };
    //var cancelAdd = function () {
    //    $("#AddDone").hide();
