<div class="panel-group" id="accordionCovering" role="tablist" aria-multiselectable="true">

    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="CoveringHeading">
            <a data-toggle="collapse" data-parent="#accordionCovering" href="#Covering" aria-expanded="false" aria-controls="Covering">
                Covering Physicians
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="Covering" class="panel-collapse collapse" role="tabpanel" aria-labelledby="CoveringHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>




                    <div class="col-md-12">
                        <div class="form-group col-md-12">
                            <label class="col-md-3 control-label small">Physician Name *</label>
                            <div class="col-md-6">
                                <input type="text" class="form-control input-sm" required />
                            </div>

                        </div>
                        <div class="clearfix"></div>
                        <div class="form-group col-md-12">
                            <label class="col-md-3 control-label small">Address *</label>
                            <div class="col-md-6">
                                <textarea class="form-control"></textarea>
                            </div>

                        </div>
                        <div class="clearfix"></div>
                        <div class="col-md-12">
                            <label class="col-md-3 control-label small">Phone Number *</label>
                            <div class="col-md-6">
                                <div>
                                    <div class="input-group">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                        </div>
                                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                            <div class="arrow"></div>
                                            <h3 class="popover-title">Select Country Dial Code</h3>
                                            <div class="popover-content">
                                                <select id="ContactContryCode">
                                                    <option>+91</option>
                                                </select>
                                            </div>
                                        </div>
                                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>



                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






