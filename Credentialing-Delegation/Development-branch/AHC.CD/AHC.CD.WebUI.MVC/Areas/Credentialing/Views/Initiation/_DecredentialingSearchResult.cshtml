<div ng-show="data.length==0">No result found.</div>
<div class="col-lg-12" ng-hide="data.length==0" style="overflow-x:auto;">
    <table ng-table="tableParams3" class="table table-hover ng-table-resizable-columns ng-table-rowselected ng-scope">
        <thead>
            <tr>
                <th>Picture</th>
                <th>NPI Number</th>
                <th>Title</th>
                <th>Name</th>
                <th>Specialty</th>
                <th>IPA</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td></td>
                <td>
                    <input type="text" placeholder="Filter NPINumber" class="form-control input-sm" ng-model="search.NPINumber" ng-change="filterData()" />
                </td>
                <td>
                    <input type="text" placeholder="Filter Title" class="form-control input-sm" ng-model="search.Title" ng-change="filterData()" />
                </td>
                <td>
                    <input type="text" placeholder="Filter Name" class="form-control input-sm" ng-model="search.Name" ng-change="filterData()" />
                </td>
                <td>
                    <input type="text" placeholder="Filter Specialty" class="form-control input-sm" ng-model="search.Speciality" ng-change="filterData()" />
                </td>
                <td>
                    <input type="text" placeholder="Filter IPA" class="form-control input-sm" ng-model="search.IPA" ng-change="filterData()" />
                </td>
                
                <td></td>
            </tr>
            <tr ng-repeat="user in $data | filter:{NPINumber:search.NPINumber,Title:search.Title,Name:search.Name,SpecialtyName:search.Speciality,GroupName:search.IPA,Plan:search.PlanName} track by $index" ng-if="user.Status=='Active'" ng-show="IsValidIndex($index)">
                <td class="col-lg-1" data-title="'Picture'">
                    <div ng-show="user.Profile.ProfilePhotoPath == null" class="thumbnail imageStyle">
                        <img ng-src="/Content/Images/no_image.png" class="img-rounded" />
                    </div>
                    <div ng-hide="user.Profile.ProfilePhotoPath == null" class="thumbnail imageStyle">
                        <img ng-src="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{user.Profile.ProfilePhotoPath}}" class="img-rounded">
                    </div>
                </td>
                <td class=" wrap-words">
                    {{user.Profile.OtherIdentificationNumber.NPINumber}}@*{{user.NPINumber}}*@
                </td>
                <td class=" wrap-words col-lg-2">
                    @*<span ng-repeat="title in user.Profile.PersonalDetail.ProviderTitles track by $index"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{title.ProviderType.Title}}</span>*@
                    {{user.Title}}
                </td>
                <td class="wrap-words">

                    {{user.Name}}
                    @*{{user.FirstName}} {{user.LastName}}*@
                </td>

                <td class="wrap-words col-lg-2">
                    @*<span ng-show="user.Profile.SpecialtyDetails.length == 0"></span>
                <span ng-repeat="s in user.Profile.SpecialtyDetails"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{s.Specialty.Name}}</span>*@
                    {{user.SpecialtyName}}
                </td>
                <td class="wrap-words">
                    @*<span ng-show="user.Profile.ContractInfoes.length == 0"></span>
                <span ng-show="user.Profile.ContractInfoes.length > 0" ng-repeat="group in user.Profile.ContractInfoes">
                    <span ng-show="group.ContractGroupInfoes.length > 0" ng-repeat="groupName in group.ContractGroupInfoes track by $index">
                        <i ng-if="$index>0">,</i><br ng-if="$index>0" />{{groupName.PracticingGroup.Group.Name}}
                    </span>
                </span>*@
                    {{user.GroupName}}
                </td>
                <td class="col-lg-1">
                    <a ng-click="SelectProviderPlanDetail(user)" data-toggle="tooltip" data-placement="left" title="Initiate Decredentialing" class="btn btn-xs btn-info pull-right"><i class="fa fa-hand-o-up"></i></a>
                    @* <div ng-show="initiateSuccess" class="col-lg-6 text-right"><a class="btn btn-success btn-sm" href="~/credentialing/cnd/index"> <i class="fa fa-home"></i></a></div>*@
                </td>
            </tr>
        </tbody>
    </table>
    <div class="col-lg-12" id="errorMsg">
        <div ng-show="activeMessageDiv=='errorSpecialty'" class="alert alert-danger" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
    </div>
</div>


 