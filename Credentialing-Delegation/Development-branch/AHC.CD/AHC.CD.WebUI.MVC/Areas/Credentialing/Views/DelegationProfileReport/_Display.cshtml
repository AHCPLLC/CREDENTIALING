
<div class="text-center zero-margin-top-bottom-hr" style="margin-top:0px">
    <strong>{{templateName}}</strong>
</div>
<strong>General Information</strong>
<table class="table table-bordered small">
    <tbody>
        <tr>
            <td colspan="2"> <label for="textinput">Provider Name:</label></td>
            <td colspan="5">{{delegationProfileObj.ProviderName}}</td>
        </tr>
        <tr>
            <td colspan="1">
                <label for="npi">NPI</label>
            </td>
            <td colspan="2">
                {{delegationProfileObj.NPINumber}}
            </td>
            <td colspan="1">
                <label for="tin">TIN</label>
            </td>

            <td colspan="3">

                {{delegationProfileObj.TIN}}
            </td>
        </tr>

        <tr>
            <td>
                <label for="textinput">Cred. Date</label>
            </td>
            <td>
                {{delegationProfileObj.CredentialDate}}
            </td>
            <td colspan="1">
                <label for="textinput">
                    Recred Date <br />
                    Cycle 1
                </label>
            </td>
            <td>
                {{delegationProfileObj.ReCredentialCycle1Date}}
            </td>
            <td>
                <label for="textinput">Recred Date <br /> Cycle 2</label>
            </td>
            <td>
                {{delegationProfileObj.ReCredentialCycle2Date}}
            </td>
        </tr>
        <tr>
            <td>
                <label for="textinput">Medical License</label>
            </td>
            <td colspan="2">
                {{delegationProfileObj.MedicalLicense[0]}}
            </td>
            <td>
                <label for="textinput">DEA</label>
            </td>
            <td colspan="2">
                {{delegationProfileObj.DEA[0]}}
            </td>
</table>


<strong>Education/Personal Information:</strong>


<table class="table table-bordered small">
    <tr ng-repeat="med in medicalEducation track by $index">
        <td colspan="1">
            <label for="textarea">
                Medical Education:<br />
                (School Name,yrs <br />Attended,Degree Earned)
            </label>
        </td>
        <td colspan="6">
            {{med}}
        </td>
    </tr>
    <tr ng-repeat="internship in filteredProviderProgramDetails=(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'internship'}) track by $index">
        <td ng-if="$index==0" rowspan="{{(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'internship'}).length}}">
            <label>
                Internship:<br />
                (Facility,<br />Specality,<br />From/To)
            </label>
        </td>

        <td colspan="2">
            {{internship.Facility}}
        </td>
        <td colspan="2">
            {{internship.Specialty}}
        </td>
        <td colspan="2">
            {{internship.FromToDate}}
        </td>
    </tr>


    <tr ng-repeat="internship in filteredProviderProgramDetails=(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'Resident'}) track by $index">
        <td ng-if="$index==0" rowspan="{{(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'Resident'}).length}}">
            <label for="textinput">
                Residency(ies)
                (Facility,Speciality,From/To)
            </label>


        </td>
        <td ng-if="$index!=0"></td>

        <td colspan="2">
            {{internship.Facility}}
        </td>




        <td colspan="2">
            {{internship.Specialty}}
        </td>




        <td colspan="2">

            {{internship.FromToDate}}
        </td>

    </tr>

    <tr ng-repeat="internship in filteredProviderProgramDetails=(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'Fellowship'}) track by $index">
        <td ng-if="$index==0" rowspan="{{(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'Fellowship'}).length}}">
            <label for="textinput">Fellowship(s)</label>
        </td>
        <td ng-if="$index!=0"></td>
        <td colspan="2">
            {{internship.Facility}}
        </td>

        <td colspan="2">
            {{internship.Specialty}}
        </td>
        <td colspan="2">

            {{internship.FromToDate}}
        </td>


    </tr>



    <tr>

        <td>
            <label for="textinput">SSN</label>

        </td>

        <td>

            {{delegationProfileObj.SSN}}
        </td>



        <td>
            <label for="textinput">DOB</label>
        </td>

        <td>
            {{delegationProfileObj.DOB}}
        </td>





        <td>

            <label for="textinput">Ethnic Origin</label>
        </td>

        <td>
            {{delegationProfileObj.EthnicOrigin}}
        </td>

    </tr>


    <tr>

        <td>
            <label for="selectbasic">Gender</label>

        </td>

        <td colspan="2">
            {{delegationProfileObj.Gender}}

        </td>





        <td>
            <label for="selectmultiple">Provider Languages</label>
        </td>

        <td colspan="2">
            {{languagesKnown}}
        </td>
    </tr>
</table>


<strong>Provider Information</strong>


<table class="table table-bordered small">
    <tr>
        <td colspan="1">
            <label for="selectbasic">PCP or Specialist</label>
        </td>

        <td colspan="1">
            {{PrimarySpecility.PcpOrSpecialist}}
        </td>

        <td colspan="1">

            <label for="textinput">Primary Specality</label>
        </td>

        <td colspan="1">
            {{PrimarySpecility.Specialty}}
        </td>

        <td colspan="1">
            <label for="textinput">Secondary Specialty</label>
        </td>

        <td colspan="1">
            {{SecondarySpecility[0].Specialty}}
        </td>

    </tr>

    <tr>
        <td colspan="1">
            <label for="radios">Board Certified</label> <br />

        </td>

        <td colspan="1">

            {{PrimarySpecility.BoardCertified}}
        </td>

        <td colspan="1">

            <label for="textinput">Certified In</label>
        </td>

        <td colspan="1">
            {{PrimarySpecility.BoardName}}
        </td>
        <td colspan="1">
            {{PrimarySpecility.SpecialtyEffectiveDates}}
            @*<input id="textinput" name="textinput" type="text" class="form-control input-md">*@

        </td>
        <td colspan="1">
            @*<input id="textinput" name="textinput" type="text" class="form-control input-md">*@
            {{SecondarySpecility[0].SpecialtyEffectiveDates}}
        </td>

    </tr>

    <tr ng-repeat="phy in delegationProfileObj.CoveringPhysicians track by $index">
        <td colspan="1" ng-if="$index==0" rowspan="{{delegationProfileObj.CoveringPhysicians.length}}">
            <label for="textinput">Covering Physicians</label>

        </td>
        <td colspan="1">
            {{phy}}
        </td>
        <td colspan="4"></td>

    </tr>

    <tr ng-repeat="hospital in delegationProfileObj.ProviderHospitalAffiliations track by $index">

        <td colspan="1" ng-if="$index==0" rowspan="{{delegationProfileObj.ProviderHospitalAffiliations.length}}">
            <label for="textinput">Hospital Affiliations</label>
        </td>

        <td colspan="1">
            {{hospital.HospitalName}}
        </td>
        <td colspan="1">
            {{hospital.EffectiveDates}}
        </td>
        <td colspan="4"></td>

    </tr>


</table>

<strong>Practice Information</strong>


<table class="table table-bordered small">
    <tbody ng-repeat="practice in delegationProfileObj.ProviderPracitceInfos track by $index">
        <tr>


            <td rowspan="2">
                <label for="textarea">
                    Location {{$index+1}}:<br />
                    Phone/Fax:
                </label>

            </td>

            <td colspan="6">

                {{practice.Address}}
            </td>



        <tr>


            <td colspan="3">
                {{practice.PhoneNumber}}
            </td>
            <td colspan="3">
                {{practice.FaxNumber}}
            </td>

        </tr>

        <tr>
            <td colspan="1">
                <label for="textinput">Office Hours:</label>
            </td>
            <td colspan="1">
                <p ng-repeat="c in practice.OfficeHourMondays track by $index"> {{c}}</p>
            </td>
            <td colspan="1">
                <p ng-repeat="c in practice.OfficeHourTuesdays track by $index"> {{c}}</p>

            </td>
            <td colspan="1">
                <p ng-repeat="c in practice.OfficeHourWednesdays track by $index"> {{c}}</p>

            </td>
            <td colspan="1">
                <p ng-repeat="c in practice.OfficeHourThursdays track by $index"> {{c}}</p>

            </td>
            <td colspan="1">
                <p ng-repeat="c in practice.OfficeHourFridays track by $index"> {{c}}</p>

            </td>
        </tr>

        <tr>


            <td rowspan="2">
                <label for="textarea">
                    Billing:<br />
                    Information:
                </label>

            </td>

            <td colspan="6">
                {{practice.BillingAddress}}
            </td>

        </tr>

        <tr>


            <td colspan="3">
                {{practice.BillingPhoneNumber}}
            </td>
            <td colspan="3">
                {{practice.BillingFaxNumber}}
            </td>

        </tr>



    </tbody>


</table>

<div ng-if="remarks">
    <strong>Remarks</strong>
    <table class="table table-bordered small">
        <tr>
            <td>
                {{remarks}}
            </td>
        </tr>
    </table>
</div>


