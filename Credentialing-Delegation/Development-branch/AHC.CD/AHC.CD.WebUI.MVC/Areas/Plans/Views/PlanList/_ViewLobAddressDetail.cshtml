
<fieldset>
    <legend>
        Address Details
    </legend>
    <div ng-repeat="lobadd in plob.LOBAddressDetails track by $index" class="col-lg-12 zero-padding-right-right-div">
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Street">Street:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.Street || 'Not available'}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Appartment">Appartment:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.Appartment || 'Not available' }}</i>
            </div>
        </div>
        <div class="col-lg-4 form-group">
            <label class="control-label small" for="City">City:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.City || 'Not available' }}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="State">State:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.State || 'Not available' }}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="State">ZipCode:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.ZipCode || 'Not available' }}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="Country">Country:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.Country || 'Not available'}}</i>
            </div>
        </div>
        <div class="col-md-4 form-group">
            <label class="control-label small" for="County">County:</label>
            <div>
                <i class="data-label  wrap-words">{{lobadd.County || 'Not available'}}</i>
            </div>
        </div>
    </div>
</fieldset>


