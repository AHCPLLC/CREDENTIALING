@*Practice Location Facility Information End*@ 

@*Practice Location Other Information Start*@
<div class="form-style">
    <fieldset>
        <legend>
            Other Information
        </legend>
        <div ng-hide="GeneralInformationEdit" class="legend2 pull-right">
            <button ng-click="editGeneralInfo(PracticeLocationDetail)" class="btn btn-xs btn-primary" title="Edit" tooltip><i class="fa fa-edit"></i></button>
            <button ng-click="sectionWiseVisibilityControlForUpdateHistory('GeneralInformationHistory');OperateUpdateHistory((PracticeLocationDetail.PracticeLocationDetailID),'PracticeLocationDetailID');" class="btn btn-xs btn-warning" title="Update Hisotry" tooltip><i class="fa fa-history"></i></button>
        </div>
        <div ng-show="!GeneralInformationEdit && UpdateHistorySectionVisibility !='GeneralInformationHistory'">
            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewGeneralInformation.cshtml")
        </div>

        <div ng-show="GeneralInformationEdit && UpdateHistorySectionVisibility !='GeneralInformationHistory'">
            <form id="updatePracticeLocationDetails{{$index}}">
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditGeneralInformation.cshtml")
            </form>
            <div class="col-lg-12">
                <div class="control-group">
                    <button class="btn btn-warning btn-sm" ng-click="editGeneralCancel()">Cancel</button>
                    <button class="btn btn-success btn-sm pull-right" ng-click="updatePracticeLocationDetailInformaton($index)">Update</button>
                </div>
            </div>
        </div>

        @*Update History button for Other Information*@
        <div ng-if="UpdateHistorySectionVisibility =='GeneralInformationHistory'">
            <div>
                <fieldset>
                    <legend>
                        Other Information History
                    </legend>
                    @Html.Partial("~/Areas/Profile/Views/MasterProfile/_ProfileUpdateHistory.cshtml")
                </fieldset>
            </div>
        </div>

        <div class="clearfix"></div>
        <div ng-if="activeMessageDiv=='UpdatePracticeLocation'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
        </div>
    </fieldset>
</div>
@*Practice Location Other Information End*@
