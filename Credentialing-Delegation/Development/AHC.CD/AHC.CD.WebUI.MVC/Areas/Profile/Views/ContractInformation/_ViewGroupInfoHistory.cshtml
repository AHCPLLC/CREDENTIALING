<fieldset>
    <legend>
        Group Information
    </legend>
    @Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
    <div class="col-md-12 zero-padding-left-right-div">
        <div class="col-md-3 form-group">
            <label class="control-label small">IPA/Group Name *</label>
            <div>
                <i class="data-label">{{groupHistory.PracticingGroup.Group.Name}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">Start Date *</label>
            <div>
                <i class="data-label">{{groupHistory.JoiningDate| date:'MM-dd-yyyy'}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">End Date</label>
            <div>
                <i class="data-label">{{groupHistory.ExpiryDate || 'Not Available' | date:'MM-dd-yyyy'}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">Status *</label>
            <div>
                <i class="data-label">{{groupHistory.ContractGroupStatus}}</i>
            </div>
        </div>
    </div>
    <div class="col-md-12 zero-padding-left-right-div">
        <div class="col-md-3 form-group">
            <label class="control-label small">Group TaxId</label>
            <div>
                <i class="data-label">{{gi.PracticingGroup.Group.TaxId || 'Not Available'}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            <label class="control-label small">Group NPI Number</label>
            <div>
                <i class="data-label">{{gi.PracticingGroup.Group.NPINumber || 'Not Available'}}</i>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="clearfix"></div>
    <div class="col-md-12 form-group">
        <div>
            <button class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
        </div>
    </div>
</fieldset>