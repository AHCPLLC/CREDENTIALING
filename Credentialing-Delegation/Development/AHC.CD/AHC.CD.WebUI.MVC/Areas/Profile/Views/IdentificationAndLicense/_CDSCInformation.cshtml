@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<div class="panel-default" id="CDSInformation">
    <div class="form-style">
        <div id="CDSInfo">
            <fieldset>
                <legend>
                    @if (!ViewBag.Roles.Contains("CCO") && !ViewBag.Roles.Contains("CRA"))
{
                        <i style="font-size:20px" tooltip data-toggle="tooltip" data-placement="top" title="This section is under observation by your credentialing co-ordinator" ng-show="CDSInformationPendingRequest" class="fa text-danger fa-exclamation-circle"></i>
                }
                    CDS Information
                </legend>
                <div class="legend2 pull-right">
                    @*<button type="button" ng-hide="CDSCInformations.length <1" class="btn btn-xs btn-primary" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="Download" data-original-title="" title="" ng-click="downloadTableData('Profile_Licenses_CDSCInformation','CDS Information')"><i class="fa fa-download"></i></button>*@
                    <button id="CDS_Print" type="button" ng-hide="CDSCInformations.length <1" class="btn btn-xs btn-primary" data-container="body" data-toggle="popover" popover data-placement="left" data-trigger="hover" data-content="Print" data-original-title="" title="" ng-click="printData('Profile_Licenses_CDSCInformation','CDS Information')"><i class="fa fa-print"></i></button>
                    <button id="CDS_HistoryIcon" type="button" ng-click="showCDSCHistory('CDShistory')" class="btn btn-xs btn-warning" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="View History"><i class="fa fa-clock-o"></i></button>
                    <div id="CDShistory" class="loading pull-right loadingHistory"><img src="/Content/Images/ajax-loader.gif" width="20" height="20"></div>
                    <button class="btn btn-xs btn-default" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="*If you have additional CDS Information include here."><i class="fa fa-question"></i></button>
                </div>
                <div id="licenseprofessional" class="panel-body">
                    <table ng-hide="CDSCInformations.length >0||newShowCDSCInformation" class="table table-bordered table-condensed customtbodyStyle table-striped">
                        <thead>
                            <tr>
                                <td ng-hide="visibilityControl=='addCDSCInformation'">
                                    No Data Available For CDS Information!
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Add, ViewBag.Roles, false))
                                {
                                        <span>
                                            Please click on <button id="noDataAddNewCDS" class="btn btn-xs btn-success" ng-click="operateViewAndAddControl('addCDSCInformation')" tooltip data-toggle="tooltip" data-placement="right" title="Add new"><i class="fa fa-plus"></i></button>
                                            to add new CDS Information
                                        </span>
                                }
                                </td>
                            </tr>
                        </thead>
                    </table>
                    <table ng-hide="CDSCInformations.length <1" class="table table-bordered table-condensed customtbodyStyle table-striped" id="Profile_Licenses_CDSCInformation">
                        <thead>
                            <tr>
                                <th>CDS Number</th>
                                <th>State Of Registration</th>
                                <th>Issue Date</th>
                                <th>Expiration Date</th>
                                <th class="hideData">Supporting Document</th>
                                <th class="hideData">Action</th>
                            </tr>
                        </thead>
                        <tbody ng-repeat="CDSCInformation in CDSCInformations track by $index">
                            <tr>
                                <td class="wrap-words">{{CDSCInformation.CertNumber}}</td>
                                <td class="wrap-words">{{CDSCInformation.State}}</td>
                                <td ng-show="CDSCInformation.IssueDate" class="wrap-words">{{CDSCInformation.IssueDate || 'Not Available'| date: "MM-dd-yyyy"}}</td>
                                <td ng-hide="CDSCInformation.IssueDate" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-hide="CDSCInformation.ExpiryDate" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformation.ExpiryDate" class="wrap-words">{{CDSCInformation.ExpiryDate || 'Not Available'| date: "MM-dd-yyyy"}}</td>
                                <td class="wrap-words hideData">
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Preview, ViewBag.Roles, false))
{
                                        <a ng-if="CDSCInformation.CDSCCerificatePath" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{CDSCInformation.CDSCCerificatePath}}" class="pointer" target="_blank" tooltip data-toggle="tooltip" data-placement="right" title="CDS Certificate"><i class="fa fa-file" title=""></i></a>
}
                                    <i ng-if="!CDSCInformation.CDSCCerificatePath">Not Available</i>
                                </td>
                                <td class="wrap-words hideData">
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.View, ViewBag.Roles, false))
{
    <button id="CDS_Table_ViewIcon" class="btn btn-xs btn-default" ng-click="operateViewAndAddControl($index+'_viewCDSCInformation')" ng-hide="viewCDSC" tooltip data-toggle="tooltip" data-placement="left" title="View"><i class="fa fa-eye"></i></button>
}
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Edit, ViewBag.Roles, false))
{
    <button id="CDS_Table_EditIcon" class="btn btn-xs btn-primary" ng-click="operateEditControl($index+'_editCDSCInformation',CDSCInformation)" ng-hide="editShowCDSCInformation" tooltip data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
}
                                    <button id="CDS_Table_RenewIcon" class="btn btn-xs btn-default" ng-click="operateRenewControl($index+'_renewCDSCInformation',CDSCInformation); RenewDivCDS(CDSCInformation);" ng-hide="renewCDSCInformation" tooltip data-toggle="tooltip" data-placement="top" title="Renew"><i class="fa fa-repeat"></i></button>
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Add, ViewBag.Roles, false))
{
    <button id="CDS_Table_RenewIcon" class="btn btn-xs btn-danger" data-toggle="tooltip" data-placement="top" title="Remove" ng-click="initCDSCWarning(CDSCInformation);" tooltip><i class="fa fa-minus-circle"></i></button>
    <button id="addNewCDS" ng-if="$index == CDSCInformations.length-1" class="btn btn-xs btn-success" ng-click="operateViewAndAddControl('addCDSCInformation');HideErrorMessages()" tooltip data-toggle="tooltip" data-placement="right" title="Add new" ng-hide="viewCDSC || editShowCDSCInformation || newShowCDSCInformation || $index+1!=CDSCInformations.length"><i class="fa fa-plus"></i></button>
}
                                    <button class="btn btn-xs btn-warning" data-toggle="tooltip" data-placement="top" title="View Update History" ng-click="sectionWiseVisibilityControlForUpdateHistory($index+'CDSCInformationUH');OperateUpdateHistory(CDSCInformation.CDSCInformationID,'CDSCInformationID');" tooltip><i class="fa fa-history"></i></button>
                                </td>
                            </tr>
                            @if (ProfileActionValidator.IsAuthorized(ProfileActionType.View, ViewBag.Roles, false))
                        {
                                <tr ng-show="visibilityControl==($index+'_viewCDSCInformation')" class="hideData">
                                    <td colspan="6" class="editTableDataStyle">
                                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewCDSCInformation.cshtml")
                                    </td>
                                </tr>
                        }
                            @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Edit, ViewBag.Roles, false))
                        {
                                <tr ng-show="visibilityControl==($index+'_editCDSCInformation')" class="hideData">
                                    <td colspan="6" id="cDSCInformationEditDiv{{$index}}" class="editTableDataStyle">
                                        <fieldset>
                                            <legend>
                                                Edit CDS Information
                                            </legend>
                                            <div>
                                                @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditCDSCInformation.cshtml")
                                            </div>
                                        </fieldset>
                                    </td>
                                </tr>
                        }
                            <tr ng-show="UpdateHistorySectionVisibility==($index+'CDSCInformationUH')" class="hideData">
                                <td colspan="7" class="editTableDataStyle">
                                    <fieldset>
                                        <legend>
                                            CDSC Information Update History
                                        </legend>
                                        <div>
                                            @Html.Partial("~/Areas/Profile/Views/MasterProfile/_ProfileUpdateHistory.cshtml")
                                        </div>
                                    </fieldset>
                                </td>
                            </tr>
                            <tr ng-show="visibilityControl==($index+'_renewCDSCInformation')" class="hideData">
                                <td colspan="7" id="CDSCInformationRenewDiv{{$index}}" class="editTableDataStyle">
                                    <fieldset>
                                        <legend>
                                            Renew CDS Information
                                        </legend>
                                        <div>
                                            @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_RenewCDSInformation.cshtml")
                                        </div>
                                    </fieldset>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="clearfix"></div>
                    <div ng-show="activeMessageDiv=='CDSError1'" class="col-lg-12">
                        <br />
                        <div class="alert alert-danger" role="alert">
                            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                            <span class="sr-only">Error:</span>
                            Error List : <br />
                            <span>{{messageDesc}}</span>
                        </div>
                    </div>
                    <div ng-show="activeMessageDiv=='addedCDS'" class="alert alert-success" role="alert">
                        <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                        <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
                    </div>
                    <div ng-show="activeMessageDiv=='removeCDS'">
                        <div class="alert alert-danger" role="alert">
                            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                            <span class="sr-only">Error:</span>
                            Error Lists :<br />
                            <span ng-repeat="err in errorCDS track by $index">{{err}}</span>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="form-style">
                        @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Add, ViewBag.Roles, false))
                    {
                            <fieldset ng-show="visibilityControl=='addCDSCInformation'">
                                <legend>
                                    Add CDS Information
                                </legend>
                                <div class="legend2 pull-right">
                                    <button class="btn btn-xs btn-default" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="*Provide all current and previous licenses/certifications"><i class="fa fa-question"></i></button>
                                </div>
                                <div id="newShowCDSCInformationDiv">
                                    @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditCDSCInformation.cshtml")
                                </div>
                            </fieldset>
                    }
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="modal fade" id="cDSCWarningModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <form id="removeCDSC">
                        <div class="modal-header">
                            <button id="CDS_Remove_Close" type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                        </div>
                        <div class="col-lg-12 text-center" ng-show="isRemoved">
                            <div style="margin-top:2%;">
                                <img src="~/Content/Images/ajax-loader.gif" style="width:50px;" /> Please Wait..
                            </div>
                        </div>
                        <div class="modal-body" ng-show="!isRemoved">
                            Do you really want to remove <b>{{tempCDSC.CertNumber}}</b> from your CDS Information list ?
                            <input type="hidden" name="CDSCInformationID" value="{{tempCDSC.CDSCInformationID}}" />
                            <input type="hidden" name="StatusType" value="2" />
                        </div>
                        <div class="modal-footer">
                            <button id="CDS_Remove_CancelButton" type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                            <button id="CDS_Remove_RemoveButton" type="button" class="btn btn-primary" ng-click="removeCDSC(CDSCInformations)">Remove</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div id="CDSInfoHistory">
            <fieldset ng-if="showCDSCInformationHistoryTable">
                <legend>
                    CDS Information History
                </legend>
                <div class="legend2 pull-right">
                    @*<button type="button" ng-hide="CDSCInformationsHistory.length < 1" class="btn btn-xs btn-primary" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="Download" data-original-title="" title="" ng-click="downloadTableData('Profile_Licenses_CDSCInformationHistory','CDS Information History')"><i class="fa fa-download"></i></button>*@
                    <button id="CDS_History_PrintButton" type="button" ng-hide="CDSCInformationsHistory.length < 1" class="btn btn-xs btn-primary" data-container="body" data-toggle="popover" popover data-placement="left" data-trigger="hover" data-content="Print" data-original-title="" title="" ng-click="printData('Profile_Licenses_CDSCInformationHistory','CDS Information History')"><i class="fa fa-print"></i></button>
                    <button id="CDS_History_CancelButton" type="button" ng-click="cancelCDSCHistory()" class="btn btn-xs btn-default" data-container="body" data-toggle="popover" data-placement="left" data-trigger="hover" data-content="View History"><i class="fa fa-times text-danger"></i></button>
                </div>
                <div class="panel-body">
                    <table ng-hide="CDSCInformationsHistory.length > 0" class="table table-bordered table-condensed customtbodyStyle table-striped">
                        <thead>
                            <tr>
                                <td>
                                    No History Available for CDS Information!
                                </td>
                            </tr>
                        </thead>
                    </table>
                    <table ng-hide="CDSCInformationsHistory.length < 1" class="table table-bordered table-condensed customtbodyStyle table-striped" id="Profile_Licenses_CDSCInformationHistory">
                        <thead>
                            <tr>
                                <th>CDS Number</th>
                                <th>State Of Registration</th>
                                <th>Issue Date</th>
                                <th>Expiration Date</th>
                                <th class="hideData">Supporting Document</th>
                                <th>Status</th>
                                <th>Deleted By</th>
                                <th>Deleted On</th>
                                <th class="hideData">Action</th>
                            </tr>
                        </thead>
                        <tbody ng-repeat="CDSCInformationHistory in CDSCInformationsHistory track by $index">
                            <tr>
                                <td ng-hide="CDSCInformationHistory.CertNumber" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformationHistory.CertNumber" class="wrap-words">{{CDSCInformationHistory.CertNumber}}</td>
                                <td ng-hide="CDSCInformationHistory.State" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformationHistory.State" class="wrap-words">{{CDSCInformationHistory.State}}</td>
                                <td ng-show="CDSCInformationHistory.IssueDate" class="wrap-words">{{CDSCInformationHistory.IssueDate || 'Not Available'| date: "MM-dd-yyyy"}}</td>
                                <td ng-hide="CDSCInformationHistory.IssueDate" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-hide="CDSCInformationHistory.ExpiryDate" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformationHistory.ExpiryDate" class="wrap-words">{{CDSCInformationHistory.ExpiryDate || 'Not Available'| date: "MM-dd-yyyy"}}</td>
                                <td class="wrap-words hideData">
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Preview, ViewBag.Roles, false))
{
                                        <a ng-if="CDSCInformationHistory.CDSCCerificatePath" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{CDSCInformationHistory.CDSCCerificatePath}}" class="pointer" target="_blank" tooltip data-toggle="tooltip" data-placement="left" title="CDS Certificate"><i class="fa fa-file" title=""></i></a>
}
                                    <i ng-if="!CDSCInformationHistory.CDSCCerificatePath">Not Available</i>
                                </td>
                                <td>{{CDSCInformationHistory.HistoryStatus}}</td>
                                <td ng-hide="CDSCInformationHistory.DeletedBy" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformationHistory.DeletedBy" class="wrap-words">{{CDSCInformationHistory.DeletedBy }}</td>
                                <td ng-hide="CDSCInformationHistory.DeletedDate" class="wrap-words"><i class="data-label">Not Available</i></td>
                                <td ng-show="CDSCInformationHistory.DeletedDate" class="wrap-words">{{CDSCInformationHistory.DeletedDate}}</td>

                                <td class="wrap-words hideData">
                                    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.View, ViewBag.Roles, false))
{
    <button id="View_History_new_CDS" class="btn btn-xs btn-default" ng-click="operateViewAndAddControl($index+'_viewCDSCInformationHistory')" ng-hide="viewCDSC" tooltip data-toggle="tooltip" data-placement="left" title="View"><i class="fa fa-eye"></i></button>
}
                                </td>
                            </tr>
                            @if (ProfileActionValidator.IsAuthorized(ProfileActionType.View, ViewBag.Roles, false))
                        {
                                <tr ng-show="visibilityControl==($index+'_viewCDSCInformationHistory')" class="hideData">
                                    <td colspan="9" class="editTableDataStyle">
                                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewCDSCInformationHistory.cshtml")
                                    </td>
                                </tr>
                        }
                        </tbody>
                    </table>
                </div>
            </fieldset>
        </div>
    </div>
</div>