                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.AppointmentProviderType)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.AppointmentProviderType, (int)value, new { data_ng_model = "tempObject1.AppointmentProviderType", data_ng_click = "getProviderTypeValue(value)" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">County :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    @Html.TextBoxFor(model => model.County, new { @class = "form-control input-sm", data_ng_model = "tempObject1.County" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Specialty :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
