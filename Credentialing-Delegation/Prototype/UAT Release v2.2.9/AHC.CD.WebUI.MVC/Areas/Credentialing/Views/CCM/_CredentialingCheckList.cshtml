                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.GapsInPracticeYesNoOption, (int)value, new { data_ng_model = "tempObject1.GapsInPracticeYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForGapsInPractice, new { @class = "form-control input-sm", data_ng_model = "tempObject1.RemarksForGapsInPractice" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Clean license :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.CleanLicenseYesNoOption, (int)value, new { data_ng_model = "tempObject1.CleanLicenseYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForCleanLicense, new { @class = "form-control input-sm", data_ng_model = "tempObject1.RemarksForCleanLicense" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">NPDB Issues :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.NPDBIssueYesNoOption, (int)value, new { data_ng_model = "tempObject1.NPDBIssueYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForNPDBIssue, new { @class = "form-control input-sm", data_ng_model = "tempObject1.RemarksForNPDBIssue" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Malpractice Issues :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.MalpracticeIssueYesNoOption, (int)value, new { data_ng_model = "tempObject1.MalpracticeIssueYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForMalpracticeIssue, new { @class = "form-control input-sm", data_ng_model = "tempObject1.remarkForMalpracticeIssues" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Years in Practice :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    @Html.TextBoxFor(model => model.YearsInPractice, new { @class = "form-control input-sm", data_ng_model = "tempObject1.YearsInPractice" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Site Visit Required. If yes, were there any issues :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.SiteVisitRequiredYesNoOption, (int)value, new { data_ng_model = "tempObject1.SiteVisitRequiredYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForSiteVisitRequired, new { @class = "form-control input-sm", data_ng_model = "tempObject1.RemarksForSiteVisitRequired" })
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Recommended Provider Level :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.CredentialingLevel)))
                    {
                        <label class="radio-inline">
                            @Html.RadioButtonFor(model => model.RecommendedCredentialingLevel, (int)value, new { data_ng_model = "tempObject1.RecommendedCredentialingLevel" }) @value.ToString()
                        </label>
                    }
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Attach Document :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                        @Html.HiddenFor(model => model.FileUploadPath, new { @Value = "{{tempObject1.FileUploadPath}}" })
                        <div class="form-control jancyFileWrapTexts" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename wraptext"></span></div>
                        <span class="input-group-addon btn btn-default btn-sm btn-file">
                            <span class="fileinput-new">Select Document</span><span class="fileinput-exists">Change</span>
                            @Html.TextBoxFor(model => model.FileUpload, new { data_ng_model = "tempObject1.FileUpload", type = "file", @class = "fileinputtag", onchange = "setFileNameWith(this); angular.element(this).scope().setFiles(this)" })

                        </span>
                        <a href="#" class="input-group-addon btn btn-default btn-sm fileinput-exists" data-dismiss="fileinput" ng-click="removeFile('ContractInfo_CVFile')">Remove</a>
                    </div>
                </div>

                <span data-ng-if="tempObject1.FileUploadPath" class="col-lg-1 col-md-1 col-sm-1">
                    <a toggle data-toggle="tooltip" data-placement="bottom" title="Preview" href="{{tempObject1.FileUploadPath}}" target="_blank"><i class="fa fa-2x fa-file"></i></a>
                </span>
            </div>
            

            <div class="form-group">
                <div class="col-lg-5 col-md-5 col-sm-5 control-label">

                </div>
                <div class="col-lg-5 col-md-5 col-sm-5 control-label">
                    <button class="btn btn-primary btn-sm" type="submit">Save</button>
                </div>
            </div>

        </div>

    </form>
</div>



<div ng-if="((credentialingFilterInfo.CredentialingLogs[0].CredentialingAppointmentDetail!=null && credentialingFilterInfo.CredentialingLogs[0].CredentialingAppointmentDetail.CredentialingAppointmentResult==null) || (tempObject1.CredentialingAppointmentDetailID!=null && viewc==false))">

    <div class="col-lg-12 col-md-12 zero-padding-left-right-div" ng-if="ViewCheckList">
        <form class="form-horizontal">
            @Html.HiddenFor(model => model.CredentialingAppointmentDetailID, new { ng_value = "tempObject1.CredentialingAppointmentDetailID" })
            @Html.HiddenFor(model => model.CredentialingAppointmentResult.SignedByID, new { ng_value = "tempObject1.CredentialingAppointmentResult.SignedByID" })
            @Html.HiddenFor(model => model.CredentialingAppointmentResult.CredentialingLevel, new { ng_value = "tempObject1.CredentialingAppointmentResult.CredentialingLevel" })
            <div id="message1" class="col-lg-12 zero-padding-left-right-div">
                <div ng-show="activeMessageDiv=='ProviderCheckList'" class="alert alert-success" role="alert">

                    {{messageDesc}}
                </div>
            </div>
            <div class="col-lg-12 text-right ng-scope" ng-if="temp==true">
                <a class="btn btn-primary btn-xs" ng-click="Edit()">Edit</a>
                <a class="btn btn-primary btn-xs" href="/credentialing/cnd/credentialingappointment">Set Appointment</a>
                <a onclick="PrintElem();" class="btn btn-xs btn-primary" data-toggle="modal" href="#" target="_blank" tooltip data-toggle="tooltip" data-placement="bottom" title="Print">Print <i class="fa fa-print"></i></a>
            </div>
            <div class="form-group">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">Name :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <label class="control-label"><i style="font-weight:100">{{credentialingFilterInfo.Profile.PersonalDetail.Salutation}} {{tempObject1.FirstName}} {{tempObject1.MiddleName}} {{tempObject1.LastName}}</i></label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">Provider Type :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.ProviderType}}</i></label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">County :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.County}}</i></label>
                </div>
            </div>
            <div class="form-group" ng-if="tempObject1.CredentialingSpecialityLists.length > 0">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">Specialty :</label>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <label class="control-label" ng-repeat="tpt in tempObject1.CredentialingSpecialityLists track by $index" ng-if="tpt.Status=='Active'">
                        <i class="label label-default">{{tpt.Name}}</i>&nbsp;&nbsp;
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">Board Certification :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.BoardCertified}}</i></label>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.RemarksForBoardCertification}}</i></label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-5 col-md-5 col-sm-5 control-label">Hospital Privileges :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.HospitalPrivileges}}</i></label>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    <label class="control-label"><i style="font-weight:100">{{tempObject1.RemarksForHospitalPrivileges}}</i></label>
                </div>
            </div>

            <div ng-if="tempObject1.CredentialingCoveringPhysicians.length > 0" class="form-group">
