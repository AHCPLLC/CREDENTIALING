@model AHC.CD.WebUI.MVC.Areas.Credentialing.Models.CredentialingCheckList.CredentialingAppointmentDetailViewModel
<style>
    .ProviderTypeSelectAutoList.popover.bottom {
        margin-top: 32px;
    }

    .table {
        width: 100%;
        max-width: 100%;
        margin-bottom: 0px;
    }
</style>
<script type="text/javascript">

    function PrintElem() {
        Popup($(ccmDiv).html());
        $('#message1').hide();
    }

    function myFunction() {
        window.print();
    }

    function Popup(data) {
        var mywindow = window.open('', data, 'height=auto,width=auto');
        mywindow.document.write('<html><head><title>my div</title>');
        mywindow.document.write('<link rel="stylesheet" href="/Content/bootstrap.min.css" type="text/css" />');
        mywindow.document.write('<link href="~/Content/SharedCss/jasny-bootstrap.css" rel="stylesheet" />');
        mywindow.document.write('<style> .form-style fieldset { padding: 15px 20px 5px 20px; } .grow { transition: all .2s ease-in-out; } .grow:hover { transform: scale(2.1); } </style>');
        //mywindow.document.write('<link href="~/Content/SharedCss/bootstrap.min.css" rel="stylesheet" />');
        mywindow.document.write('</head><body >');
        mywindow.document.write(data);
        mywindow.document.write('</body></html>');

        mywindow.document.close(); // necessary for IE >= 10
        mywindow.focus(); // necessary for IE >= 10

        setTimeout(function () {
            mywindow.print();
            mywindow.close();
        }, 500);
        return true;
    }

</script>
<div class="col-lg-12 text-right" ng-if="credentialingFilterInfo.CredentialingLogs[0].CredentialingActivityLogs.length==1">
    <a href="/Credentialing/Verification/InitiateNewPSV?profileId={{credentialingFilterInfo.Profile.ProfileID}}&credinfoId={{credentialingFilterInfo.CredentialingInfoID}}"><button class="btn btn-primary btn-xs">Perform PSV</button><span style="width:10px"></span></a>
    <div class="col-lg-12 text-center">
        <div class="col-md-12"><strong>Verification Report</strong></div>

    </div>
    <div class="col-lg-12 text-center" role="alert">
        Primary Source Verification is not completed
    </div>
</div>
<div ng-if="(credentialingFilterInfo.CredentialingLogs[0].CredentialingAppointmentDetail==null && credentialingFilterInfo.CredentialingLogs[0].CredentialingActivityLogs.length>1 && viewc==true)">
    <form id="CredentialingCheckList" class="form-horizontal" ng-submit="credentialingCheckList('CredentialingCheckList')">
        @Html.HiddenFor(model => model.CredentialingAppointmentDetailID, new { ng_value = "tempObject1.CredentialingAppointmentDetailID" })
        @Html.HiddenFor(model => model.FirstName, new { ng_value = "tempObject1.FirstName" })
        @Html.HiddenFor(model => model.MiddleName, new { ng_value = "tempObject1.MiddleName" })
        @Html.HiddenFor(model => model.LastName, new { ng_value = "tempObject1.LastName" })

