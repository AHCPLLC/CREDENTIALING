                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].SpecialtyID" value="{{tpt.SpecialtyID}}" />
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].Name" value="{{tpt.Name}}" />
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].StatusType" value="{{tpt.StatusType}}" />
                        &nbsp;&nbsp;
                    </span>
                    <span ng-repeat="tpt in tempObject1.CredentialingSpecialityLists track by $index" ng-if="tpt.StatusType==2" ng-hide="true">
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].CredentialingSpecialityListID" value="{{tpt.CredentialingSpecialityListID}}" />
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].SpecialtyID" value="{{tpt.SpecialtyID}}" />
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].Name" value="{{tpt.Name}}" />
                        <input type="hidden" name="CredentialingSpecialityLists[{{$index}}].StatusType" value="{{tpt.StatusType}}" />
                    </span>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-4 col-md-4 col-sm-4 control-label">Board Certification :</label>
                <div class="col-lg-2 col-md-2 col-sm-2">
