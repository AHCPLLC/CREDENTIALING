                    <div class="radio">
                        @foreach (var value in Enum.GetValues(typeof(AHC.CD.Entities.MasterData.Enums.YesNoOption)))
                        {
                            <label class="radio-inline">
                                @Html.RadioButtonFor(model => model.HospitalPrivilegesYesNoOption, (int)value, new { data_ng_model = "tempObject1.HospitalPrivilegesYesNoOption" }) @value.ToString()
                            </label>
                        }
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4">
                    @Html.TextBoxFor(model => model.RemarksForHospitalPrivileges, new { @class = "form-control input-sm", data_ng_model = "tempObject1.RemarksForHospitalPrivileges" })
                </div>
            </div>
            <div class="form-group">
