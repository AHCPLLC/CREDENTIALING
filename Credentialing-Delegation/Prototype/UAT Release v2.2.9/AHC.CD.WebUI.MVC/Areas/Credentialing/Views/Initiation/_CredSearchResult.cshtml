<div class="col-lg-12" ng-cloak ng-if="data.length!=0" style="overflow-x:auto;">
    <table ng-table="tableParams1" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected">
        <tr ng-repeat="user in $data track by $index">
            <td class="col-lg-1" data-title="'Picture'">
                <div ng-show="user.ProfilePhotoPath == null" class="thumbnail imageStyle">
                    <img ng-src="/Content/Images/no_image.png" class="img-rounded" />
                </div>
                <div ng-hide="user.ProfilePhotoPath == null" class="thumbnail imageStyle">
                    <img ng-src="/Document/View?path={{user.ProfilePhotoPath}}" class="img-rounded">
                </div>
            </td>
            <td class=" wrap-words" data-title="'NPI'" header-class="text-left" sortable="'NPINumber'" filter="{ 'NPINumber': 'text' }">
                <span ng-show="user.NPINumber == 0"></span>
                {{user.NPINumber}}
            </td>
            <td class=" wrap-words" data-title="'Title'" header-class="text-left" sortable="'Titles'" filter="{ 'Titles': 'text' }">
                <span ng-show="user.Titles.length==0"></span>
                <span ng-repeat="title in user.Titles track by $index"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{title}}</span>
            </td>
            <td class="wrap-words" data-title="'Name'" header-class="text-left" sortable="'FirstName' || 'LastName'" filter="{ 'FirstName': 'text' } || { 'LastName': 'text' }">
                
                {{user.FirstName}} {{user.LastName}}
            </td>
            <td class="wrap-words" data-title="'Specialties'" header-class="text-left" sortable="'Specialties'" filter="{ 'Specialties': 'text' }">
                <span ng-show="user.Specialties.length == 0"></span>
                <span ng-repeat="s in user.Specialties"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{s}}</span>
            </td>
            <td class="wrap-words col-lg-1" data-title="'IPA'" header-class="text-left" sortable="'IPAGroupNames'" filter="{ 'IPAGroupNames': 'text' }">
                <span ng-show="user.IPAGroupNames.length > 0" ng-repeat="group in user.IPAGroupNames track by $index"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{group}}</span>
                <span ng-show="user.IPAGroupNames.length == 0"></span>
            </td>
            <td class="col-lg-1" data-title="'Action'" header-class="text-right">
                <a href="~/Profile/MasterProfile/ProviderProfile/{{user.ProfileID}}" data-toggle="tooltip" data-placement="left" title="View Profile" class="btn btn-xs btn-info" target="_blank"><i class="fa fa-chevron-circle-right"></i></a>
                <a class="btn btn-primary btn-xs" ng-click="SelectProviderForCredentialingInitiation(user)" data-toggle="tooltip" data-placement="right" title="Initiate"><i class="fa fa-hand-o-up"></i></a>
                @*  <div ng-show="initiateSuccess" class="col-lg-6 text-right"><a class="btn btn-success btn-sm" href="~/credentialing/cnd/index"> <i class="fa fa-home"></i></a></div>*@
            </td>
        </tr>

    </table>
</div>

  
