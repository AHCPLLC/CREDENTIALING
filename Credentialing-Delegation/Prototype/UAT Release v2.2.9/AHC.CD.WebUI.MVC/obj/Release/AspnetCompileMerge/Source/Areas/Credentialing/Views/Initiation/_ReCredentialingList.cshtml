

<div class="panel panel-default content-panel" style="border:none">
    @*<div class="panel-heading">
            <span class="badge bg-info pull-right" ng-show="ifFilter()">{{getIndexFirst()}} - {{getIndexLast()}} of {{data.length}}</span>
            <h3 class="panel-title">List of ReCredentialing Initiations</h3>
        </div>*@
    <div class="clearfix"></div>
    <div class="panel-body" ng-cloak>
        <div class="col-lg-12" style="overflow-x:auto;">
            <table ng-table="tableParams2" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected">

                <!-- ngInclude: templates.header -->
                
                <tr ng-repeat="user in $data track by $index ">

                    <td data-title="'First Name'" header-class="text-left" filter="{ 'FirstName': 'text' }">
                        {{user.FirstName}}
                    </td>
                    <td data-title="'Last Name'" header-class="text-left" filter="{ 'LastName': 'text' }">
                        {{user.LastName}}
                    </td>
                    <td data-title="'Plan'" header-class="text-left" filter="{ 'Plan': 'text' }">
                        {{user.PlanName}}
                    </td>

                    <td data-title="'Initiated Date'" header-class="text-left" filter="{ 'InitiationDate': 'text' }" data-title-text="Initiated Date" class="ng-binding">
                        {{user.InitiationDate | date:'MM-dd-yyyy'}}
                    </td>
                    <td data-title="'Status'" header-class="text-left" filter="{ 'Status': 'text' }">
                        {{user.Status}}
                    </td>
                    <td>
                        <a class="btn btn-xs btn-info" href="~/Credentialing/CnD/Application/{{user.CredentialingInfoID}}" target="_blank"><i class="fa fa-caret-right" data-toggle="tooltip" data-placement="top" title="Use Application"></i></a>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</div>