<div><a class="btn btn-xs btn-warning pull-right" ng-click="OIGHistoryDiv=true" toggle data-toggle="tooltip" data-placement="bottom" title="OIG History"><i class="fa fa-clock-o"></i></a></div>


<div class="panel-body">
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="OIG.ViewDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_ViewOIGVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="OIG.AddDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditOIGVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="OIG.EditDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditOIGVerificationResult.cshtml")
    </div>
    <div class="clearfix"></div><br />
    <div ng-show="activeMessageDiv=='oigPSV'" class="alert alert-success" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
    <div ng-show="activeMessageDiv=='oigPSVError'" class="alert alert-danger" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
    
</div>
