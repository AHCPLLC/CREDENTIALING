<div class="col-lg-12" ng-cloak ng-if="data.length!=0" style="overflow-x:auto;">
    <table ng-table="tableParams2" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected ng-scope">

        <tr ng-repeat="user in $data track by $index">
            <td class="col-lg-1" data-title="'Picture'">
                <div ng-show="user.Profile.ProfilePhotoPath == null" class="thumbnail imageStyle">
                    <img ng-src="/Content/Images/no_image.png" class="img-rounded" />
                </div>
                <div ng-hide="user.Profile.ProfilePhotoPath == null" class="thumbnail imageStyle">
                    <img ng-src="/Document/View?path={{user.Profile.ProfilePhotoPath}}" class="img-rounded">
                </div>
            </td>
            <td class=" wrap-words" data-title="'NPI'" header-class="text-left" sortable="'NPINumber'" filter="{ 'NPINumber': 'text' }">
                {{user.Profile.OtherIdentificationNumber.NPINumber}}
            </td>
            <td class=" wrap-words" data-title="'Title'" header-class="text-left" sortable="'Titles'" filter="{ 'Titles': 'text' }">
                <span ng-show="user.Profile.PersonalDetail.ProviderTitles.length==0">Not Available</span>
                <span ng-repeat="title in user.Profile.PersonalDetail.ProviderTitles track by $index"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{title.ProviderType.Title}}</span>
            </td>
            <td class="wrap-words" data-title="'Name'" header-class="text-left" sortable="'FirstName' || 'LastName'" filter="{ 'FirstName': 'text' } || { 'LastName': 'text' }">

                {{user.Profile.PersonalDetail.FirstName}} &nbsp; {{user.Profile.PersonalDetail.LastName}}
            </td>

            <td class="wrap-words" data-title="'Specialties'" header-class="text-left" sortable="'Specialties'" filter="{ 'Specialties': 'text' }">
                <span ng-show="user.Profile.SpecialtyDetails.length == 0"></span>
                <span ng-repeat="s in user.Profile.Specialties"><i ng-if="$index>0">,</i><br ng-if="$index>0" />{{s}}</span>
            </td>
            <td class="wrap-words" data-title="'IPA'" header-class="text-left" sortable="'IPAGroupNames'" filter="{ 'IPAGroupNames': 'text' }">
                <span ng-show="user.Profile.ContractInfoes.length == 0"></span>
                <span ng-show="user.Profile.ContractInfoes.length > 0" ng-repeat="group in user.Profile.ContractInfoes">
                    <span ng-show="group.ContractGroupInfoes.length > 0" ng-repeat="groupName in group.ContractGroupInfoes track by $index">
                        <i ng-if="$index>0">,</i><br ng-if="$index>0" />{{groupName.PracticingGroup.Group.Name}}
                    </span>
                </span>
                <span ng-show="user.Profile.ContractInfoes.length == 0"></span>
            </td>
            <td class=" wrap-words" data-title="'Plan'" header-class="text-left" sortable="'PlanName'" filter="{ 'PlanName': 'text' }">
                <span ng-show="user.Plan.PlanName == null"></span>
                {{user.Plan.PlanName}}
            </td>
            <td class="col-lg-1" data-title="'Action'" header-class="text-right">
                <a href="~/Profile/MasterProfile/ProviderProfile/{{user.ProfileID}}" data-toggle="tooltip" data-placement="left" title="View Profile" class="btn btn-xs btn-info"><i class="fa fa-chevron-circle-right"></i></a>
                <a class="btn btn-primary btn-xs" ng-click="SelectProviderForReCredentialingInitiation(user)" data-toggle="tooltip" data-placement="right" title="Initiate"><i class="fa fa-hand-o-up"></i></a>
                @* <div ng-show="initiateSuccess" class="col-lg-6 text-right"><a class="btn btn-success btn-sm" href="~/credentialing/cnd/index"> <i class="fa fa-home"></i></a></div>*@
            </td>
        </tr>

    </table>
</div>

