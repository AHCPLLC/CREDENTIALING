        </td>
        <td colspan="6">
            {{med}}
        </td>
    </tr>
    <tr ng-repeat="internship in filteredProviderProgramDetails=(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'internship'}) track by $index">
        <td ng-if="$index==0" rowspan="{{(delegationProfileObj.ProviderProgramDetails | filter:{ProgramName:'internship'}).length}}">
            <label>
                Internship:<br />
                (Facility,<br />Specality,<br />From/To)
            </label>
        </td>

        <td colspan="2">
            {{internship.Facility}}
        </td>
        <td colspan="2">
            {{internship.Specialty}}
        </td>
        <td colspan="2">
            {{internship.FromToDate}}
        </td>
    </tr>

