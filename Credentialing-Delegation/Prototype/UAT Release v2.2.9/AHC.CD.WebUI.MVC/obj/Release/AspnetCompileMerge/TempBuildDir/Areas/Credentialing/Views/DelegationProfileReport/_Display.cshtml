<strong>Practice Information</strong>


<table class="table table-bordered small">
    <tbody ng-repeat="practice in delegationProfileObj.ProviderPracitceInfos track by $index">
        <tr>


            <td rowspan="2">
                <label for="textarea">
                    Location {{$index+1}}:<br />
                    Phone/Fax:
                </label>

            </td>

            <td colspan="6">

                {{practice.Address}}
            </td>



        <tr>


            <td colspan="3">
                {{practice.PhoneNumber}}
            </td>
            <td colspan="3">
                {{practice.FaxNumber}}
            </td>

        </tr>

        <tr>
            <td colspan="1">

                <label for="textinput">Office Hours</label>
            </td>

            <td colspan="1">
                {{practice.OfficeHourMonday}}
            </td>
            <td colspan="1">
                {{practice.OfficeHourTuesday}}
            </td>
            <td colspan="1">
                {{practice.OfficeHourWednesday}}
            </td>
            <td colspan="1">
                {{practice.OfficeHourThursday}}
            </td>
            <td colspan="1">
                {{practice.OfficeHourFridayday}}
            </td>
        </tr>

        <tr>


            <td rowspan="2">
                <label for="textarea">
                    Billing:<br />
                    Information:
                </label>

            </td>

            <td colspan="6">
                {{practice.BillingAddress}}
            </td>

        </tr>

        <tr>


            <td colspan="3">
                {{practice.BillingPhoneNumber}}
            </td>
            <td colspan="3">
                {{practice.BillingFaxNumber}}
            </td>

        </tr>



    </tbody>


</table>
