@*ng-if="visibilitySecondControl != 'MidLevelPractitionerEdit'"*@

<div id="SupervisingProvider">
    <fieldset>
        <legend>
            Supervising Provider
        </legend>
        <table ng-hide="PracticeLocationDetail.SupervisingProviders.length > 0 || visibilityThirdControl == 'SupervisingProviderEdit'" class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <td>
                        No Data Available for Supervising Providers !
                        <span>Please click on <button class="btn btn-xs btn-success" ng-click="operateThirdViewAndAddControl('SupervisingProviderEdit')" data-toggle="tooltip" data-placement="top" title="Add New" id="noDataAddNewPracticeInterest" tooltip><i class="fa fa-plus"></i></button> to add new Supervising Provider's information.</span>
                    </td>
                </tr>
            </thead>
        </table>
        <table ng-if="PracticeLocationDetail.SupervisingProviders.length>0" class="table table-bordered table-condensed customtbodyStyle table-striped">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Middle name</th>
                    <th>Last Name</th>
                    <th>NPI Number</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody ng-repeat="provider in PracticeLocationDetail.SupervisingProviders">
                <tr>
                    <td>{{provider.FirstName || 'Not Available'}}</td>
                    <td>{{provider.MiddleName || 'Not Available'}}</td>
                    <td>{{provider.LastName || 'Not Available'}}</td>
                    <td>{{provider.NPINumber || 'Not Available'}}</td>
                    <td class="col-lg-2 zero-padding-left-right-div">
                        <button class="btn btn-xs btn-default" data-toggle="tooltip" data-placement="left" title="View / Edit" ng-click="operateThirdViewAndAddControl($index+'_ViewSupervisingPractitioner')" tooltip><i class="fa fa-eye"></i></button>
                        <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Edit" ng-click="operateThirdEditControl('SupervisingProviderEdit'+$index, provider)" tooltip><i class="fa fa-edit"></i></button>
                        <button class="btn btn-xs btn-danger" data-toggle="tooltip" data-placement="top" title="Remove" ng-click="initSupervisingWarning(provider);" tooltip><i class="fa fa-minus-circle"></i></button>
                        <button class="btn btn-xs btn-success" data-toggle="tooltip" data-placement="right" title="Add New" ng-click="operateThirdViewAndAddControl('SupervisingProviderEdit')" ng-if=" $index+1==PracticeLocationDetail.SupervisingProviders.length" tooltip><i class="fa fa-plus"></i></button>
                    </td>
                </tr>
                <tr ng-if="visibilityThirdControl == ($index+'_ViewSupervisingPractitioner')">
                    <td colspan="5"> @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewSupervisingProvider.cshtml")</td>
                </tr>
                <tr ng-if="visibilityThirdControl == ('SupervisingProviderEdit'+$index)">
                    <td colspan="5">
                        <fieldset>
                            <legend>
                                Edit Supervising Provider
                            </legend>
                            @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditSupervisingProvider.cshtml")
                        </fieldset>
                    </td>
                </tr>
            </tbody>
        </table>
        <div ng-if="visibilityThirdControl == 'SupervisingProviderEdit'">
            <fieldset>
                <legend>
                    Add Supervising Provider
                </legend>
                @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_AddEditSupervisingProvider.cshtml")
            </fieldset>
        </div>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='supervisingProvidersSuccessMsg'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div ng-show="activeMessageDiv=='supervisingProvidersRemoveErrorMsg'" class="alert alert-danger" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div class="clearfix"></div>
    </fieldset>
    <div class="modal fade" id="supervisingWarningModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form id="editSupervisingPractioner">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                    </div>
                    <div class="modal-body">
                        Do you really want to remove <b>{{tempSupervisingProvider.FirstName}} {{tempSupervisingProvider.LastName}}</b> from your Supervising Provider's list ?
                        <input type="hidden" name="PracticeLocationID" value="{{PracticeLocationDetail.PracticeLocationDetailID}}" />
                        <input type="hidden" name="PracticeProviderID" value="{{tempSupervisingProvider.PracticeProviderID}}" />
                        <input type="hidden" name="ProfileID" value="{{tempSupervisingProvider.ProfileID}}" />
                        <input type="hidden" name="StatusType" value="2" />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-primary" ng-click="removeSupervisingProvider(PracticeLocationDetail)">Remove</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>