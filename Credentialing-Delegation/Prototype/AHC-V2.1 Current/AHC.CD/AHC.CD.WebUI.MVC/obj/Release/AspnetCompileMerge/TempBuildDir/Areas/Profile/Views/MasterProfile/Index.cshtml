                                    <li role="presentation" ng-click="getTabData('Specialty');getTabData('HospitalPrivilege');"><a href="#hospital" aria-controls="hospital" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!HospitalPrivilegeLoaded" style="width:15px" /> Hospital Privilege</a></li>
                                    <li role="presentation" ng-click="getTabData('ProfessionalLiability');"><a href="#liability" aria-controls="liability" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!ProfessionalLiabilityLoaded" style="width:15px" /> Professional Liability</a></li>
                                    <li role="presentation" ng-click="getTabData('WorkHistory');"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!WorkHistoryLoaded" style="width:15px" /> Work History</a></li>
