                                    <li role="presentation" ng-click="getTabData('IdentificationLicenses');"><a href="#identification" aria-controls="identification" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!IdentificationLicensesLoaded" style="width:15px" /> Identification & Licenses</a></li>
                                    <li role="presentation" ng-click="getTabData('EducationHistory');"><a href="#education" aria-controls="education" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!EducationHistoryLoaded" style="width:15px" /> Education History</a></li>
                                    <li role="presentation" ng-click="getTabData('Specialty');"><a href="#specialty" aria-controls="specialty" role="tab" data-toggle="tab"><img src="~/Content/Images/ajax-loader.gif" ng-show="!SpecialtyLoaded" style="width:15px" /> Specialty/Board</a></li>
