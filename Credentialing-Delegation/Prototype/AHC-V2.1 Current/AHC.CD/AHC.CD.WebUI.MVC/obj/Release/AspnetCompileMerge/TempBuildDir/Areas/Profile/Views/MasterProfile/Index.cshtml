<div id="page-wrapper" ng-app="profileApp" class="@(ViewContext.RouteData.Values["controller"].Equals("MasterProfile") ? "menuup" : "")">
    <div class="container-fluid wrap-words">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12" ng-controller="breadcrumCtrl">
                <ol class="breadcrumb">
                    @if (AHC.CD.WebUI.MVC.Areas.Profile.Attributes.ProfileActionValidator.IsAuthorized(AHC.CD.WebUI.MVC.Areas.Profile.Attributes.ProfileActionType.Edit, ViewBag.Roles, false) && (ViewBag.Roles as List<string>).Contains("PRO"))
                    {
                    <li class="active">
                        <a href="/">
                            <i class="fa fa-dashboard"></i> Dashboard
                        </a>
                    </li>
                    }
                    @if (AHC.CD.WebUI.MVC.Areas.Profile.Attributes.ProfileActionValidator.IsAuthorized(AHC.CD.WebUI.MVC.Areas.Profile.Attributes.ProfileActionType.Edit, ViewBag.Roles, false) && (ViewBag.Roles as List<string>).Contains("CCO"))
                    {
                    <li class="active">
                        <a href="/Profile/SearchProvider/SearchProvider">
                            <i class="fa fa-search"></i> Search Provider
                        </a>
                    </li>
                    }
                    <li class="active">
                        <i class="fa fa-list"></i>
                        Profiles
                    </li>
                    <li class="active" ng-cloak>
                        {{Provider.FirstName}} {{Provider.MiddleName}} {{Provider.LastName}}
                    </li>
                    <li class="text-capitalize active" ng-cloak>
                        <span id="tabname">Demographics</span>
                    </li>
                    <button ng-cloak class="btn btn-xs btn-default pull-right" data-toggle="tooltip" data-placement="bottom" title="Download as PDF" ng-click="PrintPDF();" ng-disabled="generatePDF"><i class="fa fa-file-pdf-o" ng-hide="generatePDF"></i><i ng-show="generatePDF">Generating PDF.. Please Wait.. <img src="~/Content/Images/ajax-loader.gif" style="width:15px" /></i></button>
                </ol>
            </div>
        </div>
        <div class="row col-lg-12 " ng-cloak>
            @*<div class="col-lg-12 alert alert-danger" id="showErrorPDF" role="alert" style="display:none;">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            Sorry for the Inconvenience, PDF cannot be generated. Please try again later !!
            </div>*@
            <div ng-show="activeMessageDiv=='showErrorPDF'" class="alert alert-danger" role="alert">
                <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
            </div>
        </div>
        <!-- /.row -->
