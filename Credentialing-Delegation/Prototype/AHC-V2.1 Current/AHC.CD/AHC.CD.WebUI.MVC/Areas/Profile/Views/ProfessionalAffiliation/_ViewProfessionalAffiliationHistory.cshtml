@model AHC.CD.WebUI.MVC.Areas.Profile.Models.ProfessionalAffiliation.ProfessionalAffiliationDetailViewModel


<form class="professionalAffiliationForm">
    <div class="col-lg-12 zero-padding-left-right-div">
        <fieldset>
            <legend>
                Professional Affiliation
            </legend>
            <div class=" col-lg-12">
                <div class="col-lg-12 form-group">
                    @Html.LabelFor(model => model.OrganizationName, new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{paHistory.OrganizationName}}</i>
                    </div>
                </div>
            </div>
            <div class=" col-lg-12">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.StartDate, new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{paHistory.StartDate| date:'MM-dd-yyyy'}}</i>
                        <i class="data-label" ng-show="!paHistory.StartDate">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.EndDate, new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{paHistory.EndDate| date:'MM-dd-yyyy'}}</i>
                        <i class="data-label" ng-show="!paHistory.EndDate">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.PositionOfficeHeld, new { @class = "control-label small" })

                    <div>
                        <i class="data-label wrap-words">{{paHistory.PositionOfficeHeld}}</i>
                        <i class="data-label" ng-show="!paHistory.PositionOfficeHeld">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.Member, new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{paHistory.Member}}</i>
                        <i class="data-label" ng-show="!paHistory.Member">Not Available</i>
                    </div>

                </div>
            </div><div class="clearfix"></div>
          
            <div class="clearfix"></div>
            <div class="col-md-12 form-group">
                <div>
                    <button class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                   
                </div>
            </div>

        </fieldset>

    </div>
</form>

