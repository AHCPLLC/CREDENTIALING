            <legend>
                State License Information
            </legend>
            <div>
                <div id="licensedea" class="panel-body zero-padding-left-right-div">
                    <div class="zero-padding-left-right-div">
                        <div class="col-md-12">
                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.DEANumber, new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{FederalDEALicense.DEANumber}}</i>
                                </div>
                            </div>

                            <div class="form-group col-md-3">
                                @Html.LabelFor(model => model.StateOfReg, new { @class = "control-label small" })
