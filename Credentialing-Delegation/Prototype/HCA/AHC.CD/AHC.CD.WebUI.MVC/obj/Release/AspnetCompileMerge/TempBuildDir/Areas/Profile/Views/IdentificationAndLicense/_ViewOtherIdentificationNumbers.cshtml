@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.OtherIdentificationNumberViewModel


<div class="panel-default">
    <div class="form-style">
        <fieldset>
            <legend>
                Other Identification Numbers
            </legend>
            <div class="legend2 pull-right">
                <button class="btn btn-xs btn-primary" ng-click="editOtherId(OtherIdentificationNumbers)" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
            </div>
            <div id="otherIdenNumbers" class="panel-body zero-padding-left-right-div">
                <form class="otherIdentificationNumbersForm">
                   
                    <div ng-hide="showOtherId">                       
                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewNPIDetails.cshtml")
                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewCAQHDetails.cshtml")




                       
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.UPINNumber, new { @class = " control-label small" })
                                <div>
                                    <i class="data-label">{{OtherIdentificationNumbers.UPINNumber}}</i>
                                </div>
                            </div>

                            <div class="form-group col-md-4 zero-padding-left-right-div">
                                @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
                                <div>
                                    <i class="data-label">{{OtherIdentificationNumbers.USMLENumber}}</i>
                                </div>
                            </div>
                        </div>
                        </div>
                        @*<div class="col-md-12">
                            <div class="col-md-12">
                                <button class="btn btn-warning pull-left" ng-click="saveOtherIdentificationNumber(otherIdentificationNumber)">Cancel</button>
                            </div>
                        </div>*@
           
                    <div ng-show="showOtherId">
                        @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_OtherIdentificationNumbers.cshtml")


                    </div>
                </form>
            </div>
        </fieldset>
    </div>
</div>
