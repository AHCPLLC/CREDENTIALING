@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.StateLicenseViewModel

@*<script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>*@

<form class="stateLicenseForm">
    <div class="form-style">
        <fieldset>
            <legend>
                State License Information
            </legend>
            <div>
                <div class="col-lg-12 zero-padding-left-right-div">
                    <div class="col-md-12">
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.LicenseNumber, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.LicenseNumber}}</i>
                            </div>
                        </div>
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.StateLicenseType, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.StateLicenseType}}</i>
                            </div>
                        </div>
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.StateLicenseStatus, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.StateLicenseStatus}}</i>
                            </div>
                        </div>
                        <div class="col-md-3 form-group">
                            <label class="control-label small">Are you currently practicing in this state?</label>
                            <div class="radio">
                                <div>
                                    <i class="data-label">{{stateLicense.PracticingState}}</i>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                       
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.IssueDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.IssueDate}}</i>
                            </div>
                        </div>
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.ExpiryDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.ExpiryDate}}</i>
                            </div>
                        </div>
                        <div class="col-md-3 form-group">
                            @Html.LabelFor(model => model.CurrentIssueDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.CurrentIssueDate}}</i>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            @Html.LabelFor(model => model.LicenseInGoodStanding, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.LicenseInGoodStanding}}</i>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                      
                        <div class="form-group col-md-12">
                            <label class="control-label small">Supporting Document</label>
                            <div>
                                <i class="data-label" ng-if="stateLicense.StateLicenseDocumentPath==''">Not Available</i>
                                <i class="data-label" ng-if="stateLicense.StateLicenseDocumentPath!=''"><a href="{{stateLicense.StateLicenseDocumentPath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="State License Certificate"><i class="fa fa-file">State License</i></a></i>
                            </div>
                        </div>
                        @*<div class="form-group col-lg-3">
                            @Html.LabelFor(model => model.IsRelinquished, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.IsRelinquished}}</i>
                            </div>
                        </div>
                        <div class="col-lg-3 form-group">
                            @Html.LabelFor(model => model.RellinquishedDate, new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{stateLicense.RellinquishedDate}}</i>
                            </div>
                        </div>*@
                    </div>
                    <div class="form-group col-md-12">
                       
                         @*<div class="col-md-2 form-group">
                            <span>@Html.LabelFor(model => model.StateLicenseDocumentPath, htmlAttributes: new { @class = "control-label small" })</span>
                            <i class="data-label">{{stateLicense.StateLicenseDocumentPath}}</i>

                        </div>
                        <div class="col-md-1 form-group">
                            <div>
                                <a href="/Content/Document/ATTESTATIONOFSITEVISIT.pdf" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="Preview"><i class="fa fa-file"></i></a>
                            </div>
                        </div>*@
                    </div>

                    <div class="col-md-12 form-group">
                        <div class="col-md-12">
                            <button type="button" class="btn btn-warning btn-sm" ng-click="cancelStateLicense()">Cancel</button>
                            <button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="editStateLicense($index,sl)">Edit</button>
                        </div>
                    </div>
                </div>
</div>
            </fieldset>
        </div>
</form>
