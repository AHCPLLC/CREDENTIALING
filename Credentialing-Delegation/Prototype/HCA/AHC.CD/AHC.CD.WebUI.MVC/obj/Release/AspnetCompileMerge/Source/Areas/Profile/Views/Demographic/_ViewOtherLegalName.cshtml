@model AHC.CD.WebUI.MVC.Areas.Profile.Models.Demographic.OtherLegalNameViewModel

<fieldset>
    <legend>
        Other Legal Name
    </legend>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.OtherFirstName, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{OtherLegalName.OtherFirstName}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.OtherMiddleName, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{OtherLegalName.OtherMiddleName}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.OtherLastName, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{OtherLegalName.OtherLastName}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">
            <label class="control-label small">Suffix</label>
            <div>
                <i class="data-label">{{OtherLegalName.Suffix}}</i>
            </div>
        </div>
    </div>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-md-3 form-group">
            @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{OtherLegalName.StartDate | date: "MM-dd-yyyy"}}</i>
            </div>
        </div>
        <div class="col-md-3 form-group">
            @Html.LabelFor(model => model.EndDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{OtherLegalName.EndDate | date: "MM-dd-yyyy"}}</i>
            </div>
        </div>
        <div class="col-md-6">
            <label class="control-label small">Supporting Document</label>
            <div>
                <i class="data-label"><a href="{{OtherLegalName.DocumentPath}}" class="pointer" target="_blank" data-toggle="tooltip" data-placement="top" title="Other Legal Name Certificate"><i class="fa fa-file"> Other Legal Name Certificate</i></a></i>
            </div>
        </div>
    </div>
    <div class="col-md-12 form-group">
        <button type="button" class="btn btn-warning btn-sm" ng-click="viewEditOtherLegalName($index,OtherLegalName,'close')">Cancel</button>
        <button class="btn btn-sm btn-primary pull-right" ng-click="viewEditOtherLegalName($index,OtherLegalName,'edit')">Edit</button>
    </div>
</fieldset>
