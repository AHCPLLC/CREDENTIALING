@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.FederalDEAInformationViewModel
<div>
    @*<div class="panel-heading" role="tab" id="licenseOne">
            <a data-toggle="collapse" data-parent="#accordion5" href="#licenseprofessional" aria-expanded="true" aria-controls="licenseprofessional">
                State License
                <span class="pull-right">
                    <span class="glyphicon glyphicon-minus"></span>
                </span>
            </a>
        </div>*@
    <div class="form-style">
        <fieldset>
            <legend>
                CDS Information
            </legend>
            <div class="col-md-12 text-right" style="font-style:italic;font-size:x-small;">
                <label>*If you have additional CDS include here.</label>
            </div>
            <table ng-hide="CDSCInformations.length >= 0" class="table table-bordered table-condensed customtbodyStyle table-striped">
                <thead>
                    <tr>
                        <td>No Data Available ! To Add New, Click "Add CDS Information"</td>
                    </tr>
                </thead>
                @*<div class="form-group col-md-3">
                <label class="control-label small">Do you have any State Controlled Substance Registration Certificate?</label>
                <div class="radio">
                    <label class="radio-inline">
                        <input type="radio" name="cdsc" value="Y" ng-click="addCDSCInformation()" data-ng-model="StateControlledSubstanceRegistrationAvail"> Yes
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="cdsc" value="N" data-ng-model="StateControlledSubstanceRegistrationAvail"> No
                    </label>
                </div>
            </div>*@
            </table>
            <div id="licenseprofessional" class="panel-body">
                <table ng-hide="CDSCInformations.length <= 0 || StateControlledSubstanceRegistrationAvail=='N'" class="table table-bordered table-condensed customtbodyStyle table-striped">
                    <thead>
                        <tr>
                            <th>Certificate Number</th>
                            <th>State Of Registration</th>
                            <th>Issue Date</th>
                            <th>Expiration Date</th>
                            @*<th>Certificate</th>*@
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody ng-repeat="ci in CDSCInformations">
                        <tr>
                            <td>{{ci.CertNumber}}</td>
                            <td>{{ci.State}}</td>
                            <td>{{ci.IssueDate| date: "MM-dd-yyyy"}}</td>
                            <td>{{ci.ExpiryDate| date: "MM-dd-yyyy"}}</td>

                           @* <td><a href="{{ci.CDSCCertPath}}" class="pointer" target="_blank" title="{{ci.CDSCCertPath}}"><i class="fa fa-file" title=""></i></a></td>*@
                            <td>
                                <button class="btn btn-xs btn-default" ng-click="viewCDSCInformation($index,ci)" ng-hide="viewCDSC" data-toggle="tooltip" data-placement="left" title="View"><i class="fa fa-eye"></i></button>
                                <button class="btn btn-xs btn-primary" ng-click="editCDSCInformation($index,ci)" ng-hide="editShowCDSCInformation" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
                                <button class="btn btn-xs btn-success" ng-click="addCDSCInformation()" data-toggle="tooltip" data-placement="right" title="Add new" ng-hide="viewCDSC || editShowCDSCInformation || newShowCDSCInformation || $index+1!=CDSCInformations.length"><i class="fa fa-plus"></i></button>
                            </td>
                        </tr>
                        <tr ng-show="viewCDSC && ci==cDSCInformation">
                            <td colspan="6" id="cDSCInformationEditDiv{{$index}}" class="editTableDataStyle">
                                @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_ViewCDSCInformation.cshtml")
                            </td>
                        </tr>
                        <tr ng-show="editShowCDSCInformation && ci==cDSCInformation">
                            <td colspan="6" id="cDSCInformationEditDiv{{$index}}" class="editTableDataStyle">
                                @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditCDSCInformation.cshtml")
                            </td>
                        </tr>
                    </tbody>
                </table>

                    @*<div class="col-lg-12 clearfix" style="padding-bottom:15px;"></div>*@
                    <div class="clearfix"></div>
                    <div class="form-style">
                        <fieldset ng-show="newShowCDSCInformation">
                            <legend>
                                Add CDS Information
                            </legend>
                            <div id="newCDSCInformationDiv">
                                @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditCDSCInformation.cshtml")
                            </div>
                        </fieldset>
                    </div>
                <div ng-hide="CDSCInformations.length > 0 || newShowCDSCInformation" class="col-md-12 text-right">
                    <button class="btn btn-xs btn-primary addCDSCBtn" ng-click="addCDSCInformation()"><i class="fa fa-plus"></i> Add CDS Information</button>
                </div>
</div>
        </fieldset>
    </div>
</div>