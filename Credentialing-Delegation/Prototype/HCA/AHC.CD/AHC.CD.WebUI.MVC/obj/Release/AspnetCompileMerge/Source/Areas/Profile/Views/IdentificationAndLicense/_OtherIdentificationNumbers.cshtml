@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.OtherIdentificationNumberViewModel


<div class="panel-default">
    <div class="form-style">
        <div class="col-md-12 text-right" style="font-style:italic;font-size:x-small;">
            <label>*If you have additional professional Id's report here.</label>
        </div>
            <div id="otherIdenNumbers" class="panel-body zero-padding-left-right-div">
                <form class="otherIdentificationNumbersForm">
                    @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditNPIDetails.cshtml")
                    @Html.Partial("~/Areas/Profile/Views/IdentificationAndLicense/_AddEditCAQHDetails.cshtml")




                    <div class="form-group col-md-12">

                        <div class="form-group col-md-4 ">
                            @Html.LabelFor(model => model.UPINNumber, new { @class = "control-label small" })
                            <div>
                                @Html.TextBoxFor(model => model.UPINNumber, new { @class = "form-control input-sm", data_ng_model = "TempOtherIdentificationNumbers.UPINNumber" })
                            </div>
                            @Html.ValidationMessageFor(model => model.UPINNumber, "", new { @class = "text-danger" })

                        </div>

                        <div class="form-group col-md-4 ">
                            @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
                            <div>
                                @Html.TextBoxFor(model => model.USMLENumber, new { @class = "form-control input-sm", data_ng_model = "TempOtherIdentificationNumbers.USMLENumber" })
                            </div>
                            @Html.ValidationMessageFor(model => model.USMLENumber, "", new { @class = "text-danger" })

                        </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-12">
                                <button class="btn btn-warning pull-left" ng-click="cancelOtherIdentificationNumber(otherIdentificationNumber)">Cancel</button>

                                <button class="btn btn-success pull-right" ng-click="saveOtherIdentificationNumber(otherIdentificationNumber)">Save</button>
                            </div>
                        </div>
                    
                </form>
            </div>
    </div>
</div>