            @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.StartDate}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">

            @Html.LabelFor(model => model.EndDate, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.EndDate}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">

            @Html.LabelFor(model => model.CompletedEducationAtThisSchool, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.CompletedEducationAtThisSchool}}</i>
            </div>
        </div>
        <div class="col-md-12 form-group">

            <button type="button" class="btn btn-warning btn-sm" ng-click="cancelViewFifth()">Cancel</button>
            <button type="button" class="btn btn-primary btn-sm pull-right" ng-click="editFifthPath($index,fifthPathwayDetailViewModel)">Edit</button>

        </div>
    </div>
</div>