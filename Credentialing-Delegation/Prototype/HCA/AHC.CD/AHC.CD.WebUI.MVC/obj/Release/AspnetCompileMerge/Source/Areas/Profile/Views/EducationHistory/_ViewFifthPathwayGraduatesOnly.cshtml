@model AHC.CD.WebUI.MVC.Areas.Profile.Models.EducationHistory.FifthPathwayDetailViewModel

<div id="viewfifthPath">
    <div class="col-lg-12 zero-padding-left-right-div">
        <div class="col-lg-3 form-group">

            @Html.LabelFor(model => model.Institution, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.Institution}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">

            @Html.LabelFor(model => model.Address, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.Address}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">

            @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.City}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">

            @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.State}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">

            @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.County}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">

            @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.Country}}</i>
            </div>
        </div>

        <div class="form-group col-md-3">

            @Html.LabelFor(model => model.ZipCode, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.ZipCode}}</i>
            </div>
        </div>


        <div class="col-lg-3 form-group">

            @Html.LabelFor(model => model.Fax, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.Fax}}</i>
            </div>
        </div>
        <div class="col-lg-3 form-group">
            @Html.LabelFor(model => model.Telephone, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{fifthPathwayDetailViewModel.Telephone}}</i>
            </div>
        </div>

