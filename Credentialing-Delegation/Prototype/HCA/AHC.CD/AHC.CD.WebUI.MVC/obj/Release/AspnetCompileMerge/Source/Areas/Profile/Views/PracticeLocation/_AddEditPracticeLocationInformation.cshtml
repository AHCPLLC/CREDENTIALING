<!--Office Hours-->
<div class="form-style">
    <fieldset>
        <legend>
            Office Hours
        </legend>
        <div id="practice13" class="panel-body">

            <div class="form-group">
                <div class="col-md-12">
                    <div class="col-md-6">
                        <label class="control-label small ">Re-use information from another practice location </label>
                        <select class="form-control input-sm ">
                            <option></option>
                            <option></option>
                        </select>
                    </div>
                </div>
                <div class="clearfix"></div>
                <br />
                <div class="col-md-12">
                    <div class="col-md-2 form-group">
                        <label class="control-label small">Day</label>
                    </div>
                    <div class="col-md-10">
                        <div class="col-md-4 form-group">
                            <label class="control-label small">Start Time</label>
                        </div>
                        <div class="col-md-4 form-group">
                            <label class="control-label small">End Time</label>
                        </div>
                        <div class="col-md-4 form-group">
                            <label class="control-label small">Actions</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="col-md-2 form-group">
                        <label class="control-label small">Monday</label>
                    </div>
                    <div class="col-md-10">
                        <div ng-repeat="t in Days[0].Times" class="col-md-12 zero-padding-left-right-div">
                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="08:00" />
                            </div>
                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="17:00" />
                            </div>
                            <div class="col-md-4 form-group">
                                <button class="btn btn-xs btn-success" ng-click="addTime(Days[0].Day)">
                                    <i class="fa fa-plus-circle"></i>
                                </button>
                                <button class="btn btn-xs btn-danger" ng-click="removeTime(Days[0].Day, $index)">  <i class="fa fa-minus-circle"></i></button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="col-md-2 form-group">
                        <label class="control-label small">Tuesday</label>
                    </div>
                    <div class="col-md-10">
                        <div ng-repeat="t in Days[1].Times" class="col-md-12 zero-padding-left-right-div">
                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="08:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="17:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <button class="btn btn-xs btn-success" ng-click="addTime(Days[1].Day)">
                                    <i class="fa fa-plus-circle"></i>{{t.Day}}
                                </button>
                                <button class="btn btn-xs btn-danger" ng-click="removeTime(Days[1].Day, $index)">  <i class="fa fa-minus-circle"></i></button>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="col-lg-2 form-group">
                        <label class="control-label small">Wednesday</label>
                    </div>
                    <div class="col-md-10">
                        <div ng-repeat="t in Days[2].Times" class="col-md-12 zero-padding-left-right-div">
                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="08:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="17:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <button class="btn btn-xs btn-success" ng-click="addTime(Days[2].Day)">
                                    <i class="fa fa-plus-circle"></i>
                                </button>
                                <button class="btn btn-xs btn-danger" ng-click="removeTime(Days[2].Day, $index)">  <i class="fa fa-minus-circle"></i></button>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-md-12">
                    <div class="col-md-2 form-group">
                        <label class="control-label small">Thrusday</label>
                    </div>
                    <div class="col-md-10">
                        <div ng-repeat="t in Days[3].Times" class="col-md-12 zero-padding-left-right-div">
                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="08:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <input type="time" class="form-control input-sm" value="17:00" />
                            </div>

                            <div class="col-md-4 form-group">
                                <button class="btn btn-xs btn-success" ng-click="addTime(Days[3].Day)">
