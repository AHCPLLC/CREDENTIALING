                        <button ng-show="!primaryPracticeLocationFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelPrimaryLocationInformation()">Cancel</button>
                        <button ng-show="!primaryPracticeLocationFormStatus" type="button" class="btn btn-success btn-sm pull-right" ng-click="savePrimaryPracticeLocation(primaryPracticeLocation)">Save</button>
                        <button ng-show="primaryPracticeLocationFormStatus" type="button" class="btn btn-success btn-sm pull-right" ng-click="updatePrimaryPracticeLocation(primaryPracticeLocation)">Save</button>
                    </div>
                </div>
            </div>

        </div>
    </fieldset>
</div>

