<!--Primary Info-->
<div class="form-group">
    <fieldset>
        <legend>
            General Information
        </legend>
        <div class="col-md-12">
            <label class=" col-md-12 control-label small">Type *</label>
            <div class=" col-md-12 radio">
                <label class="radio-inline">
                    <input type="radio" name="current" value="Primary" data-ng-model="primaryPracticeLocations.Type"> Primary
                </label>
                <label class="radio-inline">
                    <input type="radio" name="current" value="Secondary" data-ng-model="primaryPracticeLocations.Type"> Secondary
                </label>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-4">
                <label class="control-label small">Currently Practicing At This Address? *</label>
                <div class="radio">
                    <label class="radio-inline">
                        <input type="radio" name="current" value="Yes" data-ng-model="primaryPracticeLocations.CurrentlyPracticing"> Yes
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="current" value="No" data-ng-model="primaryPracticeLocations.CurrentlyPracticing"> No
                    </label>
                </div>
            </div>
            <div class="form-group col-md-4" ng-show="primaryPracticeLocations.CurrentlyPracticing=='No'">
                <label class="control-label small">What Is Your Expected Start Date?</label>
                <div>
                    <input type="date" class="form-control input-sm" data-ng-model="primaryPracticeLocations.ExpectedStartDate" required />
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">Corporate or Practice Name</label>
                <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.PracticeName" />
            </div>
        </div>
        <div class="col-md-12">

            <div class="form-group col-md-4">
                <label for="MobileNo" class="control-label small">Telephone *</label>
                <div>
                    <div class="input-group">
                        <div class="input-group-btn">
                            <button type="button" class="btn btn-sm btn-default"><span class="caret"></span></button>
                        </div>
                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                            <div class="arrow"></div>
                            <h3 class="popover-title">Select Country Dial Code</h3>
                            <div class="popover-content">
                                <select id="ContactContryCode">
                                    <option>+91</option>
                                </select>
                            </div>
                        </div>
                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" data-ng-model="primaryPracticeLocations.Telephone" required>
                    </div>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label for="MobileNo" class="control-label small">Fax</label>
                <div>
                    <div class="input-group">
                        <div class="input-group-btn">
                            <button type="button" class="btn btn-sm btn-default"><span class="caret"></span></button>
                        </div>
                        <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                            <div class="arrow"></div>
                            <h3 class="popover-title">Select Country Dial Code</h3>
                            <div class="popover-content">
                                <select id="ContactContryCode">
                                    <option>+91</option>
                                </select>
                            </div>
                        </div>
                        <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Fax Number" data-ng-model="primaryPracticeLocations.Fax" required>
                    </div>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">Office e-mail address</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.EmailId" />
                </div>
            </div>
        </div>
        <div class="col-md-12">
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-3">
                <label class="control-label small">Number *</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.Number" required />
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">Street *</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.Street" required />
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">Suite/Building</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.Suite" required />
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">County </label>
                <div>
                    <select class="form-control input-sm" data-ng-model="primaryPracticeLocations.County">
                        <option>County1</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="form-group col-md-3">
                <label class="control-label small">Country *</label>
                <div>
                    <select id="state" name="" class="form-control input-sm" data-ng-model="primaryPracticeLocations.Country" required>
                        <option>US</option>
                        <option>India</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">State *</label>
                <div>
                    <select id="state" name="" class="form-control input-sm" data-ng-model="primaryPracticeLocations.State" required>
                        <option>State1</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">City *</label>
                <div>
                    <select id="city" name="" class="form-control input-sm" data-ng-model="primaryPracticeLocations.City" required>
                        <option>City1</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-3">
                <label class="control-label small">ZIP Code *</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.ZIP" required />
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="form-group col-md-4">
                <label class="control-label small">Send general correspondence here? *</label>
                <div class="radio">
                    <label class="radio-inline">
                        <input type="radio" name="current" value="Yes" data-ng-model="primaryPracticeLocations.GeneralCorrespondence"> Yes
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="current" value="No" data-ng-model="primaryPracticeLocations.GeneralCorrespondence"> No
                    </label>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">Individual Tax ID</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.IndividualTaxID" />
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">Group Tax ID</label>
                <div>
                    <input type="text" class="form-control input-sm" data-ng-model="primaryPracticeLocations.GroupTaxID" />
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12">
            <div class="form-group col-md-6">
                <label class="control-label small">Primary Tax ID(one only) *</label>
                <div class="radio">
                    <label class="radio-inline">
                        <input type="radio" name="tax" value="Use individual tax ID" data-ng-model="primaryPracticeLocations.Primary"> Use individual tax ID
                    </label>
                    <label class="radio-inline">
                        <input type="radio" name="tax" value="Use group tax ID" data-ng-model="primaryPracticeLocations.Primary"> Use group tax ID
                    </label>
                </div>
            </div>
        </div>
        <br />
        <div class="col-md-12">
            <div class="col-md-12 form-group">
                <button ng-show="!primaryPracticeLocationFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelPrimaryLocationInformation()">Cancel</button>
                <button ng-show="!primaryPracticeLocationFormStatus" type="button" class="btn btn-success btn-sm pull-right" ng-click="updatePrimaryPracticeLocation(primaryPracticeLocation)">Save</button>
            </div>
        </div>
    </fieldset>
</div>

