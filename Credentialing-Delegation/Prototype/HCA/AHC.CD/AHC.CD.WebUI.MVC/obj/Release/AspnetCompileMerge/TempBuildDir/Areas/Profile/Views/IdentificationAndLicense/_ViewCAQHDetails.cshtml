@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.CAQHDetailViewModel
<div class="form-group col-md-12">
    <div class="form-group col-md-4 zero-padding-left-right-div">
        @Html.LabelFor(model => model.Number, new { @class = "  control-label small" })
        <div>
            <i class="data-label">{{OtherIdentificationNumbers.CAQHDetails.Number}}</i>
        </div>
    </div>
    <div class="form-group col-md-4 zero-padding-left-right-div">
        @Html.LabelFor(model => model.UserName, new { @class = "  control-label small", type = "password" })
        <div>
            <i class="data-label">{{OtherIdentificationNumbers.CAQHDetails.UserName}}</i>
        </div>
    </div>
    <div class="form-group col-md-4 zero-padding-left-right-div">
        @Html.LabelFor(model => model.Password, new { @class = "  control-label small", type = "password" })
        <div>
            <i class="data-label">{{OtherIdentificationNumbers.CAQHDetails.Password}}</i>
        </div>
    </div>
</div>
