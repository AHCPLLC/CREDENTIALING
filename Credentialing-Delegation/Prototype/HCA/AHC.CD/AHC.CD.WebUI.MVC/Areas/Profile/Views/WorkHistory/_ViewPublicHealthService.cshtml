@model AHC.CD.WebUI.MVC.Areas.Profile.Models.WorkHistory.PublicHealthServiceViewModel


<form class="publicHealthServiceForm">
    <div class="col-lg-12 zero-padding-left-right-div">
        @*@Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
        @Html.HiddenFor(model => model.PublicHealthServiceID, new { data_ng_model = "publicHealthService.PublicHealthServiceID" })*@
        <div class="col-md-12">
            <div class="form-group">
                <div class="col-md-4">
                    @Html.LabelFor(model => model.StartDate, new { @class = "control-label small" })
                    <div>
                        <div>
                            <i class="data-label">{{publicHealthService.StartDate}}</i>
                        </div>
                        @*@Html.TextBoxFor(model => model.StartDate, new { @class = "form-control input-sm", placeholder = "start date", data_ng_mode = "publicHealthService.StartDate" })*@
                       
                    </div>
                </div>
                <div class="col-md-4">
                    @Html.LabelFor(model => model.EndDate, new { @class = "control-label small" })
                    <div>
                        <div>
                            <i class="data-label">{{publicHealthService.EndDate}}</i>
                        </div>
                        @*@Html.TextBoxFor(model => model.EndDate, new { @class = "form-control input-sm", placeholder = "status", data_ng_mode = "publicHealthService.EndDate" })*@
                       
                    </div>
                </div>
                <div class="col-md-4">
                    @Html.LabelFor(model => model.LastLocation, new { @class = "control-label small" })
                    <div>
                        <div>
                            <i class="data-label">{{publicHealthService.LastLocation}}</i>
                        </div>
                        @*@Html.TextBoxFor(model => model.LastLocation, new { @class = "form-control input-sm", placeholder = "status", data_ng_mode = "publicHealthService.LastLocation" })*@
                        
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <br />
        <div class="col-md-12">
            <div class="col-md-12 form-group">
              
                    <button class="btn btn-warning btn-sm" ng-click="cancelPublicHealthService()">Cancel</button>
                    <button class="btn btn-primary btn-sm pull-right" ng-click="editPublicHealthService($index,phs)">Edit</button>
               
        </div>
    </div>
</form>