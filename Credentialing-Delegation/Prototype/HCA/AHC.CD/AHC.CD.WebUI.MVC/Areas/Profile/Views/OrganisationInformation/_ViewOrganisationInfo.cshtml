<fieldset>
    <legend>
        
    </legend>
    <form class="organisationInfo">

        @Html.ValidationSummary(true, "", new { @class = "text-danger validSummary" })
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-6 form-group">
                <label class="col-md-6 control-label">Provider Relationship *</label>
                <div class="col-md-6 input-group input-group-sm">
                    <label class="radio-inline">
                        {{Oi.ProviderRelationship}}
                    </label>
                </div>
            </div>
            <div class="col-md-12 form-group">
                <label class="col-md-3 control-label">Are you a part of any Group?</label>
                <div class="col-md-7 input-group input-group-sm">
                    <label class="radio-inline">
                        Yes
                    </label>
                </div>
            </div>
            <div ng-show="Oi.IsPartOfGroup == 'true'" class="col-md-12 form-group">
                <label class="col-md-3 control-label">Group Names </label>
                <div class="col-md-7 input-group input-group-sm">
                    <label  class="radio-inline">
                        Access
                    </label>
		</div>
            </div>

            <div class="col-md-12 form-group">
                <label class="col-md-3 control-label">Upload CV</label>
                <div class="col-md-7">
                    <span ng-show="Oi.Certificate==''">Not Available</span><a ng-show="Oi.Certificate!=''" href="{{Oi.Certificate}}" data-toggle="tooltip" data-placement="top" title="{{hospitalPrivilegeInformation.DocumentPath}}" target="_blank"><i class="fa fa-file"></i> CV Document</a>
                </div>
            </div>
            <div class="col-md-12 form-group">
                <label class="col-md-3 control-label">Contract Doc</label>
                <div class="col-md-7">
                    <span>Not Available</span>
                </div>
            </div>

            <div class="col-md-12 form-group">
                <button type="button" class="btn btn-warning btn-sm" ng-click="cancelOrganisationInfo()">Cancel</button>
                <button type="button" class="btn btn-primary btn-sm pull-right">Edit</button>
                @*<button type="submit" class="btn btn-primary btn-sm pull-right" ng-click="saveProfessionalReference(professionalReference)">{{submitButtonText}}</button>*@
            </div>
        </div>

    </form>
</fieldset>
