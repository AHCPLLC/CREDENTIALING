
<div class="col-lg-12">
    <div class="col-lg-3 form-group">
        <label class="control-label small">Program Type *</label>
        <div>
            <i class="data-label">{{residency.ProgramType}}</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small">Specialty *</label>
        <div>
            <i class="data-label">{{residency.Specialty}}</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small">Department *</label>
        <div>
            <i class="data-label">{{residency.Department}}</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small">Start Date *</label>
        <div>
            <i class="data-label">{{residency.StartDate | date:'MM-dd-yyyy'}}</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small">End Date *</label>
        <div>
            <i class="data-label">{{residency.EndDate | date:'MM-dd-yyyy'}}</i>
        </div>
    </div>
    <div class="col-lg-3 form-group">
        <label class="control-label small">Supporting Document</label>
        <div>
            <a href="~/Content/Document/{{residency.Document}}" target="_blank" title="{{residency.DocumentName}}"><i class="fa fa-file" title="">{{residency.DocumentName}}</i></a>
        </div>
    </div>
    
    <div class="col-md-12 form-group">
        <button type="button" class="btn btn-warning btn-sm" ng-click="cancelViewResidency()">Cancel</button>
        <button type="button" class="btn btn-primary btn-sm pull-right" ng-click="editResidency($index, residency)">Edit</button>
    </div> 
</div>