
<div class="form-style">
    <fieldset>
        <legend>
           Primary Credentialing Contact
        </legend>

        <form>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-8 form-group">
                    <label class="control-label small"><input type="checkbox" name="OfficeManager" data-ng-model="primaryCredentialingContact.OfficeManagerAvail" /> Check here to use the Office Manager and Address of the Primary Practice Location as the Credentialing Information.</label>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Office Manager</label>
                    <select class="form-control input-sm col-lg-3" data-ng-model="primaryCredentialingContact.OfficeManager">
                        <option disabled selected hidden>Choose Office Manager</option>
                        <option value="John Paul">John Paul</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">First Name</label>
                    <input class="form-control input-sm" type="text" placeholder="First Name" data-ng-model="primaryCredentialingContact.FirstName" />
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Middle Name</label>
                    <input class="form-control input-sm" type="text" placeholder="Middle Name" data-ng-model="primaryCredentialingContact.MiddleName" />
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Last Name</label>
                    <input class="form-control input-sm" type="text" placeholder="Last Name" data-ng-model="primaryCredentialingContact.LastName" />
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Number</label>
                    <input class="form-control input-sm" type="text" placeholder="Number" data-ng-model="primaryCredentialingContact.Number" />
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Street</label>
                    <input class="form-control input-sm" type="text" placeholder="Street" data-ng-model="primaryCredentialingContact.Street" />
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Suite/Building</label>
                    <input class="form-control input-sm" type="text" placeholder="Suite/Building" data-ng-model="primaryCredentialingContact.Building" />
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-4 form-group">
                    <label class="control-label small">City</label>
                    <select class="form-control input-sm col-lg-3" data-ng-model="primaryCredentialingContact.City">
                        <option disabled selected hidden>Choose City</option>
                        <option value="St Petersburg">St Petersburg</option>
                    </select>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">State</label>
                    <select class="form-control input-sm col-lg-3" data-ng-model="primaryCredentialingContact.State">
                        <option disabled selected hidden>Choose State</option>
                        <option value="Florida">Florida</option>
                    </select>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Zip Code</label>
                    <input class="form-control input-sm" type="text" placeholder="Zip Code" data-ng-model="primaryCredentialingContact.ZipCode" />
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="form-group col-md-4">
                    <label for="MobileNo" class="control-label small">Telephone Number  *</label>
                    <div>
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button type="button" class="btn btn-sm"><span class="caret"></span></button>
                            </div>
                            <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                <div class="arrow"></div>
                                <h3 class="popover-title">Select Country Dial Code</h3>
                                <div class="popover-content">
                                    <select id="ContactContryCode">
                                        <option>+91</option>
                                    </select>
                                </div>
                            </div>
                            <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" data-ng-model="primaryCredentialingContact.Telephone" required>
                        </div>

                    </div>
                </div>
                <div class="form-group col-md-4">

                    <label for="Fax" class="control-label small">Fax</label>
                    <div>
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button type="button" class="btn btn-sm"><span class="caret"></span></button>
                            </div>
                            <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                <div class="arrow"></div>
                                <h3 class="popover-title">Select Country Dial Code</h3>
                                <div class="popover-content">
                                    <select id="ContactContryCode">
                                        <option>+91</option>
                                    </select>
                                </div>
                            </div>
                            <input type="text" name="Fax" class="form-control input-sm" id="Fax" placeholder="Fax Number" data-ng-model="primaryCredentialingContact.Fax" required>
                        </div>

                    </div>
                </div>
                <div class="col-lg-4 form-group">
                    <label class="control-label small">Email ID</label>
                    <input class="form-control input-sm" type="text" placeholder="Email ID" data-ng-model="primaryCredentialingContact.EmailId" />
                </div>
            </div>
            <div class="col-lg-12">
                <button class="btn btn-warning btn-sm" ng-click="cancelCredentialingContact()">Cancel</button>
                @*<button class="btn btn-success pull-right">Initiate Verification</button>*@
                <button class="btn btn-success btn-sm pull-right">Save</button>
            </div>
        </form>

        </fieldset>
    </div>
