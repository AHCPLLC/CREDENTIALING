<br />

    <div ng-controller="viewPlanAppCtrl">

   
    <div class="form-style">
        <fieldset>
            <legend>
                Plan Details
            </legend>
            <div id="Plans" class="panel-body">
                <div class="panel-heading">
                    <span class="badge bg-info pull-right" ng-show="ifFilter()">{{getIndexFirst()}} - {{getIndexLast()}} of {{data.length}}</span>
                    
                </div>
                <table ng-table="tableParams" show-filter="true" class="table ng-table-rowselected">
                    <tbody ng-repeat="plan in data">
                        <tr>
                            <td data-title="'No'">
                                {{$index+1}}.
                            </td>
                            <td data-title="'Logo'">
                                <img src="~/Content/Images/NewPlans/{{plan.PlanImage}}" class="datatableimg" style="width:134px; height:57px;"/>
                            </td>
                            <td data-title="'Plan Name'" sortable="'PlanName'" filter="{ 'PlanName': 'text' }">
                                {{plan.PlanName}}
                            </td>
                            <td data-title="'Plan Type'" sortable="'PlanType'" filter="{ 'PlanType': 'text' }">
                                {{plan.PlanType}}
                            </td>
                            @*<td data-title="'Location'" sortable="'Location'" filter="{ 'Location': 'text' }">
                                {{plan.Location}}
                            </td>*@
                            @*<td data-title="'Group'" sortable="'Group'" filter="{ 'Group': 'text' }">
                                {{plan.Group}}
                            </td>*@
                            @*<td data-title="'Speciality'" sortable="'Speciality'" filter="{ 'Speciality': 'text' }">
                                {{plan.Speciality}}
                            </td>*@
                            @*<td data-title="'Status'" sortable="'Status'" filter="{ 'Status': 'text' }">
                                {{plan.Status}}
                            </td>*@
                            <td data-title="'Effective Date'" sortable="'EffectiveDate'" filter="{ 'EffectiveDate': 'text' }">
                                {{plan.EffectiveDate}}
                            </td>
                            <td data-title="'Term Date'" sortable="'TermDate'" filter="{ 'TermDate': 'text' }">
                                {{plan.TermDate}}
                            </td>
                        </tr>
                      
                    </tbody>
                </table>
            </div>
        </fieldset>
    </div>
</div>
  

    <style type="text/css">
             .datatableimg {
                    max-width: 100%;
                    margin: 0 0 10px;
                }
          
    </style>
