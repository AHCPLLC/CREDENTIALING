<div class="col-lg-12 zero-padding-left-right-div">
    <!--Search provider starts-->
    <div class="panel panel-default small">
        <div class="panel-heading">
            <button class="pull-right btn btn-xs" ng-click="SearchProviderPanelToggle('AuditSearch');"><i class="fa fa-th-list"></i></button>
            <div>Search Provider</div>
        </div>
        <div role="tabpanel">
            <div id="AuditSearch" class="panel-body closePanel">
                <div class="col-md-12">
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            <!---Provider partial starts here-->
                            @Html.Partial("~/Areas/Credentialing/Views/Auditing/_Audit_SearchByDetails.cshtml")
                            <!---Provider partial ends here-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div ng-show="loadingAjax">
        <div class="progress">
            <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                <span class="sr-only">Loading...Please wait...</span>
            </div>
        </div>
    </div>
    <!--Loading ends-->
    <!--Search result starts-->
    <div class="panel panel-default small" ng-show="allProviders.length>0">
        <div class="panel-heading">
            Search Result
            <button class="pull-right btn btn-xs" ng-click="SearchProviderPanelToggle('SearchProviderResultPanel');"><i class="fa fa-th-list"></i></button>
            
        </div>
        <div role="tabpanel">
            <div id="SearchProviderResultPanel" class="panel-body closePanel">
                <div class="col-md-12 zero-padding-left-right-div">
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            @*<a class="btn btn-primary btn-xs pull-right" href="#" data-toggle="tooltip" data-placement="right" title="Download Selected"><i class="fa fa-download"> Download Selected</i></a>*@
                            <!---Provider partial starts here-->
                              @Html.Partial("~/Areas/Credentialing/Views/Auditing/_Audit_SearchResult.cshtml")
                            <!---Provider partial ends here-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Search result ends-->

    <!-- selected Provider List-->
    <div class="panel panel-default small" ng-show="allProviders.length > 0">
        <div class="panel-heading">
            Selected Providers
            <button class="pull-right btn btn-xs" ng-click="SearchProviderPanelToggle('SearchProviderResultPanel1');"><i class="fa fa-th-list"></i></button>
        </div>
        <div role="tabpanel">
            <div id="SearchProviderResultPanel1" class="panel-body closePanel">
                <div class="col-md-12 zero-padding-left-right-div">
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            <!---Provider partial starts here-->
                            @Html.Partial("~/Areas/Credentialing/Views/Auditing/_selectedProvider.cshtml")
                            <!---Provider partial ends here-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end selected provider list -->

    <!-- selected Provider List-->
    <div ng-if="DataSubmited" class="panel panel-default small">
        <div class="panel-heading">
            Select Document for Audit
            <button class="pull-right btn btn-xs" ng-click="SearchProviderPanelToggle('SearchProviderResultPanel2');"><i class="fa fa-th-list"></i></button>
        </div>
        <div role="tabpanel">
            <div id="SearchProviderResultPanel2" class="panel-body closePanel">
                <div class="col-md-12 zero-padding-left-right-div">
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            <!---Provider partial starts here-->
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">                                    
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> Profile Export
                                    </label>
                                    <select class="">
                                        <option selected>CAQH</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> CV
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> Medical License
                                    </label>                                    
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> DEA
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> Professional Liability
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> Residency
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-down"></i></a>
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> Fellowship
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-1 zero-padding-left-right-div">
                                    <a class="btn btn-info btn-xs"><i class="fa fa-sort-up"></i></a>
                                    
                                </div>
                                <div class="col-lg-11 zero-padding-left-right-div">
                                    <label>
                                        <input type="checkbox"> PSV
                                    </label>
                                </div>
                            </div>
                            
                            <a ng-click="generatePackage();" class="btn btn-sm btn-primary pull-right">Generate Package</a>
                            @*<a ng-click="AuditDoctSubmit()" class="btn btn-sm btn-primary pull-right">Next</a>*@
                            <!---Provider partial ends here-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end selected provider list -->

    <!-- package Provider List-->
    <div class="panel panel-default small" ng-show="generate_package">
        <div class="panel-heading">
            Audit Package
            <button class="pull-right btn btn-xs" ng-click="SearchProviderPanelToggle('packageview');"><i class="fa fa-th-list"></i></button>
        </div>
        <div role="tabpanel">
            <div id="packageview" class="panel-body">
                <div class="col-md-12 zero-padding-left-right-div">
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane active">
                            <!---Provider partial starts here-->
                            @Html.Partial("~/Areas/Credentialing/Views/Auditing/_generatedpackage.cshtml")
                            <!---Provider partial ends here-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end package provider list -->

    

