


<div class="form-style">
    <fieldset>
        <legend>
            Professional Work Experience
        </legend>

        <div class="form-style">

            <div class="col-lg-12 zero-padding-left-right-div">
                <table class="table table-bordered table-condensed customtbodyStyle">
                    <thead>
                        <tr>
                            <th>Emmployer Name</th>
                            <th>Start Date</th>
                            <th>End Date</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody ng-repeat="wh in WorkHistories">
                        <tr>
                            <td>{{wh.EmployerName}}</td>
                            <td>{{wh.StartDate}}</td>
                            <td>{{wh.EndDate}}</td>
                            <td>
                                <button class="btn btn-xs btn-primary" ng-click="editWorkHistory($index,wh)"><i class="fa fa-edit"></i></button>
                            </td>
                        </tr>
                        <tr ng-show="showingDetails && wh==workHistory">
                            <td colspan="4" id="workHistoryEditDiv{{$index}}" class="editTableDataStyle">Java Script or Jquery or Angular Error!!!!!!!!</td>
                        </tr>
                    </tbody>
                </table>
            </div>



            <div class="clearfix"></div>
            <div id="workHistoryForm" style="display:none;">
                <div class="col-lg-12 zero-padding-left-right-div">

                    <fieldset>
                        <legend>
                            Employer Details
                        </legend>
                        <div class="panel-body">
                            <div class="col-md-12">
                                <div class="form-group col-md-4">
                                    <label class="control-label small">Employer Name *</label>
                                    <div>
                                        <input type="text" ng-model="workHistory.EmployerName" class="form-control input-sm" />
                                    </div>
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label small">Job Title *</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>


                                <div class="form-group col-md-4">
                                    <label for="MobileNo" class="control-label small">Mobile Number *</label>
                                    <div>
                                        <div class="input-group">
                                            <div class="input-group-btn">
                                                <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                            </div>
                                            <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                                <div class="arrow"></div>
                                                <h3 class="popover-title">Select Country Dial Code</h3>
                                                <div class="popover-content">
                                                    <select id="ContactContryCode">
                                                        <option>+91</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Mobile Number" required>
                                        </div>

                                    </div>
                                </div>

                            </div>

                            <div class="col-md-12">
                                <div class="form-group col-md-4">
                                    <label for="MobileNo" class="control-label small">Fax Number *</label>
                                    <div>
                                        <div class="input-group">
                                            <div class="input-group-btn">
                                                <button type="button" class="btn btn-default btn-sm"><span class="caret"></span></button>
                                            </div>
                                            <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                                                <div class="arrow"></div>
                                                <h3 class="popover-title">Select Country Dial Code</h3>
                                                <div class="popover-content">
                                                    <select id="ContactContryCode">
                                                        <option>+91</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group col-md-8">
                                    <label for="Contact" class="control-label small">May we contact your employer ? *</label>
                                    <div class="radio">
                                        <label class="radio-inline">
                                            <input type="radio" name="Contact"> Yes
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="Contact"> No
                                        </label>
                                    </div>
                                </div>


                            </div>


                            <div class="col-md-12">
                                <div class="form-group col-md-6">
                                    <label class="control-label small">Department *</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>

                                <div class="form-group col-md-6">
                                    <label class="control-label small">Title *</label>
                                    <div>
                                        <select id="selectNameTitle" name="" class="form-control input-sm" required>
                                            <option>MO</option>
                                            <option>PO</option>

                                        </select>
                                    </div>
                                </div>

                            </div>

                            <div class="col-md-12">
                                <div class="form-group col-md-12">
                                    <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                        <div class="form-control" data-trigger="fileinput"><i class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span></div>
                                        <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new"> Upload Resume</span><span class="fileinput-exists">Change</span><input type="file" name="..."></span>
                                        <a href="#" class="input-group-addon btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>

                    <fieldset>
                        <legend>
                            Address
                        </legend>
                        <div class="panel-body">
                            <div class="col-md-12">
                                <div class="form-group col-md-4">
                                    <label class="control-label small">State *</label>
                                    <div>
                                        <select id="selectNameTitle" name="" class="form-control input-sm" required>
                                            <option>Massachusetts </option>

                                        </select>
                                    </div>
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label small">City *</label>
                                    <div>
                                        <select id="selectNameTitle" name="" class="form-control input-sm" required>
                                            <option>San Francisco</option>

                                        </select>

                                    </div>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label small">County *</label>
                                    <div>
                                        <select id="selectNameTitle" name="" class="form-control input-sm" required>
                                            <option>Oregon</option>

                                        </select>

                                    </div>
                                </div>
                            </div>


                            <div class="col-md-12">

                                <div class="form-group col-md-3">
                                    <label class="control-label small">Number *</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label small">Suite/Building*</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label small">Street *</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>

                                <div class="form-group col-md-3">
                                    <label class="control-label small">Zip Code *</label>
                                    <div>
                                        <input type="text" class="form-control input-sm" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>



                    <fieldset>
                        <legend>
                            Other
                        </legend>
                        <div class="panel-body">

                            <div class="col-md-12">
                                <div class="col-md-12">
                                    <label class="control-label small">Reason For Deaprture</label>
                                    <textarea class="form-control input-sm"></textarea>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <div class="form-group col-md-6">
                                    <label class="control-label small">Start Date *</label>
                                    <div>
                                        <input type="date" class="form-control input-sm" />
                                    </div>
                                </div>

                                <div class="form-group col-md-6">
                                    <label class="control-label small">End Date *</label>
                                    <div>
                                        <input type="date" class="form-control input-sm" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </fieldset>

                    <div class="col-md-12 form-group">
                        <button ng-show="workHistoryFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelWorkHistory('editCancel')">Cancel</button>
                        <button ng-show="!workHistoryFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelWorkHistory('saveCancel')">Cancel</button>
                        <button ng-show="!workHistoryFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="saveWorkHistory(workHistory)">Add</button>
                        <button ng-show="workHistoryFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="updateWorkHistory(workHistory)">Update</button>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <fieldset ng-show="newWorkHistoryForm && !workHistoryFormStatus">
                <legend>
                    Add Work History
                </legend>
                <div id="newWorkHistoryFormDiv">
                    <div class="col-lg-12 zero-padding-left-right-div">
                        <form id="newWorkHistoryForm">
                           
                          @Html.Partial("~/Views/Profile/_ProfessionalWorkEmployer.cshtml")

                          @Html.Partial("~/Views/Profile/_ProfessionalWorkAddress.cshtml") 

                          @Html.Partial("~/Views/Profile/_ProfessionalWorkOther.cshtml") 

                        </form>

                        <div class="col-md-12 form-group">
                            
                            <button ng-show="!workHistoryFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelWorkHistory('saveCancel')">Cancel</button>
                            <button ng-show="!workHistoryFormStatus" type="submit" class="btn btn-primary btn-sm pull-right" ng-click="saveWorkHistory()">Add</button>
                            
                        </div>
                    </div>
                </div>
            </fieldset>
            <div ng-hide="workHistoryFormStatus || newWorkHistoryForm" class="col-md-12 text-right">
                <button class="btn btn-xs btn-primary" ng-click="addWorkHistory()"><i class="fa fa-plus"></i> Add Work History</button>
            </div>
            <div class="clearfix"></div>
        </div>

    </fieldset>

</div>

