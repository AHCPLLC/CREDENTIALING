
<table ng-table="SearchProviderTable1" class="table table-bordered table-hover table-condensed alignTableStyle">
    <thead>
        <tr>
            <th rowspan="2">Profile Pic</th>
            <th>NPI Number</th>
            <th>First Name</th>
            <th>Last Name</th>
            @*<th>Plan</th>
                <th>Group</th>*@
            <th>Status</th>
            <th>Approval Date</th>
            <th rowspan="2">Select</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="p in $data" ng-if="p.IsCkecked">
            <td class="col-lg-1">
                <div class="thumbnail imageStyle">
                    <img src="~/Content/Images/Providers/{{p.Image}}" class="img-rounded">
                </div>
            </td>
            <td>{{p.NPI}}</td>
            <td>{{p.FirstName}}</td>
            <td>{{p.LastName}}</td>
            @*<td><span ng-repeat="s in p.Plan"><span ng-if="$index > 0">, </span>{{s}}</span></td>
            <td><span ng-repeat="g in p.Groups"><span ng-if="$index > 0">, </span>{{g}}</span></td>*@
            <td>{{p.CredType}}</td>
            <td>{{p.CredDate}}</td>
            <td class="col-lg-1">
                <a class="btn btn-sm btn-danger" ng-click="p.IsCkecked=false;"><i class="fa fa-remove"></i></a>
            </td>
        </tr>
    </tbody>
</table>

<a ng-click="InitiateCredentialing()" class="btn btn-sm btn-primary pull-right">Submit</a>