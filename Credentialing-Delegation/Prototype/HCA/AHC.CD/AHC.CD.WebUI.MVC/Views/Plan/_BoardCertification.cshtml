<div class="panel-group" id="accordionBoard" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="BoardHeading">
            <a data-toggle="collapse" data-parent="#accordionBoard" href="#board" aria-expanded="false" aria-controls="board">
                Board Certification
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="board" class="panel-collapse collapse" role="tabpanel" aria-labelledby="BoardHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-6">
                            <label class="control-label small">Speciality Board Name</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                        <div class="col-lg-6">
                            <label class="control-label small">Certification Status</label>
                            <select class="form-control input-sm">
                                <option>Active</option>
                                <option>Inactive</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label class="control-label small">Certification Date</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-6">
                            <label class="control-label small">Expiration Date</label>
                            <input type="date" class="form-control input-sm" />
                        </div>
                        <div class="col-md-12">
                            <label class="control-label small">If you are not Board Certified, on what day will you be (or were) first eligible to complete your Board examinaton?</label>
                            <input type="text" class="form-control input-sm" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

