@{
    ViewBag.Title = "ProvidersList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/Lib/Angular/angular.min.js"></script>
<script src="~/Scripts/Lib/Angular/ng-table.js"></script>

<script src="~/Areas/Credentialing/Scripts/verification.js"></script>

<div id="page-wrapper" ng-app="VerificationApp" ng-cloak>
    <div class="container-fluid" ng-controller="VerificationCtrl" ng-init="LoadData()">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="fa fa-dashboard"></i>  <a href="/Dashboard/Index">Dashboard</a>
                    </li>
                    @*<li class="active">
                        <i class="fa fa-list-alt"></i> Verfications
                    </li>*@
                </ol>
            </div>
        </div>
        <!-- /.row -->
        <!-- Page Heading -->
        <div class="row">
            <div class="panel panel-default content-panel">
                <div class="panel-heading">
                    <h3 class="panel-title">List of Primary Source Verfication</h3>
                </div>
                <div class="progress col-lg-12" ng-show="progressbar" ng-cloak>
                    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                        Loading...Please wait...
                    </div>
                </div>
                <div class="panel-body">
                    <p>
                        <table ng-table="tableParams" show-filter="true" class="table table-hover ng-table-resizable-columns ng-table-rowselected">
                            <tr ng-repeat="user in $data track by $index">
                                <td data-title="'Title'" sortable="'Title'" filter="{ 'Title': 'text' }">
                                    {{user.Title}}
                                </td>
                                <td data-title="'First Name'" sortable="'FirstName'" filter="{ 'FirstName': 'text' }">
                                    {{user.FirstName}}
                                </td>
                                <td data-title="'Last Name'" sortable="'LastName'" filter="{ 'LastName': 'text' }">
                                    {{user.LastName}}
                                </td>
                                <td data-title="'Plan'" sortable="'PlanName'" filter="{ 'PlanName': 'text' }">

                                    @*<a href="/Credentialing/Verification/InitiateNewPSV?credinfoId={{user.CredentialingInfoID}}">*@ {{user.PlanName}}</a>

                                </td>
                                <td data-title="'Initiation Date'" sortable="'InitiationDate'" filter="{ 'InitiationDate': 'text' }">{{user.InitiationDate | date:'MM/dd/yyyy'}}</td>
                                <td data-title="'Action'">
                                    <a class="btn btn-xs btn-success" href="/Credentialing/Verification/InitiateNewPSV?profileId={{user.ProfileID}}&credinfoId={{user.CredentialingInfoID}}" tooltip data-toggle="tooltip" data-placement="top" title="PSV"><i class="fa fa-caret-right"></i></a>
                                    @*<a class="btn btn-xs btn-info" href="/Credentialing/Verification/GetPendingPSVList?credinfoId={{user.CredentialingInfoID}}" tooltip data-toggle="tooltip" data-placement="top" title="Continue PSV"><i class="fa fa-forward"></i></a>*@

                                </td>
                            </tr>
                            <tr ng-if="showNoData">
                                <td>No data Available</td>
                            </tr>
                        </table>
                        



