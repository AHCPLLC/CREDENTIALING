
<div ng-show="NoPlan">
    <div class="col-lg-12">
        <div class="alert alert-danger" role="alert">
            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
            <span class="sr-only">Error:</span>
            Error List : <br />
            <span>No plan is selected!!!</span>
        </div>
    </div>
</div>
<div ng-hide="NoPlan">
    <label class="control-label small">
        Are you want to initaite for {{selectedPlan.PlanName}} plan.
    </label>
    <div class="col-lg-12 zero-padding-left-right-div">
        <div ng-show="activeMessageDiv=='successfulInitiated'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div ng-show="activeMessageDiv=='errorInitiated1'" class="alert alert-danger" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
    </div>
    <div class="col-md-12 pull-right">
        <button class="btn btn-primary btn-sm pull-right" type="submit" ng-click="InitiateCredentialing();">Initiate</button>
    </div>
</div>