@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocation.PracticeServiceViewModel
<div class="form-style" ng-hide="ServicesEdit">
    <fieldset>
        <legend>
            Services
        </legend>
        <div ng-hide="ServicesEdit" class="legend2 pull-right">
            <button ng-click="ServicesEdit=true" class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
        </div>

        <div class="form-group">
            <div class="col-md-12">
                <div class="col-md-12">
                    <label for="Contact" class="control-label">Does this location provide any of the following services?</label>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-3">
                    <label class="control-label small">Laboratory Services?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.LaboratoryServices}}</i>
                    </div>
                </div>
                <div ng-show="program" class="form-group col-md-3">
                    <label class="control-label small">Provide Accrediting/Certifying Program</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Accrediting}}</i>
                    </div>
                </div>

                <div class="form-group col-md-3">
                    <label class="control-label small">Radiology Services?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.RadiologyServices}}</i>
                    </div>
                </div>
                <div ng-show="xray" class="form-group col-md-3">
                    <label class="control-label small">Provide X-Ray certificate type</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.XRay}}</i>
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">EKGS?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.EKGS}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Allergy Injection?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.AllergyInjection}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Allergy Skin Testing?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.SkinTesting}}</i>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>

            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">Routing Office Gynecology(Pelvi/Pap)?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Gynecology}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Drawing Blood?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Blood}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Age Appropriate Immunization?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Immunization}}</i>
                    </div>
                </div>
            </div>

            <div class="clearfix"></div>

            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">Flexible Sigmoidoscopy?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Sigmoidoscopy}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Tympanometry/Audiometry Screening?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Tympanometry}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Asthma Treatment?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Asthma}}</i>
                    </div>
                </div>
            </div>

            <div class="clearfix"></div>


            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">Osteopathic Manipulation?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Osteopathic}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">IV Hydration/Treatment?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Hydration}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Cardiac Stress Test?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Cardiac}}</i>
                    </div>
                </div>
            </div>

            <div class="clearfix"></div>


            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label class="control-label small">Pulmonary Function Testing?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Pulmonary}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Physical Therapy?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Physical}}</i>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="control-label small">Care Of Minor Lacerations?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Lacerations}}</i>
                    </div>
                </div>
            </div>
            <hr class="zero-margin-top-bottom-hr" />
            <div class="clearfix"></div>

            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label class="control-label small">Is Anesthesia Administered In Your Office?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.Anesthesia}}</i>
                    </div>
                </div>
                <div ng-show="admin">
                    <div class="form-group col-md-6">
                        <label class="control-label small">What Class/Category Do You Use</label>
                        <div>
                            <i class="data-label">{{primaryPracticeLocations.Services.Category}}</i>
                        </div>
                    </div>


                    <div class="clearfix"></div>

                    <div class="col-md-12">
                        <div class="form-group col-md-4">
                            <label class="control-label small">Who Administers It?</label>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label small">First Name</label>
                            <div>
                                <i class="data-label">{{primaryPracticeLocations.Services.FirstName}} </i>
                            </div>
                        </div>
                        <div class="form-group col-md-4">
                            <label class="control-label small">Last Name</label>
                            <div>
                                <i class="data-label"> {{primaryPracticeLocations.Services.LastName}} </i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>

            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label class="control-label small">Type Of Practice?</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.TypeOfPractice}}</i>
                    </div>
                </div>


                <div class="form-group col-md-6">
                    <label class="control-label small">Additional Office Procedures Provided(Including Surgical Procedures)</label>
                    <div>
                        <i class="data-label">{{primaryPracticeLocations.Services.AdditionalOffice}}</i>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
</div>

