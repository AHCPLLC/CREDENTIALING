
<div><a class="btn btn-xs btn-warning pull-right" ng-click="NPDBHistoryDiv=true" toggle data-toggle="tooltip" data-placement="bottom" title="NPDB History"><i class="fa fa-clock-o"></i></a></div>


<div class="panel-body">
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="NPDB.ViewDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_ViewNPDBVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="NPDB.AddDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditNPDBVerificationResult.cshtml")
    </div>
    <div class="col-lg-12 zero-padding-left-right-div" ng-if="NPDB.EditDiv">
        @Html.Partial("~/Areas/Credentialing/Views/Verification/_AddEditNPDBVerificationResult.cshtml")
    </div>
    <div class="clearfix"></div><br />
    <div ng-show="activeMessageDiv=='npdbPSV'" class="alert alert-success" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
    <div ng-show="activeMessageDiv=='statePSVError'" class="alert alert-danger" role="alert">
        @*<button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>*@
        {{messageDesc}}
    </div>
    
</div>

