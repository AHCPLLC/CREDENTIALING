@model AHC.CD.WebUI.MVC.Areas.Profile.Models.EducationHistory.EducationDetailViewModel
@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<div id="viewGraduation">
    <div class=" col-lg-12 zero-padding-left-right-div">
        <fieldset class="wrap-words">
            <legend>
                Graduate/Medical School Details
            </legend>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.USGraduateYesNoOption, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{graduationDetailViewModelHistory.IsUSGraduate || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.GraduateType, htmlAttributes: new { @class = "control-label small" })
                    <div ng-if="graduationDetailViewModelHistory.GraduateType == 1">
                        <i class="data-label">{{graduationDetailViewModelHistory.GraduationType || 'Not Available'}}</i>
                    </div>
                    <div ng-if="graduationDetailViewModelHistory.GraduateType == 2">
                        <i class="data-label">Fifth Pathway Graduate</i>
                    </div><div ng-if="!graduationDetailViewModelHistory.GraduateType">
                        <i class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.SchoolInformation.SchoolName, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.SchoolName || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.QualificationDegree, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.QualificationDegree || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.Street, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.Street || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.Building, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.Building" class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.Building}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.Building" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.City, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.City || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.State, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.State || 'Not Available'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.ZipCode, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.ZipCode" class="data-label">{{graduationDetailViewModelHistory.SchoolInformation.ZipCode}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.ZipCode" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.Country, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.Country || 'Not Available'}}</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.County, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.County" class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.County}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.County" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    @Html.LabelFor(model => model.SchoolInformation.Email, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.Email" class="data-label wrap-words">{{graduationDetailViewModelHistory.SchoolInformation.Email}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.Email" class="data-label">Not Available</i>
                    </div>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.SchoolInformation.Phone, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.Phone" class="data-label">{{graduationDetailViewModelHistory.SchoolInformation.PhoneCountryCode}}-{{graduationDetailViewModelHistory.SchoolInformation.Phone}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.Phone" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.SchoolInformation.Fax, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i ng-if="graduationDetailViewModelHistory.SchoolInformation.Fax" class="data-label">{{graduationDetailViewModelHistory.SchoolInformation.FaxCountryCode}}-{{graduationDetailViewModelHistory.SchoolInformation.Fax}}</i>
                        <i ng-if="!graduationDetailViewModelHistory.SchoolInformation.Fax" class="data-label">Not Available</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.StartDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{graduationDetailViewModelHistory.StartDate  || 'Not Available' | date:'MM-dd-yyyy' }}</i>
                    </div>
                </div>
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.EndDate, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{graduationDetailViewModelHistory.EndDate || 'Not Available' | date:'MM-dd-yyyy'}}</i>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-lg-3 form-group">
                    @Html.LabelFor(model => model.CompletedYesNoOption, htmlAttributes: new { @class = "control-label small" })
                    <div>
                        <i class="data-label">{{graduationDetailViewModelHistory.IsCompleted || 'Not Available'}}</i>
                    </div>
                </div>
                @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Preview, ViewBag.Roles, false))
                {
                    <div class="col-lg-3 form-group">
                        <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                            @Html.LabelFor(model => model.CertificatePath, htmlAttributes: new { @class = "control-label small" })
                            <div ng-if="graduationDetailViewModelHistory.CertificatePath">
                                <i class="data-label"><a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{graduationDetailViewModelHistory.CertificatePath}}" cl class="pointer" target="_blank" tooltip data-toggle="tooltip" data-placement="top" title="Diploma"><i class="fa fa-file"></i></a></i>
                            </div>
                            <div ng-if="!graduationDetailViewModelHistory.CertificatePath">
                                <i class="data-label"> Not available</i>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <br />
            <div class="col-lg-12 zero-padding-left-right-div">
                <div class="col-md-12 form-group">
                    <button type="button" class="btn btn-warning btn-sm" ng-click="operateCancelControl('')">Cancel</button>
                </div>
            </div>
        </fieldset>
    </div>
</div>