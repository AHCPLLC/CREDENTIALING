                        <div class="col-md-6 ">
                            <label class="control-label small">{{masterAccessibilityQuestions[0].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[0].Answer || 'Not Available'}}</i>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <hr class="zero-margin-top-bottom-hr">
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label class="control-label small">{{masterAccessibilityQuestions[1].Title}}</label>
                        </div>
                        <div class="col-md-3">
                            <label class="control-label small">{{masterAccessibilityQuestions[1].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[1].Answer || 'Not Available'}}</i>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="control-label small">{{masterAccessibilityQuestions[2].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[2].Answer || 'Not Available'}}</i>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="control-label small">{{masterAccessibilityQuestions[3].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[3].Answer || 'Not Available'}}</i>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label class="control-label small">Other Handicapped Access</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.OtherHandicappedAccess || 'Not Available'}}</i>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <hr class="zero-margin-top-bottom-hr">
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label class="control-label small">{{masterAccessibilityQuestions[4].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[4].Answer || 'Not Available'}}</i>
