                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </fieldset>
    @*Facility Accessibilities Information End*@

    @*Facility Services Information Start*@
    <fieldset class="wrap-words">
        <legend>
            Services
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="form-group">
                <div class="col-md-12">
                    <div class="col-md-12">
                        <label for="Contact" class="control-label">Does this location provide any of the following services?</label>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group col-md-4">
                        @Html.LabelFor(model => model.FacilityDetail.Service.LaboratoryServicesYesNoOption, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.LaboratoryServices || 'Not Available'}}</i>
                        </div>
                    </div>
                    <div ng-show="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.LaboratoryServicesYesNoOption == '1'" class="form-group col-md-8">
                        @Html.LabelFor(model => model.FacilityDetail.Service.LaboratoryAccreditingCertifyingProgram, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.LaboratoryAccreditingCertifyingProgram || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <hr class="zero-margin-top-bottom-hr">
                <div class="clearfix"></div>
                <br />
                <div class="col-md-12">
                    <div class="form-group col-md-4">
                        @Html.LabelFor(model => model.FacilityDetail.Service.RadiologyServicesYesNoOption, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.RadiologyServices || 'Not Available'}}</i>
                        </div>
                    </div>
                    <div ng-show="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.RadiologyServicesYesNoOption == '1'" class="form-group col-md-8">
                        @Html.LabelFor(model => model.FacilityDetail.Service.RadiologyXRayCertificateType, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.RadiologyXRayCertificateType || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <hr class="zero-margin-top-bottom-hr">
                <div class="clearfix"></div>
                <br />
                <div class="col-md-12">
                    <div class="form-group col-md-4" ng-repeat="serviceQuestion in masterServiceQuestions">
                        <label class="control-label small">{{serviceQuestion.ShortTitle}}</label>
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.FacilityServiceQuestionAnswers[$index].Answer || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <hr class="zero-margin-top-bottom-hr">
                <div class="clearfix"></div>
                <br />
                <div class="col-md-12">
                    <div class="form-group col-md-6">
                        @Html.LabelFor(model => model.FacilityDetail.Service.IsAnesthesiaAdministeredYesNoOption, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.IsAnesthesiaAdministered || 'Not Available'}}</i>
                        </div>
                    </div>
                    <div ng-show="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.IsAnesthesiaAdministeredYesNoOption == '1'">
                        <div class="form-group col-md-6">
                            @Html.LabelFor(model => model.FacilityDetail.Service.AnesthesiaCategory, htmlAttributes: new { @class = "control-label small" })
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.AnesthesiaCategory || 'Not Available'}}</i>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <label class="control-label small col-md-12">Who Administers It?</label>
                        <div class="col-md-12 zero-padding-left-right-div">
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.FacilityDetail.Service.AnesthesiaAdminFirstName, htmlAttributes: new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.AnesthesiaAdminFirstName || 'Not Available'}}</i>
                                </div>
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.FacilityDetail.Service.AnesthesiaAdminLastName, htmlAttributes: new { @class = "control-label small" })
                                <div>
                                    <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.AnesthesiaAdminLastName || 'Not Available'}}</i>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <hr class="zero-margin-top-bottom-hr" />
                <div class="clearfix"></div><br />
                <div class="col-md-12">
                    <div class="form-group col-md-6">
                        @Html.LabelFor(model => model.FacilityDetail.Service.PracticeTypeID, htmlAttributes: new { @class = "control-label small" })
                        <div ng-if="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.PracticeTypeID == '1'">
                            <i class="data-label">Solo Practice</i>
                        </div>
                        <div ng-if="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.PracticeTypeID == '2'">
                            <i class="data-label">Single Specialty Group</i>
                        </div>
                        <div ng-if="PracticeLocationDetailHistory.Facility.FacilityDetail.Service.PracticeTypeID == '3'">
                            <i class="data-label">Multi-Specialty Group</i>
                        </div>
                        <div ng-if="!PracticeLocationDetailHistory.Facility.FacilityDetail.Service.PracticeTypeID">
                            <i class="data-label">Not Available</i>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        @Html.LabelFor(model => model.FacilityDetail.Service.AdditionalOfficeProcedures, htmlAttributes: new { @class = "control-label small" })
                        <div>
                            <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Service.AdditionalOfficeProcedures || 'Not Available'}}</i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
    @*Facility Services Information End*@

    @*Facility Languages Information Start*@
    @Html.Partial("~/Areas/Profile/Views/PracticeLocation/_ViewFacilityLanguageHistory.cshtml")
    @*Facility Languages Information End*@

    <fieldset class="wrap-words">
        <legend>
            Office Hours
        </legend>
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-md-2 form-group">
                <label class="control-label small">Day</label>
            </div>
            <div class="col-md-10">
                <div class="col-md-4 form-group">
                    <label class="control-label small">Start Time</label>
                </div>
                <div class="col-md-4 form-group">
                    <label class="control-label small">End Time</label>
                </div>
            </div>
            <div class="col-md-12 zero-padding-left-right-div" ng-repeat="PracticeDay in PracticeLocationDetailHistory.Facility.FacilityDetail.PracticeOfficeHour.PracticeDays">
                <div class="col-md-2 form-group">
                    <label class="control-label small">{{PracticeDay.DayName || 'Not Available'}}</label>
                </div>
                <div class="col-md-10">
                    <!-- ngRepeat: t in Days[0].Times -->
                    <div ng-repeat="DailyHour in PracticeDay.DailyHours" class="col-md-12 zero-padding-left-right-div ng-scope">
                        <div class="col-md-4 form-group">
                            <i>{{ DailyHour.DayOff == 'YES' ? 'Day Off' : DailyHour.StartTime }}</i>
                        </div>
                        <div class="col-md-4 form-group">
                            <i>{{ DailyHour.DayOff == 'YES' ? 'Day Off' : DailyHour.EndTime }}</i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </fieldset>
    <fieldset class="wrap-words">
        <legend>
            Mid-Level Practitioners
        </legend>
        <div class="col-lg-12">
            <table ng-if="PracticeLocationDetailHistory.Facility.FacilityDetail.FacilityPracticeProviders.length < 1 || !(PracticeLocationDetailHistory.Facility.FacilityDetail.FacilityPracticeProviders)" class=" table table-bordered table-condensed customtbodystyle table-striped">
                <tbody>
                    <tr>
                        <td>No Mid-Levels Available For This Facility</td>
                    </tr>
                </tbody>
            </table>
            <table ng-if="PracticeLocationDetailHistory.Facility.FacilityDetail.FacilityPracticeProviders.length > 0" class=" table table-bordered table-condensed customtbodystyle table-striped">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Middle name</th>
                        <th>Last Name</th>
                        <th>NPI Number </th>
                    </tr>
                </thead>
                <tbody ng-repeat="practitioner in PracticeLocationDetailHistory.Facility.FacilityDetail.FacilityPracticeProviders">
                    <tr>
                        <td>{{practitioner.FirstName || 'Not Available'}}</td>
                        <td>{{practitioner.MiddleName || 'Not Available'}}</td>
                        <td>{{practitioner.LastName || 'Not Available'}}</td>
                        <td>{{practitioner.NPINumber || 'Not Available'}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </fieldset>
</div>