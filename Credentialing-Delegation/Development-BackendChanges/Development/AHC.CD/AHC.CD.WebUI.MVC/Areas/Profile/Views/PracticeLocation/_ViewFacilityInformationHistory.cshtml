                    <br />
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <label class="control-label small">{{masterAccessibilityQuestions[7].ShortTitle}}</label>
                            <div>
                                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[7].Answer || 'Not Available'}}</i>
                            </div>
                            <br />
                        </div>
                        <div ng-show="PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[7].AnswerYesNoOption == '1'">
                            <div class="col-md-3">
                                <label class="control-label small">{{masterAccessibilityQuestions[8].ShortTitle}}</label>
                                <div>
                                    <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[8].Answer || 'Not Available'}}</i>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="control-label small">{{masterAccessibilityQuestions[9].ShortTitle}}</label>
                                <div>
                                    <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[9].Answer || 'Not Available'}}</i>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="control-label small">{{masterAccessibilityQuestions[10].ShortTitle}}</label>
                                <div>
                                    <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityDetail.Accessibility.FacilityAccessibilityQuestionAnswers[10].Answer || 'Not Available'}}</i>
                                </div>
