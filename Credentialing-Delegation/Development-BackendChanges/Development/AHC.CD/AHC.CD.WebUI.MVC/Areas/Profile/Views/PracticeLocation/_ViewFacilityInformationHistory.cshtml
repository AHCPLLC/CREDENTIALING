@model AHC.CD.WebUI.MVC.Areas.Profile.Models.PracticeLocationInformation.PracticeLocationViewModel

@*Facility General Information Start*@
<fieldset class="wrap-words">
    <legend>General Information</legend>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.FacilityName, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.FacilityName || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Name, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.Name || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Street, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.Street || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Building, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.Building || 'Not Available'}}</i>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.City, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.City || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.State, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.State || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.ZipCode, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.ZipCode || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.Country || 'Not Available'}}</i>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.County, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.County || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Telephone, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label" ng-show="PracticeLocationDetailHistory.Facility.Telephone">{{PracticeLocationDetailHistory.Facility.CountryCodeTelephone}} - </i><i class="data-label">{{PracticeLocationDetailHistory.Facility.Telephone || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.Fax, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label" ng-show="PracticeLocationDetailHistory.Facility.Fax">{{PracticeLocationDetailHistory.Facility.CountryCodeFax}} - </i><i class="data-label">{{PracticeLocationDetailHistory.Facility.Fax || 'Not Available'}}</i>
            </div>
        </div>
        <div class="form-group col-md-3">
            @Html.LabelFor(model => model.EmailAddress, htmlAttributes: new { @class = "control-label small" })
            <div>
                <i class="data-label">{{PracticeLocationDetailHistory.Facility.EmailAddress || 'Not Available'}}</i>
            </div>
        </div>
    </div>

</fieldset>
<div ng-show="activeMessageDiv=='updatedFacility'" class="alert alert-success" role="alert">
    <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
    {{messageDesc}}
</div>
@*Facility General Information End*@

<div ng-if="PracticeLocationToggle">
    @*Facility Accessibilities Information Start*@
    <fieldset class="wrap-words">
        <legend>
            Accessibilities
        </legend>
        <div id="newPracLocAccessibilitiesFormDiv">
            <div class="col-lg-12 zero-padding-left-right-div">
