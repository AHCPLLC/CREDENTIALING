<div class="panel-group" id="accordionciti" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="citiHeading">
            <a data-toggle="collapse" data-parent="#accordionciti" href="#citi" aria-expanded="false" aria-controls="citi">
               Citizenship
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="citi" class="panel-collapse collapse" role="tabpanel" aria-labelledby="citiHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="form-group col-md-12">
                            <div class="col-md-6">
                                <label class="control-label small">Are you a citizen of US?</label>
                                <div>
                                    <label class="radio-inline">
                                        <input type="radio" name="Citizen" checked="checked"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="Citizen"> No
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label class=" control-label small">Are you authorised to work in US?</label>
                                <div>
                                    <label class="radio-inline">
                                        <input type="radio" name="work" checked="checked"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="work"> No
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4">
                                <label class="control-label small">Visa number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>

                            <div class="form-group col-md-4">
                                <label class="control-label small">Type of Visa</label>
                                <div>
                                    <select id="selectNameTitle" name="visatype" class="form-control input-sm" required>
                                        <option>Type1</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-md-4">
                                <label class="control-label small">Visa status</label>
                                <div>
                                    <select id="selectNameTitle" name="visastatus" class="form-control input-sm" required>
                                        <option>Status1</option>
                                    </select>
                                </div>
                            </div>
                        </div>


                        <div class="form-group col-md-12">
                            <div class="form-group col-md-4">
                                <label class="control-label small">Visa Sponsor</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>

                            <div class="form-group col-md-4">
                                <label class="control-label small">Visa Expiration</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>

                            <div class="form-group col-md-4">
                                <label class="control-label small">Green Card Number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">National Identification Number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" />
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label small">Country Of Issue</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



