<div class="row">
    <div class="col-lg-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right-div form-style" style="border-top: 1px solid #eee; padding-top: 10px;" ng-if="TableView">
            <div class="progress col-lg-12" ng-if="!progressbar" ng-cloak>
                <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                    Loading...Please wait...
                </div>
            </div>
            <div ng-if="progressbar">
                @*<div class="col-lg-12 zero-padding-left-right-div">
                    <div ng-show="activeMessageDiv=='successfullySaved'" class="alert alert-success" role="alert">
                        <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
                        
                    </div>
                    <div ng-show="activeMessageDiv=='errorInitiated'" class="col-lg-12">
                        <div class="alert alert-danger" role="alert">
                            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                            <span class="sr-only">Error:</span>
                            
                        </div>
                    </div>
                </div>*@
                <div ng-show="trackerItems==0 && progressbar">
                    <label>No Tasks Assigned.</label>
                </div>
                <table ng-show="trackerItems.length>0 && progressbar" class='table table-striped table-bordered customtbodyStyle medium wrap-words' st-pipe="mc.callServer" st-persist="mc.displayed" st-table="mc.displayed" st-safe-src="trackerItems">
                    <thead>
                        <tr>
                            <th st-sort="ProviderName" st-ratio="14">
                                Provider Name
                            </th>
                            <th st-sort="Subject" st-ratio="9">
                                Subject
                            </th>
                            <th st-sort="SubSectionName" st-ratio="10">
                                Sub Section Name
                            </th>
                            <th st-sort-default="false" st-sort="NextFollowUpDate" st-ratio="10" ng-if="tabStatus.followUpDate">
                                Next FollowUp Date
                            </th>
                            <th st-sort="AssignedTo" st-ratio="14" ng-if="tabStatus.Assigned">
                                Assigned To
                            </th>
                            <th st-sort-default="false" st-sort="ModifiedDate" ng-if="tabStatus.updatedOn" st-ratio="10">
                                Updated On
                            </th>
                            <th st-sort-default="false" st-sort="UpdatedBy" st-ratio="14" ng-if="tabStatus.updatedby">
                                Updated By
                            </th>
                            <th st-sort="CompleteStatus" st-ratio="7">
                                Task Status
                            </th>
                            <th st-sort="InsuranceCompanyName" st-ratio="12" ng-if="tabStatus.insurance">
                                Insurance Company
                            </th>
                            <th st-ratio="12" ng-if="tabStatus.followup">
                                Mode Of Follow Up
                            </th>
                            <th st-ratio="12">Action</th>
                        </tr>
                        <tr>
                            <td st-ratio="14"><input st-search="ProviderName" type="text" placeholder="Filter Provider Name" class="form-control input-sm"></td>
                            <td st-ratio="9"><input st-search="Subject" type="text" placeholder="Filter Subject" class="form-control input-sm" /></td>
                            <td st-ratio="10"><input st-search="SubSectionName" type="text" placeholder="Filter Sub Section Name" class="form-control input-sm" /></td>
                            <td st-ratio="10" ng-if="tabStatus.followUpDate"><input st-search="NextFollowUpDate" type="text" placeholder="Filter Next FollowUp Date" class="form-control input-sm" /></td>
                            <td st-ratio="14" ng-if="tabStatus.Assigned"><input st-search="AssignedTo" type="text" placeholder="Filter Assigned To" class="form-control input-sm" /></td>
                            <td st-ratio="10" ng-if="tabStatus.updatedOn"><input st-search="ModifiedDate" type="text" placeholder="Filter Date" class="form-control input-sm" /></td>
                            <td st-ratio="14" ng-if="tabStatus.updatedby"><input st-search="UpdatedBy" type="text" placeholder="Filter Updated By" class="form-control input-sm" /></td>
                            <td st-ratio="7"><input st-search="CompleteStatus" type="text" placeholder="Filter Task Status" class="form-control input-sm" /></td>
                            <td st-ratio="12" ng-if="tabStatus.insurance"><input st-search=" InsuranceCompanyName" type="text" placeholder="Filter Insurance Company" class="form-control input-sm" /></td>
                            <td st-ratio="12" ng-if="tabStatus.followup"><input st-search="ModeOfFollowUp" type="text" placeholder="Filter Follow Up Mode" class="form-control input-sm" /></td>
                            <td st-ratio="12"></td>
                        </tr>
                    </thead>
                    <tbody>
                        @*{{mc.displayed.length}}
                {{mc.displayed.ProviderName}}*@
                        <tr ng-repeat="c in mc.displayed ">
                            <td>{{c.ProviderName}}</td>
                            <td>{{c.Subject}}</td>
                            <td>{{c.SubSectionName}}</td>
                            <td ng-if="tabStatus.followUpDate">{{c.NextFollowUpDate}}</td>
                            <td ng-if="tabStatus.Assigned">{{c.AssignedTo}}</td>
                            <td ng-if="tabStatus.updatedOn">{{c.ModifiedDate}}</td>
                            <td ng-if="tabStatus.updatedby">{{c.UpdatedBy}}</td>
                            <td>{{c.CompleteStatus}}</td>
                            <td ng-if="tabStatus.insurance">{{c.InsuranceCompanyName || 'Not Available'}}</td>
                            @*<td><p ng-repeat="m in c.InsuranceCompanyName"><span class="label label-info">{{m}}</span></p></td>*@
                            <td ng-if="tabStatus.followup"><p ng-repeat="m in c.ModeOfFollowUp"><span class="label label-info">{{m.Name}}</span></p></td>
                            <td>
                                <a class="btn btn-xs btn-default" href='@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Profile/MasterProfile/ProviderProfile/{{c.ProfileID}}{{c.TabID}}{{c.SubSectionID}}' tooltip data-toggle="tooltip" data-placement="top" title="Redirect" ng-if="tabStatus.editBtn"><i class="fa fa-repeat"></i></a>
                                <button class="btn btn-xs btn-default" ng-click="showDetailViewforTab1(c)" tooltip data-toggle="tooltip" data-placement="top" title="View"><i class="fa fa-eye"></i></button>
                                <button class="btn btn-xs btn-primary" ng-click="showeditView(c,'editViewforTab')" ng-hide="c.CompleteStatus=='Closed'" tooltip data-toggle="tooltip" data-placement="top" title="Edit" ng-if="tabStatus.editBtn"><i class="fa fa-edit"></i></button>
                                <button class="btn btn-xs btn-danger" data-toggle="tooltip" ng-hide="c.CompleteStatus=='Closed'" data-placement="top" title="Close" ng-click="QuickRemove(c);" tooltip ng-if="tabStatus.removeBtn"><i class="fa fa-remove"></i></button>
                                <button class="btn btn-xs btn-warning" ng-click="showHistory(c)" tooltip data-toggle="tooltip" data-placement="top" title="Task Update History" ng-if="tabStatus.historyBtn"><i class="fa fa-clock-o"></i></button>
                                <button class="btn btn-xs btn-primary" data-toggle="tooltip" ng-hide="c.CompleteStatus=='Open'" data-placement="top" title="Open" ng-click="Quickreopen(c);" tooltip ng-if="c.CompleteStatus=='Closed'"><i class="fa fa-repeat"></i></button>
                            </td>
                        </tr>
                    </tbody>
                    <tfoot style="cursor:pointer">
                        <tr>
                            <td class="text-center" st-pagination="" st-items-by-page="10" colspan="9"></td>
                        </tr>
                    </tfoot>
                </table>
            </div>
            @*<div class="clearfix"></div>
                <div ng-if="CurrentPageforTasksAssigned.length > 0" ccolicensedetailstable></div>
                <pagination ng-if="CurrentPageforTasksAssigned.length > 0" total-items="bigTotalItemsforTasksAssigned" ng-model="bigCurrentPageforTasksAssigned" max-size="maxSize" class="pagination-sm" ng-change="pageChangedforTasksAssigned(bigCurrentPageforTasksAssigned)" boundary-links="true" rotate="false" num-pages="numPages" style="margin:0;"></pagination>
                <div class="clearfix"></div><br />*@
        </div>
        <div ng-if="VisibilityControl == 'editViewforTab'" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right-div form-style">
            <fieldset>
                <legend>
                    Edit Task
                </legend>
                <div class="legend2 pull-right">
                    <button ng-click="cancelEditViewforTab1()" class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="top" title="close"><i class="fa fa-close"></i></button>
                </div>
                <div id="editFormDiv">
                    @Html.Partial("~/Views/TaskTracker/_AddEditTask.cshtml")
                </div>
            </fieldset>
        </div>
        <div ng-if="detailViewfortab" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right-div form-style">
            <fieldset>
                <legend>
                    View Task
                </legend>
                <div class="legend2 pull-right">
                    <button ng-click="cancelViewforTab1()" class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="top" title="close"><i class="fa fa-close"></i></button>
                </div>
                <div id="detailFormDiv">
                    @Html.Partial("~/Views/TaskTracker/_ViewTask.cshtml")
                </div>
            </fieldset>
        </div>
    </div>
</div>
<div class="modal fade" id="inactiveWarningModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form id="removeTask">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                </div>
                <div class="col-lg-12 text-center" ng-show="isRemoved">
                    <div style="margin-top:2%;">
                        <img src="~/Content/Images/ajax-loader.gif" style="width:50px;" /> Please Wait..
                    </div>
                </div>
                <div class="modal-body" ng-show="!isRemoved">
                    Do you really want to Close the Task ?
                    <input type="hidden" name="TaskTrackerId" value="{{task.TaskTrackerId}}" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" ng-click="removeTask();cancelForRemoveTask()" data-dismiss="modal">Continue</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="modal fade" id="reactiveWarningModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form id="removeTask">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                </div>
                <div class="col-lg-12 text-center" ng-show="isRemoved">
                    <div style="margin-top:2%;">
                        <img src="~/Content/Images/ajax-loader.gif" style="width:50px;" /> Please Wait..
                    </div>
                </div>
                <div class="modal-body" ng-show="!isRemoved">
                    Do you really want to re-open the Task ?
                    <input type="hidden" name="TaskTrackerId" value="{{task.TaskTrackerId}}" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" ng-click="reopenTask();cancelForReopenTask()" data-dismiss="modal">Continue</button>
                </div>
            </form>
        </div>
    </div>
</div>

