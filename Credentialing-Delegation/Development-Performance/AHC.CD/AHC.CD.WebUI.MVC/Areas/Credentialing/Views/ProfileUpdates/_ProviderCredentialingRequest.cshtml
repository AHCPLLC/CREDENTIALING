<div style="margin:10px" ng-show="!progressBarForCredentialingRequests">
    <table ng-hide="AllCredentilaingRequestData.length == 0" class='table table-striped table-bordered small wrap-words'>
        <thead>
            <tr>
                <th class='col-lg-2 col-md-3 col-sm-3 col-xs-3'>
                    First Name
                </th>
                <th class='col-lg-2 col-md-2 col-sm-3 col-xs-3'>
                    Last Name
                </th>
                <th class='col-lg-2 col-md-2 col-sm-2 col-xs-2'>
                    Plan Name
                </th>
                <th class='col-lg-2 col-md-2 col-sm-2 col-xs-2'>
                    Initiated Date
                </th>
                <th class='col-lg-2 col-md-1 col-sm-1 col-xs-1'>
                    Status
                </th>
                <th class='col-lg-2 col-md-2 col-sm-1 col-xs-1'>Action</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat='license in AllCredentilaingRequestData'>
                <td>{{license.FirstName}}</td>
                <td>{{license.LastName}}</td>
                <td>{{license.PlanName}}</td>
                <td ng-bind="ConvertDateTo(license.LastModifiedDate)"></td>
                <td>{{license.Status}}</td>
                <td>
                    <a ng-click="pushCredentialingRequestdata(license);" data-toggle="tooltip" data-placement="right" title="View Details" class="pointer"><i class="fa fa-eye"></i></a>
                </td>
                @*<td><a class='btn btn-xs btn-default' ng-click="InitiateCredentialing(license);" tooltip data-toggle='tooltip' data-placement='right' title='Initiate'><i class='fa fa-repeat'></i></a></td>*@
            </tr>
        </tbody>
    </table>
    <table ng-show="AllCredentilaingRequestData.length == 0">
        <tr>
            <td><label class="text-center">No Data Available !</label></td>
        </tr>
    </table>
</div>
<div ng-show="activeMessageDiv=='successfulInitiated'" class="alert alert-success" role="alert">
    <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
    <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
</div>
<div ng-show="activeMessageDiv=='errorInitiated1'" class="alert alert-danger" role="alert">
    <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
    <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
</div>