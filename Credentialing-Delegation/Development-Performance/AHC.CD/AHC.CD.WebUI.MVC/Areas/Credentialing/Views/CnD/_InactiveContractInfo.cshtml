<style>
    .select-page {
        width: 50px;
        text-align: center;
    }

    .loadingHistory {
        padding-left: 7px;
    }

    .pagination li a input {
        padding: 0;
        margin: -5px 0;
    }

    .table {
        width: 100%;
        max-width: 100%;
        margin-bottom: 0px;
    }

    .ProviderTypeSelectAutoList.popover.bottom {
        margin-top: 32px;
    }

    .ProviderTypeSelectAutoList1.popover.bottom {
        margin-top: 32px;
    }

    .my-animation.ng-enter {
        -webkit-animation: flipInY 1s;
        -moz-animation: flipInY 1s;
        animation: flipInY 1s;
    }

    .my-animation.ng-leave {
        -webkit-animation: flipOutY 1s;
        -moz-animation: flipOutY 1s;
        animation: flipOutY 1s;
    }

    .contractgrid {
        cursor: pointer;
    }
</style>
<div ng-controller="InactiveContractinfoController as ic">
    <div class="panel-default form-style">
        <fieldset>
            <div class=" zero-padding-left-right-div" >
                <div ng-hide="isView || AjaxLoading">
                    <div class="pull-right col-lg-4 form-group">
                <input class="form-control input-sm" name="ProvidersName" autocomplete="off" placeholder="Search for provider" ng-model="ProvidersName" ng-focus="searchCumDropDown('ProviderLists')" />
                <div class="ProviderTypeSelectAutoList popover fade bottom in" role="tooltip" id="ProviderLists">
                    <table class="table table-striped table-bordered table-hover table-condensed" style="margin-bottom: 0px;">
                        <tbody>
                            <tr ng-repeat="provider in filteredProvidersResults = (ProviderData | filter:ProvidersName)" ng-click="SelectProvider(provider)" class="pointer">
                                <td>{{provider.Name}}</td>
                            </tr>
                            <tr ng-if="ProviderData.length == 0">
                                <td>No data..... </td>
                            </tr>
                            <tr ng-if="filteredProvidersResults.length == 0">
                                <td>No data..... </td>
                            </tr>
                            @*<tr ng-if="ProviderData.length > 0 && filteredProvidersResults.length == 0">
                                <td>Provider not found..... </td>
                            </tr>*@
                        </tbody>
                    </table>
                </div>
            </div>
                    <div class="clearfix"></div>
                    <div class="col-lg-12 form-group">
                        <table class="table table-bordered table-condensed customtbodyStyle table-striped" st-pipe="ic.callServer" st-persist="ic.displayed" st-table="ic.displayed" st-safe-src="randomsItems" id="Contract_ContractInformation">
                            <thead>
                                <tr>
                                    @*<th class="col-lg-1">Name</th>*@
                                    <th st-ratio="9" st-sort="NPINumber" class="contractgrid">NPI Number</th>
                                    <th st-ratio="9" st-sort="ProviderFirstName" class="contractgrid">Provider</th>
                                    <th st-ratio="10" st-sort-default="reverse" st-sort="PlanName" class="contractgrid">Plan</th>
                                    <th st-ratio="6" st-sort="LOBCode" class="contractgrid">LOB</th>
                                    <th st-ratio="10" st-sort="ParticipatingStatus" class="contractgrid">Status</th>
                                    <th st-ratio="13" st-sort="GroupID" class="contractgrid">Group ID</th>
                                    <th st-ratio="12" st-sort="ProviderID" class="contractgrid">Individual ID</th>
                                    <th st-ratio="11" st-sort="InitiatedDate" class="contractgrid">Effective Date</th>
                                    <th st-ratio="11" st-sort="TerminationDate" class="contractgrid">Term Date</th>
                                    <th st-ratio="13" class="hidecontent" st-skip-natural="true">Action</th>
                                </tr>
                                <tr>
                                    @*<td class="hideData"><input st-search="id" type="text" placeholder="Filter Name" class="form-control input-sm"/></td>*@
                                    <td st-ratio="9" class="hideData"><input st-search="NPINumber" type="text" placeholder="Filter NPI Number" class="form-control input-sm" /></td>
                                    <td st-ratio="9" class="hideData"><input st-search="ProviderFullName" type="text" placeholder="Filter Provider" id="Provider" class="form-control input-sm" /></td>
                                    <td st-ratio="10" class="hideData"><input st-search="PlanName" type="text" placeholder="Filter Plan" class="form-control input-sm" /></td>
                                    <td st-ratio="6" class="hideData"><input st-search="LOBCode" type="text" placeholder="Filter LOB" class="form-control input-sm" /></td>
                                    <td st-ratio="10" class="hideData"><input st-search="ParticipatingStatus" type="text" placeholder="Filter Status" class="form-control input-sm" /></td>
                                    <td st-ratio="13" class="hideData"><input st-search="GroupID" type="text" placeholder="Filter Group ID" class="form-control input-sm" /></td>
                                    <td st-ratio="12" class="hideData"><input st-search="ProviderID" type="text" placeholder="Filter Individual ID" class="form-control input-sm" /></td>
                                    <td st-ratio="11" class="hideData"><input st-search="InitiatedDate" type="text" placeholder="Filter Effective Date" class="form-control input-sm" /></td>
                                    <td st-ratio="11" class="hideData"><input st-search="TerminationDate" type="text" placeholder="Filter Term Date" class="form-control input-sm" /></td>
                                    <td st-ratio="13" class="hidecontent" class="hideData"></td>
                                </tr>
                            </thead>

                            <tbody ng-show="!ic.isLoading">

                                <tr ng-repeat="c in ic.displayed">
                                    @*<td class="col-lg-1">{{c.ProviderFirstName}}</td>*@
                                    <td st-ratio="9" class="wrap-words">{{c.NPINumber}}</td>
                                    <td st-ratio="9" class="wrap-words">{{c.ProviderFullName}}</td>
                                    <td st-ratio="10" class="wrap-words">{{c.PlanName}}</td>
                                    <td st-ratio="6" class="wrap-words"><span class="label label-primary">{{c.LOBCode || 'Not Available'}}</span></td>
                                    <td st-ratio="10" class="wrap-words">{{c.ParticipatingStatus || 'Not Available'}}</td>
                                    <td st-ratio="13" class="wrap-words">{{c.GroupID || 'Not Available'}}</td>
                                    <td st-ratio="12" class="wrap-words">{{c.ProviderID || 'Not Available'}}</td>
                                    <td st-ratio="11" class="wrap-words">{{c.InitiatedDate  || 'Not Available'}}</td>
                                    <td st-ratio="11" class="wrap-words">{{c.TerminationDate || 'Not Available'}}</td>
                                    <td st-ratio="13" class="hidecontent wrap-words">
                                        <button class="btn btn-xs btn-default" ng-click="viewContractGrid(c.ContractGridID,c.PlanName)" tooltip data-toggle="tooltip" data-placement="left" title="View"><i class="fa fa-eye"></i></button>
                                        @*<button class="btn btn-xs btn-primary" ng-click="EditContractGrid(c.ContractGridID)" tooltip data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>*@
                                        <button class="btn btn-xs btn-primary" ng-click="reactivateContractGrid(c.ContractGridID)" tooltip data-toggle="tooltip" data-placement="top" title="Reactivate"><i class="fa fa-repeat"></i></button>
                                    </td>
                                </tr>

                            </tbody>
                            <tbody ng-show="ic.isLoading">
                                <tr>
                                    <td colspan="9" class="text-center">
                                        <div class="loading-indicator"></div>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td style="cursor:pointer" class="text-center" st-pagination="" st-items-by-page="10" colspan="10"></td>
                                </tr>
                            </tfoot>
                        </table>

                        @*<div ng-show="!mc.isLoading " ng-if="mc.displayed==0" class="text-center" ng-cloak>
                    <label>No Data Available.</label>
                </div>*@
                        <table style="display:none" ng-if="AllData" class="table table-bordered table-condensed customtbodyStyle table-striped" id="InactiveContractInformation1">
                            <caption ng-if="NameToPrint!=''">{{NameToPrint}}</caption>
                            <thead>
                                <tr>
                                    <th>NPI Number</th>
                                    <th>Provider</th>
                                    <th>Plan</th>
                                    <th>LOB</th>
                                    <th>Status</th>
                                    <th>Group ID</th>
                                    <th>Individual ID</th>
                                    <th>Effective Date</th>
                                    <th>Term Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="c in filtered">
                                    <td>{{c.NPINumber}}</td>
                                    <td><p>{{c.ProviderFullName}}</p></td>
                                    <td>{{c.PlanName}}</td>
                                    <td>{{c.LOBCode || 'Not Available'}}</td>
                                    <td>{{c.ParticipatingStatus || 'Not Available'}}</td>
                                    <td>{{c.GroupID || 'Not Available'}}</td>
                                    <td>{{c.ProviderID || 'Not Available'}}</td>
                                    <td>{{c.InitiatedDate || 'Not Available'}}</td>
                                    <td>{{c.TerminationDate || 'Not Available'}}</td>
                                </tr>
                            </tbody>

                        </table>


                    </div>

                    <div class="clearfix"></div>
                    <div class="col-lg-12">
                        <div ng-show="!ic.displayed.length==0 && !ic.isLoading">
                            <span class="label label-primary">C- COMMERCIAL</span>&nbsp;
                            <span class="label label-primary">CE- COMMERCIAL EXCHANGE</span>&nbsp;
                            <span class="label label-primary">FFS- FEE FOR SERVICE</span>&nbsp;
                            <span class="label label-primary">MC- MEDICARE</span>&nbsp;
                            <span class="label label-primary">MD- MEDICAID</span>&nbsp;
                            <span class="label label-primary">MR- MEDICARE RISK</span>&nbsp;
                            <span class="label label-primary">WC- WORKERS COMPENSATION</span>&nbsp;
                            <span class="label label-primary">IME- IME</span>&nbsp;
                            <span class="label label-primary">US DOL- US DOL</span>
                        </div>
                    </div>

                </div>
            </div>
            <div class=" zero-padding-left-right-div" ng-show="isView && !AjaxLoading">
                    @Html.Partial("~/Areas/Credentialing/Views/CnD/_ViewContractGrid.cshtml")
                </div>
            @*<div class=" zero-padding-left-right-div" ng-show="isEdit && !AjaxLoading">
                    @Html.Partial("~/Areas/Credentialing/Views/CnD/_EditContractGrid.cshtml")
                </div>*@
            @*<div ng-show="AjaxLoading" class="row">
                    <div style="margin-left:45%;margin-top:15%;height:630px">
                        <img src="~/Areas/Credentialing/img/loading.GIF" />&nbsp;
                        <span>Loading...Please Wait</span>
                    </div>
                </div>*@
        </fieldset>

        <div class="modal fade" id="WarningModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <form id="removeStateLicense">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Warning!!!!</h4>
                        </div>
                        <div class="modal-body">
                            Do you really want to reactivate this record ?

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                            <button type="button" class="btn btn-primary" ng-click="ReactivateInactiveReport()">Reactivate</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="hiddenPrintDiv1" style="display:none;">


</div>