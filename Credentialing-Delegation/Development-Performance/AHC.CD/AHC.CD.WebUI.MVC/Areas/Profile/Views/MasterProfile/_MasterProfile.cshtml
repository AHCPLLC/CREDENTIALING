@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;

<div id="loading" align="center" style="display:none">
</div>

<div class="row" ng-cloak style="top: 50px; padding-top: 25px; position: fixed; background: #fff; z-index: 1; width: 95.5%;">
    <div class="panel panel-default" style="border-radius: 0px; margin-bottom:0px;">
        <div class="panel-body form-style">
            <div class="legend2 pull-right" style="margin-top: -35px;">
                @if (System.Configuration.ConfigurationManager.AppSettings["TurnOffTab"] == "true")
                {
                    <button ng-click="tabDeactivate=true;loadAllData();" ng-show="!tabDeactivate" class="btn btn-xs btn-info" tooltip data-toggle="tooltip" data-placement="bottom" title="Turn Off Tabs"><i class="fa fa-list"></i></button>
                    <button ng-click="tabDeactivate=false" ng-show="tabDeactivate" class="btn btn-xs btn-info" tooltip data-toggle="tooltip" data-placement="top" title="Turn On Tabs"><i class="fa fa-tags"></i></button>
                }
                @if (System.Configuration.ConfigurationManager.AppSettings["PrintProfileAsPDF"] == "true")
                {
                    <button class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="bottom" title="Download as PDF" ng-click="PrintPDF();" ng-disabled="generatePDF"><i class="fa fa-file-pdf-o" ng-hide="generatePDF"></i><i ng-show="generatePDF">Generating PDF.. Please Wait.. <img src="~/Content/Images/ajax-loader.gif" style="width:15px" /></i></button>
                }
                <a class="btn btn-xs btn-primary" tooltip data-toggle="tooltip" data-placement="bottom" title="Search Provider" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Profile/SearchProvider/SearchProvider">
                    <i class="fa fa-search"></i>
                </a>
            </div>
            <div class="row">
                <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                    <img style="width: 44px;" ng-src="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Document/View?path=@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]{{MasterProfile.Demographics.ProfilePhotoPath || '/Content/Images/a0.jpg'}}">
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <label class="small">Name</label>
                    <div>
                        <i class="data-label">{{MasterProfile.Demographics.PersonalDetail.ProviderLevel.Name}} {{MasterProfile.Demographics.PersonalDetail.FirstName+' '+ MasterProfile.Demographics.PersonalDetail.MiddleName + ' '+ MasterProfile.Demographics.PersonalDetail.LastName}}</i>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label class="small">Gender</label>
                    <div>
                        <i class="data-label">{{MasterProfile.Demographics.PersonalDetail.Gender}}</i>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label class="small">Primary Phone #</label>
                    <div>
                        <i class="data-label">primary practice phone</i>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label class="small">NPI</label>
                    <div>
                        @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                        {
                            <i class="data-label">######</i>
                        }
                        else
                        {
                            <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPINumber || 'Not Available'}}</i>
                        }
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                    <label class="small">CAQH</label>
                    <div>
                        <div>
                            @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                            {
                                <i class="data-label">######</i>
                            }
                            else
                            {
                                <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.CAQHNumber || 'Not Available'}}</i>
                            }
                        </div>
                    </div>
                </div>
            </div>

            <hr style="margin-top: 5px; margin-bottom: 5px; margin-left: -15px; margin-right: -15px;">

            <div ng-init="tabDeactivate=false;" ng-show="!tabDeactivate">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist" style="border-bottom: 0px;">
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.Demographics">Demographics</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.IdentificationLicense">Identification & Licenses</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.EducationHistory">Education History</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.Specialty">Specialty/Board</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.PracticeLocation" ng-click="getPractitionerData();">Practice Location</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.HospitalPrivilege">Hospital Privilege</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ProfessionalLiability">Professional Liability</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.WorkHistory">Work History</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ProfessionalReference">Professional Reference</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ProfessionalAffiliation">Professional Affiliation</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.DisclosureQuestion">Disclosure Questions</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ContractInformation">Contract Information</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.DocumentRepository">Document Repository</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ProfileDashboard">Profile Dashboard</a></li>
                    <li role="presentation" ui-sref-active="active" ng-if="ccoprofile"><a ui-sref="Profile.CustomField">Custom Field</a></li>
                    <li role="presentation" ui-sref-active="active" ng-disabled='disabled' ng-if="ccoprofile"><a ui-sref="Profile.DocumentationCheckList">Document CheckList</a></li>
                    <li role="presentation" ui-sref-active="active"><a ui-sref="Profile.ContractGrid">Contract Grid</a></li>
                    <li role="presentation" ui-sref-active="active" ng-if="ccoprofile"><a ui-sref="Profile.Tasks">Tasks</a></li>
                </ul>
            </div>
        </div>
    </div>
</div>

<div class="row" ng-cloak style="margin-top: 195px;">
    <div ng-show="activeMessageDiv=='showErrorPDF'" class="alert alert-danger" role="alert">
        <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
        <i class="fa fa-2x fa-arrow-up"></i> {{messageDesc}}
    </div>
</div>

<div class="row" ng-cloak>
    <div class="panel panel-default" style="border-radius: 0px; border-top: 0px;">
        <div class="panel-body" style="padding-top: 0px;">
            <div class="tab-pane active" style="margin-top:-12px;">
                <div class="col-lg-12 text-center" ng-show="LoadingData" style="height:200px;">
                    <div style="margin-top:10%;">
                        <img src="~/Content/Images/ajax-loader.gif" style="width:50px;" /> {{LoadingDataTitle}}
                    </div>
                </div>
                <div ng-hide="LoadingData" ui-view class="row"></div>
            </div>
        </div>
    </div>
</div>

