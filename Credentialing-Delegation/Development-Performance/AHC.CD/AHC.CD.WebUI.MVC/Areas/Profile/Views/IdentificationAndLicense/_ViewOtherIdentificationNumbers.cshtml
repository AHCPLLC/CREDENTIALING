                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.UPINNumber">Not Available</i>
            </div>
        </div>
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.USMLENumber, new { @class = " control-label small" })
            <div class="wrap-words">
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.USMLENumber">######</i>

                }
                else
                {
                    <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.USMLENumber}}</i>
                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.USMLENumber">Not Available</i>
            </div>
        </div>
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.LastCAQHAttestationDate, new { @class = " control-label small" })
            <div class="wrap-words">
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.LastCAQHAttestationDate">######</i>
                }
                else
                {                    
                    <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.LastCAQHAttestationDate|| 'Not Available'  | date: "MM-dd-yyyy"}}</i>
                }
                
            </div>
        </div>
    </div>
    <div class="form-group col-md-12 zero-padding-left-right-div">
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.NextAttestationDate, new { @class = " control-label small" })
            <div class="wrap-words">
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NextAttestationDate">Not Available</i>

                }
                else
                {                    
                    <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.NextAttestationDate | date: "MM-dd-yyyy"}}</i>
                }

                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.NextAttestationDate">Not Available</i>
            </div>
        </div>
      </div>
        <div class="clearfix"></div>
        <div ng-show="activeMessageDiv=='updatedID'" class="alert alert-success" role="alert">
            <button ng-click="closeAlertMessage('')" class="close"><span>&times;</span></button>
            {{messageDesc}}
        </div>
        <div class="clearfix"></div>
    </div>









