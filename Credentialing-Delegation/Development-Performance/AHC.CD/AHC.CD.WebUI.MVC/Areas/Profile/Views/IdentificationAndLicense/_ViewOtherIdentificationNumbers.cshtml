                    <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.CAQHNumber}}</i>
                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.CAQHNumber">Not Available</i>
            </div>
        </div>
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.CAQHUserName, new { @class = "  control-label small", type = "password" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
