@model AHC.CD.WebUI.MVC.Areas.Profile.Models.IdentificationAndLicenses.OtherIdentificationNumberViewModel
@using AHC.CD.WebUI.MVC.Areas.Profile.Attributes;


    @if (ProfileActionValidator.IsAuthorized(ProfileActionType.Edit, ViewBag.Roles, true))
    {
        <div class="legend2 pull-right" ng-show="(visibilityControl!='editOtherIdentificationNumbers') && (MasterProfile.IdentificationLicense.OtherIdentificationNumber != null)">
            <button id="editnoDataAddNewOtherIdentificationDetails" class="btn btn-xs btn-primary" ng-click="operateEditControl('editOtherIdentificationNumbers',MasterProfile.IdentificationLicense.OtherIdentificationNumber)" tooltip data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-edit"></i></button>
        </div>
    }

        

<div class="wrap-words">
    <div class="form-group col-md-12 zero-padding-left-right-div">
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.NPINumber, new { @class = "  control-label small" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPINumber">######</i>
                }
                else
                {
                    <i class="data-label">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPINumber}}</i>
                }

            </div>
        </div>
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.NPIUserName, new { @class = "  control-label small", type = "password" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIUserName">######</i>

                }
                else
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIUserName">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIUserName}}</i>
                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIUserName">Not Available</i>
            </div>
        </div>
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.NPIPassword, new { @class = "  control-label small", type = "password" })
            <div>
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIPassword">######</i>

                }
                else
                {
                    <i class="data-label" ng-show="MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIPassword">{{MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIPassword}}</i>
                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.NPIPassword">Not Available</i>
            </div>
        </div>
    </div>
    <div class="form-group col-md-12 zero-padding-left-right-div">
