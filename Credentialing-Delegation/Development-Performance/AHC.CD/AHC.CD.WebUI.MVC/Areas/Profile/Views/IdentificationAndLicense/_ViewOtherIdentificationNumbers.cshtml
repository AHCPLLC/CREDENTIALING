                }
                <i ng-if="!MasterProfile.IdentificationLicense.OtherIdentificationNumber.CAQHPassword">Not Available</i>
            </div>
        </div>
    </div>

    <div class="form-group col-md-12 zero-padding-left-right-div">
        <div class="form-group col-md-4 zero-padding-left-right-div">
            @Html.LabelFor(model => model.UPINNumber, new { @class = " control-label small" })
            <div class="wrap-words">
                @if (ProfileActionValidator.IsMasked(ViewBag.Roles))
