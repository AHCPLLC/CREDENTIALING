<div ng-show="loading" class="progress col-lg-12" ng-cloak>
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        Loading...Please wait...
    </div>
</div>
<div ng-if="errorInitiatedForNoPSV">
    <div class="col-lg-12">
        <label class="control-label small">Last PSV performed on {{psvDate | date:'MM-dd-yyyy'}}.</label>
    </div>
    &nbsp;&nbsp;&nbsp;
    <div class="col-md-12">
        <button class="btn btn-primary btn-xs center-block" type="submit" ng-click="CallView4();">View Details</button>
    </div>
</div>

<div ng-if="!errorInitiatedForNoPSV && !loading">
    <div class="col-lg-12">
        <label class="control-label small">No PSV information available.</label>
    </div>
    &nbsp;&nbsp;&nbsp;
    <div class="col-lg-12">
        <a class="pull-right" href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Credentialing/Verification/InitiateProfilePSV/{{profileID}}"><button class="btn btn-primary btn-xs"> Perform PSV</button></a>
    </div>
</div>