<div class="col-lg-12" ng-cloak>
    <ul class="nav nav-tabs" role="tablist">
        <li ng-class="{active: tabStatus == 0,disabled:addTabStatus < 0 || addTabStatus==2}" class="pointer" ng-click="addTabStatus < 0 || addTabStatus==2?'':tabAddSelect(0)"><a>General Information<i ng-show="addTabStatus > 0" class="fa fa-check-circle text-success"></i></a></li>
        <li ng-class="{active: tabStatus == 1,disabled:addTabStatus < 1 || addTabStatus==2}" class="pointer" ng-click="addTabStatus < 1 || addTabStatus==2?'':tabAddSelect(1)"><a>LOB Information<i ng-show="addTabStatus > 1" class="fa fa-check-circle text-success"></i></a></li>
        <li ng-class="{active: tabStatus == 2,disabled:addTabStatus < 2}" class="pointer" ng-click="addTabStatus < 2?'':tabAddSelect(2)"><a>Plan Contract</a></li>
    </ul>
    <br />
    <div class="tab-content">
        <div class="tab-pane fade active" ng-class="{'active in': tabStatus == 0}">@Html.Partial("_AddPlanForm")</div>
        <div class="tab-pane fade" ng-class="{'active in': tabStatus == 1}">@Html.Partial("_Add_LOB_BE_Mapping")</div>
        <div class="tab-pane fade" ng-class="{'active in': tabStatus == 2}">@Html.Partial("_AddPlanContract")</div>
    </div>
</div>
