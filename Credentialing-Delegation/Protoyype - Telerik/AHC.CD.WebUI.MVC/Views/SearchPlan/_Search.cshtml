
<div class="panel-group" id="accordionSp" role="tablist" aria-multiselectable="true">

    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="Sp_heading">

            <a data-toggle="collapse" data-parent="#accordionSp" href="#SP" aria-expanded="true" aria-controls="SP">
               Search Plan
                <span class="pull-right">
                    <span class="glyphicon glyphicon-minus"></span>
                </span>
            </a>

        </div>
        <div id="SP" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="Sp_heading">
            <div class="panel-body table-bordered" ng-controller="SearchPlanController">
                <div class="col-md-12">
                    <div class="form-group col-md-4">
                        <label class="control-label small">Plan Category</label>
                        <div>
                            <select id="selectNameTitle" name="planCategory" class="form-control input-sm">
                                <option>Type1</option>
                                <option>Type2</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="control-label small">Plan Location</label>
                        <div>
                            <select id="selectNameTitle" name="locations" class="form-control input-sm" required>
                                <option>Florida</option>
                                <option>New York</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label small">Group</label>
                        <div>
                            <select id="selectNameTitle" name="groupName" class="form-control input-sm" required>
                                <option>Group1</option>
                                <option>Group2</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12">

                    <table ng-table="tableParams1" class="table table-hover ng-table-resizable-columns ng-table-rowselected">
                       <thead>
                           
                        <th>Insurance Company</th>
                        <th>Plan Name</th>
                        <th>Plan Category</th>
                        <th>Providers</th>
                        <th>Locations</th>
                        <th>Group</th>
                        </thead> 
                        <tr ng-repeat="user in searchPlanData">
                            <td class="text-center">{{user.companyName}}</td>
                            <td class="text-center">{{user.planName}}</td>
                            <td class="text-center" filter="{ 'planCategory': 'text' }">{{user.planCategory}}</td>
                            <td class="text-center">{{user.providers}}</td>
                            <td class="text-center" filter="{ 'locations': 'text' }">{{user.locations}}</td>
                            <td class="text-center" filter="{ 'groupName': 'text' }">{{user.groupName}}</td>
                        </tr>
                    </table>
                </div>
            </div>
            </div>
        </div>
</div>