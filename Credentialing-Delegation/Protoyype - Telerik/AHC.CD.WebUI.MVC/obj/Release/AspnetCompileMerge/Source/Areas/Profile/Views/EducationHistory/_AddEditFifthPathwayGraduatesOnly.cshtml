


    <div id="fifthPathForm" style="display:none;">
        <div class="col-lg-12 zero-padding-left-right-div">
            <div class="col-lg-4 form-group">
                <label class="control-label small">Institution/Hospital</label>
                <input type="text" class="form-control input-sm" />
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">Addresss</label>
                <input type="text" class="form-control input-sm" />
            </div>

            <div class="form-group col-md-4">
                <label class="control-label small">Country *</label>
                <div>
                    <select id="selectNameTitle" name="countryName" class="form-control input-sm" required>
                        <option selected disabled hidden>Select Country</option>
                        <option>USA</option>
                        <option>India</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">State *</label>
                <div>
                    <select id="selectNameTitle" name="stateName" class="form-control input-sm" required>
                        <option selected disabled hidden>Select State</option>
                        <option>Florida</option>
                        <option>New York</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">County</label>
                <div>
                    <input type="text" class="form-control input-sm" />
                </div>
            </div>



            <div class="form-group col-md-4">
                <label class="control-label small">City *</label>
                <div>
                    <select class="form-control input-sm" required>
                        <option></option>
                        <option>Spring Hill</option>
                        <option>Largo</option>
                        <option>Lakeland</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label class="control-label small">Zip code *</label>
                <div>
                    <input type="text" class="form-control input-sm" />
                </div>
            </div>


            <div class="col-lg-4 form-group">
                <label class="control-label small">Fax</label>
                <div class="input-group">
                    <div class="input-group-btn">
                        <button type="button" class="btn btn-sm"><span class="caret"></span></button>
                    </div>
                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                        <div class="arrow"></div>
                        <h3 class="popover-title">Select Country Dial Code</h3>
                        <div class="popover-content">
                            <select id="ContactContryCode">
                                <option>+91</option>
                            </select>
                        </div>
                    </div>
                    <input type="text" name="Fax" class="form-control input-sm" id="Fax" placeholder="Fax Number" required>
                </div>
            </div>
            <div class="col-lg-4 form-group">
                <label for="MobileNo" class="control-label small">Telephone Number  *</label>
                <div class="input-group">
                    <div class="input-group-btn">
                        <button type="button" class="btn btn-sm"><span class="caret"></span></button>
                    </div>
                    <div class="popover fade bottom in" role="tooltip" id="countryDailCodeContainer">
                        <div class="arrow"></div>
                        <h3 class="popover-title">Select Country Dial Code</h3>
                        <div class="popover-content">
                            <select id="ContactContryCode">
                                <option>+91</option>
                            </select>
                        </div>
                    </div>
                    <input type="text" name="PhoneNo" class="form-control input-sm" id="PhoneNo" placeholder="Phone Number" required>
                </div>
            </div>

            <div class="col-lg-4 form-group">
                <label class="control-label small">Start Date</label>
                <input class="form-control input-sm" type="date" />
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">End Date</label>
                <input class="form-control input-sm" type="date" />
            </div>
            <div class="col-lg-4 form-group">
                <label class="control-label small">Did you complete your education at this school?</label><br />
                @*&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;*@
                <label class="radio-inline">
                    <input type="radio" name="iscompleteEducation" value="Y" checked="checked" required="" autocomplete="off"> Yes
                </label>
                <label class="radio-inline">
                    <input type="radio" name="iscompleteEducation" value="N" required="" autocomplete="off"> No
                </label>
            </div>
            <div class="col-md-12 form-group">
                <button ng-show="fifthPathFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelFifthPath('editCancel')">cancel</button>
                <button ng-show="!fifthPathFormStatus" type="button" class="btn btn-warning btn-sm" ng-click="cancelFifthPath('saveCancel')">Cancel</button>
                <button ng-show="!fifthPathFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="saveFifthPath(FifthPath)">Add</button>
                <button ng-show="fifthPathFormStatus" type="button" class="btn btn-primary btn-sm pull-right" ng-click="updateFifthPath(FifthPath)">Update</button>
            </div>
        </div>
    </div>

    <div class="form-style">
        <fieldset ng-show="newFifthPathForm && !fifthPathFormStatus">
            <legend>
                Add Fifth Path
            </legend>
            <div id="newFifthPathFormDiv">

            </div>
        </fieldset>
    </div>

   

    

