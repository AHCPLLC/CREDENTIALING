@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Lib/Jquery/jquery-1.11.2.min.js"></script>
<script src="~/Scripts/Lib/Angular/angular.min.js"></script>
<script src="~/Scripts/Lib/dirPagination.js"></script>
<script src="~/Scripts/Lib/Angular-DatePicker/ui-bootstrap-tpls-0.12.0.min.js"></script>
<script src="~/Areas/Credentialing/Scripts/LicenseNumber.js"></script>

<div id="page-wrapper" ng-app="LicenseNumberApp" ng-controller="LicenseNumberCtrl" ng-cloak>
    <div class="container-fluid">
        <div class="panel panel-default">
            <div class="panel-heading">
                Report
                </div>
            <div class="panel-body">

                <table class="table table-bordered table-responsive table-condensed table-striped small table-hover">
                    <thead>
                        <tr>
                            <th>NPI Number</th>
                            <th>Provider Name</th>
                            <th>State License Number</th>
                            <th>DEA Number</th>
                            <th>Medicare Number</th>
                            <th>Medicaid Number</th>
                            <th>CDS Number</th>
                            <th>CAQH Number</th>
                            <th>UPIN Number</th>
                            <th>USMLE Number</th>
                        </tr>
                        <tr>
                            <td><input type="text" placeholder="NPI No." class="form-control input-sm" ng-model="search.NPINumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="Name" class="form-control input-sm" ng-model="search.Name" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="State License No." class="form-control input-sm" ng-model="search.StateLicenseNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="DEA No." class="form-control input-sm" ng-model="search.DEANumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="Medicare No." class="form-control input-sm" ng-model="search.MedicareNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="Medicaid No." class="form-control input-sm" ng-model="search.MedicaidNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="CDS No." class="form-control input-sm" ng-model="search.CDSNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="CAQH No." class="form-control input-sm" ng-model="search.CAQHNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="UPIN No." class="form-control input-sm" ng-model="search.UPINNumber" ng-change="filterData()" /></td>
                            <td><input type="text" placeholder="USMLE No." class="form-control input-sm" ng-model="search.USMLENumber" ng-change="filterData()" /></td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr dir-paginate="data in TableData | filter:{NPINumber:search.NPINumber, Name:search.Name, StateLicenseNumber:search.StateLicenseNumber,DEANumber:search.DEANumber,MedicareNumber:search.MedicareNumber,MedicaidNumber:search.MedicaidNumber,CDSNumber:search.CDSNumber,CAQHNumber:search.CAQHNumber,UPINNumber:search.UPINNumber,USMLENumber:search.USMLENumber} | itemsPerPage:10">
                            <td class="wrap-words">{{data.NPINumber}}</td>
                            <td class="wrap-words">{{data.Name}}</td>
                            <td class="wrap-words">{{data.StateLicenseNumber}}</td>
                            <td class="wrap-words">{{data.DEANumber}}</td>
                            <td class="wrap-words">{{data.MedicareNumber}}</td>
                            <td class="wrap-words">{{data.MedicaidNumber}}</td>
                            <td class="wrap-words">{{data.CDSNumber}}</td>
                            <td class="wrap-words">{{data.CAQHNumber}}</td>
                            <td class="wrap-words">{{data.UPINNumber}}</td>
                            <td class="wrap-words">{{data.USMLENumber}}</td>
                        </tr>
                    </tbody>
                </table>
                <dir-pagination-controls max-size="5"
                                         direction-links="true"
                                         boundary-links="true">
                </dir-pagination-controls>
                @*<pagination ng-if="TableData.length > 0" total-items="bigTotalItems" ng-model="bigCurrentPage" max-size="maxSize" class="pagination-sm" ng-change="pageChanged(bigCurrentPage)" boundary-links="true" rotate="false" num-pages="numPages" style="margin:0;"></pagination>*@
                </div>
                </div>
        </div>
    </div>
