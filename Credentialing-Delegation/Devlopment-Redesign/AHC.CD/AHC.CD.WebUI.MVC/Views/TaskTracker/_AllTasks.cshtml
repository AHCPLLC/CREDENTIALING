<div class="row" >
    <div class="col-lg-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right-div form-style" style="border-top: 1px solid #eee; padding-top: 10px;" ng-if="TableView">
            <div class="progress col-lg-12" ng-if="!progressbar" ng-cloak>
                <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                    Loading...Please wait...
                </div>
            </div>
            <div ng-if="progressbar">
                <div ng-show="RemainingTasks.length==0 && progressbar">
                    <label>No Tasks Assigned.</label>
                </div>
                <table ng-show="RemainingTasks.length>0 && progressbar" class='table table-striped table-bordered customtbodyStyle medium wrap-words'>
                    <thead>
                        <tr>
                            <th class='col-lg-2'>
                                Provider Name
                            </th>
                            <th class='col-lg-1 col-md-2 col-sm-2 col-xs-2'>
                                Subject
                            </th>
                            <th class='col-lg-2 col-md-2 col-sm-2 col-xs-2'>
                                Sub Section Name
                            </th>
                            <th class='col-lg-2 col-md-2 col-sm-2 col-xs-2'>
                                Next FollowUp Date
                            </th>
                            <th class='col-lg-2 col-md-2 col-sm-2 col-xs-2'>
                                Assigned To
                            </th>
                            <th class='col-lg-1 col-md-1 col-sm-1 col-xs-1'>
                                Task Status
                            </th>
                            <th class='col-lg-1 col-md-2 col-sm-2 col-xs-2'>Action</th>
                        </tr>
                        <tr>
                            <td class='col-lg-2 col-md-2 col-sm-2 col-xs-2'><input type="text" placeholder="Filter Provider Name" class="form-control input-sm" ng-model="search.ProviderName"></td>
                            <td class='col-lg-2 col-md-2 col-sm-2 col-xs-2'><input type="text" placeholder="Filter Subject" class="form-control input-sm" ng-model="search.Subject" /></td>
                            <td class='col-lg-2 col-md-2 col-sm-2 col-xs-2'><input type="text" placeholder="Filter Sub Section Name" class="form-control input-sm" ng-model="search.SubSectionName" /></td>
                            <td class='col-lg-1 col-md-1 col-sm-1 col-xs-1'><input type="text" placeholder="Filter Next FollowUp Date" class="form-control input-sm" ng-model="search.NextFollowUpDate" /></td>
                            <td class='col-lg-2 col-md-2 col-sm-2 col-xs-2'><input type="text" placeholder="Filter Assigned To" class="form-control input-sm" ng-model="search.AssignedTo" /></td>
                            <td class='col-lg-1 col-md-1 col-sm-1 col-xs-1'><input type="text" placeholder="Filter Task Status" class="form-control input-sm" ng-model="search.CompleteStatus" /></td>
                            <td class='col-lg-2 col-md-2 col-sm-2 col-xs-2'></td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="c in RemainingTasksPagination | filter:{ProviderName:search.ProviderName,Subject:search.Subject,SubSectionName:search.SubSectionName,NextFollowUpDate:search.NextFollowUpDate,AssignedTo:search.AssignedTo,CompleteStatus:search.CompleteStatus}   track by $index">
                            <td>{{c.ProviderName}}</td>
                            <td>{{c.Subject}}</td>
                            <td>{{c.SubSectionName}}</td>
                            <td>{{c.NextFollowUpDate}}</td>
                            <td>{{c.AssignedTo}}</td>
                            <td>{{c.CompleteStatus}}</td>
                            <td>
                                <button class="btn btn-xs btn-default" ng-click="showDetailViewforTab3(c)" tooltip data-toggle="tooltip" data-placement="top" title="View"><i class="fa fa-eye"></i></button>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="clearfix"></div>
            <div ng-if="CurrentPageforRemainingTasks.length > 0" ccolicensedetailstable></div>
            <pagination ng-if="CurrentPageforRemainingTasks.length > 0" total-items="bigTotalItemsforRemainingTasks" ng-model="bigCurrentPageforRemainingTasks" max-size="maxSize" class="pagination-sm" ng-change="pageChangedforRemainingTasks(bigCurrentPageforRemainingTasks)" boundary-links="true" rotate="false" num-pages="numPages" style="margin:0;"></pagination>
            <div class="clearfix"></div><br />
        </div>
        <div ng-if="detailViewfortab3" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zero-padding-left-right-div form-style">
            <fieldset>
                <legend>
                    View Task
                </legend>
                <div class="legend2 pull-right">
                    <button ng-click="cancelViewforTab3()" class="btn btn-xs btn-default" tooltip data-toggle="tooltip" data-placement="top" title="close"><i class="fa fa-close"></i></button>
                </div>
                <div id="detailFormDiv">
                    @Html.Partial("~/Views/TaskTracker/_ViewTask.cshtml")
                </div>
            </fieldset>
        </div>
    </div>
</div>







