@{
    ViewBag.Title = "Auditing";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section AddToHead{
    <script src="~/Scripts/Lib/Angular/angular.min.js"></script>

    <link href="~/Content/SharedCss/bootstrap-duallistbox.css" rel="stylesheet" />
    <link href="~/Content/SharedCss/select2/select2.css" rel="stylesheet" />
    <script src="~/Scripts/Lib/Jquery/jquery-1.11.0.js"></script>
    @*<script src="~/Scripts/Custom/Profile/Util.js"></script>*@
    <script src="~/Scripts/Lib/Jquery/select2.min.js"></script>
    <script src="~/Scripts/Lib/Bootstrap/jquery.bootstrap-duallistbox.js"></script>
    @*<script src="~/Scripts/Custom/Credentialing/CredentialingInitNew.js"></script>*@
    <script src="~/Scripts/Lib/Angular/ng-table.js"></script>
    <script src="~/Areas/Credentialing/Scripts/InitiateCredentialingJS.js"> </script>
}
<style>
    .vertical-align {
        align-items: center;
    }

    .selectedProvider {
        background-color: rgb(150, 162, 225);
    }

    .imageStyle {
        padding: 0px;
        margin-bottom: 0px;
    }

    .alignTableStyle > thead > tr > th,
    .alignTableStyle > tbody > tr > th,
    .alignTableStyle > tfoot > tr > th,
    .alignTableStyle > thead > tr > td,
    .alignTableStyle > tbody > tr > td,
    .alignTableStyle > tfoot > tr > td {
        vertical-align: middle;
        text-align: center;
    }

    .marginTop {
        margin-top: 22px;
    }

    .marginBottom {
        margin-bottom: 22px;
    }

    .select2-container {
        display: block;
    }
</style>


<div id="page-wrapper" ng-app="InitCredApp" ng-cloak>
    <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="fa fa-dashboard"></i>  <a href="@System.Configuration.ConfigurationManager.AppSettings["AppRoot"]/Dashboard/Index">Dashboard</a>
                    </li>
                    <li class="active">
                        <i class="fa fa-search"></i> Audits
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
        <div role="tabpanel">

            <div class="col-lg-12 zero-padding-left-right-div">
                <!-- Tab panes -->
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="provider">
                        <!---Provider partial starts here-->
                        @Html.Partial("~/Areas/Credentialing/Views/CnD/_AuditSearchProvider.cshtml")

                        <!---Provider partial ends here-->
                    </div>

                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<!-- /#page-wrapper -->
