<div class="panel-group" id="accordiondea" role="tablist" aria-multiselectable="true">
    @*<div class="panel panel-default">
            <div class="panel-heading text-right">
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="left" title="Expand All" onclick="expandAll(this)"><i class="fa fa-minus-square-o"></i></button>
                <button class="btn btn-xs btn-primary" data-toggle="tooltip" data-placement="right" title="Collapse All" onclick="collapseAll(this)"><i class="fa fa-plus-square-o"></i></button>
            </div>
        </div>*@
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="deaHeading">
            <a data-toggle="collapse" data-parent="#accordiondea" href="#dea" aria-expanded="false" aria-controls="dea">
               DEA Information
                <span class="collapsed pull-right">
                    <span class="glyphicon glyphicon-plus"></span>
                </span>
            </a>
        </div>
        <div id="dea" class="panel-collapse collapse" role="tabpanel" aria-labelledby="deaHeading">
            <div class="panel-body table-bordered">
                <div class="form-group">
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">DEA Number</label>
                                <div>
                                    <input type="text" class="form-control input-sm" required />
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label class="control-label small">State of registration</label>
                                <div>
                                    <select class="form-control input-sm" required>
                                        <option>License State</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group col-md-6">
                                <label class="control-label small">Issue Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>

                            <div class="form-group col-md-6">
                                <label for="MobileNo" class="control-label small">Expiration Date</label>
                                <div>
                                    <input type="date" class="form-control input-sm" />
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="Contact" class="col-md-3 control-label small">Schedules</label>
                                <div class="checkbox">
                                    <label class="checkbox-inline control-label small">
                                        <input type="checkbox" name="Contact"> Schedule I
                                    </label>
                                    <label class="checkbox-inline control-label small">
                                        <input type="checkbox" name="Contact"> Schedule II
                                    </label>
                                    <label class="checkbox-inline control-label small">
                                        <input type="checkbox" name="Contact"> Schedule III
                                    </label>
                                    <label class="checkbox-inline control-label small">
                                        <input type="checkbox" name="Contact"> Schedule IV
                                    </label>
                                    <label class="checkbox-inline control-label small">
                                        <input type="checkbox" name="Contact"> Schedule V
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="Contact" class="col-md-4 control-label small">License in  Good Standing?</label>
                                <div class="col-md-8 radio">
                                    <label class="radio-inline">
                                        <input type="radio" name="DEAStanding"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="DEAStanding"> No
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="col-md-6">
                                    <label for="Contact" class="control-label small">Limited or Restricted</label>
                                    <div>
                                        <select class="form-control input-sm" required>
                                            <option>Restricted</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <label for="MobileNo" class="control-label small">Explain Restriction</label>
                                    <div>
                                        <textarea class="form-control input-sm"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="Contact" class="col-md-4 control-label small">Do you have a State Controlled Substance Registration certificate?</label>
                                <div class="col-md-8 radio">
                                    <label class="radio-inline">
                                        <input type="radio" name="CSD" checked="checked"> Yes
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" name="CSD"> No
                                    </label>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <div class="col-md-12 panel-footer">
                        <div class="col-md-12">
                            <button class="btn btn-success pull-right">Cancel</button>
                            <button class="btn btn-warning">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




